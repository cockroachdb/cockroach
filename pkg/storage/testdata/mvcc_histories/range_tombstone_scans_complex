scan: "h" -> /BYTES/h3 @3.000000000,0
scan: "k" -> /BYTES/k5 @5.000000000,0

# Forward scans at all timestamps with tombstones and intents.
run ok
scan k=a end=z ts=1 tombstones inconsistent
----
scan: "a"-"z" -> <no data>

