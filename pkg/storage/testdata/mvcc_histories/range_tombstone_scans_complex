# Forward scans at all timestamps.
run ok
scan k=a end=z ts=1
----
scan: "a"-"z" -> <no data>

run ok
scan k=a end=z ts=2
----
scan: "a" -> /BYTES/a2 @2.000000000,0
scan: "d" -> /BYTES/d2 @2.000000000,0
scan: "f" -> /BYTES/f2 @2.000000000,0
scan: "g" -> /BYTES/g2 @2.000000000,0

run ok
scan k=a end=z ts=3
----
