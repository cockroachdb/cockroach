# Set up some point keys, point tombstones x, range tombstones o--o,
# and intents []. The initial state is:
#
# 9
# 8                         [g8]
# 7
# 6          o-------------------------------o
# 5
# 4  o---------------------------------------o
# 3
# 2                  x
# 1              d1  e1  f1  g1      1   1
#    a   b   c   d   e   f   g   h   i   j   k
run ok
put k=d ts=1 v=d1
put k=e ts=1 v=e1
del k=e ts=2
put k=f ts=1 v=f1
put k=g ts=1 v=g1
increment k=i ts=1
increment k=j ts=1
del_range_ts k=a end=k ts=3
del_range_ts k=c end=k ts=5
with t=A ts=8
  txn_begin
  put k=g v=8
----
inc: current value = 1
inc: current value = 1
>> at end:
txn: "A" meta={id=00000000 key=/Min pri=0.00000000 epo=0 ts=8.000000000,0 min=0,0 seq=0} lock=true stat=PENDING rts=8.000000000,0 wto=false gul=0,0
rangekey: {a-c}/[3.000000000,0]
rangekey: {c-k}/[5.000000000,0 3.000000000,0]
data: "d"/1.000000000,0 -> /BYTES/d1
data: "e"/2.000000000,0 -> /<empty>
data: "e"/1.000000000,0 -> /BYTES/e1
data: "f"/1.000000000,0 -> /BYTES/f1
meta: "g"/0,0 -> txn={id=00000000 key=/Min pri=0.00000000 epo=0 ts=8.000000000,0 min=0,0 seq=0} ts=8.000000000,0 del=false klen=12 vlen=6 mergeTs=<nil> txnDidNotUpdateMeta=true
