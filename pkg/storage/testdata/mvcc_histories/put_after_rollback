run ok
put k=k5 v=foo ts=3
with t=B k=k5
  txn_begin ts=5
  txn_step  seq=10
  put       v=a
  txn_step  seq=20
  put       v=b
  txn_step  seq=30
  txn_ignore_seqs seqs=(15-25)
  put       v=c
  check_intent
  txn_step  seq=40
  txn_ignore_seqs seqs=(5-35)
  put       v=d
  check_intent
  resolve_intent status=COMMITTED
