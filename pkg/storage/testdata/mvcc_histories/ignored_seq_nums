data: "n"/40.000000000,0 -> /BYTES/c


# Do a push. The intent should remain, unmodified, save for the timestamp.
run ok
with t=D
  check_intent   k=n
  get            k=n
  txn_advance    ts=45
  resolve_intent k=n status=PENDING
  check_intent   k=n
  get            k=n
----
meta: "n" -> txn={id=00000000 key=/Min pri=0.00000000 epo=0 ts=40.000000000,0 min=0,0 seq=30} ts=40.000000000,0 del=false klen=12 vlen=6 ih={{10 /BYTES/a}{20 /BYTES/b}}
