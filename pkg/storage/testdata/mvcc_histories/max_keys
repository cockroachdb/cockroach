# Same case as above, except with a committed value at the key after MaxKeys.

run ok
with t=B ts=12,0 max=3
  txn_begin
  txn_step  seq=10
  put       k=k  v=a
  put       k=l  v=a
  put       k=m  v=a
  txn_step  seq=20
  put       k=k  v=b
  put       k=l  v=b
  put       k=m  v=b
  txn_step  seq=30
  put       k=k  v=c
  put       k=l  v=c
  put       k=m  v=c
  txn_step  seq=20
  scan      k=k end=o
