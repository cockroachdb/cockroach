# WholeRows on non-SQL row data.
run ok
with ts=10,0
  scan k=a end=z max=3 wholeRows
----
scan: "a" -> /BYTES/val-a @1.000000000,0
scan: "c" -> /BYTES/val-c @1.000000000,0
scan: "e" -> /BYTES/val-e @1.000000000,0
