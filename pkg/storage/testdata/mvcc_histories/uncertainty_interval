# Setup:
#
# k1: value  @ ts 20
# k1: value  @ ts 10
#
# k2: intent @ ts 20
# k2: value  @ ts 10
#

run ok
with k=k1
  put v=v ts=10,0
  put v=v2 ts=20,0
----
>> at end:
data: "k1"/20.000000000,0 -> /BYTES/v2
data: "k1"/10.000000000,0 -> /BYTES/v

run ok
with k=k2
  put v=v3 ts=10,0
  txn_begin t=A ts=20,0
  put t=A v=v4
----
>> at end:
txn: "A" meta={id=00000000 key="k2" pri=0.00000000 epo=0 ts=20.000000000,0 min=0,0 seq=0} lock=true stat=PENDING rts=20.000000000,0 wto=false gul=0,0
data: "k1"/20.000000000,0 -> /BYTES/v2
data: "k1"/10.000000000,0 -> /BYTES/v
meta: "k2"/0,0 -> txn={id=00000000 key="k2" pri=0.00000000 epo=0 ts=20.000000000,0 min=0,0 seq=0} ts=20.000000000,0 del=false klen=12 vlen=7
data: "k2"/20.000000000,0 -> /BYTES/v4
data: "k2"/10.000000000,0 -> /BYTES/v3

# Test cases:
# 
# for ts in (5, 10, 15, 20, 25):
#   for globalUncertaintyLimit in (5, 10, 15, 20, 25):
#     if globalUncertaintyLimit < ts: continue
#     for k in (k1, k2):
#       for op in (get, scan):
#         testCase()
#

run ok
txn_begin t=txn1 ts=5,0 globalUncertaintyLimit=5,0
----
>> at end:
txn: "txn1" meta={id=00000000 key=/Min pri=0.00000000 epo=0 ts=5.000000000,0 min=0,0 seq=0} lock=true stat=PENDING rts=5.000000000,0 wto=false gul=5.000000000,0

run ok
get t=txn1 k=k1
----
get: "k1" -> <no data>

run ok
scan t=txn1 k=k1
----
scan: "k1"-"k1\x00" -> <no data>

run ok
get t=txn1 k=k2
----
get: "k2" -> <no data>

run ok
scan t=txn1 k=k2
----
scan: "k2"-"k2\x00" -> <no data>


run ok
