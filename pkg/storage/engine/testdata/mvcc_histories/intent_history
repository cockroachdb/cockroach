## Write the base (default) value.

run ok
with t=A
  begin_txn  ts=1
  mvcc_put   k=a v=default
  commit_txn
----
txn: "A" meta={id=00000000 key=/Min pri=0.00000000 epo=0 ts=0.000000001,0 min=0.000000000,0 seq=1} rw=true stat=COMMITTED orig=0.000000001,0 max=0.000000000,0 rts=0.000000000,0 wto=false int=1
data: "k/a"/0.000000001,0 -> /BYTES/default

## See how the intent history evolves throughout the test.

run trace ok
with t=A
  begin_txn  ts=2
  with k=a
  mvcc_put   v=first
  step_txn
  mvcc_put   v=second
  step_txn   n=2
  mvcc_del
  step_txn   n=6
  mvcc_put   v=first
  commit_txn
----
>> begin_txn ts=2 t=A
txn: "A" meta={id=00000000 key=/Min pri=0.00000000 epo=0 ts=0.000000002,0 min=0.000000000,0 seq=1} rw=true stat=PENDING orig=0.000000002,0 max=0.000000000,0 rts=0.000000000,0 wto=false
>> mvcc_put v=first k=a t=A
meta: "k/a"/0.000000000,0 -> txn={id=00000000 key=/Min pri=0.00000000 epo=0 ts=0.000000002,0 min=0.000000000,0 seq=1} ts=0.000000002,0 del=false klen=12 vlen=10
data: "k/a"/0.000000002,0 -> /BYTES/first
data: "k/a"/0.000000001,0 -> /BYTES/default
>> step_txn k=a t=A
txn: "A" meta={id=00000000 key=/Min pri=0.00000000 epo=0 ts=0.000000002,0 min=0.000000000,0 seq=2} rw=true stat=PENDING orig=0.000000002,0 max=0.000000000,0 rts=0.000000000,0 wto=false
>> mvcc_put v=second k=a t=A
meta: "k/a"/0.000000000,0 -> txn={id=00000000 key=/Min pri=0.00000000 epo=0 ts=0.000000002,0 min=0.000000000,0 seq=2} ts=0.000000002,0 del=false klen=12 vlen=11 ih={{1 /BYTES/first}}
data: "k/a"/0.000000002,0 -> /BYTES/second
data: "k/a"/0.000000001,0 -> /BYTES/default
>> step_txn n=2 k=a t=A
txn: "A" meta={id=00000000 key=/Min pri=0.00000000 epo=0 ts=0.000000002,0 min=0.000000000,0 seq=4} rw=true stat=PENDING orig=0.000000002,0 max=0.000000000,0 rts=0.000000000,0 wto=false
>> mvcc_del k=a t=A
meta: "k/a"/0.000000000,0 -> txn={id=00000000 key=/Min pri=0.00000000 epo=0 ts=0.000000002,0 min=0.000000000,0 seq=4} ts=0.000000002,0 del=true klen=12 vlen=0 ih={{1 /BYTES/first}{2 /BYTES/second}}
data: "k/a"/0.000000002,0 -> /<empty>
data: "k/a"/0.000000001,0 -> /BYTES/default
>> step_txn n=6 k=a t=A
txn: "A" meta={id=00000000 key=/Min pri=0.00000000 epo=0 ts=0.000000002,0 min=0.000000000,0 seq=10} rw=true stat=PENDING orig=0.000000002,0 max=0.000000000,0 rts=0.000000000,0 wto=false
>> mvcc_put v=first k=a t=A
meta: "k/a"/0.000000000,0 -> txn={id=00000000 key=/Min pri=0.00000000 epo=0 ts=0.000000002,0 min=0.000000000,0 seq=10} ts=0.000000002,0 del=false klen=12 vlen=10 ih={{1 /BYTES/first}{2 /BYTES/second}{4 /<empty>}}
data: "k/a"/0.000000002,0 -> /BYTES/first
data: "k/a"/0.000000001,0 -> /BYTES/default
>> commit_txn k=a t=A
txn: "A" meta={id=00000000 key=/Min pri=0.00000000 epo=0 ts=0.000000002,0 min=0.000000000,0 seq=10} rw=true stat=COMMITTED orig=0.000000002,0 max=0.000000000,0 rts=0.000000000,0 wto=false int=2
data: "k/a"/0.000000002,0 -> /BYTES/first
data: "k/a"/0.000000001,0 -> /BYTES/default
