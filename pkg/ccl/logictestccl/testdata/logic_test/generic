# Now use a more complex query that could have multiple join orderings.
statement ok
PREPARE p AS
SELECT * FROM t
JOIN c ON t.k = c.a
JOIN g ON c.k = g.a
WHERE t.a = $1 AND t.c = $2

statement ok
EXECUTE p(1, 2);
EXECUTE p(10, 20);
EXECUTE p(100, 200);
EXECUTE p(1000, 2000);

# The first five executions will use a custom plan. This is the fifth.
query T
EXPLAIN ANALYZE EXECUTE p(10000, 20000)
