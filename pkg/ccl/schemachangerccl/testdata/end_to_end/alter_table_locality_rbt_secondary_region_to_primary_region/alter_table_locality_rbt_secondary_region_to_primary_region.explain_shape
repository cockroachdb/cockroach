/* setup */
CREATE DATABASE multiregion_db PRIMARY REGION "us-east1" REGIONS "us-east2", "us-east3";
CREATE TABLE multiregion_db.public.t (  
  pk INT PRIMARY KEY,
  i INT NOT NULL,
  j STRING,
  INDEX(i)
) LOCALITY REGIONAL BY TABLE IN "us-east2";

/* test */
EXPLAIN (DDL, SHAPE) ALTER TABLE multiregion_db.t SET LOCALITY REGIONAL BY TABLE;
----
Schema change plan for ALTER TABLE ‹multiregion_db›.‹t› SET LOCALITY REGIONAL BY TABLE IN PRIMARY REGION;
 └── execute 1 system table mutations transaction
