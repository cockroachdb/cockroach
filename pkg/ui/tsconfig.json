{
  "compilerOptions": {
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "jsx": "preserve",
    "module": "commonjs",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": false,
    "target": "es6"
  },
  "include": [
    "./src/index.tsx",
    "./src/interfaces/custom.d.ts",
    "./generated/protos.d.ts"
  ]
}
