system-sql
SELECT tenant_id, name, value FROM system.tenant_settings
WHERE name IN (
'sql.scatter.allow_for_secondary_tenant.enabled',
'sql.split_at.allow_for_secondary_tenant.enabled'
)
ORDER BY tenant_id, name
