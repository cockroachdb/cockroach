init
----

# Initialization. Will get 1600ms of tokens.
adjust time-millis=500 cpu-millis=1000 cpu-count=2
----
----
adjust
  setTenantCPUTokensBurstLimit(1600.0 millis, true)
CTTA t=500.0 millis: total-cpu=1000.0 millis, last-tokens=1600.0 millis, mult=1.0
  tokens: 1600.0 millis, 0.0 millis, tenant: 1600.0 millis, 0.0 millis
  granter: tokens: 1600.0 millis

remaining tick 1000
  tenantCPUTokensTick(1.6 millis)
CTTA t=500.0 millis: total-cpu=1000.0 millis, last-tokens=1600.0 millis, mult=1.0
  tokens: 1600.0 millis, 1.6 millis, tenant: 1600.0 millis, 1.6 millis
  granter: tokens: 1600.0 millis

remaining tick 999
  tenantCPUTokensTick(1.6 millis)
CTTA t=500.0 millis: total-cpu=1000.0 millis, last-tokens=1600.0 millis, mult=1.0
  tokens: 1600.0 millis, 3.2 millis, tenant: 1600.0 millis, 3.2 millis
  granter: tokens: 1600.0 millis

remaining tick 1
  tenantCPUTokensTick(1.6 millis)
CTTA t=500.0 millis: total-cpu=1000.0 millis, last-tokens=1600.0 millis, mult=1.0
  tokens: 1600.0 millis, 1600.0 millis, tenant: 1600.0 millis, 1600.0 millis
  granter: tokens: 1600.0 millis

----
----

# Burst consumes 3200ms, i.e., exhausts the initial burst capacity and all the
# 1600ms of tokens added in the last 1s. Only 2000ms of cpu time is consumed,
# since CPU can't burst beyond its capacity, so work must be waiting in the
# goroutine scheduler. We recognize this and mult remains 1.0 (instead of
# becoming < 1.0). Since there are no excessTokens used, the tenant tokens
# also remain 1600ms/s.
adjust time-millis=1500 cpu-millis=3000 cpu-count=2 int-tokens=3200 cur-tokens-in-bucket=-10
----
----
adjust
  setTenantCPUTokensBurstLimit(1600.0 millis, true)
CTTA t=1500.0 millis: total-cpu=3000.0 millis, last-tokens=0.0 millis, mult=1.0
  tokens: 1600.0 millis, 0.0 millis, tenant: 1600.0 millis, 0.0 millis
  granter: tokens: 0.0 millis

remaining tick 1000
  tenantCPUTokensTick(1.6 millis)
CTTA t=1500.0 millis: total-cpu=3000.0 millis, last-tokens=0.0 millis, mult=1.0
  tokens: 1600.0 millis, 1.6 millis, tenant: 1600.0 millis, 1.6 millis
  granter: tokens: 1.6 millis

remaining tick 999
  tenantCPUTokensTick(1.6 millis)
CTTA t=1500.0 millis: total-cpu=3000.0 millis, last-tokens=0.0 millis, mult=1.0
  tokens: 1600.0 millis, 3.2 millis, tenant: 1600.0 millis, 3.2 millis
  granter: tokens: 3.2 millis

remaining tick 1
  tenantCPUTokensTick(1.6 millis)
CTTA t=1500.0 millis: total-cpu=3000.0 millis, last-tokens=0.0 millis, mult=1.0
  tokens: 1600.0 millis, 1600.0 millis, tenant: 1600.0 millis, 1600.0 millis
  granter: tokens: 1600.0 millis

----
----

# Again consumes 2000ms of cpu, while only 1600ms of tokens are consumed. This
# is because of the backlog in the goroutine scheduler, but we can't tell what
# the backlog is so mult becomes 1.1, which means tokens reduce to 1422.2ms/s.
# There were no excess tokens used, so tenant token rate stays equal to token
# rate.
adjust time-millis=2500 cpu-millis=5000 cpu-count=2 int-tokens=1600 cur-tokens-in-bucket=-10
----
----
adjust
  setTenantCPUTokensBurstLimit(1422.2 millis, true)
CTTA t=2500.0 millis: total-cpu=5000.0 millis, last-tokens=0.0 millis, mult=1.1
  tokens: 1422.2 millis, 0.0 millis, tenant: 1422.2 millis, 0.0 millis
  granter: tokens: 0.0 millis

remaining tick 1000
  tenantCPUTokensTick(1.4 millis)
CTTA t=2500.0 millis: total-cpu=5000.0 millis, last-tokens=0.0 millis, mult=1.1
  tokens: 1422.2 millis, 1.4 millis, tenant: 1422.2 millis, 1.4 millis
  granter: tokens: 1.4 millis

remaining tick 999
  tenantCPUTokensTick(1.4 millis)
CTTA t=2500.0 millis: total-cpu=5000.0 millis, last-tokens=0.0 millis, mult=1.1
  tokens: 1422.2 millis, 2.8 millis, tenant: 1422.2 millis, 2.8 millis
  granter: tokens: 2.8 millis

remaining tick 1
  tenantCPUTokensTick(1.4 millis)
CTTA t=2500.0 millis: total-cpu=5000.0 millis, last-tokens=0.0 millis, mult=1.1
  tokens: 1422.2 millis, 1422.2 millis, tenant: 1422.2 millis, 1422.2 millis
  granter: tokens: 1422.2 millis

----
----

# Same as previous. The mult further increases to 1.2 which causes token rate
# to decline. Also the 1600 tokens consumed is higher than the 1422ms of
# tokens that were expected so tenant token rate is reduced.
adjust time-millis=3500 cpu-millis=7000 cpu-count=2 int-tokens=1600 cur-tokens-in-bucket=-10
----
----
