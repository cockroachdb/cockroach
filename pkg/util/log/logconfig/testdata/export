# Default config
yaml
----
@startuml
left to right direction
component sources {
() DEV
() OPS
() HEALTH
() STORAGE
() SESSIONS
() SQL_SCHEMA
() USER_ADMIN
() PRIVILEGES
() SENSITIVE_ACCESS
() SQL_EXEC
() SQL_PERF
() SQL_INTERNAL_PERF
() TELEMETRY
() KVPROBER_TRACES
cloud stray as "stray\nerrors"
}
queue stderr
card buffer2 as "buffer"
card p__1 as "format:crdb-v2"
artifact files {
 folder "/default-dir" {
  file f1 as "cockroach.log"
  file stderrfile as "cockroach-stderr.log"
 }
}
DEV --> p__1
OPS --> p__1
HEALTH --> p__1
STORAGE --> p__1
SESSIONS --> p__1
SQL_SCHEMA --> p__1
USER_ADMIN --> p__1
PRIVILEGES --> p__1
SENSITIVE_ACCESS --> p__1
SQL_EXEC --> p__1
SQL_PERF --> p__1
SQL_INTERNAL_PERF --> p__1
TELEMETRY --> p__1
KVPROBER_TRACES --> p__1
p__1 --> buffer2
buffer2 --> f1
stray --> stderrfile
@enduml
