# Default config
yaml
----
@startuml
left to right direction
component sources {
() DEV
() STORAGE
() SESSIONS
() SENSITIVE_ACCESS
() SQL_EXEC
() SQL_PERF
() SQL_INTERNAL_PERF
cloud stray as "stray\nerrors"
}
queue stderr
card p__1 as "format:crdb-v1"
artifact files {
 folder "/default-dir" {
  file f1 as "cockroach.log"
  file stderrfile as "cockroach-stderr.log"
 }
}
DEV --> p__1
STORAGE --> p__1
SESSIONS --> p__1
SENSITIVE_ACCESS --> p__1
SQL_EXEC --> p__1
SQL_PERF --> p__1
SQL_INTERNAL_PERF --> p__1
p__1 --> f1
stray --> stderrfile
@enduml
