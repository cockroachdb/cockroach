# Capture everything to one file with sync and warnings only to stderr.
yaml only-channels=DEV,SESSIONS
sinks:
  file-groups:
    everything:
      sync-writes: true
      redact: true
      channels: ALL
  stderr:
    filter: WARNING
  fluent-servers:
    local:
      channels: SESSIONS
      redactable: false
      address: localhost:5170
----
@startuml
left to right direction
component sources {
() DEV
() SESSIONS
cloud stray as "stray\nerrors"
}
queue stderr
card sync2 as "sync"
card p__1 as "redact"
card p__2 as "format:crdb-v1"
card p__3 as "strip"
card p__4 as "format:json-fluent-compact"
card p__5 as "format:crdb-v1-tty"
card p__6 as "filter:W"
artifact files {
 folder "/default-dir" {
  file f1 as "cockroach-everything.log"
  file stderrfile as "cockroach-stderr.log"
 }
}
cloud network {
 queue s__local as "fluent: tcp:localhost:5170"
}
DEV --> p__2
SESSIONS --> p__2
p__1 --> sync2
p__2 --> p__1
sync2 --> f1
stray --> stderrfile
SESSIONS --> p__4
p__3 ..> s__local
p__4 --> p__3
DEV --> p__6
SESSIONS --> p__6
p__5 --> stderr
p__6 --> p__5
@enduml
