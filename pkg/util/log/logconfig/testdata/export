# Capture everything to one file with sync and warnings only to stderr.
yaml only-channels=DEV,SESSIONS
sinks:
  file-groups:
    everything:
      sync-writes: true
      redact: true
      channels: ALL
  stderr:
    filter: WARNING
----
@startuml
left to right direction
component sources {
() DEV
() SESSIONS
cloud stray as "stray\nerrors"
}
queue stderr
card sync2 as "sync"
card p__1 as "redact"
card p__2 as "format:crdb-v1"
card p__3 as "format:crdb-v1-tty"
card p__4 as "filter:W"
artifact files {
 folder "/default-dir" {
  file f1 as "cockroach-everything.log"
  file stderrfile as "cockroach-stderr.log"
 }
}
DEV --> p__2
SESSIONS --> p__2
p__1 --> sync2
p__2 --> p__1
sync2 --> f1
stray --> stderrfile
DEV --> p__4
SESSIONS --> p__4
p__3 --> stderr
p__4 --> p__3
@enduml
