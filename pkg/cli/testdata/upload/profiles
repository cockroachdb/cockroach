# Colliding tags - env provided by the user should take precedence
upload-profiles tags=env:SH
{
  "nodes": {
    "1": {
      "profiles": [
        {"type": "cpu", "timestamp": 1718972610, "duration": 20},
        {"type": "heap", "timestamp": 1718974401, "duration": 20}
      ]
    }
  }
}
