CREATE TABLE public.t (
	i INT8 NULL,
	f FLOAT8 NULL,
	s STRING NULL,
	b BYTES NULL,
	d DATE NULL,
	t TIME NULL,
	ts TIMESTAMP NULL,
	n INTERVAL NULL,
	o BOOL NULL,
	e DECIMAL NULL,
	u UUID NULL,
	ip INET NULL,
	j JSONB NULL,
	ary STRING[] NULL,
	tz TIMESTAMPTZ NULL,
	e1 DECIMAL(2) NULL,
	e2 DECIMAL(2,1) NULL,
	s1 STRING(1) NULL,
	s2 STRING COLLATE en_u_ks_level2 NULL,
	oi OID NULL,
	FAMILY "primary" (i, f, d, t, ts, n, o, u, ip, j, ary, tz, e1, e2, s1, s2, oi, rowid),
	FAMILY fam_1_s (s),
	FAMILY fam_2_b (b),
	FAMILY fam_3_e (e)
);

