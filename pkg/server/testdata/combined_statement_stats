query sort=PCT_RUNTIME limit=100 internal=false
----
--WHERE--
 WHERE app_name NOT LIKE '$ internal%' AND app_name NOT LIKE '$$ %'
--ORDER AND LIMIT--
 ORDER BY ((statistics -> 'statistics' -> 'svcLat' ->> 'mean')::FLOAT *
                         (statistics -> 'statistics' ->> 'cnt')::FLOAT) DESC LIMIT $1
--ARGS--
[20000]

