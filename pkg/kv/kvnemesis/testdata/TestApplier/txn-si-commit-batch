db0.Txn(ctx, func(ctx context.Context, txn *kv.Txn) error {
  txn.SetIsoLevel(isolation.Snapshot)
  txn.Put(ctx, tk(5), sv(5)) // @<ts> <nil>
  b := &kv.Batch{}
  b.Get(tk(1)) // (<nil>, <nil>)
  b.Put(tk(6), sv(6)) // <nil>
  txn.CommitInBatch(ctx, b) // @<ts> <nil>
  return nil
}) // @<ts> <nil>
