db0.Txn(ctx, func(ctx context.Context, txn *kv.Txn) error {
  txn.SetIsoLevel(isolation.Serializable)
  txn.Put(ctx, tk(5), sv(5)) // @<ts> <nil>
  {
    b := &kv.Batch{}
    b.Put(tk(6), sv(6)) // <nil>
    b.DelRange(tk(3), tk(5), true /* @s1 */) // <nil>
    txn.Run(ctx, b) // @<ts> <nil>
  }
  return nil
}) // @<ts> <nil>
// ^-- txnpb:<txn>
