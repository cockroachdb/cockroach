db1.Txn(ctx, func(ctx context.Context, txn *kv.Txn) error {
  txn.Put(ctx, uint64ToKey(5), seq2val(kvnemesisutil.Seq(5))) // @<ts> <nil>
  {
    b := &kv.Batch{}
    b.Put(uint64ToKey(6), seq2val(kvnemesisutil.Seq(6))) // <nil>
    b.DelRange(uint64ToKey(3), uint64ToKey(5), true /* @s1 */) // <nil>
    txn.Run(ctx, b) // @<ts> <nil>
  }
  return nil
}) // @<ts> <nil>
// ^-- txnpb:<txn>
