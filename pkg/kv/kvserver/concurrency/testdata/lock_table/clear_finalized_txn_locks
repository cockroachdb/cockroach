   waiting readers:
    req: 9, txn: 00000000-0000-0000-0000-000000000001
   distinguished req: 9
 lock: "d"
  holder: txn: 00000000-0000-0000-0000-000000000004, ts: 11.000000000,1, info: repl epoch: 0, seqs: [0]

release txn=txn3 span=b
----
num=2
 lock: "c"
  holder: txn: 00000000-0000-0000-0000-000000000004, ts: 11.000000000,1, info: repl epoch: 0, seqs: [0]
   waiting readers:
    req: 9, txn: 00000000-0000-0000-0000-000000000001
