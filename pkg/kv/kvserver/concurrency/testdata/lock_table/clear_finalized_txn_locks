   distinguished req: 5
 lock: "b"
  holder: txn: 00000000-0000-0000-0000-000000000002, ts: 11.000000000,1, info: repl epoch: 0, seqs: [0]
   queued writers:
    active: false req: 5, txn: 00000000-0000-0000-0000-000000000001

