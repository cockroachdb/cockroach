  name=f pri=high-pri done=true  waited=true  err=<nil>
  name=g pri=low-pri  done=false waited=false err=<nil>
range_id=2 tenant_id={1} local_replica_id=1

wait_for_eval name=h range_id=2 pri=HighPri
