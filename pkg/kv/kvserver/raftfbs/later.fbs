namespace raftfbs;

table Timestamp {
  wall: int64;
  logical: int32;
}

table Entry {
  version: ubyte; // raft cmd type (sideloaded or not) and raft type (conf change or not)
  term: uint64;
  index: uint64;
  cmd: Command;
}


