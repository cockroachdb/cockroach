setup
CREATE TABLE defaultdb.ownertbl (id INT NOT NULL);
CREATE SEQUENCE defaultdb.ownedseq OWNED BY defaultdb.ownertbl.id;
----

build
DROP SEQUENCE defaultdb.ownedseq CASCADE
