create-table
CREATE TABLE defaultdb.t1 (id INT PRIMARY KEY, name varchar(256))
----

create-view
CREATE VIEW defaultdb.v1 AS (SELECT name FROM defaultdb.t1)
----

build
DROP VIEW defaultdb.v1
----
- DROP View:{DescID: 53}
  state: PUBLIC
  details:
    dependedOnBy: []
