setup
CREATE SCHEMA db.sc;
CREATE TABLE db.sc.t (k INT, v STRING);
CREATE TYPE db.sc.e AS ENUM('a', 'b', 'c');
----
...
+schema {54 0 sc} -> 57
+object {54 57 t} -> 58
+object {54 57 e} -> 59
+object {54 57 _e} -> 60

