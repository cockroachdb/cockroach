  descriptor IDs: [53]
# end PreCommitPhase
commit transaction #1
# begin PostCommitPhase
begin transaction #2
commit transaction #2
# end PostCommitPhase

setup
CREATE SCHEMA db.sc;
CREATE TABLE db.sc.t (k INT, v STRING);
CREATE TYPE db.sc.e AS ENUM('a', 'b', 'c');
----
...
+schema {52 0 sc} -> 54
+object {52 54 t} -> 55
+object {52 54 e} -> 56
+object {52 54 _e} -> 57

