/* setup */
CREATE TABLE roaches();
set enable_row_level_security = on;
ALTER TABLE roaches ENABLE ROW LEVEL SECURITY;

/* test */
EXPLAIN (DDL, SHAPE) ALTER TABLE roaches DISABLE ROW LEVEL SECURITY;
