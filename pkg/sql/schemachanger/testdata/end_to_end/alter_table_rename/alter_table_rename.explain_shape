/* setup */
CREATE TABLE t1 (id INT PRIMARY KEY, data TEXT);
CREATE TABLE t2 (id INT PRIMARY KEY, value INT);
CREATE VIEW v1 AS SELECT * FROM t1;
CREATE SEQUENCE seq1 START 1;

/* test */
