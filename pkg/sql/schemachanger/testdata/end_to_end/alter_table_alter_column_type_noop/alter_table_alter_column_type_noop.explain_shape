/* setup */
CREATE TYPE e AS ENUM();
CREATE TABLE t (i INT PRIMARY KEY, j e);

/* test */
EXPLAIN (DDL, SHAPE) ALTER TABLE t ALTER COLUMN j SET DATA TYPE e;
