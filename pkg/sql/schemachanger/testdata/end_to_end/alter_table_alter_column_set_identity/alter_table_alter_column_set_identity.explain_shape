/* setup */
CREATE TABLE t (
    i INT,
    j INT NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 2 INCREMENT 3)
);

/* test */
EXPLAIN (DDL, SHAPE) ALTER TABLE t ALTER COLUMN j SET GENERATED BY DEFAULT;
