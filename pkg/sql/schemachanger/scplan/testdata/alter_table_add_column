- from: [TemporaryIndex:{DescID: 10005, IndexID: 5, ConstraintID: 5, SourceIndexID: 1}, TRANSIENT_DELETE_ONLY]
  to:   [IndexColumn:{DescID: 10005, ColumnID: 1, IndexID: 5}, TRANSIENT_ABSENT]
  kind: Precedence
  rule: index drop mutation visible before cleaning up index columns
- from: [TemporaryIndex:{DescID: 10005, IndexID: 5, ConstraintID: 5, SourceIndexID: 1}, TRANSIENT_DELETE_ONLY]
  to:   [IndexColumn:{DescID: 10005, ColumnID: 2, IndexID: 5}, TRANSIENT_ABSENT]
  kind: Precedence
  rule: index drop mutation visible before cleaning up index columns
- from: [TemporaryIndex:{DescID: 10005, IndexID: 5, ConstraintID: 5, SourceIndexID: 1}, TRANSIENT_DELETE_ONLY]
  to:   [PrimaryIndex:{DescID: 10005, IndexID: 4, ConstraintID: 4, TemporaryIndexID: 5, SourceIndexID: 1}, WRITE_ONLY]
  kind: Precedence
  rule: temp index disappeared before its master index reaches WRITE_ONLY
- from: [TemporaryIndex:{DescID: 10005, IndexID: 5, ConstraintID: 5, SourceIndexID: 1}, TRANSIENT_DELETE_ONLY]
  to:   [TemporaryIndex:{DescID: 10005, IndexID: 5, ConstraintID: 5, SourceIndexID: 1}, TRANSIENT_ABSENT]
  kind: PreviousTransactionPrecedence
  rule: TemporaryIndex transitions to TRANSIENT_ABSENT uphold 2-version invariant: TRANSIENT_DELETE_ONLY->TRANSIENT_ABSENT
- from: [TemporaryIndex:{DescID: 10005, IndexID: 5, ConstraintID: 5, SourceIndexID: 1}, WRITE_ONLY]
  to:   [IndexData:{DescID: 10005, IndexID: 5}, PUBLIC]
  kind: SameStagePrecedence
  rule: temp index data exists as soon as temp index accepts writes
- from: [TemporaryIndex:{DescID: 10005, IndexID: 5, ConstraintID: 5, SourceIndexID: 1}, WRITE_ONLY]
  to:   [PrimaryIndex:{DescID: 10005, IndexID: 4, ConstraintID: 4, TemporaryIndexID: 5, SourceIndexID: 1}, BACKFILLED]
  kind: Precedence
  rule: temp index is WRITE_ONLY before backfill
- from: [TemporaryIndex:{DescID: 10005, IndexID: 5, ConstraintID: 5, SourceIndexID: 1}, WRITE_ONLY]
  to:   [TemporaryIndex:{DescID: 10005, IndexID: 5, ConstraintID: 5, SourceIndexID: 1}, TRANSIENT_DELETE_ONLY]
  kind: PreviousTransactionPrecedence
  rule: TemporaryIndex transitions to TRANSIENT_ABSENT uphold 2-version invariant: WRITE_ONLY->TRANSIENT_DELETE_ONLY
