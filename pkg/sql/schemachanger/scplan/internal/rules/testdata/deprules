    - $column-node[CurrentStatus] = TRANSIENT_ABSENT
    - joinTargetNode($dependent, $dependent-target, $dependent-node)
    - joinTargetNode($column, $column-target, $column-node)
- name: dependents removed before index
  from: dependent-node
  kind: Precedence
  to: index-node
