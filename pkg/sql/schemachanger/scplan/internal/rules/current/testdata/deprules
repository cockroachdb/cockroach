- name: ensure columns are in increasing order
  from: later-column-Node
  kind: Precedence
  to: earlier-column-Node
  query:
    - $later-column[Type] = '*scpb.Column'
    - joinTargetNode($later-column, $later-column-Target, $later-column-Node)
