    - ToPublicOrTransient($later-column-Target, $earlier-column-Target)
    - $status IN [WRITE_ONLY, PUBLIC]
    - $later-column-Node[CurrentStatus] = $status
    - $earlier-column-Node[CurrentStatus] = $status
