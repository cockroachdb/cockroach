setup
CREATE TABLE parent (id INT PRIMARY KEY);
CREATE TABLE tbl (
  id INT PRIMARY KEY,
  name STRING NOT NULL,
  price DECIMAL(8,2),
  INDEX sec (name) STORING (price) WHERE (id > 0),
  CONSTRAINT myfk FOREIGN KEY (id) REFERENCES parent (id)
);
----

