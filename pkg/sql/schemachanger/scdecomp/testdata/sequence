setup
CREATE SEQUENCE seq;
CREATE TABLE tbl (
  id INT PRIMARY KEY,
  cexpr INT DEFAULT (nextval('seq')) ON UPDATE (123)
);
COMMENT ON TABLE tbl IS 'tbl is good table';
COMMENT ON INDEX tbl@tbl_pkey IS 'tbl_pkey is a primary key';
COMMENT ON COLUMN tbl.id IS 'id is a identifier';
----

