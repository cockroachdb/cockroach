parse
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE = 'foo'
----
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE = 'foo'
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE = ('foo') -- fully parenthesized
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE = '_' -- literals removed
ALTER DATABASE _ ALTER LOCALITY GLOBAL CONFIGURE ZONE = 'foo' -- identifiers removed

parse
EXPLAIN ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE = 'foo'
----
EXPLAIN ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE = 'foo'
EXPLAIN ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE = ('foo') -- fully parenthesized
EXPLAIN ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE = '_' -- literals removed
EXPLAIN ALTER DATABASE _ ALTER LOCALITY GLOBAL CONFIGURE ZONE = 'foo' -- identifiers removed

parse
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING foo = bar, baz = yay
----
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING foo = bar, baz = yay
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING foo = (bar), baz = (yay) -- fully parenthesized
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING foo = bar, baz = yay -- literals removed
ALTER DATABASE _ ALTER LOCALITY GLOBAL CONFIGURE ZONE USING _ = _, _ = _ -- identifiers removed

parse
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING foo.bar = yay
----
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING "foo.bar" = yay -- normalized!
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING "foo.bar" = (yay) -- fully parenthesized
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING "foo.bar" = yay -- literals removed
ALTER DATABASE _ ALTER LOCALITY GLOBAL CONFIGURE ZONE USING _ = _ -- identifiers removed

parse
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE DISCARD
----
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE DISCARD
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE DISCARD -- fully parenthesized
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE DISCARD -- literals removed
ALTER DATABASE _ ALTER LOCALITY GLOBAL CONFIGURE ZONE DISCARD -- identifiers removed

parse
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING DEFAULT
----
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING DEFAULT
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING DEFAULT -- fully parenthesized
ALTER DATABASE db ALTER LOCALITY GLOBAL CONFIGURE ZONE USING DEFAULT -- literals removed
ALTER DATABASE _ ALTER LOCALITY GLOBAL CONFIGURE ZONE USING DEFAULT -- identifiers removed

parse
ALTER DATABASE db ALTER LOCALITY REGIONAL CONFIGURE ZONE = 'foo'
----
ALTER DATABASE db ALTER LOCALITY REGIONAL CONFIGURE ZONE = 'foo'
ALTER DATABASE db ALTER LOCALITY REGIONAL CONFIGURE ZONE = ('foo') -- fully parenthesized
ALTER DATABASE db ALTER LOCALITY REGIONAL CONFIGURE ZONE = '_' -- literals removed
ALTER DATABASE _ ALTER LOCALITY REGIONAL CONFIGURE ZONE = 'foo' -- identifiers removed

parse
ALTER DATABASE db ALTER LOCALITY REGIONAL IN "us-west1" CONFIGURE ZONE = 'foo'
----
ALTER DATABASE db ALTER LOCALITY REGIONAL IN "us-west1" CONFIGURE ZONE = 'foo'
ALTER DATABASE db ALTER LOCALITY REGIONAL IN "us-west1" CONFIGURE ZONE = ('foo') -- fully parenthesized
ALTER DATABASE db ALTER LOCALITY REGIONAL IN "us-west1" CONFIGURE ZONE = '_' -- literals removed
ALTER DATABASE _ ALTER LOCALITY REGIONAL IN _ CONFIGURE ZONE = 'foo' -- identifiers removed
