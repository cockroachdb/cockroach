ALTER DATABASE a DROP REGION IF EXISTS "us-west-1" -- fully parenthesized
ALTER DATABASE a DROP REGION IF EXISTS "us-west-1" -- literals removed
ALTER DATABASE _ DROP REGION IF EXISTS _ -- identifiers removed

