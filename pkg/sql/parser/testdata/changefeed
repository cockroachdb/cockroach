parse
EXPERIMENTAL CHANGEFEED FOR TABLE foo
----
EXPERIMENTAL CHANGEFEED FOR foo -- normalized!
EXPERIMENTAL CHANGEFEED FOR (foo) -- fully parenthesized
EXPERIMENTAL CHANGEFEED FOR foo -- literals removed
EXPERIMENTAL CHANGEFEED FOR _ -- identifiers removed

parse
EXPERIMENTAL CHANGEFEED FOR TABLE foo FAMILY bar
----
EXPERIMENTAL CHANGEFEED FOR foo FAMILY bar -- normalized!
EXPERIMENTAL CHANGEFEED FOR (foo) FAMILY bar -- fully parenthesized
EXPERIMENTAL CHANGEFEED FOR foo FAMILY bar -- literals removed
EXPERIMENTAL CHANGEFEED FOR _ FAMILY _ -- identifiers removed

parse
EXPLAIN CREATE CHANGEFEED FOR TABLE foo INTO 'sink'
----
EXPLAIN CREATE CHANGEFEED FOR foo INTO 'sink' -- normalized!
EXPLAIN CREATE CHANGEFEED FOR (foo) INTO ('sink') -- fully parenthesized
EXPLAIN CREATE CHANGEFEED FOR foo INTO '_' -- literals removed
EXPLAIN CREATE CHANGEFEED FOR _ INTO 'sink' -- identifiers removed

parse
CREATE CHANGEFEED FOR foo INTO 'sink'
----
CREATE CHANGEFEED FOR foo INTO 'sink'
CREATE CHANGEFEED FOR (foo) INTO ('sink') -- fully parenthesized
CREATE CHANGEFEED FOR foo INTO '_' -- literals removed
CREATE CHANGEFEED FOR _ INTO 'sink' -- identifiers removed

parse
CREATE CHANGEFEED FOR TABLE foo INTO sink
----
CREATE CHANGEFEED FOR foo INTO 'sink' -- normalized!
CREATE CHANGEFEED FOR (foo) INTO ('sink') -- fully parenthesized
CREATE CHANGEFEED FOR foo INTO '_' -- literals removed
CREATE CHANGEFEED FOR _ INTO 'sink' -- identifiers removed

parse
CREATE CHANGEFEED FOR TABLE foo, db.bar, foo FAMILY bar, schema.db.foo INTO 'sink'
----
CREATE CHANGEFEED FOR foo, db.bar, foo FAMILY bar, schema.db.foo INTO 'sink' -- normalized!
CREATE CHANGEFEED FOR (foo), (db.bar), (foo) FAMILY bar, (schema.db.foo) INTO ('sink') -- fully parenthesized
CREATE CHANGEFEED FOR foo, db.bar, foo FAMILY bar, schema.db.foo INTO '_' -- literals removed
CREATE CHANGEFEED FOR _, _._, _ FAMILY _, _._._ INTO 'sink' -- identifiers removed

parse
CREATE CHANGEFEED FOR TABLE foo INTO 'sink'
----
CREATE CHANGEFEED FOR foo INTO 'sink' -- normalized!
CREATE CHANGEFEED FOR (foo) INTO ('sink') -- fully parenthesized
CREATE CHANGEFEED FOR foo INTO '_' -- literals removed
CREATE CHANGEFEED FOR _ INTO 'sink' -- identifiers removed

