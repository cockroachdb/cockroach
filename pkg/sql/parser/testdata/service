CREATE SERVICE abc WITH DEFAULT OPTIONS (a = '_') USING RULES FROM TABLE cde -- literals removed
CREATE SERVICE _ WITH DEFAULT OPTIONS (_ = 'b') USING RULES FROM TABLE _ -- identifiers removed

