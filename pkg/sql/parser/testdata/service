parse
CREATE SERVICE abc
----
CREATE SERVICE abc
CREATE SERVICE abc -- fully parenthesized
CREATE SERVICE abc -- literals removed
CREATE SERVICE _ -- identifiers removed

parse
CREATE SERVICE abc WITH DEFAULT OPTIONS (a = b)
----
CREATE SERVICE abc WITH DEFAULT OPTIONS (a = 'b') -- normalized!
CREATE SERVICE abc WITH DEFAULT OPTIONS (a = ('b')) -- fully parenthesized
CREATE SERVICE abc WITH DEFAULT OPTIONS (a = '_') -- literals removed
CREATE SERVICE _ WITH DEFAULT OPTIONS (_ = 'b') -- identifiers removed

parse
CREATE SERVICE abc USING RULES FROM TABLE cde
----
CREATE SERVICE abc USING RULES FROM TABLE cde
CREATE SERVICE abc USING RULES FROM TABLE cde -- fully parenthesized
CREATE SERVICE abc USING RULES FROM TABLE cde -- literals removed
CREATE SERVICE _ USING RULES FROM TABLE _ -- identifiers removed

