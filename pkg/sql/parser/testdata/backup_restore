parse
BACKUP TABLE foo TO 'bar'
----
BACKUP TABLE foo TO 'bar'
BACKUP TABLE (foo) TO ('bar') -- fully parenthesized
BACKUP TABLE foo TO '_' -- literals removed
BACKUP TABLE _ TO 'bar' -- identifiers removed

parse
BACKUP foo TO 'bar'
----
BACKUP TABLE foo TO 'bar' -- normalized!
BACKUP TABLE (foo) TO ('bar') -- fully parenthesized
BACKUP TABLE foo TO '_' -- literals removed
BACKUP TABLE _ TO 'bar' -- identifiers removed

parse
BACKUP TO 'bar'
----
