BACKUP TABLE foo INTO LATEST IN '_' WITH incremental_location = '_' -- literals removed
BACKUP TABLE _ INTO LATEST IN 'bar' WITH incremental_location = 'baz' -- identifiers removed

parse
BACKUP TABLE foo INTO 'subdir' IN 'bar'
----
