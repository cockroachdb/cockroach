----
CREATE INVERTED INDEX a ON b (c) -- normalized!
CREATE INVERTED INDEX a ON b (c) -- fully parenthesized
CREATE INVERTED INDEX a ON b (c) -- literals removed
CREATE INVERTED INDEX _ ON _ (_) -- identifiers removed

parse
