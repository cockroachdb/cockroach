CREATE TABLE a AS SELECT (*) FROM b LIMIT (3) -- fully parenthesized
CREATE TABLE a AS SELECT * FROM b LIMIT _ -- literals removed
CREATE TABLE _ AS SELECT * FROM _ LIMIT 3 -- identifiers removed

