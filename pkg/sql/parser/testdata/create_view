CREATE OR REPLACE VIEW a AS SELECT (*) FROM b -- fully parenthesized
CREATE OR REPLACE VIEW a AS SELECT * FROM b -- literals removed
CREATE OR REPLACE VIEW _ AS SELECT * FROM _ -- identifiers removed

parse
