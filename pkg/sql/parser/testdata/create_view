parse
CREATE VIEW a AS SELECT * FROM b
----
CREATE VIEW a AS SELECT * FROM b
CREATE VIEW a AS SELECT (*) FROM b -- fully parenthesized
CREATE VIEW a AS SELECT * FROM b -- literals removed
CREATE VIEW _ AS SELECT * FROM _ -- identifiers removed

