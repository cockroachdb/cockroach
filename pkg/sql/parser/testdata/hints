parse
SELECT 'a' FROM t@bar
----
SELECT 'a' FROM t@bar
SELECT ('a') FROM t@bar -- fully parenthesized
SELECT '_' FROM t@bar -- literals removed
SELECT 'a' FROM _@_ -- identifiers removed

