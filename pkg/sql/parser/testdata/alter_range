ALTER RANGE RELOCATE NONVOTERS TO ((1) + (1)) FROM ((1) + (2)) FOR SELECT (range_id) FROM foo -- fully parenthesized
ALTER RANGE RELOCATE NONVOTERS TO _ + _ FROM _ + _ FOR SELECT range_id FROM foo -- literals removed
ALTER RANGE RELOCATE NONVOTERS TO 1 + 1 FROM 1 + 2 FOR SELECT _ FROM _ -- identifiers removed
