CREATE ROLE ('foo') -- fully parenthetized
CREATE ROLE _ -- literals removed
CREATE ROLE '_' -- UNEXPECTED REPARSED AST WITHOUT LITERALS
CREATE ROLE 'foo' -- identifiers removed

