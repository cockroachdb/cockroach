ALTER TYPE s.t ADD VALUE 'hi' BEFORE 'hello' -- fully parenthesized
ALTER TYPE s.t ADD VALUE 'hi' BEFORE 'hello' -- literals removed
ALTER TYPE _._ ADD VALUE _ BEFORE _ -- identifiers removed

