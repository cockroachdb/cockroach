CREATE INDEX IF NOT EXISTS i ON a (b) WHERE ((c) > (3)) -- fully parenthesized
CREATE INDEX IF NOT EXISTS i ON a (b) WHERE c > _ -- literals removed
CREATE INDEX IF NOT EXISTS _ ON _ (_) WHERE _ > 3 -- identifiers removed

