----
SELECT substring('RoacH', 3, 2) -- normalized!
SELECT (substring(('RoacH'), (3), (2))) -- fully parenthesized
SELECT substring(_, _, _) -- literals removed
SELECT substring('RoacH', 3, 2) -- identifiers removed

parse
