parse
CREATE SCHEMA IF NOT EXISTS foo
----
CREATE SCHEMA IF NOT EXISTS foo
CREATE SCHEMA IF NOT EXISTS foo -- fully parenthesized
CREATE SCHEMA IF NOT EXISTS foo -- literals removed
CREATE SCHEMA IF NOT EXISTS _ -- identifiers removed

