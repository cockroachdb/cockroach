SELECT ((((('{}')::JSONB) <@ (('{}')::JSONB))) = (false)) -- fully parenthesized
SELECT (_::JSONB <@ _::JSONB) = _ -- literals removed
SELECT ('{}'::JSONB <@ '{}'::JSONB) = false -- identifiers removed


