PREPARE a (INT8) AS INSERT INTO a VALUES (($1)) -- fully parenthesized
PREPARE a (INT8) AS INSERT INTO a VALUES ($1) -- literals removed
PREPARE _ (INT8) AS INSERT INTO _ VALUES ($1) -- identifiers removed

