PREPARE a (INT8) AS DELETE FROM a WHERE ((b) = ($1)) -- fully parenthesized
PREPARE a (INT8) AS DELETE FROM a WHERE b = $1 -- literals removed
PREPARE _ (INT8) AS DELETE FROM _ WHERE _ = $1 -- identifiers removed

