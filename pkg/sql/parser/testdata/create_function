	IMMUTABLE
	LEAKPROOF
	LANGUAGE SQL
	AS $$_$$ -- literals removed
CREATE OR REPLACE FUNCTION _(IN _ INT8 DEFAULT 7)
	RETURNS INT8
	CALLED ON NULL INPUT
	IMMUTABLE
	LEAKPROOF
	LANGUAGE SQL
	AS $$_$$ -- identifiers removed

