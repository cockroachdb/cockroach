# We're omitting the following columns since they are not deterministic.
# - total_live_data_bytes,
# - perc_live_data
# - total_data_bytes
# - replication_size_bytes
query
SELECT
  table_name,
  db_name,
  schema_name,
  db_id,
  table_id,
  total_columns,
  total_indexes,
  store_ids,
  total_ranges,
  last_update_error
FROM system.table_metadata
ORDER BY (db_name, table_name)
