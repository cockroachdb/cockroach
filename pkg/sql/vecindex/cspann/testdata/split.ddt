# ----------
# Split of partition that's too small or not a child of the parent is a no-op.
# ----------
new-index min-partition-size=1 max-partition-size=3 beam-size=2
vec1: (0, -1)
vec2: (3, 9)
vec3: (-2, 8)
vec4: (4, 3)
vec5: (2, 2)
