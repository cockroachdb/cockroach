query T
SELECT '2017-01-10 16:05:50.734049+00:00' COLLATE en::TIMESTAMP WITH TIME ZONE
----
2017-01-10 16:05:50.734049 +0000 +0000

query T
SELECT '40 days' COLLATE en::INTERVAL
----
