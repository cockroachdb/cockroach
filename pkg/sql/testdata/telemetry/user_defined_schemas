feature-usage
CREATE SCHEMA s
----
sql.uds.create_schema

feature-usage
ALTER SCHEMA s RENAME TO sc
----
sql.uds.alter_schema

feature-usage
DROP SCHEMA sc
----
sql.uds.drop_schema

exec
CREATE DATABASE d1;
CREATE DATABASE d2;
----

exec
CREATE SCHEMA s
----

