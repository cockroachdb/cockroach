query T
SELECT conname
FROM pg_catalog.pg_constraint con
JOIN pg_catalog.pg_namespace n ON con.connamespace = n.oid
WHERE n.nspname = 'constraint_db' AND contype NOT IN ('c', 'f', 'p', 'u')
ORDER BY con.oid
----

query TITTT colnames
