 │    ├── right columns: uniontest.v:7
 │    ├── ordering: -11
 │    ├── limit hint: 2.00
 │    ├── sort
 │    │    ├── columns: uniontest.v:2
 │    │    ├── ordering: -2
 │    │    ├── limit hint: 2.00
 │    │    └── project
 │    │         ├── columns: uniontest.v:2
