 ├── columns: case:1(int)
 ├── values
 │    └── tuple [type=tuple]
 └── projections
      └── case [type=int]
           ├── true [type=bool]
           ├── when [type=int]
           │    ├── null [type=unknown]
           │    └── const: 1 [type=int]
           └── const: 2 [type=int]

build
