 │         │              │         └── columns: a:5(int!null) b:6(int) c:7(int)
 │         │              └── aggregations
 │         │                   └── count-rows [type=int]
 │         └── const: 3 [type=int]
 └── mult [type=int]
      ├── subquery [type=int]
      │    └── max1-row
      │         ├── columns: count_rows:12(int)
      │         └── scalar-group-by
      │              ├── columns: count_rows:12(int)
