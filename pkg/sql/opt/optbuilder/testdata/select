 ├── scan a
 │    └── columns: x:1(int!null) y:2(float)
 └── filters
      └── and [type=bool]
           ├── gt [type=bool]
           │    ├── variable: x [type=int]
           │    └── const: 10 [type=int]
           └── and [type=bool]
                ├── lt [type=bool]
                │    ├── variable: x [type=int]
                │    └── const: 20 [type=int]
                └── ne [type=bool]
                     ├── variable: x [type=int]
                     └── const: 13 [type=int]

