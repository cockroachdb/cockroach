      │         └── group-by
      │              ├── columns: count:12(int)
      │              ├── project
      │              │    └── scan abc
      │              │         └── columns: abc.a:9(int!null) abc.b:10(int) abc.c:11(int)
      │              └── aggregations
      │                   └── count-rows [type=int]
      └── const: 2 [type=int]

