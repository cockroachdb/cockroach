 ├── columns: filename:4 rows:5 bytes:6
 ├── format: CSV
 ├── project
 │    ├── columns: a:1 b:2
 │    └── scan ab
 │         └── columns: a:1 b:2 rowid:3!null
 ├── 'nodelocal://0/foo'
 └── k-v-options
      └── k-v-options-item foo
           └── $1
