 ├── columns: foo1:1!null moo2:2 moo3:3
 ├── project
 │    ├── columns: moo1:1!null moo2:2 moo3:3
 │    └── scan xyz [as=moo]
 │         └── columns: moo1:1!null moo2:2 moo3:3 crdb_internal_mvcc_timestamp:4
 └── filters
      └── moo1:1 < 7

build
