 ├── columns: z:12
 ├── scan t3
 │    └── columns: t3.a:1 t3.b:2 t3.rowid:3!null
 └── projections
      └── subquery [as=z:12]
           └── max1-row
                ├── columns: y:11
                └── project
                     ├── columns: y:11
                     ├── scan t2
                     │    └── columns: t2.a:4 t2.b:5 t2.rowid:6!null
                     └── projections
                          └── subquery [as=y:11]
                               └── max1-row
                                    ├── columns: column10:10
                                    └── group-by
                                         ├── columns: column10:10
                                         ├── grouping columns: column10:10
                                         └── project
                                              ├── columns: column10:10
