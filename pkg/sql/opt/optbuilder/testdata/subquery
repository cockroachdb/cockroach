 ├── columns: column9:9(bool)
 ├── values
 │    └── tuple [type=tuple{}]
 └── projections
      └── eq [type=bool]
           ├── subquery [type=tuple{int, int, int}]
           │    ├── max1-row
           │    │    ├── columns: column7:7(tuple{int, int, int})
           │    │    └── project
           │    │         ├── columns: column7:7(tuple{int, int, int})
           │    │         ├── project
           │    │         │    ├── columns: column1:1(int) column2:2(int) column3:3(int)
           │    │         │    ├── values
           │    │         │    │    └── tuple [type=tuple{}]
           │    │         │    └── projections
           │    │         │         ├── const: 1 [type=int]
           │    │         │         ├── const: 2 [type=int]
           │    │         │         └── const: 3 [type=int]
