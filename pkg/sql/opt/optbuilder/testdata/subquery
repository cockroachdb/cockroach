# tests adapted from logictest -- subquery

# Tests for subqueries (SELECT statements which are part of a bigger statement).

build
SELECT (SELECT 1)
----
project
 ├── columns: "(SELECT 1)":2(int)
 ├── values
 │    └── tuple [type=tuple{}]
 └── projections
      └── subquery [type=int]
           └── max1-row
                ├── columns: "1":1(int!null)
                └── project
                     ├── columns: "1":1(int!null)
                     ├── values
                     │    └── tuple [type=tuple{}]
                     └── projections
                          └── const: 1 [type=int]

build
SELECT 1 IN (SELECT 1)
----
project
 ├── columns: "1 IN (SELECT 1)":2(bool)
 ├── values
 │    └── tuple [type=tuple{}]
 └── projections
      └── any: eq [type=bool]
           ├── project
           │    ├── columns: "1":1(int!null)
           │    ├── values
           │    │    └── tuple [type=tuple{}]
           │    └── projections
           │         └── const: 1 [type=int]
           └── const: 1 [type=int]

