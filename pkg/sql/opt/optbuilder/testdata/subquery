 ├── columns: x:1(int!null)
 └── select
      ├── columns: x:1(int!null) y:2(int) z:3(int)
      ├── scan xyz
      │    └── columns: x:1(int!null) y:2(int) z:3(int)
      └── filters
           └── any: eq [type=bool]
                ├── project
                │    ├── columns: x:4(int!null)
                │    └── scan xyz
                │         └── columns: x:4(int!null) y:5(int) z:6(int)
                └── variable: x [type=int]

# This test checks that the double sub-query plan expansion caused by a
# sub-expression being shared by two or more plan nodes does not
# panic.
exec-ddl
CREATE TABLE tab4 (col0 INTEGER, col1 FLOAT, col3 INTEGER, col4 FLOAT, INDEX idx_tab4_0 (col4,col0))
----

build
SELECT col0 FROM tab4 WHERE (col0 <= 0 AND col4 <= 5.38) OR (col4 IN (SELECT col1 FROM tab4 WHERE col1 > 8.27)) AND (col3 <= 5 AND (col3 BETWEEN 7 AND 9))
----
project
 ├── columns: col0:1(int!null)
 └── select
      ├── columns: col0:1(int!null) col1:2(float) col3:3(int) col4:4(float!null) rowid:5(int!null)
      ├── scan tab4
      │    └── columns: col0:1(int) col1:2(float) col3:3(int) col4:4(float) rowid:5(int!null)
      └── filters
           └── or [type=bool]
                ├── and [type=bool]
                │    ├── le [type=bool]
                │    │    ├── variable: col0 [type=int]
                │    │    └── const: 0 [type=int]
                │    └── le [type=bool]
                │         ├── variable: col4 [type=float]
                │         └── const: 5.38 [type=float]
                └── and [type=bool]
                     ├── any: eq [type=bool]
                     │    ├── project
                     │    │    ├── columns: col1:7(float!null)
                     │    │    └── select
                     │    │         ├── columns: col0:6(int) col1:7(float!null) col3:8(int) col4:9(float) rowid:10(int!null)
                     │    │         ├── scan tab4
                     │    │         │    └── columns: col0:6(int) col1:7(float) col3:8(int) col4:9(float) rowid:10(int!null)
                     │    │         └── filters
                     │    │              └── gt [type=bool]
                     │    │                   ├── variable: col1 [type=float]
                     │    │                   └── const: 8.27 [type=float]
                     │    └── variable: col4 [type=float]
                     └── and [type=bool]
                          ├── le [type=bool]
                          │    ├── variable: col3 [type=int]
                          │    └── const: 5 [type=int]
                          └── and [type=bool]
                               ├── ge [type=bool]
                               │    ├── variable: col3 [type=int]
                               │    └── const: 7 [type=int]
                               └── le [type=bool]
                                    ├── variable: col3 [type=int]
                                    └── const: 9 [type=int]

