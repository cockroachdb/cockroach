 │         ├── max [as=max:8]
 │         │    └── a:7
 │         └── max [as=max:18]
 │              └── column17:17
 └── projections
      └── subquery [as="?column?":21]
           └── max1-row
                ├── columns: "?column?":20
                └── project
                     ├── columns: "?column?":20
                     ├── scan t2
                     │    └── columns: t2.a:4 t2.b:5 t2.rowid:6!null
                     └── projections
                          └── tuple [as="?column?":20]
                               ├── not
                               │    └── any: ge
