                ├── max1-row
                │    ├── columns: column4:4(tuple{int, int, int})
                │    └── project
                │         ├── columns: column4:4(tuple{int, int, int})
                │         ├── project
                │         │    ├── columns: column1:1(int) column2:2(int) column3:3(int)
