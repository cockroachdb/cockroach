 ├── values
 │    └── tuple [type=tuple{}]
 └── projections
      ├── subquery [type=int]
      │    └── max1-row
      │         ├── columns: "1":1(int!null)
      │         └── project
      │              ├── columns: "1":1(int!null)
      │              ├── values
      │              │    └── tuple [type=tuple{}]
      │              └── projections
      │                   └── const: 1 [type=int]
      └── subquery [type=int]
           └── max1-row
                ├── columns: "2":3(int!null)
                └── project
                     ├── columns: "2":3(int!null)
                     ├── values
                     │    └── tuple [type=tuple{}]
                     └── projections
                          └── const: 2 [type=int]

