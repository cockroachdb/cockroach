 │              │    └── project
 │              │         ├── columns: column11:11(bool)
 │              │         ├── project
 │              │         │    ├── columns: tab4.col1:7(float)
 │              │         │    ├── select
 │              │         │    │    ├── columns: tab4.col0:6(int) tab4.col1:7(float) tab4.col3:8(int) tab4.col4:9(float) tab4.rowid:10(int!null)
 │              │         │    │    ├── scan tab4
 │              │         │    │    │    └── columns: tab4.col0:6(int) tab4.col1:7(float) tab4.col3:8(int) tab4.col4:9(float) tab4.rowid:10(int!null)
