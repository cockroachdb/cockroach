 ├── columns: "(SELECT 1, 2) IN ((1, 2))":4(bool)
 ├── values
 │    └── tuple [type=tuple{}]
 └── projections
      └── in [type=bool]
           ├── subquery [type=tuple{int, int}]
           │    └── max1-row
           │         ├── columns: column3:3(tuple{int, int})
           │         └── project
           │              ├── columns: column3:3(tuple{int, int})
           │              ├── project
           │              │    ├── columns: "1":1(int!null) "2":2(int!null)
           │              │    ├── values
           │              │    │    └── tuple [type=tuple{}]
           │              │    └── projections
           │              │         ├── const: 1 [type=int]
           │              │         └── const: 2 [type=int]
           │              └── projections
           │                   └── tuple [type=tuple{int, int}]
           │                        ├── variable: 1 [type=int]
