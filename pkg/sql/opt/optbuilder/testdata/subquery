                                              │    │    └── columns: t1.a:7 t1.b:8 t1.rowid:9!null
                                              │    └── projections
                                              │         └── t3.a:1 [as=a:11]
                                              └── aggregations
                                                   └── count-rows [as=count_rows:10]

