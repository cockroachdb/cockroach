 ├── columns: r:9
 ├── scan t.public.kv
 │    └── columns: t.public.kv.k:1!null t.public.kv.v:2 t.public.kv.crdb_internal_mvcc_timestamp:3
 └── projections
      └── subquery [as=r:9]
           └── max1-row
                ├── columns: r:8
                └── project
                     ├── columns: r:8
                     ├── scan db1.public.kv
                     │    └── columns: db1.public.kv.k:4!null db1.public.kv.v:5 db1.public.kv.crdb_internal_mvcc_timestamp:6
                     └── projections
                          └── subquery [as=r:8]
                               └── max1-row
                                    ├── columns: r:7
                                    └── project
                                         ├── columns: r:7
                                         ├── values
                                         │    └── ()
                                         └── projections
                                              └── t.public.kv.k:1 + db1.public.kv.k:4 [as=r:7]

