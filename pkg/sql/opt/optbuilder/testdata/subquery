           └── subquery [type=tuple{int, int, int}]
                ├── max1-row
                │    ├── columns: column8:8(tuple{int, int, int})
                │    └── project
                │         ├── columns: column8:8(tuple{int, int, int})
                │         ├── project
                │         │    ├── columns: column4:4(int) column5:5(int) column6:6(int)
                │         │    ├── values
                │         │    │    └── tuple [type=tuple{}]
                │         │    └── projections
                │         │         ├── const: 1 [type=int]
