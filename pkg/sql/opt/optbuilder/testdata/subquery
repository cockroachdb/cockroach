 ├── columns: max:3(string)
 ├── scalar-group-by
 │    ├── columns: max:3(string)
 │    ├── project
 │    │    ├── columns: a:1(string)
 │    │    └── scan t
 │    │         └── columns: a:1(string) t.rowid:2(int!null)
 │    └── aggregations
 │         └── max [type=string]
 │              └── variable: a [type=string]
 └── filters
      └── any: lt [type=bool]
           ├── project
           │    ├── columns: b:4(string)
