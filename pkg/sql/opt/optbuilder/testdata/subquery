 ├── columns: column6:6(bool)
 ├── values
 │    └── tuple [type=tuple{}]
 └── projections
      └── any [type=bool]
           └── project
                ├── columns: column5:5(bool)
                ├── project
                │    ├── columns: column1:1(tuple{int, int})
                │    ├── values
                │    │    └── tuple [type=tuple{}]
                │    └── projections
                │         └── tuple [type=tuple{int, int}]
                │              ├── const: 1 [type=int]
                │              └── const: 2 [type=int]
                └── projections
                     └── eq [type=bool]
                          ├── subquery [type=tuple{int, int}]
