 ├── columns: a:12
 ├── scan t3
 │    └── columns: t3.a:1 t3.b:2 t3.rowid:3!null
 └── projections
      └── subquery [as=a:12]
           └── max1-row
                ├── columns: a:11
                └── project
                     ├── columns: a:11
                     ├── scan t2
                     │    └── columns: t2.a:4 t2.b:5 t2.rowid:6!null
                     └── projections
                          └── subquery [as=a:11]
                               └── max1-row
                                    ├── columns: a:10
                                    └── distinct-on
                                         ├── columns: a:10
                                         ├── grouping columns: a:10
                                         └── project
                                              ├── columns: a:10
                                              ├── scan t1
                                              │    └── columns: t1.a:7 t1.b:8 t1.rowid:9!null
                                              └── projections
                                                   └── t3.a:1 [as=a:10]

