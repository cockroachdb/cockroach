 │         └── CASE WHEN c:9 IS NULL THEN column1:5 ELSE c:9 END [as=upsert_c:13]
 └── f-k-checks
      └── f-k-checks-item: cpq(p,q) -> pq(p,q)
           └── anti-join (hash)
                ├── columns: column2:14!null column3:15!null
                ├── with-scan &1
                │    ├── columns: column2:14!null column3:15!null
                │    └── mapping:
                │         ├──  column2:6 => column2:14
