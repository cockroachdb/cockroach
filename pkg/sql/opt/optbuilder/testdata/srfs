 │         ├── tuple [type=tuple]
 │         └── const: 'c' [type=string]
 └── const: 1 [type=int]

# upper

# Regular scalar functions can be used as functions too. #22312
build
SELECT * FROM upper('abc')
----
zip
 ├── columns: upper:1(string)
 └── function: upper [type=string]
      └── const: 'abc' [type=string]

