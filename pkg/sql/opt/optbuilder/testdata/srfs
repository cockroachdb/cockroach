 ├── columns: "?column?":1!null b:2!null "?column?":3!null
 ├── project
 │    ├── columns: "?column?":1!null b:2!null "?column?":3!null
 │    ├── limit hint: 1.00
 │    ├── project-set
 │    │    ├── limit hint: 1.00
 │    │    ├── values
 │    │    │    ├── limit hint: 1.00
 │    │    │    └── ()
 │    │    └── zip
 │    │         └── crdb_internal.unary_table()
 │    └── projections
 │         ├── 'a' [as="?column?":1]
 │         ├── () [as=b:2]
 │         └── 'c' [as="?column?":3]
 └── 1

