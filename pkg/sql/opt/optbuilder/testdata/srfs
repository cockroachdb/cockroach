 ├── columns: "?column?":3 "?column?":4
 ├── project-set
 │    ├── columns: generate_series:1 unnest:2
 │    ├── values
 │    │    └── ()
 │    └── zip
 │         ├── generate_series(0, 1)
 │         └── unnest(ARRAY[2,4])
 └── projections
      ├── 1 + generate_series:1 [as="?column?":3]
      └── unnest:2 - 1 [as="?column?":4]

build
