 │    │    │    │    └── columns: xyz.x:int:1 xyz.y:int:null:2 xyz.z:float:null:3
 │    │    │    └── projections [outer=(2)]
 │    │    │         └── variable: xyz.y [type=int, outer=(2)]
 │    │    └── aggregations
 │    └── projections
 └── aggregations
      └── function: count_rows [type=int]

