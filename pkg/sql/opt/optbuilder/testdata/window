      ├── project
      │    ├── columns: avg_1_filter:9(bool) k:1(int!null) v:2(int) w:3(int) f:4(float) d:5(decimal) s:6(string) b:7(bool)
      │    ├── scan kv
      │    │    └── columns: k:1(int!null) v:2(int) w:3(int) f:4(float) d:5(decimal) s:6(string) b:7(bool)
      │    └── projections
      │         └── eq [type=bool]
      │              ├── variable: k [type=int]
      │              └── const: 1 [type=int]
      └── windows
           └── agg-filter [type=float]
                ├── avg [type=float]
                │    └── variable: f [type=float]
                └── variable: avg_1_filter [type=bool]

