build
SELECT
  rank() OVER (w),
  rank() OVER w,
  row_number() OVER (w2),
  row_number() OVER (w2 ORDER BY f),
  row_number() OVER w2
FROM kv WINDOW w as (ORDER BY v), w2 as (PARTITION BY v)
