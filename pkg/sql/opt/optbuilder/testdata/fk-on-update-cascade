 │         ├── columns: q_new:9 pk:5!null p:6!null q:7 crdb_internal_mvcc_timestamp:8
 │         ├── select
 │         │    ├── columns: pk:5!null p:6!null q:7 crdb_internal_mvcc_timestamp:8
 │         │    ├── scan parent_multi
