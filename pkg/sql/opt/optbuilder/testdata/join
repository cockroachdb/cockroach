 │    │         └── const: 1 [type=int]
 │    └── true [type=bool]
 └── projections
      └── variable: k [type=int]

# TODO(rytaft): This test case will fail until we implement subquery
# replacement.
# build
# select * from (select 1 as k), (select 2 as k) where 1 in (select k from kv)
# ----
# select
#  ├── columns: k:int:null:1 k:int:null:2
#  ├── inner-join
#  │    ├── columns: k:int:null:1 k:int:null:2
#  │    ├── project
#  │    │    ├── columns: k:int:null:1
#  │    │    ├── values
#  │    │    │    └── tuple [type=tuple{}]
#  │    │    └── projections
#  │    │         └── const: 1 [type=int]
#  │    ├── project
#  │    │    ├── columns: k:int:null:2
#  │    │    ├── values
#  │    │    │    └── tuple [type=tuple{}]
#  │    │    └── projections
#  │    │         └── const: 2 [type=int]
#  │    └── true [type=bool]
#  └── in [type=bool]
#       ├── const: 1 [type=int]
#       └── subquery [type=NULL]
#            ├── project
#            │    ├── columns: kv.k:int:3
#            │    ├── scan
#            │    │    └── columns: kv.k:int:3 kv.v:int:null:4 kv.w:int:null:5 kv.s:string:null:6
#            │    └── projections
#            │         └── variable: kv.k [type=int]
#            └── variable: kv.k [type=int]
