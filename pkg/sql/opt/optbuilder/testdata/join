 ├── columns: x:1!null y:3!null
 └── inner-join (hash)
      ├── columns: x:1!null a.rowid:2!null y:3!null b.rowid:4!null
      ├── scan a
      │    └── columns: x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: y:3 b.rowid:4!null
      └── filters
           └── x:1 = y:3

build
SELECT * FROM empty AS a JOIN onecolumn AS b USING(x)
----
project
 ├── columns: x:1!null
 └── inner-join (hash)
      ├── columns: a.x:1!null a.rowid:2!null b.x:3!null b.rowid:4!null
      ├── scan a
      │    └── columns: a.x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: b.x:3 b.rowid:4!null
      └── filters
           └── a.x:1 = b.x:3

build
SELECT * FROM onecolumn AS a(x) LEFT OUTER JOIN empty AS b(y) ON a.x = b.y ORDER BY a.x
----
