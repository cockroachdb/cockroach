# tests adapted from logictest -- join

exec-ddl
CREATE TABLE onecolumn (x INT)
----

build
SELECT * FROM onecolumn AS a(x) CROSS JOIN onecolumn AS b(y)
----
project
 ├── columns: x:1 y:3
 └── inner-join (cross)
      ├── columns: x:1 a.rowid:2!null y:3 b.rowid:4!null
      ├── scan a
      │    └── columns: x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: y:3 b.rowid:4!null
      └── filters (true)

# Check that name resolution chokes on ambiguity when it needs to.
build
SELECT x FROM onecolumn AS a, onecolumn AS b
----
error (42702): column reference "x" is ambiguous (candidates: a.x, b.x)

# Check that name resolution does not choke on ambiguity if an
# unqualified column name is requested and there is an anonymous
# source providing this name in addition to two or more named sources
# that also provide it.
build
SELECT x FROM (SELECT 1 AS x), onecolumn AS a, onecolumn AS b
----
project
 ├── columns: x:1!null
 └── inner-join (cross)
      ├── columns: x:1!null a.x:2 a.rowid:3!null b.x:4 b.rowid:5!null
      ├── project
      │    ├── columns: x:1!null
      │    ├── values
      │    │    └── ()
      │    └── projections
      │         └── 1 [as=x:1]
      ├── inner-join (cross)
      │    ├── columns: a.x:2 a.rowid:3!null b.x:4 b.rowid:5!null
      │    ├── scan a
      │    │    └── columns: a.x:2 a.rowid:3!null
      │    ├── scan b
      │    │    └── columns: b.x:4 b.rowid:5!null
      │    └── filters (true)
      └── filters (true)

build
SELECT * FROM onecolumn AS a(x) JOIN onecolumn AS b(y) ON a.x = b.y
----
project
 ├── columns: x:1!null y:3!null
 └── inner-join (hash)
      ├── columns: x:1!null a.rowid:2!null y:3!null b.rowid:4!null
      ├── scan a
      │    └── columns: x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: y:3 b.rowid:4!null
      └── filters
           └── x:1 = y:3

build
SELECT * FROM onecolumn AS a JOIN onecolumn as b USING(x) ORDER BY x
----
sort
 ├── columns: x:1!null
 ├── ordering: +1
 └── project
      ├── columns: a.x:1!null
      └── inner-join (hash)
           ├── columns: a.x:1!null a.rowid:2!null b.x:3!null b.rowid:4!null
           ├── scan a
           │    └── columns: a.x:1 a.rowid:2!null
           ├── scan b
           │    └── columns: b.x:3 b.rowid:4!null
           └── filters
                └── a.x:1 = b.x:3

build
SELECT * FROM onecolumn AS a NATURAL JOIN onecolumn as b
----
project
 ├── columns: x:1!null
 └── inner-join (hash)
      ├── columns: a.x:1!null a.rowid:2!null b.x:3!null b.rowid:4!null
      ├── scan a
      │    └── columns: a.x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: b.x:3 b.rowid:4!null
      └── filters
           └── a.x:1 = b.x:3

build
SELECT * FROM onecolumn AS a(x) LEFT OUTER JOIN onecolumn AS b(y) ON a.x = b.y
----
project
 ├── columns: x:1 y:3
 └── left-join (hash)
      ├── columns: x:1 a.rowid:2!null y:3 b.rowid:4
      ├── scan a
      │    └── columns: x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: y:3 b.rowid:4!null
      └── filters
           └── x:1 = y:3

build
SELECT * FROM onecolumn AS a LEFT OUTER JOIN onecolumn AS b USING(x) ORDER BY x
----
sort
 ├── columns: x:1
 ├── ordering: +1
 └── project
      ├── columns: a.x:1
      └── left-join (hash)
           ├── columns: a.x:1 a.rowid:2!null b.x:3 b.rowid:4
           ├── scan a
           │    └── columns: a.x:1 a.rowid:2!null
           ├── scan b
           │    └── columns: b.x:3 b.rowid:4!null
           └── filters
                └── a.x:1 = b.x:3

# Check that ORDER BY chokes on ambiguity if no table less columns
# were introduced by USING. (#12239)
build
SELECT * FROM onecolumn AS a, onecolumn AS b ORDER BY x
----
error (42P09): ORDER BY "x" is ambiguous

build
SELECT * FROM (SELECT x, x FROM onecolumn) AS a JOIN onecolumn AS b USING (x)
----
error (42701): duplicate column name: "x"

build
SELECT * FROM onecolumn AS a JOIN (SELECT x, x FROM onecolumn) AS b USING (x)
----
error (42701): duplicate column name: "x"

build
SELECT * FROM (SELECT x, x FROM onecolumn) AS a NATURAL JOIN onecolumn AS b
----
error (42701): duplicate column name: "x"

build
SELECT * FROM onecolumn AS a NATURAL JOIN (SELECT x, x FROM onecolumn) AS b
----
error (42701): duplicate column name: "x"

build
SELECT * FROM onecolumn AS a NATURAL LEFT OUTER JOIN onecolumn AS b
----
project
 ├── columns: x:1
 └── left-join (hash)
      ├── columns: a.x:1 a.rowid:2!null b.x:3 b.rowid:4
      ├── scan a
      │    └── columns: a.x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: b.x:3 b.rowid:4!null
      └── filters
           └── a.x:1 = b.x:3

build
SELECT * FROM onecolumn AS a(x) RIGHT OUTER JOIN onecolumn AS b(y) ON a.x = b.y
----
project
 ├── columns: x:1 y:3
 └── right-join (hash)
      ├── columns: x:1 a.rowid:2 y:3 b.rowid:4!null
      ├── scan a
      │    └── columns: x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: y:3 b.rowid:4!null
      └── filters
           └── x:1 = y:3

build
SELECT * FROM onecolumn AS a RIGHT OUTER JOIN onecolumn AS b USING(x) ORDER BY x
----
sort
 ├── columns: x:3
 ├── ordering: +3
 └── project
      ├── columns: b.x:3
      └── right-join (hash)
           ├── columns: a.x:1 a.rowid:2 b.x:3 b.rowid:4!null
           ├── scan a
           │    └── columns: a.x:1 a.rowid:2!null
           ├── scan b
           │    └── columns: b.x:3 b.rowid:4!null
           └── filters
                └── a.x:1 = b.x:3

build
SELECT * FROM onecolumn AS a NATURAL RIGHT OUTER JOIN onecolumn AS b
----
project
 ├── columns: x:3
 └── right-join (hash)
      ├── columns: a.x:1 a.rowid:2 b.x:3 b.rowid:4!null
      ├── scan a
      │    └── columns: a.x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: b.x:3 b.rowid:4!null
      └── filters
           └── a.x:1 = b.x:3

exec-ddl
CREATE TABLE onecolumn_w(w INT)
----

build
SELECT * FROM onecolumn AS a NATURAL JOIN onecolumn_w as b
----
project
 ├── columns: x:1 w:3
 └── inner-join (cross)
      ├── columns: x:1 a.rowid:2!null w:3 b.rowid:4!null
      ├── scan a
      │    └── columns: x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: w:3 b.rowid:4!null
      └── filters (true)

exec-ddl
CREATE TABLE othercolumn (x INT)
----

build
SELECT * FROM onecolumn AS a FULL OUTER JOIN othercolumn AS b ON a.x = b.x ORDER BY a.x,b.x
----
sort
 ├── columns: x:1 x:3
 ├── ordering: +1,+3
 └── project
      ├── columns: a.x:1 b.x:3
      └── full-join (hash)
           ├── columns: a.x:1 a.rowid:2 b.x:3 b.rowid:4
           ├── scan a
           │    └── columns: a.x:1 a.rowid:2!null
           ├── scan b
           │    └── columns: b.x:3 b.rowid:4!null
           └── filters
                └── a.x:1 = b.x:3

build
SELECT * FROM onecolumn AS a FULL OUTER JOIN othercolumn AS b USING(x) ORDER BY x
----
sort
 ├── columns: x:5
 ├── ordering: +5
 └── project
      ├── columns: x:5
      └── project
           ├── columns: x:5 a.x:1 a.rowid:2 b.x:3 b.rowid:4
           ├── full-join (hash)
           │    ├── columns: a.x:1 a.rowid:2 b.x:3 b.rowid:4
           │    ├── scan a
           │    │    └── columns: a.x:1 a.rowid:2!null
           │    ├── scan b
           │    │    └── columns: b.x:3 b.rowid:4!null
           │    └── filters
           │         └── a.x:1 = b.x:3
           └── projections
                └── COALESCE(a.x:1, b.x:3) [as=x:5]

# Check that the source columns can be selected separately from the
# USING column (#12033).
build
SELECT x AS s, a.x, b.x FROM onecolumn AS a FULL OUTER JOIN othercolumn AS b USING(x) ORDER BY s
----
sort
 ├── columns: s:5 x:1 x:3
 ├── ordering: +5
 └── project
      ├── columns: a.x:1 b.x:3 x:5
      └── project
           ├── columns: x:5 a.x:1 a.rowid:2 b.x:3 b.rowid:4
           ├── full-join (hash)
           │    ├── columns: a.x:1 a.rowid:2 b.x:3 b.rowid:4
           │    ├── scan a
           │    │    └── columns: a.x:1 a.rowid:2!null
           │    ├── scan b
           │    │    └── columns: b.x:3 b.rowid:4!null
           │    └── filters
           │         └── a.x:1 = b.x:3
           └── projections
                └── COALESCE(a.x:1, b.x:3) [as=x:5]

build
SELECT * FROM onecolumn AS a NATURAL FULL OUTER JOIN othercolumn AS b ORDER BY x
----
sort
 ├── columns: x:5
 ├── ordering: +5
 └── project
      ├── columns: x:5
      └── project
           ├── columns: x:5 a.x:1 a.rowid:2 b.x:3 b.rowid:4
           ├── full-join (hash)
           │    ├── columns: a.x:1 a.rowid:2 b.x:3 b.rowid:4
           │    ├── scan a
           │    │    └── columns: a.x:1 a.rowid:2!null
           │    ├── scan b
           │    │    └── columns: b.x:3 b.rowid:4!null
           │    └── filters
           │         └── a.x:1 = b.x:3
           └── projections
                └── COALESCE(a.x:1, b.x:3) [as=x:5]

# Check that a limit on the JOIN's result do not cause rows from the
# JOIN operands to become invisible to the JOIN.
build
SELECT * FROM (SELECT x FROM onecolumn ORDER BY x DESC) NATURAL JOIN (VALUES (42)) AS v(x) LIMIT 1
----
limit
 ├── columns: x:1!null
 ├── project
 │    ├── columns: x:1!null
 │    ├── limit hint: 1.00
 │    └── inner-join (hash)
 │         ├── columns: x:1!null column1:3!null
 │         ├── limit hint: 1.00
 │         ├── project
 │         │    ├── columns: x:1
 │         │    └── scan onecolumn
 │         │         └── columns: x:1 rowid:2!null
 │         ├── values
 │         │    ├── columns: column1:3!null
 │         │    └── (42,)
 │         └── filters
 │              └── x:1 = column1:3
 └── 1

exec-ddl
CREATE TABLE empty (x INT)
----

build
SELECT * FROM onecolumn AS a(x) CROSS JOIN empty AS b(y)
----
project
 ├── columns: x:1 y:3
 └── inner-join (cross)
      ├── columns: x:1 a.rowid:2!null y:3 b.rowid:4!null
      ├── scan a
      │    └── columns: x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: y:3 b.rowid:4!null
      └── filters (true)

build
SELECT * FROM empty AS a CROSS JOIN onecolumn AS b
----
project
 ├── columns: x:1 x:3
 └── inner-join (cross)
      ├── columns: a.x:1 a.rowid:2!null b.x:3 b.rowid:4!null
      ├── scan a
      │    └── columns: a.x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: b.x:3 b.rowid:4!null
      └── filters (true)

build
SELECT * FROM onecolumn AS a(x) JOIN empty AS b(y) ON a.x = b.y
----
project
 ├── columns: x:1!null y:3!null
 └── inner-join (hash)
      ├── columns: x:1!null a.rowid:2!null y:3!null b.rowid:4!null
      ├── scan a
      │    └── columns: x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: y:3 b.rowid:4!null
      └── filters
           └── x:1 = y:3

build
SELECT * FROM onecolumn AS a JOIN empty AS b USING(x)
----
project
 ├── columns: x:1!null
 └── inner-join (hash)
      ├── columns: a.x:1!null a.rowid:2!null b.x:3!null b.rowid:4!null
      ├── scan a
      │    └── columns: a.x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: b.x:3 b.rowid:4!null
      └── filters
           └── a.x:1 = b.x:3

build
SELECT * FROM empty AS a(x) JOIN onecolumn AS b(y) ON a.x = b.y
----
