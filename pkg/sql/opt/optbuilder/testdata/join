      │    │              └── variable: xyv.y [type=int]
      │    └── projections
      │         ├── coalesce [type=int]
      │         │    ├── variable: xyu.x [type=int]
      │         │    └── variable: xyv.x [type=int]
      │         └── coalesce [type=int]
      │              ├── variable: xyu.y [type=int]
      │              └── variable: xyv.y [type=int]
      └── filters
           └── gt [type=bool]
                ├── variable: x [type=int]
                └── const: 2 [type=int]

