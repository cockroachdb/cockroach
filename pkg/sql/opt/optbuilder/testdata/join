exec-ddl
CREATE TABLE twocolumn (x INT, y INT)
----

# Natural joins with partial match
build
SELECT * FROM onecolumn NATURAL JOIN twocolumn
----
project
 ├── columns: x:1!null y:4
 └── inner-join (hash)
      ├── columns: onecolumn.x:1!null onecolumn.rowid:2!null twocolumn.x:3!null y:4 twocolumn.rowid:5!null
      ├── scan onecolumn
      │    └── columns: onecolumn.x:1 onecolumn.rowid:2!null
      ├── scan twocolumn
      │    └── columns: twocolumn.x:3 y:4 twocolumn.rowid:5!null
      └── filters
           └── onecolumn.x:1 = twocolumn.x:3

build
SELECT * FROM onecolumn JOIN twocolumn USING(x)
----
project
 ├── columns: x:1!null y:4
 └── inner-join (hash)
      ├── columns: onecolumn.x:1!null onecolumn.rowid:2!null twocolumn.x:3!null y:4 twocolumn.rowid:5!null
      ├── scan onecolumn
      │    └── columns: onecolumn.x:1 onecolumn.rowid:2!null
      ├── scan twocolumn
      │    └── columns: twocolumn.x:3 y:4 twocolumn.rowid:5!null
      └── filters
           └── onecolumn.x:1 = twocolumn.x:3

build
SELECT * FROM twocolumn AS a JOIN twocolumn AS b ON a.x = b.y
----
project
 ├── columns: x:1!null y:2 x:4 y:5!null
 └── inner-join (hash)
      ├── columns: a.x:1!null a.y:2 a.rowid:3!null b.x:4 b.y:5!null b.rowid:6!null
      ├── scan a
      │    └── columns: a.x:1 a.y:2 a.rowid:3!null
      ├── scan b
      │    └── columns: b.x:4 b.y:5 b.rowid:6!null
      └── filters
           └── a.x:1 = b.y:5

build
SELECT * FROM twocolumn AS a JOIN twocolumn AS b ON a.x = a.y
----
project
 ├── columns: x:1!null y:2!null x:4 y:5
 └── inner-join (cross)
      ├── columns: a.x:1!null a.y:2!null a.rowid:3!null b.x:4 b.y:5 b.rowid:6!null
      ├── scan a
      │    └── columns: a.x:1 a.y:2 a.rowid:3!null
      ├── scan b
      │    └── columns: b.x:4 b.y:5 b.rowid:6!null
      └── filters
           └── a.x:1 = a.y:2

build
SELECT * FROM onecolumn AS a JOIN twocolumn AS b ON ((a.x)) = ((b.y))
----
project
 ├── columns: x:1!null x:3 y:4!null
 └── inner-join (hash)
      ├── columns: a.x:1!null a.rowid:2!null b.x:3 y:4!null b.rowid:5!null
      ├── scan a
      │    └── columns: a.x:1 a.rowid:2!null
      ├── scan b
      │    └── columns: b.x:3 y:4 b.rowid:5!null
      └── filters
           └── a.x:1 = y:4

build
SELECT * FROM onecolumn JOIN twocolumn ON onecolumn.x = twocolumn.y
----
project
 ├── columns: x:1!null x:3 y:4!null
 └── inner-join (hash)
      ├── columns: onecolumn.x:1!null onecolumn.rowid:2!null twocolumn.x:3 y:4!null twocolumn.rowid:5!null
      ├── scan onecolumn
      │    └── columns: onecolumn.x:1 onecolumn.rowid:2!null
      ├── scan twocolumn
      │    └── columns: twocolumn.x:3 y:4 twocolumn.rowid:5!null
      └── filters
           └── onecolumn.x:1 = y:4

# Inner join with filter predicate
build
SELECT * FROM twocolumn AS a JOIN twocolumn AS b ON a.x = 44
----
project
 ├── columns: x:1!null y:2 x:4 y:5
 └── inner-join (cross)
      ├── columns: a.x:1!null a.y:2 a.rowid:3!null b.x:4 b.y:5 b.rowid:6!null
      ├── scan a
      │    └── columns: a.x:1 a.y:2 a.rowid:3!null
      ├── scan b
      │    └── columns: b.x:4 b.y:5 b.rowid:6!null
      └── filters
           └── a.x:1 = 44

build
SELECT o.x, t.y FROM onecolumn o INNER JOIN twocolumn t ON (o.x=t.x AND t.y=53)
----
project
 ├── columns: x:1!null y:4!null
 └── inner-join (cross)
      ├── columns: o.x:1!null o.rowid:2!null t.x:3!null y:4!null t.rowid:5!null
      ├── scan o
      │    └── columns: o.x:1 o.rowid:2!null
      ├── scan t
      │    └── columns: t.x:3 y:4 t.rowid:5!null
      └── filters
           └── (o.x:1 = t.x:3) AND (y:4 = 53)

# Outer joins with filter predicate
build
SELECT o.x, t.y FROM onecolumn o LEFT OUTER JOIN twocolumn t ON (o.x=t.x AND t.y=53)
----
project
 ├── columns: x:1 y:4
 └── left-join (cross)
      ├── columns: o.x:1 o.rowid:2!null t.x:3 y:4 t.rowid:5
      ├── scan o
      │    └── columns: o.x:1 o.rowid:2!null
      ├── scan t
      │    └── columns: t.x:3 y:4 t.rowid:5!null
      └── filters
           └── (o.x:1 = t.x:3) AND (y:4 = 53)

build
SELECT o.x, t.y FROM onecolumn o LEFT OUTER JOIN twocolumn t ON (o.x=t.x AND o.x=44)
----
project
 ├── columns: x:1 y:4
 └── left-join (cross)
      ├── columns: o.x:1 o.rowid:2!null t.x:3 y:4 t.rowid:5
      ├── scan o
      │    └── columns: o.x:1 o.rowid:2!null
      ├── scan t
      │    └── columns: t.x:3 y:4 t.rowid:5!null
      └── filters
           └── (o.x:1 = t.x:3) AND (o.x:1 = 44)

build
SELECT o.x, t.y FROM onecolumn o LEFT OUTER JOIN twocolumn t ON (o.x=t.x AND t.x=44)
----
project
 ├── columns: x:1 y:4
 └── left-join (cross)
      ├── columns: o.x:1 o.rowid:2!null t.x:3 y:4 t.rowid:5
      ├── scan o
      │    └── columns: o.x:1 o.rowid:2!null
      ├── scan t
      │    └── columns: t.x:3 y:4 t.rowid:5!null
      └── filters
           └── (o.x:1 = t.x:3) AND (t.x:3 = 44)

build
SELECT x, a.x, b.y FROM (SELECT * FROM onecolumn AS a NATURAL JOIN twocolumn AS b) AS q
----
error (42P01): no data source matches prefix: a

build
SELECT x, a.x, b.y FROM (SELECT * FROM onecolumn AS a NATURAL JOIN twocolumn AS b)
----
error (42P01): no data source matches prefix: a


## Simple test cases for inner, left, right, and outer joins

exec-ddl
CREATE TABLE a (i int)
----

exec-ddl
CREATE TABLE b (i int, b bool)
----

build
SELECT * FROM a INNER JOIN b ON a.i = b.i
----
project
 ├── columns: i:1!null i:3!null b:4
 └── inner-join (hash)
      ├── columns: a.i:1!null a.rowid:2!null b.i:3!null b:4 b.rowid:5!null
      ├── scan a
      │    └── columns: a.i:1 a.rowid:2!null
      ├── scan b
      │    └── columns: b.i:3 b:4 b.rowid:5!null
      └── filters
           └── a.i:1 = b.i:3

build
SELECT * FROM a LEFT OUTER JOIN b ON a.i = b.i
----
project
 ├── columns: i:1 i:3 b:4
 └── left-join (hash)
      ├── columns: a.i:1 a.rowid:2!null b.i:3 b:4 b.rowid:5
      ├── scan a
      │    └── columns: a.i:1 a.rowid:2!null
      ├── scan b
      │    └── columns: b.i:3 b:4 b.rowid:5!null
      └── filters
           └── a.i:1 = b.i:3

build
SELECT * FROM a RIGHT OUTER JOIN b ON a.i = b.i ORDER BY b.i, b.b
----
sort
 ├── columns: i:1 i:3 b:4
 ├── ordering: +3,+4
 └── project
      ├── columns: a.i:1 b.i:3 b:4
      └── right-join (hash)
           ├── columns: a.i:1 a.rowid:2 b.i:3 b:4 b.rowid:5!null
           ├── scan a
           │    └── columns: a.i:1 a.rowid:2!null
           ├── scan b
           │    └── columns: b.i:3 b:4 b.rowid:5!null
           └── filters
                └── a.i:1 = b.i:3

build
SELECT * FROM a FULL OUTER JOIN b ON a.i = b.i ORDER BY b.i, b.b
----
sort
 ├── columns: i:1 i:3 b:4
 ├── ordering: +3,+4
 └── project
      ├── columns: a.i:1 b.i:3 b:4
      └── full-join (hash)
           ├── columns: a.i:1 a.rowid:2 b.i:3 b:4 b.rowid:5
           ├── scan a
           │    └── columns: a.i:1 a.rowid:2!null
           ├── scan b
           │    └── columns: b.i:3 b:4 b.rowid:5!null
           └── filters
                └── a.i:1 = b.i:3

# Full outer join with filter predicate
build
SELECT * FROM a FULL OUTER JOIN b ON (a.i = b.i and a.i>2) ORDER BY a.i, b.i
----
sort
 ├── columns: i:1 i:3 b:4
 ├── ordering: +1,+3
 └── project
      ├── columns: a.i:1 b.i:3 b:4
      └── full-join (cross)
           ├── columns: a.i:1 a.rowid:2 b.i:3 b:4 b.rowid:5
           ├── scan a
           │    └── columns: a.i:1 a.rowid:2!null
           ├── scan b
           │    └── columns: b.i:3 b:4 b.rowid:5!null
           └── filters
                └── (a.i:1 = b.i:3) AND (a.i:1 > 2)

# Check column orders and names.
build
SELECT * FROM (onecolumn CROSS JOIN twocolumn JOIN onecolumn AS a(b) ON a.b=twocolumn.x JOIN twocolumn AS c(d,e) ON a.b=c.d AND c.d=onecolumn.x) ORDER BY 1 LIMIT 1
----
limit
 ├── columns: x:1!null x:3!null y:4 b:6!null d:8!null e:9
 ├── internal-ordering: +1
 ├── ordering: +1
 ├── sort
 │    ├── columns: onecolumn.x:1!null twocolumn.x:3!null y:4 b:6!null d:8!null e:9
 │    ├── ordering: +1
 │    ├── limit hint: 1.00
 │    └── project
 │         ├── columns: onecolumn.x:1!null twocolumn.x:3!null y:4 b:6!null d:8!null e:9
 │         └── inner-join (cross)
 │              ├── columns: onecolumn.x:1!null onecolumn.rowid:2!null twocolumn.x:3!null y:4 twocolumn.rowid:5!null b:6!null a.rowid:7!null d:8!null e:9 c.rowid:10!null
 │              ├── inner-join (hash)
 │              │    ├── columns: onecolumn.x:1 onecolumn.rowid:2!null twocolumn.x:3!null y:4 twocolumn.rowid:5!null b:6!null a.rowid:7!null
 │              │    ├── inner-join (cross)
 │              │    │    ├── columns: onecolumn.x:1 onecolumn.rowid:2!null twocolumn.x:3 y:4 twocolumn.rowid:5!null
 │              │    │    ├── scan onecolumn
 │              │    │    │    └── columns: onecolumn.x:1 onecolumn.rowid:2!null
 │              │    │    ├── scan twocolumn
 │              │    │    │    └── columns: twocolumn.x:3 y:4 twocolumn.rowid:5!null
 │              │    │    └── filters (true)
 │              │    ├── scan a
 │              │    │    └── columns: b:6 a.rowid:7!null
 │              │    └── filters
 │              │         └── b:6 = twocolumn.x:3
 │              ├── scan c
 │              │    └── columns: d:8 e:9 c.rowid:10!null
 │              └── filters
 │                   └── (b:6 = d:8) AND (d:8 = onecolumn.x:1)
 └── 1

# Check sub-queries in ON conditions.
build
SELECT * FROM onecolumn JOIN twocolumn ON twocolumn.x = onecolumn.x AND onecolumn.x IN (SELECT x FROM twocolumn WHERE y >= 52)
----
project
 ├── columns: x:1!null x:3!null y:4
 └── inner-join (cross)
      ├── columns: onecolumn.x:1!null onecolumn.rowid:2!null twocolumn.x:3!null y:4 twocolumn.rowid:5!null
      ├── scan onecolumn
      │    └── columns: onecolumn.x:1 onecolumn.rowid:2!null
      ├── scan twocolumn
      │    └── columns: twocolumn.x:3 y:4 twocolumn.rowid:5!null
      └── filters
           └── and
                ├── twocolumn.x:3 = onecolumn.x:1
                └── any: eq
                     ├── project
                     │    ├── columns: twocolumn.x:6
                     │    └── select
                     │         ├── columns: twocolumn.x:6 y:7!null twocolumn.rowid:8!null
                     │         ├── scan twocolumn
                     │         │    └── columns: twocolumn.x:6 y:7 twocolumn.rowid:8!null
                     │         └── filters
                     │              └── y:7 >= 52
                     └── onecolumn.x:1

# Check sub-queries as data sources.
build
SELECT * FROM onecolumn JOIN (VALUES (41),(42),(43)) AS a(x) USING(x)
----
project
 ├── columns: x:1!null
 └── inner-join (hash)
      ├── columns: x:1!null rowid:2!null column1:3!null
      ├── scan onecolumn
      │    └── columns: x:1 rowid:2!null
      ├── values
      │    ├── columns: column1:3!null
      │    ├── (41,)
      │    ├── (42,)
      │    └── (43,)
      └── filters
           └── x:1 = column1:3

build
SELECT * FROM onecolumn JOIN (SELECT x + 2 AS x FROM onecolumn) USING(x)
----
project
 ├── columns: x:1!null
 └── inner-join (hash)
      ├── columns: onecolumn.x:1!null rowid:2!null x:5!null
      ├── scan onecolumn
      │    └── columns: onecolumn.x:1 rowid:2!null
      ├── project
      │    ├── columns: x:5
      │    ├── scan onecolumn
      │    │    └── columns: onecolumn.x:3 rowid:4!null
      │    └── projections
      │         └── onecolumn.x:3 + 2 [as=x:5]
      └── filters
           └── onecolumn.x:1 = x:5

# Check that a single column can have multiple table aliases.
build
SELECT * FROM (twocolumn AS a JOIN twocolumn AS b USING(x) JOIN twocolumn AS c USING(x)) ORDER BY x LIMIT 1
----
limit
 ├── columns: x:1!null y:2 y:5 y:8
 ├── internal-ordering: +1
 ├── ordering: +1
 ├── sort
 │    ├── columns: a.x:1!null a.y:2 b.y:5 c.y:8
 │    ├── ordering: +1
 │    ├── limit hint: 1.00
 │    └── project
 │         ├── columns: a.x:1!null a.y:2 b.y:5 c.y:8
 │         └── inner-join (hash)
 │              ├── columns: a.x:1!null a.y:2 a.rowid:3!null b.x:4!null b.y:5 b.rowid:6!null c.x:7!null c.y:8 c.rowid:9!null
 │              ├── inner-join (hash)
 │              │    ├── columns: a.x:1!null a.y:2 a.rowid:3!null b.x:4!null b.y:5 b.rowid:6!null
 │              │    ├── scan a
 │              │    │    └── columns: a.x:1 a.y:2 a.rowid:3!null
 │              │    ├── scan b
 │              │    │    └── columns: b.x:4 b.y:5 b.rowid:6!null
 │              │    └── filters
 │              │         └── a.x:1 = b.x:4
 │              ├── scan c
 │              │    └── columns: c.x:7 c.y:8 c.rowid:9!null
 │              └── filters
 │                   └── a.x:1 = c.x:7
 └── 1

build
SELECT a.x AS s, b.x, c.x, a.y, b.y, c.y FROM (twocolumn AS a JOIN twocolumn AS b USING(x) JOIN twocolumn AS c USING(x)) ORDER BY s
----
sort
 ├── columns: s:1!null x:4!null x:7!null y:2 y:5 y:8
 ├── ordering: +1
 └── project
      ├── columns: a.x:1!null a.y:2 b.x:4!null b.y:5 c.x:7!null c.y:8
      └── inner-join (hash)
           ├── columns: a.x:1!null a.y:2 a.rowid:3!null b.x:4!null b.y:5 b.rowid:6!null c.x:7!null c.y:8 c.rowid:9!null
           ├── inner-join (hash)
           │    ├── columns: a.x:1!null a.y:2 a.rowid:3!null b.x:4!null b.y:5 b.rowid:6!null
           │    ├── scan a
           │    │    └── columns: a.x:1 a.y:2 a.rowid:3!null
           │    ├── scan b
           │    │    └── columns: b.x:4 b.y:5 b.rowid:6!null
           │    └── filters
           │         └── a.x:1 = b.x:4
           ├── scan c
           │    └── columns: c.x:7 c.y:8 c.rowid:9!null
           └── filters
                └── a.x:1 = c.x:7

build
SELECT * FROM (onecolumn AS a JOIN onecolumn AS b USING(y))
----
error (42703): column "y" specified in USING clause does not exist in left table

build
SELECT * FROM (onecolumn AS a JOIN onecolumn AS b USING(x, x))
----
error (42701): column "x" appears more than once in USING clause

exec-ddl
CREATE TABLE othertype (x TEXT)
----

build
SELECT * FROM (onecolumn AS a JOIN othertype AS b USING(x))
----
error (42804): JOIN/USING types int for left and string for right cannot be matched for column "x"

build
SELECT * FROM (onecolumn JOIN onecolumn USING(x))
----
error (42712): source name "onecolumn" specified more than once (missing AS clause)

build
SELECT * FROM (onecolumn JOIN twocolumn USING(x) JOIN onecolumn USING(x))
----
error (42712): source name "onecolumn" specified more than once (missing AS clause)

# Check that star expansion works across anonymous sources.
build
SELECT * FROM (SELECT * FROM onecolumn), (SELECT * FROM onecolumn)
----
inner-join (cross)
 ├── columns: x:1 x:3
 ├── project
 │    ├── columns: x:1
 │    └── scan onecolumn
 │         └── columns: x:1 rowid:2!null
 ├── project
 │    ├── columns: x:3
 │    └── scan onecolumn
 │         └── columns: x:3 rowid:4!null
 └── filters (true)

# Check that anonymous sources are properly looked up without ambiguity.
build
SELECT x FROM (onecolumn JOIN othercolumn USING (x)) JOIN (onecolumn AS a JOIN othercolumn AS b USING(x)) USING(x)
----
project
 ├── columns: x:1!null
 └── inner-join (hash)
      ├── columns: onecolumn.x:1!null onecolumn.rowid:2!null othercolumn.x:3!null othercolumn.rowid:4!null a.x:5!null a.rowid:6!null b.x:7!null b.rowid:8!null
      ├── inner-join (hash)
      │    ├── columns: onecolumn.x:1!null onecolumn.rowid:2!null othercolumn.x:3!null othercolumn.rowid:4!null
      │    ├── scan onecolumn
      │    │    └── columns: onecolumn.x:1 onecolumn.rowid:2!null
      │    ├── scan othercolumn
      │    │    └── columns: othercolumn.x:3 othercolumn.rowid:4!null
      │    └── filters
      │         └── onecolumn.x:1 = othercolumn.x:3
      ├── inner-join (hash)
      │    ├── columns: a.x:5!null a.rowid:6!null b.x:7!null b.rowid:8!null
      │    ├── scan a
      │    │    └── columns: a.x:5 a.rowid:6!null
      │    ├── scan b
      │    │    └── columns: b.x:7 b.rowid:8!null
      │    └── filters
      │         └── a.x:5 = b.x:7
      └── filters
           └── onecolumn.x:1 = a.x:5

# Check that multiple anonymous sources cause proper ambiguity errors.
build
SELECT x FROM (SELECT * FROM onecolumn), (SELECT * FROM onecolumn)
----
error (42702): column reference "x" is ambiguous (candidates: <anonymous>.x)

build
SELECT * FROM (onecolumn AS a JOIN onecolumn AS b ON x > 32)
----
error (42702): column reference "x" is ambiguous (candidates: a.x, b.x)

build
SELECT * FROM (onecolumn AS a JOIN onecolumn AS b ON a.y > y)
----
error (42703): column "a.y" does not exist

# THe following queries verify that only the necessary columns are scanned.
build
SELECT a.x, b.y FROM twocolumn AS a, twocolumn AS b
----
project
 ├── columns: x:1 y:5
 └── inner-join (cross)
      ├── columns: a.x:1 a.y:2 a.rowid:3!null b.x:4 b.y:5 b.rowid:6!null
      ├── scan a
      │    └── columns: a.x:1 a.y:2 a.rowid:3!null
      ├── scan b
      │    └── columns: b.x:4 b.y:5 b.rowid:6!null
      └── filters (true)

build
SELECT b.y FROM (twocolumn AS a JOIN twocolumn AS b USING(x))
----
project
 ├── columns: y:5
 └── inner-join (hash)
      ├── columns: a.x:1!null a.y:2 a.rowid:3!null b.x:4!null b.y:5 b.rowid:6!null
      ├── scan a
      │    └── columns: a.x:1 a.y:2 a.rowid:3!null
      ├── scan b
      │    └── columns: b.x:4 b.y:5 b.rowid:6!null
      └── filters
           └── a.x:1 = b.x:4

build
SELECT b.y FROM (twocolumn AS a JOIN twocolumn AS b ON a.x = b.x)
----
project
 ├── columns: y:5
 └── inner-join (hash)
      ├── columns: a.x:1!null a.y:2 a.rowid:3!null b.x:4!null b.y:5 b.rowid:6!null
      ├── scan a
      │    └── columns: a.x:1 a.y:2 a.rowid:3!null
      ├── scan b
      │    └── columns: b.x:4 b.y:5 b.rowid:6!null
      └── filters
           └── a.x:1 = b.x:4

build
SELECT a.x FROM (twocolumn AS a JOIN twocolumn AS b ON a.x < b.y)
----
project
 ├── columns: x:1!null
 └── inner-join (cross)
      ├── columns: a.x:1!null a.y:2 a.rowid:3!null b.x:4 b.y:5!null b.rowid:6!null
      ├── scan a
      │    └── columns: a.x:1 a.y:2 a.rowid:3!null
      ├── scan b
      │    └── columns: b.x:4 b.y:5 b.rowid:6!null
      └── filters
           └── a.x:1 < b.y:5

build
SELECT * FROM (SELECT * FROM (VALUES (9, 1), (8, 2)) AS a (u, k) ORDER BY k)
  INNER JOIN (VALUES (1, 1), (2, 2)) AS b (k, w) USING (k) ORDER BY u
----
sort
 ├── columns: k:2!null u:1!null w:4!null
 ├── ordering: +1
 └── project
      ├── columns: column1:1!null column2:2!null column2:4!null
      └── inner-join (hash)
           ├── columns: column1:1!null column2:2!null column1:3!null column2:4!null
           ├── values
           │    ├── columns: column1:1!null column2:2!null
           │    ├── (9, 1)
           │    └── (8, 2)
           ├── values
           │    ├── columns: column1:3!null column2:4!null
           │    ├── (1, 1)
           │    └── (2, 2)
           └── filters
                └── column2:2 = column1:3

# Tests for filter propagation through joins.

exec-ddl
CREATE TABLE square (n INT PRIMARY KEY, sq INT)
----

exec-ddl
CREATE TABLE pairs (a INT, b INT)
----

# The filter expression becomes an equality constraint.
build
SELECT * FROM pairs, square WHERE pairs.b = square.n
----
project
 ├── columns: a:1 b:2!null n:4!null sq:5
 └── select
      ├── columns: a:1 b:2!null rowid:3!null n:4!null sq:5
      ├── inner-join (cross)
      │    ├── columns: a:1 b:2 rowid:3!null n:4!null sq:5
      │    ├── scan pairs
      │    │    └── columns: a:1 b:2 rowid:3!null
      │    ├── scan square
      │    │    └── columns: n:4!null sq:5
      │    └── filters (true)
      └── filters
           └── b:2 = n:4

# The filter expression becomes an ON predicate.
build
SELECT * FROM pairs, square WHERE pairs.a + pairs.b = square.sq
----
project
 ├── columns: a:1 b:2 n:4!null sq:5
 └── select
      ├── columns: a:1 b:2 rowid:3!null n:4!null sq:5
      ├── inner-join (cross)
      │    ├── columns: a:1 b:2 rowid:3!null n:4!null sq:5
      │    ├── scan pairs
      │    │    └── columns: a:1 b:2 rowid:3!null
      │    ├── scan square
      │    │    └── columns: n:4!null sq:5
      │    └── filters (true)
      └── filters
           └── (a:1 + b:2) = sq:5

# Query similar to the one above, but the filter refers to a rendered
# expression and can't "break through". See the comment for propagateFilters
# in fitler_opt.go for all the details.
build
SELECT a, b, n, sq FROM (SELECT a, b, a + b AS sum, n, sq FROM pairs, square) WHERE sum = sq
----
project
 ├── columns: a:1 b:2 n:4!null sq:5!null
 └── select
      ├── columns: a:1 b:2 n:4!null sq:5!null sum:6!null
      ├── project
      │    ├── columns: sum:6 a:1 b:2 n:4!null sq:5
      │    ├── inner-join (cross)
      │    │    ├── columns: a:1 b:2 rowid:3!null n:4!null sq:5
      │    │    ├── scan pairs
      │    │    │    └── columns: a:1 b:2 rowid:3!null
      │    │    ├── scan square
      │    │    │    └── columns: n:4!null sq:5
      │    │    └── filters (true)
      │    └── projections
      │         └── a:1 + b:2 [as=sum:6]
      └── filters
           └── sum:6 = sq:5

# The filter expression must stay on top of the outer join.
build
SELECT * FROM pairs FULL OUTER JOIN square ON pairs.a + pairs.b = square.sq
----
project
 ├── columns: a:1 b:2 n:4 sq:5
 └── full-join (cross)
      ├── columns: a:1 b:2 rowid:3 n:4 sq:5
      ├── scan pairs
      │    └── columns: a:1 b:2 rowid:3!null
      ├── scan square
      │    └── columns: n:4!null sq:5
      └── filters
           └── (a:1 + b:2) = sq:5

build
SELECT * FROM pairs FULL OUTER JOIN square ON pairs.a + pairs.b = square.sq WHERE pairs.b%2 <> square.sq%2
----
project
 ├── columns: a:1 b:2 n:4 sq:5
 └── select
      ├── columns: a:1 b:2 rowid:3 n:4 sq:5
      ├── full-join (cross)
      │    ├── columns: a:1 b:2 rowid:3 n:4 sq:5
      │    ├── scan pairs
      │    │    └── columns: a:1 b:2 rowid:3!null
      │    ├── scan square
      │    │    └── columns: n:4!null sq:5
      │    └── filters
      │         └── (a:1 + b:2) = sq:5
      └── filters
           └── (b:2 % 2) != (sq:5 % 2)

# Filter propagation through outer joins.

build
SELECT *
  FROM (SELECT * FROM pairs LEFT JOIN square ON b = sq AND a > 1 AND n < 6)
 WHERE b > 1 AND (n IS NULL OR n > 1) AND (n IS NULL OR a  < sq)
----
select
 ├── columns: a:1 b:2!null n:4 sq:5
 ├── project
 │    ├── columns: a:1 b:2 n:4 sq:5
 │    └── left-join (cross)
 │         ├── columns: a:1 b:2 rowid:3!null n:4 sq:5
 │         ├── scan pairs
 │         │    └── columns: a:1 b:2 rowid:3!null
 │         ├── scan square
 │         │    └── columns: n:4!null sq:5
 │         └── filters
 │              └── ((b:2 = sq:5) AND (a:1 > 1)) AND (n:4 < 6)
 └── filters
      └── ((b:2 > 1) AND ((n:4 IS NULL) OR (n:4 > 1))) AND ((n:4 IS NULL) OR (a:1 < sq:5))

build
SELECT *
  FROM (SELECT * FROM pairs RIGHT JOIN square ON b = sq AND a > 1 AND n < 6)
 WHERE (a IS NULL OR a > 2) AND n > 1 AND (a IS NULL OR a < sq)
----
select
 ├── columns: a:1 b:2 n:4!null sq:5
 ├── project
 │    ├── columns: a:1 b:2 n:4!null sq:5
 │    └── right-join (cross)
 │         ├── columns: a:1 b:2 rowid:3 n:4!null sq:5
 │         ├── scan pairs
 │         │    └── columns: a:1 b:2 rowid:3!null
 │         ├── scan square
 │         │    └── columns: n:4!null sq:5
 │         └── filters
 │              └── ((b:2 = sq:5) AND (a:1 > 1)) AND (n:4 < 6)
 └── filters
      └── (((a:1 IS NULL) OR (a:1 > 2)) AND (n:4 > 1)) AND ((a:1 IS NULL) OR (a:1 < sq:5))

# The simpler plan for an inner join, to compare.
build
SELECT *
  FROM (SELECT * FROM pairs JOIN square ON b = sq AND a > 1 AND n < 6)
 WHERE (a IS NULL OR a > 2) AND n > 1 AND (a IS NULL OR a < sq)
----
select
 ├── columns: a:1!null b:2!null n:4!null sq:5!null
 ├── project
 │    ├── columns: a:1!null b:2!null n:4!null sq:5!null
 │    └── inner-join (cross)
 │         ├── columns: a:1!null b:2!null rowid:3!null n:4!null sq:5!null
 │         ├── scan pairs
 │         │    └── columns: a:1 b:2 rowid:3!null
 │         ├── scan square
 │         │    └── columns: n:4!null sq:5
 │         └── filters
 │              └── ((b:2 = sq:5) AND (a:1 > 1)) AND (n:4 < 6)
 └── filters
      └── (((a:1 IS NULL) OR (a:1 > 2)) AND (n:4 > 1)) AND ((a:1 IS NULL) OR (a:1 < sq:5))


exec-ddl
CREATE TABLE t1 (col1 INT, x INT, col2 INT, y INT)
----

exec-ddl
CREATE TABLE t2 (col3 INT, y INT, x INT, col4 INT)
----

build
SELECT * FROM t1 JOIN t2 USING(x)
----
project
 ├── columns: x:2!null col1:1 col2:3 y:4 col3:6 y:7 col4:9
 └── inner-join (hash)
      ├── columns: col1:1 t1.x:2!null col2:3 t1.y:4 t1.rowid:5!null col3:6 t2.y:7 t2.x:8!null col4:9 t2.rowid:10!null
      ├── scan t1
      │    └── columns: col1:1 t1.x:2 col2:3 t1.y:4 t1.rowid:5!null
      ├── scan t2
      │    └── columns: col3:6 t2.y:7 t2.x:8 col4:9 t2.rowid:10!null
      └── filters
           └── t1.x:2 = t2.x:8

build
SELECT * FROM t1 NATURAL JOIN t2
----
project
 ├── columns: x:2!null y:4!null col1:1 col2:3 col3:6 col4:9
 └── inner-join (hash)
      ├── columns: col1:1 t1.x:2!null col2:3 t1.y:4!null t1.rowid:5!null col3:6 t2.y:7!null t2.x:8!null col4:9 t2.rowid:10!null
      ├── scan t1
      │    └── columns: col1:1 t1.x:2 col2:3 t1.y:4 t1.rowid:5!null
      ├── scan t2
      │    └── columns: col3:6 t2.y:7 t2.x:8 col4:9 t2.rowid:10!null
      └── filters
           ├── t1.x:2 = t2.x:8
           └── t1.y:4 = t2.y:7

