 ├── columns: i:1(int) i:3(int) b:4(bool)
 ├── ordering: +3,+4
 └── project
      ├── columns: a.i:1(int) b.i:3(int) b:4(bool)
      └── full-join
           ├── columns: a.i:1(int) a.rowid:2(int) b.i:3(int) b:4(bool) b.rowid:5(int)
