# Regression: computed columns are not wrapped with Variable outside join.
build
SELECT * FROM (SELECT x, x+1 AS plus1 FROM t1) NATURAL FULL OUTER JOIN (SELECT x, 2 AS two FROM t2)
----
project
 ├── columns: x:13 plus1:6 two:12
 └── project
      ├── columns: x:13 t1.x:2 plus1:6 t2.x:9 two:12
      ├── full-join (hash)
      │    ├── columns: t1.x:2 plus1:6 t2.x:9 two:12
      │    ├── project
      │    │    ├── columns: plus1:6 t1.x:2
      │    │    ├── scan t1
      │    │    │    └── columns: col1:1 t1.x:2 col2:3 t1.y:4 t1.rowid:5!null
      │    │    └── projections
      │    │         └── t1.x:2 + 1 [as=plus1:6]
      │    ├── project
      │    │    ├── columns: two:12!null t2.x:9
      │    │    ├── scan t2
      │    │    │    └── columns: col3:7 t2.y:8 t2.x:9 col4:10 t2.rowid:11!null
      │    │    └── projections
      │    │         └── 2 [as=two:12]
      │    └── filters
      │         └── t1.x:2 = t2.x:9
      └── projections
           └── COALESCE(t1.x:2, t2.x:9) [as=x:13]

build
SELECT * FROM t1 FULL OUTER JOIN t2 ON t1.x=t2.x
----
project
 ├── columns: col1:1 x:2 col2:3 y:4 col3:6 y:7 x:8 col4:9
 └── full-join (hash)
      ├── columns: col1:1 t1.x:2 col2:3 t1.y:4 t1.rowid:5 col3:6 t2.y:7 t2.x:8 col4:9 t2.rowid:10
      ├── scan t1
      │    └── columns: col1:1 t1.x:2 col2:3 t1.y:4 t1.rowid:5!null
      ├── scan t2
      │    └── columns: col3:6 t2.y:7 t2.x:8 col4:9 t2.rowid:10!null
      └── filters
           └── t1.x:2 = t2.x:8

