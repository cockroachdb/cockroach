           └── t1.y:4 = t2.y:7

# Tests for merge join ordering information.
exec-ddl
CREATE TABLE pkBA (a INT, b INT, c INT, d INT, PRIMARY KEY(b,a))
----

exec-ddl
CREATE TABLE pkBC (a INT, b INT, c INT, d INT, PRIMARY KEY(b,c))
----

exec-ddl
CREATE TABLE pkBAC (a INT, b INT, c INT, d INT, PRIMARY KEY(b,a,c))
----

exec-ddl
CREATE TABLE pkBAD (a INT, b INT, c INT, d INT, PRIMARY KEY(b,a,d))
----

build
SELECT * FROM pkBA AS l JOIN pkBC AS r ON l.a = r.a AND l.b = r.b AND l.c = r.c
----
inner-join (cross)
 ├── columns: a:1!null b:2!null c:3!null d:4 a:5!null b:6!null c:7!null d:8
 ├── scan l
 │    └── columns: l.a:1!null l.b:2!null l.c:3 l.d:4
 ├── scan r
 │    └── columns: r.a:5 r.b:6!null r.c:7!null r.d:8
 └── filters
      └── ((l.a:1 = r.a:5) AND (l.b:2 = r.b:6)) AND (l.c:3 = r.c:7)

build
SELECT * FROM pkBA NATURAL JOIN pkBAD
----
project
 ├── columns: a:1!null b:2!null c:3!null d:4!null
 └── inner-join (hash)
      ├── columns: pkba.a:1!null pkba.b:2!null pkba.c:3!null pkba.d:4!null pkbad.a:5!null pkbad.b:6!null pkbad.c:7!null pkbad.d:8!null
      ├── scan pkba
      │    └── columns: pkba.a:1!null pkba.b:2!null pkba.c:3 pkba.d:4
      ├── scan pkbad
      │    └── columns: pkbad.a:5!null pkbad.b:6!null pkbad.c:7 pkbad.d:8!null
      └── filters
           ├── pkba.a:1 = pkbad.a:5
           ├── pkba.b:2 = pkbad.b:6
           ├── pkba.c:3 = pkbad.c:7
           └── pkba.d:4 = pkbad.d:8

build
SELECT * FROM pkBAC AS l JOIN pkBAC AS r USING(a, b, c)
----
project
 ├── columns: a:1!null b:2!null c:3!null d:4 d:8
 └── inner-join (hash)
      ├── columns: l.a:1!null l.b:2!null l.c:3!null l.d:4 r.a:5!null r.b:6!null r.c:7!null r.d:8
      ├── scan l
      │    └── columns: l.a:1!null l.b:2!null l.c:3!null l.d:4
      ├── scan r
      │    └── columns: r.a:5!null r.b:6!null r.c:7!null r.d:8
      └── filters
           ├── l.a:1 = r.a:5
           ├── l.b:2 = r.b:6
           └── l.c:3 = r.c:7

build
SELECT * FROM pkBAC AS l JOIN pkBAD AS r ON l.c = r.d AND l.a = r.a AND l.b = r.b
----
inner-join (cross)
 ├── columns: a:1!null b:2!null c:3!null d:4 a:5!null b:6!null c:7 d:8!null
 ├── scan l
 │    └── columns: l.a:1!null l.b:2!null l.c:3!null l.d:4
 ├── scan r
 │    └── columns: r.a:5!null r.b:6!null r.c:7 r.d:8!null
 └── filters
      └── ((l.c:3 = r.d:8) AND (l.a:1 = r.a:5)) AND (l.b:2 = r.b:6)

# Tests with joins with merged columns of collated string type.
exec-ddl
CREATE TABLE str1 (a INT PRIMARY KEY, s STRING COLLATE en_u_ks_level1)
----

exec-ddl
CREATE TABLE str2 (a INT PRIMARY KEY, s STRING COLLATE en_u_ks_level1)
----

build
SELECT s, str1.s, str2.s FROM str1 INNER JOIN str2 USING(s)
----
project
 ├── columns: s:2!null s:2!null s:4!null
 └── inner-join (hash)
      ├── columns: str1.a:1!null str1.s:2!null str2.a:3!null str2.s:4!null
      ├── scan str1
      │    └── columns: str1.a:1!null str1.s:2
      ├── scan str2
      │    └── columns: str2.a:3!null str2.s:4
      └── filters
           └── str1.s:2 = str2.s:4

build
SELECT s, str1.s, str2.s FROM str1 LEFT OUTER JOIN str2 USING(s)
----
project
 ├── columns: s:2 s:2 s:4
 └── left-join (hash)
      ├── columns: str1.a:1!null str1.s:2 str2.a:3 str2.s:4
      ├── scan str1
      │    └── columns: str1.a:1!null str1.s:2
      ├── scan str2
      │    └── columns: str2.a:3!null str2.s:4
      └── filters
           └── str1.s:2 = str2.s:4

build
SELECT s, str1.s, str2.s FROM str1 RIGHT OUTER JOIN str2 USING(s)
----
project
 ├── columns: s:5 s:2 s:4
 └── project
      ├── columns: s:5 str1.a:1 str1.s:2 str2.a:3!null str2.s:4
      ├── right-join (hash)
      │    ├── columns: str1.a:1 str1.s:2 str2.a:3!null str2.s:4
      │    ├── scan str1
      │    │    └── columns: str1.a:1!null str1.s:2
      │    ├── scan str2
      │    │    └── columns: str2.a:3!null str2.s:4
      │    └── filters
      │         └── str1.s:2 = str2.s:4
      └── projections
           └── COALESCE(str1.s:2, str2.s:4) [as=s:5]

build
SELECT s, str1.s, str2.s FROM str1 FULL OUTER JOIN str2 USING(s)
----
project
 ├── columns: s:5 s:2 s:4
 └── project
      ├── columns: s:5 str1.a:1 str1.s:2 str2.a:3 str2.s:4
      ├── full-join (hash)
      │    ├── columns: str1.a:1 str1.s:2 str2.a:3 str2.s:4
      │    ├── scan str1
      │    │    └── columns: str1.a:1!null str1.s:2
      │    ├── scan str2
      │    │    └── columns: str2.a:3!null str2.s:4
      │    └── filters
      │         └── str1.s:2 = str2.s:4
      └── projections
           └── COALESCE(str1.s:2, str2.s:4) [as=s:5]

# Verify that we resolve the merged column a to str2.a but use IFNULL for
# column s which is a collated string.
build
SELECT * FROM str1 RIGHT OUTER JOIN str2 USING(a, s)
----
project
 ├── columns: a:3!null s:5
 └── project
      ├── columns: s:5 str1.a:1 str1.s:2 str2.a:3!null str2.s:4
      ├── right-join (hash)
      │    ├── columns: str1.a:1 str1.s:2 str2.a:3!null str2.s:4
      │    ├── scan str1
      │    │    └── columns: str1.a:1!null str1.s:2
      │    ├── scan str2
      │    │    └── columns: str2.a:3!null str2.s:4
      │    └── filters
      │         ├── str1.a:1 = str2.a:3
      │         └── str1.s:2 = str2.s:4
      └── projections
           └── COALESCE(str1.s:2, str2.s:4) [as=s:5]


exec-ddl
CREATE TABLE xyu (x INT, y INT, u INT, PRIMARY KEY(x,y,u))
----

exec-ddl
CREATE TABLE xyv (x INT, y INT, v INT, PRIMARY KEY(x,y,v))
----

build
