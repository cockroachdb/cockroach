 │         ├── columns: a:1(int!null) b:2(int!null) rowid:3(int!null) n:4(int!null) sq:5(int!null)
 │         ├── scan pairs
 │         │    └── columns: a:1(int) b:2(int) rowid:3(int!null)
 │         ├── scan square
 │         │    └── columns: n:4(int!null) sq:5(int)
 │         └── filters
 │              └── and [type=bool]
 │                   ├── and [type=bool]
