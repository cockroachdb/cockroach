 ├── sort
 │    ├── columns: onecolumn.x:1(int!null) twocolumn.x:3(int!null) twocolumn.y:4(int) onecolumn.x:6(int!null) twocolumn.x:8(int!null) twocolumn.y:9(int)
 │    ├── ordering: +1
 │    └── project
 │         ├── columns: onecolumn.x:1(int!null) twocolumn.x:3(int!null) twocolumn.y:4(int) onecolumn.x:6(int!null) twocolumn.x:8(int!null) twocolumn.y:9(int)
 │         └── inner-join
 │              ├── columns: onecolumn.x:1(int!null) onecolumn.rowid:2(int!null) twocolumn.x:3(int!null) twocolumn.y:4(int) twocolumn.rowid:5(int!null) onecolumn.x:6(int!null) onecolumn.rowid:7(int!null) twocolumn.x:8(int!null) twocolumn.y:9(int) twocolumn.rowid:10(int!null)
 │              ├── inner-join
 │              │    ├── columns: onecolumn.x:1(int) onecolumn.rowid:2(int!null) twocolumn.x:3(int!null) twocolumn.y:4(int) twocolumn.rowid:5(int!null) onecolumn.x:6(int!null) onecolumn.rowid:7(int!null)
 │              │    ├── inner-join
 │              │    │    ├── columns: onecolumn.x:1(int) onecolumn.rowid:2(int!null) twocolumn.x:3(int) twocolumn.y:4(int) twocolumn.rowid:5(int!null)
 │              │    │    ├── scan onecolumn
