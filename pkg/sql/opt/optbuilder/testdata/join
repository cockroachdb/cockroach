 │                   │    │    ├── variable: b [type=int]
 │                   │    │    └── variable: sq [type=int]
 │                   │    └── gt [type=bool]
 │                   │         ├── variable: a [type=int]
 │                   │         └── const: 1 [type=int]
 │                   └── lt [type=bool]
 │                        ├── variable: n [type=int]
 │                        └── const: 6 [type=int]
 └── filters
      └── and [type=bool]
           ├── and [type=bool]
           │    ├── or [type=bool]
           │    │    ├── is [type=bool]
           │    │    │    ├── variable: a [type=int]
           │    │    │    └── null [type=unknown]
           │    │    └── gt [type=bool]
           │    │         ├── variable: a [type=int]
           │    │         └── const: 2 [type=int]
