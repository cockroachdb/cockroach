# The left side of BETWEEN is typed differently in the two comparisons.
build-scalar
'' BETWEEN ''::BYTES AND ''
----
and [type=bool]
 ├── ge [type=bool]
 │    ├── const: '\x' [type=bytes]
 │    └── const: '\x' [type=bytes]
 └── le [type=bool]
      ├── const: '' [type=string]
      └── const: '' [type=string]

build-scalar
'' BETWEEN SYMMETRIC ''::BYTES AND ''
----
or [type=bool]
 ├── and [type=bool]
 │    ├── ge [type=bool]
 │    │    ├── const: '\x' [type=bytes]
 │    │    └── const: '\x' [type=bytes]
 │    └── le [type=bool]
 │         ├── const: '' [type=string]
 │         └── const: '' [type=string]
 └── and [type=bool]
