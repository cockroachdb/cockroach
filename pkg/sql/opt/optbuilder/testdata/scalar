 ├── unary-plus [type=int]
 │    └── variable: @1 [type=int]
 └── unary-minus [type=int]
      └── variable: @2 [type=int]

build-scalar vars=(int, int)
CASE WHEN @1 = 2 THEN 1 ELSE 2 END
----
searched-case [type=int]
 ├── when-list [type=int]
 │    └── when [type=int]
 │         ├── eq [type=bool]
 │         │    ├── variable: @1 [type=int]
 │         │    └── const: 2 [type=int]
 │         └── const: 1 [type=int]
 └── const: 2 [type=int]


