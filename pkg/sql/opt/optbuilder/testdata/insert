      ├── project
      │    ├── columns: column8:8(int) column9:9(int!null) column10:10(int) column1:7(int)
      │    ├── values
      │    │    ├── columns: column1:7(int)
      │    │    └── tuple [type=tuple{int}]
      │    │         └── cast: INT8 [type=int]
