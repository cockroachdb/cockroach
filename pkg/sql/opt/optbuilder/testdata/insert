      └── project
           ├── columns: column17:17 y:13 "?column?":14 column15:15!null column16:16
           ├── project
           │    ├── columns: column15:15!null column16:16 y:13 "?column?":14
           │    ├── with-scan &1 (a)
           │    │    ├── columns: y:13 "?column?":14
