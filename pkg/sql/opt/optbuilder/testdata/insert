      │    ├── column9:9 => c:3
      │    ├── column11:11 => d:4
      │    ├── "?column?":7 => e:5
      │    └── column10:10 => rowid:6
      └── project
           ├── columns: column11:11 "?column?":7!null column8:8 column9:9!null column10:10
           ├── project
           │    ├── columns: column8:8 column9:9!null column10:10 "?column?":7!null
           │    ├── project
           │    │    ├── columns: "?column?":7!null
           │    │    ├── values
           │    │    │    └── ()
           │    │    └── projections
           │    │         └── 1 [as="?column?":7]
