 ├── columns: <none>
 ├── insert-mapping:
 │    ├── column1:8 => a:1
 │    ├── column2:9 => b:2
 │    ├── column3:10 => c:3
 │    ├── column12:12 => d:4
 │    ├── column1:8 => e:5
 │    └── column11:11 => rowid:6
 └── project
      ├── columns: column12:12 column1:8!null column2:9 column3:10!null column11:11
      ├── project
      │    ├── columns: column11:11 column1:8!null column2:9 column3:10!null
      │    ├── values
      │    │    ├── columns: column1:8!null column2:9 column3:10!null
