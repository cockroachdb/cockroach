           ├── columns: "?column?":9 "?column?":10 "?column?":11
           ├── values
           │    └── ()
           └── projections
                ├── $1 [as="?column?":9]
                ├── $2 + 1 [as="?column?":10]
                └── $3 + 1.0 [as="?column?":11]

