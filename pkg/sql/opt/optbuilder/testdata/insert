 │              │    ├── columns: c_default:15!null rowid_default:16 y:10 "?column?":14
 │              │    ├── project
 │              │    │    ├── columns: "?column?":14 y:10
 │              │    │    ├── scan xyz
 │              │    │    │    └── columns: x:9!null y:10 z:11 xyz.crdb_internal_mvcc_timestamp:12 xyz.tableoid:13
 │              │    │    └── projections
 │              │    │         └── y:10 + 1 [as="?column?":14]
