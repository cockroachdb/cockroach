      │         ├── $1 [as="?column?":5]
      │         ├── $2 + 1 [as="?column?":6]
      │         └── $3 + 1.0 [as="?column?":7]
      └── project
           ├── columns: "?column?":8 "?column?":9 "?column?":10
           ├── values
           │    └── ()
           └── projections
                ├── $1 [as="?column?":8]
                ├── $2 + 1 [as="?column?":9]
                └── $3 + 1.0 [as="?column?":10]

