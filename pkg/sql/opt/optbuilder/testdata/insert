      │    └── projections
      │         └── abcde.c:7 + 1 [as=column11:11]
      └── projections
           ├── abcde.b:6 < column11:11 [as=check1:12]
           └── abcde.a:5 > 0 [as=check2:13]

# ------------------------------------------------------------------------------
# Test decimal column rounding.
# ------------------------------------------------------------------------------

