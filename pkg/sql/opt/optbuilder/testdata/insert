 │         │    └── rowid_default:12 => rowid:6
 │         └── project
 │              ├── columns: d_comp:13 column1:9!null b_default:10 c_default:11!null rowid_default:12
 │              ├── project
 │              │    ├── columns: b_default:10 c_default:11!null rowid_default:12 column1:9!null
