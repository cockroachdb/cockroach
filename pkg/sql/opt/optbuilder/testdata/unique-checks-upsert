 │    └── projections
 │         └── NULL::INT8 [as=d_default:15]
 └── unique-checks
      ├── unique-checks-item: uniq_overlaps_pk(b,c)
      │    └── project
      │         ├── columns: b:23 c:24
      │         └── semi-join (hash)
      │              ├── columns: a:22 b:23 c:24 d:25
