      │    └── projections
      │         └── plus [type=int]
      │              ├── variable: column14 [type=int]
      │              └── const: 1 [type=int]
      └── projections
           ├── case [type=int]
           │    ├── true [type=bool]
           │    ├── when [type=int]
           │    │    ├── is [type=bool]
