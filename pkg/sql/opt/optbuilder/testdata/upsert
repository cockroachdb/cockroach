----
insert xyz
 ├── columns: <none>
 ├── insert-mapping:
 │    ├── column1:4 => x:1
 │    ├── column2:5 => y:2
 │    └── column3:6 => z:3
 └── upsert-distinct-on
      ├── columns: column1:4!null column2:5!null column3:6!null
      ├── grouping columns: column2:5!null column3:6!null
      ├── project
      │    ├── columns: column1:4!null column2:5!null column3:6!null
      │    └── select
      │         ├── columns: column1:4!null column2:5!null column3:6!null x:7 y:8 z:9
      │         ├── left-join (hash)
      │         │    ├── columns: column1:4!null column2:5!null column3:6!null x:7 y:8 z:9
      │         │    ├── values
      │         │    │    ├── columns: column1:4!null column2:5!null column3:6!null
