insert xyz
 ├── columns: <none>
 ├── arbiter indexes: secondary
 ├── insert-mapping:
 │    ├── column1:5 => x:1
 │    ├── column2:6 => y:2
 │    └── column3:7 => z:3
 └── upsert-distinct-on
      ├── columns: column1:5!null column2:6!null column3:7!null
