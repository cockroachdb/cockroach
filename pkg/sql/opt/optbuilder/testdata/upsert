 ├── columns: <none>
 ├── canary column: 7
 ├── fetch columns: x:7(int) y:8(int) z:9(int)
 ├── insert-mapping:
 │    ├──  upsert_x:10 => x:1
 │    ├──  upsert_y:11 => y:2
 │    └──  upsert_z:12 => z:3
 ├── update-mapping:
 │    ├──  upsert_y:11 => y:2
 │    └──  upsert_z:12 => z:3
