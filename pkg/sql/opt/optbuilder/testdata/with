                │         └──  x.b:5(int) => b:10(int)
                ├── scan x
                │    └── columns: x.a:11(int) x.b:12(int) rowid:13(int!null)
                └── filters
                     ├── eq [type=bool]
                     │    ├── variable: a [type=int]
                     │    └── variable: x.a [type=int]
                     └── eq [type=bool]
                          ├── variable: b [type=int]
                          └── variable: x.b [type=int]

