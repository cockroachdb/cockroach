 ├── values
 │    ├── columns: column1:4!null
 │    ├── (1,)
 │    ├── (2,)
 │    └── (3,)
 └── insert a
      ├── columns: <none>
      ├── insert-mapping:
      │    ├── w:6 => x:1
      │    └── column7:7 => rowid:2
      └── project
           ├── columns: column7:7 w:6!null
           ├── project
           │    ├── columns: w:6!null
           │    ├── with-scan &1 (b)
           │    │    ├── columns: z:5!null
           │    │    └── mapping:
           │    │         └──  column1:4 => z:5
           │    └── projections
