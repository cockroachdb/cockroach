           │                   │              └── variable: y:2
           │                   └── const: 1
           ├── params: a_arg:4
           └── body
                └── limit
                     ├── columns: b:6
                     ├── project
                     │    ├── columns: b:6
                     │    └── select
                     │         ├── columns: a:5!null b:6 c:7 crdb_internal_mvcc_timestamp:8 tableoid:9
                     │         ├── scan abc
                     │         │    └── columns: a:5!null b:6 c:7 crdb_internal_mvcc_timestamp:8 tableoid:9
                     │         └── filters
                     │              └── eq
                     │                   ├── variable: a:5
                     │                   └── variable: a_arg:4
                     └── const: 1

