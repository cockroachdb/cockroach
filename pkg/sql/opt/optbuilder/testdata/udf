      │                   │    ├── values
      │                   │    │    └── tuple
      │                   │    └── projections
      │                   │         └── const: 1 [as="?column?":8]
      │                   └── const: 1
      └── plus [as="?column?":12]
           ├── variable: b:2
           └── udf: two
                └── body
                     ├── project
                     │    ├── columns: "?column?":10!null
