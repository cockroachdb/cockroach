# --------------------------------------------------
# UDFs with anonymous arguments.
# --------------------------------------------------

exec-ddl
CREATE FUNCTION add_anon(INT, INT) RETURNS INT LANGUAGE SQL AS $$
  SELECT $1+$2;
$$;
----

build format=show-scalars
SELECT add_anon(1, 2)
