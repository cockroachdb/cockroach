 │    ├── input binding: &1
 │    ├── cascades
 │    │    └── fk
 │    └── project
 │         ├── columns: q:9 pk:5!null p:6!null parent_multi.q:7 crdb_internal_mvcc_timestamp:8
 │         ├── select
 │         │    ├── columns: pk:5!null p:6!null parent_multi.q:7 crdb_internal_mvcc_timestamp:8
 │         │    ├── scan parent_multi
 │         │    │    └── columns: pk:5!null p:6 parent_multi.q:7 crdb_internal_mvcc_timestamp:8
 │         │    └── filters
 │         │         └── p:6 > 1
 │         └── projections
 │              └── parent_multi.q:7 * 10 [as=q:9]
 └── cascade
