exec-ddl
CREATE TABLE parent (p INT PRIMARY KEY)
----

exec-ddl
CREATE TABLE child (c INT PRIMARY KEY, p INT NOT NULL REFERENCES parent(p) ON UPDATE SET NULL)
----

build-cascades
UPDATE parent SET p = p * 10 WHERE p > 1
----
root
 ├── update parent
 │    ├── columns: <none>
 │    ├── fetch columns: parent.p:3
 │    ├── update-mapping:
 │    │    └── p:5 => parent.p:1
 │    ├── input binding: &1
 │    ├── cascades
 │    │    └── fk_p_ref_parent
 │    └── project
 │         ├── columns: p:5!null parent.p:3!null crdb_internal_mvcc_timestamp:4
 │         ├── select
 │         │    ├── columns: parent.p:3!null crdb_internal_mvcc_timestamp:4
 │         │    ├── scan parent
 │         │    │    └── columns: parent.p:3!null crdb_internal_mvcc_timestamp:4
 │         │    └── filters
 │         │         └── parent.p:3 > 1
 │         └── projections
 │              └── parent.p:3 * 10 [as=p:5]
 └── cascade
      └── update child
           ├── columns: <none>
           ├── fetch columns: c:9 child.p:10
           ├── update-mapping:
           │    └── p:14 => child.p:7
           └── project
                ├── columns: p:14 c:9!null child.p:10!null p:12!null p:13!null
                ├── inner-join (hash)
                │    ├── columns: c:9!null child.p:10!null p:12!null p:13!null
                │    ├── scan child
                │    │    └── columns: c:9!null child.p:10!null
                │    ├── select
                │    │    ├── columns: p:12!null p:13!null
                │    │    ├── with-scan &1
                │    │    │    ├── columns: p:12!null p:13!null
                │    │    │    └── mapping:
                │    │    │         ├──  parent.p:3 => p:12
                │    │    │         └──  p:5 => p:13
                │    │    └── filters
                │    │         └── p:12 IS DISTINCT FROM p:13
                │    └── filters
                │         └── child.p:10 = p:12
                └── projections
                     └── NULL::INT8 [as=p:14]

exec-ddl
CREATE TABLE parent_multi (
  pk INT PRIMARY KEY,
  p INT, q INT,
  UNIQUE (p, q),
  FAMILY (pk),
  FAMILY (p),
  FAMILY (q)
)
----

exec-ddl
CREATE TABLE child_multi (
  c INT PRIMARY KEY,
  p INT, q INT,
  UNIQUE (c, q),
  CONSTRAINT fk FOREIGN KEY (p, q) REFERENCES parent_multi(p, q) ON UPDATE SET NULL 
)
----

build-cascades
UPDATE parent_multi SET p = p * 10, q = q + 1 WHERE pk > 1
----
root
 ├── update parent_multi
 │    ├── columns: <none>
 │    ├── fetch columns: pk:5 parent_multi.p:6 parent_multi.q:7
 │    ├── update-mapping:
 │    │    ├── p:9 => parent_multi.p:2
 │    │    └── q:10 => parent_multi.q:3
 │    ├── input binding: &1
 │    ├── cascades
 │    │    └── fk
 │    └── project
 │         ├── columns: p:9 q:10 pk:5!null parent_multi.p:6 parent_multi.q:7 crdb_internal_mvcc_timestamp:8
 │         ├── select
 │         │    ├── columns: pk:5!null parent_multi.p:6 parent_multi.q:7 crdb_internal_mvcc_timestamp:8
 │         │    ├── scan parent_multi
 │         │    │    └── columns: pk:5!null parent_multi.p:6 parent_multi.q:7 crdb_internal_mvcc_timestamp:8
 │         │    └── filters
 │         │         └── pk:5 > 1
 │         └── projections
 │              ├── parent_multi.p:6 * 10 [as=p:9]
 │              └── parent_multi.q:7 + 1 [as=q:10]
 └── cascade
      └── update child_multi
           ├── columns: <none>
           ├── fetch columns: c:15 child_multi.p:16 child_multi.q:17
           ├── update-mapping:
           │    ├── p:23 => child_multi.p:12
           │    └── p:23 => child_multi.q:13
           └── project
                ├── columns: p:23 c:15!null child_multi.p:16!null child_multi.q:17!null p:19!null q:20!null p:21 q:22
                ├── inner-join (hash)
                │    ├── columns: c:15!null child_multi.p:16!null child_multi.q:17!null p:19!null q:20!null p:21 q:22
                │    ├── scan child_multi
                │    │    └── columns: c:15!null child_multi.p:16 child_multi.q:17
                │    ├── select
                │    │    ├── columns: p:19 q:20 p:21 q:22
                │    │    ├── with-scan &1
                │    │    │    ├── columns: p:19 q:20 p:21 q:22
                │    │    │    └── mapping:
                │    │    │         ├──  parent_multi.p:6 => p:19
                │    │    │         ├──  parent_multi.q:7 => q:20
                │    │    │         ├──  p:9 => p:21
                │    │    │         └──  q:10 => q:22
                │    │    └── filters
                │    │         └── (p:19 IS DISTINCT FROM p:21) OR (q:20 IS DISTINCT FROM q:22)
                │    └── filters
                │         ├── child_multi.p:16 = p:19
                │         └── child_multi.q:17 = q:20
                └── projections
                     └── NULL::INT8 [as=p:23]

# Update only one of the two FK columns.
build-cascades
UPDATE parent_multi SET p = p * 10 WHERE p > 1
----
root
 ├── update parent_multi
 │    ├── columns: <none>
 │    ├── fetch columns: pk:5 parent_multi.p:6 q:7
 │    ├── update-mapping:
 │    │    └── p:9 => parent_multi.p:2
 │    ├── input binding: &1
 │    ├── cascades
 │    │    └── fk
 │    └── project
 │         ├── columns: p:9!null pk:5!null parent_multi.p:6!null q:7 crdb_internal_mvcc_timestamp:8
 │         ├── select
 │         │    ├── columns: pk:5!null parent_multi.p:6!null q:7 crdb_internal_mvcc_timestamp:8
 │         │    ├── scan parent_multi
 │         │    │    └── columns: pk:5!null parent_multi.p:6 q:7 crdb_internal_mvcc_timestamp:8
 │         │    └── filters
 │         │         └── parent_multi.p:6 > 1
 │         └── projections
 │              └── parent_multi.p:6 * 10 [as=p:9]
 └── cascade
      └── update child_multi
           ├── columns: <none>
           ├── fetch columns: c:14 child_multi.p:15 child_multi.q:16
           ├── update-mapping:
           │    ├── p:22 => child_multi.p:11
           │    └── p:22 => child_multi.q:12
           └── project
                ├── columns: p:22 c:14!null child_multi.p:15!null child_multi.q:16!null p:18!null q:19!null p:20!null q:21
                ├── inner-join (hash)
                │    ├── columns: c:14!null child_multi.p:15!null child_multi.q:16!null p:18!null q:19!null p:20!null q:21
                │    ├── scan child_multi
                │    │    └── columns: c:14!null child_multi.p:15 child_multi.q:16
                │    ├── select
                │    │    ├── columns: p:18!null q:19 p:20!null q:21
                │    │    ├── with-scan &1
                │    │    │    ├── columns: p:18!null q:19 p:20!null q:21
                │    │    │    └── mapping:
                │    │    │         ├──  parent_multi.p:6 => p:18
                │    │    │         ├──  parent_multi.q:7 => q:19
                │    │    │         ├──  p:9 => p:20
                │    │    │         └──  parent_multi.q:7 => q:21
                │    │    └── filters
                │    │         └── (p:18 IS DISTINCT FROM p:20) OR (q:19 IS DISTINCT FROM q:21)
                │    └── filters
                │         ├── child_multi.p:15 = p:18
                │         └── child_multi.q:16 = q:19
                └── projections
                     └── NULL::INT8 [as=p:22]

# Test a two-level cascade.
exec-ddl
CREATE TABLE grandchild (
  g INT PRIMARY KEY,
  c INT, q INT,
  CONSTRAINT fk2 FOREIGN KEY (c, q) REFERENCES child_multi(c, q) ON UPDATE SET NULL
)
----

build-cascades
UPDATE parent_multi SET q = q * 10 WHERE p > 1
----
root
 ├── update parent_multi
 │    ├── columns: <none>
 │    ├── fetch columns: pk:5 p:6 parent_multi.q:7
 │    ├── update-mapping:
