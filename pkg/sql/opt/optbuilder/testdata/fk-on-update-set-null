      │    ├── update-mapping:
      │    │    ├── p:22 => child_multi.p:11
      │    │    └── p:22 => child_multi.q:12
      │    ├── input binding: &2
      │    ├── cascades
      │    │    └── fk2
      │    └── project
      │         ├── columns: p:22 c:14!null child_multi.p:15!null child_multi.q:16!null p:18!null q:19!null p:20!null q:21
      │         ├── inner-join (hash)
      │         │    ├── columns: c:14!null child_multi.p:15!null child_multi.q:16!null p:18!null q:19!null p:20!null q:21
      │         │    ├── scan child_multi
      │         │    │    └── columns: c:14!null child_multi.p:15 child_multi.q:16
      │         │    ├── select
      │         │    │    ├── columns: p:18!null q:19 p:20!null q:21
      │         │    │    ├── with-scan &1
      │         │    │    │    ├── columns: p:18!null q:19 p:20!null q:21
      │         │    │    │    └── mapping:
      │         │    │    │         ├──  parent_multi.p:6 => p:18
      │         │    │    │         ├──  parent_multi.q:7 => q:19
      │         │    │    │         ├──  parent_multi.p:6 => p:20
      │         │    │    │         └──  q:9 => q:21
      │         │    │    └── filters
      │         │    │         └── (p:18 IS DISTINCT FROM p:20) OR (q:19 IS DISTINCT FROM q:21)
      │         │    └── filters
      │         │         ├── child_multi.p:15 = p:18
      │         │         └── child_multi.q:16 = q:19
      │         └── projections
      │              └── NULL::INT8 [as=p:22]
      └── cascade
           └── update grandchild
                ├── columns: <none>
                ├── fetch columns: g:27 grandchild.c:28 grandchild.q:29
                ├── update-mapping:
                │    ├── c:35 => grandchild.c:24
                │    └── c:35 => grandchild.q:25
                └── project
                     ├── columns: c:35 g:27!null grandchild.c:28!null grandchild.q:29!null c:31!null q:32!null c:33!null p:34
                     ├── inner-join (hash)
                     │    ├── columns: g:27!null grandchild.c:28!null grandchild.q:29!null c:31!null q:32!null c:33!null p:34
                     │    ├── scan grandchild
                     │    │    └── columns: g:27!null grandchild.c:28 grandchild.q:29
                     │    ├── select
                     │    │    ├── columns: c:31!null q:32!null c:33!null p:34
                     │    │    ├── with-scan &2
                     │    │    │    ├── columns: c:31!null q:32!null c:33!null p:34
                     │    │    │    └── mapping:
