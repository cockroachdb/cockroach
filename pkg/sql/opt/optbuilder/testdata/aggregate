 ├── columns: avg:5 avg:5 avg:7 avg:7
 ├── project
 │    ├── columns: column6:6 x:1!null
 │    ├── scan xyz
 │    │    └── columns: x:1!null y:2 z:3 crdb_internal_mvcc_timestamp:4
 │    └── projections
 │         └── y:2 > 0 [as=column6:6]
 └── aggregations
      ├── agg-distinct [as=avg:5]
      │    └── avg
      │         └── x:1
      └── agg-filter [as=avg:7]
           ├── agg-distinct
           │    └── avg
           │         └── x:1
           └── column6:6

