 ├── columns: array_agg:7
 ├── project
 │    ├── columns: column6:6!null
 │    ├── scan kv
 │    │    └── columns: k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5
 │    └── projections
 │         └── 1 [as=column6:6]
 └── aggregations
      └── array-agg [as=array_agg:7]
           └── column6:6

build
