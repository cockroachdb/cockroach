# FILTER.

build
SELECT sum(abc.d) FILTER (WHERE abc.d > 0) FROM abc
----
scalar-group-by
 ├── columns: sum:7
 ├── project
 │    ├── columns: column6:6 d:4
 │    ├── scan abc
 │    │    └── columns: a:1!null b:2 c:3 d:4 crdb_internal_mvcc_timestamp:5
 │    └── projections
 │         └── d:4 > 0 [as=column6:6]
 └── aggregations
      └── agg-filter [as=sum:7]
           ├── sum
           │    └── d:4
           └── column6:6

# Ensure aggregates with FILTER coexist properly with non-FILTER aggregates.
build
SELECT
    sum(x) FILTER (WHERE y > 0),
    avg(DISTINCT z),
    avg(DISTINCT z) FILTER (WHERE y > 0)
FROM xyz
----
scalar-group-by
 ├── columns: sum:6 avg:7 avg:8
 ├── project
 │    ├── columns: column5:5 x:1!null z:3
 │    ├── scan xyz
 │    │    └── columns: x:1!null y:2 z:3 crdb_internal_mvcc_timestamp:4
 │    └── projections
 │         └── y:2 > 0 [as=column5:5]
 └── aggregations
      ├── agg-filter [as=sum:6]
      │    ├── sum
      │    │    └── x:1
      │    └── column5:5
      ├── agg-distinct [as=avg:7]
      │    └── avg
      │         └── z:3
      └── agg-filter [as=avg:8]
           ├── agg-distinct
           │    └── avg
           │         └── z:3
           └── column5:5

# Ensure aggregates involving FILTER are deduplicated.
build
SELECT
    avg(DISTINCT x),
    avg(DISTINCT x),
    avg(DISTINCT x) FILTER (WHERE y > 0),
    avg(DISTINCT x) FILTER (WHERE y > 0)
FROM xyz
----
