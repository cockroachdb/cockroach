 ├── columns: v:2 count:6!null
 ├── ordering: -2
 └── group-by
      ├── columns: v:2 count:6!null
      ├── grouping columns: v:2
      ├── project
      │    ├── columns: k:1!null v:2
      │    └── scan kv
      │         └── columns: k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5
      └── aggregations
           └── count [as=count:6]
                └── k:1

build
SELECT v, count(k) FROM kv GROUP BY v ORDER BY count(k) DESC
----
sort
 ├── columns: v:2 count:6!null
 ├── ordering: -6
 └── group-by
      ├── columns: v:2 count:6!null
      ├── grouping columns: v:2
      ├── project
      │    ├── columns: k:1!null v:2
      │    └── scan kv
      │         └── columns: k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5
      └── aggregations
           └── count [as=count:6]
                └── k:1

build
SELECT v, count(k) FROM kv GROUP BY v ORDER BY v-count(k)
----
sort
 ├── columns: v:2 count:6!null  [hidden: column7:7]
 ├── ordering: +7
 └── project
      ├── columns: column7:7 v:2 count:6!null
      ├── group-by
      │    ├── columns: v:2 count:6!null
      │    ├── grouping columns: v:2
      │    ├── project
      │    │    ├── columns: k:1!null v:2
      │    │    └── scan kv
      │    │         └── columns: k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5
      │    └── aggregations
      │         └── count [as=count:6]
      │              └── k:1
      └── projections
           └── v:2 - count:6 [as=column7:7]

build
SELECT v FROM kv GROUP BY v ORDER BY sum(k)
----
sort
 ├── columns: v:2  [hidden: sum:6!null]
 ├── ordering: +6
 └── group-by
      ├── columns: v:2 sum:6!null
      ├── grouping columns: v:2
      ├── project
      │    ├── columns: k:1!null v:2
      │    └── scan kv
      │         └── columns: k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5
      └── aggregations
           └── sum [as=sum:6]
                └── k:1

