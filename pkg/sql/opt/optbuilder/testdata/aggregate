# Regression test for #46914.
exec-ddl
CREATE TABLE t0(c0 INT)
----

build format=(show-types,show-scalars)
SELECT * FROM t0 GROUP BY t0.c0 HAVING min((CASE WHEN false THEN NULL END):::FLOAT) IS NOT NAN
----
project
 ├── columns: c0:1(int)
 └── select
      ├── columns: c0:1(int) min:5(float!null)
      ├── group-by
      │    ├── columns: c0:1(int) min:5(float)
      │    ├── grouping columns: c0:1(int)
      │    ├── project
      │    │    ├── columns: column4:4(float) c0:1(int)
      │    │    ├── scan t0
      │    │    │    └── columns: c0:1(int) rowid:2(int!null) crdb_internal_mvcc_timestamp:3(decimal)
      │    │    └── projections
      │    │         └── case [as=column4:4, type=float]
      │    │              ├── true [type=bool]
      │    │              ├── when [type=float]
      │    │              │    ├── false [type=bool]
      │    │              │    └── cast: FLOAT8 [type=float]
      │    │              │         └── null [type=unknown]
      │    │              └── null [type=float]
      │    └── aggregations
      │         └── min [as=min:5, type=float]
      │              └── variable: column4:4 [type=float]
      └── filters
           └── ne [type=bool]
                ├── variable: min:5 [type=float]
                └── const: NaN [type=float]

# Regression test for #51877.
