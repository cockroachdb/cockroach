 │    │         └── variable: bools.b [type=bool]
 │    └── aggregations
 └── projections
      └── and [type=bool]
           ├── and [type=bool]
           │    ├── variable: bools.b [type=bool]
           │    └── variable: abc.c [type=bool]
           └── variable: bools.b [type=bool]

