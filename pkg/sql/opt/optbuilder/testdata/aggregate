 ├── columns: r:7!null  [hidden: max:6]
 ├── ordering: +6
 ├── scalar-group-by
 │    ├── columns: max:6
 │    ├── project
 │    │    ├── columns: v:2
 │    │    └── scan kv
 │    │         └── columns: k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5
 │    └── aggregations
 │         └── max [as=max:6]
 │              └── v:2
 └── projections
      └── 123 [as=r:7]

# Check that ordering columns are projected correctly.
build
SELECT array_agg(y) FROM (SELECT * FROM xyz ORDER BY x+y)
----
scalar-group-by
 ├── columns: array_agg:6
 ├── internal-ordering: +5
 ├── sort
 │    ├── columns: y:2 column5:5
 │    ├── ordering: +5
 │    └── project
 │         ├── columns: y:2 column5:5
 │         └── project
 │              ├── columns: column5:5 x:1!null y:2 z:3
 │              ├── scan xyz
 │              │    └── columns: x:1!null y:2 z:3 crdb_internal_mvcc_timestamp:4
 │              └── projections
 │                   └── x:1 + y:2 [as=column5:5]
 └── aggregations
      └── array-agg [as=array_agg:6]
           └── y:2

build
SELECT array_agg(y) FROM (SELECT * FROM xyz ORDER BY x DESC)
----
scalar-group-by
 ├── columns: array_agg:5
 ├── internal-ordering: -1
 ├── project
 │    ├── columns: x:1!null y:2
 │    ├── ordering: -1
 │    └── project
 │         ├── columns: x:1!null y:2 z:3
 │         ├── ordering: -1
 │         └── scan xyz,rev
 │              ├── columns: x:1!null y:2 z:3 crdb_internal_mvcc_timestamp:4
 │              └── ordering: -1
 └── aggregations
      └── array-agg [as=array_agg:5]
           └── y:2

