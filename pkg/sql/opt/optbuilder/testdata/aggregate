                ├── columns: abxy.a:1!null abxy.b:2!null x:3 y:4 abxy.crdb_internal_mvcc_timestamp:5 ab.a:6!null ab.b:7!null ab.crdb_internal_mvcc_timestamp:8
                ├── scan abxy
                │    └── columns: abxy.a:1!null abxy.b:2!null x:3 y:4 abxy.crdb_internal_mvcc_timestamp:5
                ├── scan ab
                │    └── columns: ab.a:6!null ab.b:7 ab.crdb_internal_mvcc_timestamp:8
