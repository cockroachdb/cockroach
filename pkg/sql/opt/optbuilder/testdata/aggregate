 │    ├── columns: k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5 column6:6 array_agg:7
 │    ├── project
 │    │    ├── columns: column6:6 k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5
 │    │    ├── scan kv
 │    │    │    └── columns: k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5
