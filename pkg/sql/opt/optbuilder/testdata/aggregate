                          │    └── columns: t2.a:6!null t2.b:7!null t2.x:8 t2.y:9 t2.crdb_internal_mvcc_timestamp:10
                          ├── scan abxy [as=t3]
                          │    └── columns: t3.a:11!null t3.b:12!null t3.x:13 t3.y:14 t3.crdb_internal_mvcc_timestamp:15
                          └── filters
                               └── sum:17 = t3.x:13

# Regression test for #45631.
