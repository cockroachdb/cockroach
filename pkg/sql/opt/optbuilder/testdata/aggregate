# Grouping columns cannot be reused inside an aggregate input expression
# because the aggregate input expressions and grouping expressions are
# built as part of the same projection. 
build
SELECT max((k+v)/(k-v)) AS r, (k+v)*(k-v) AS s FROM kv GROUP BY k+v, k-v
----
project
 ├── columns: r:7 s:10
 ├── group-by
 │    ├── columns: max:7 column8:8 column9:9
 │    ├── grouping columns: column8:8 column9:9
 │    ├── project
 │    │    ├── columns: column6:6 column8:8 column9:9
 │    │    ├── scan kv
 │    │    │    └── columns: k:1!null v:2 w:3 kv.s:4 crdb_internal_mvcc_timestamp:5
 │    │    └── projections
 │    │         ├── (k:1 + v:2) / (k:1 - v:2) [as=column6:6]
 │    │         ├── k:1 + v:2 [as=column8:8]
 │    │         └── k:1 - v:2 [as=column9:9]
 │    └── aggregations
 │         └── max [as=max:7]
 │              └── column6:6
 └── projections
      └── column8:8 * column9:9 [as=s:10]

build
SELECT max((k+v)/(k-v)) AS r, (k+v)*(k-v) AS s FROM kv GROUP BY k+v, (k+v)/(k-v), (k+v)*(k-v)
----
project
 ├── columns: r:7 s:9
 └── group-by
      ├── columns: column6:6 max:7 column8:8 column9:9
      ├── grouping columns: column6:6 column8:8 column9:9
      ├── project
      │    ├── columns: column6:6 column8:8 column9:9
      │    ├── scan kv
      │    │    └── columns: k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5
      │    └── projections
      │         ├── (k:1 + v:2) / (k:1 - v:2) [as=column6:6]
      │         ├── k:1 + v:2 [as=column8:8]
      │         └── (k:1 + v:2) * (k:1 - v:2) [as=column9:9]
      └── aggregations
           └── max [as=max:7]
                └── column6:6

