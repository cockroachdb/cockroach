 ├── columns: json_agg:6
 ├── project
 │    ├── columns: v:2
 │    └── scan kv
 │         └── columns: k:1!null v:2 w:3 s:4 crdb_internal_mvcc_timestamp:5
 └── aggregations
      └── json-agg [as=json_agg:6]
           └── v:2

build
