 ├── columns: column6:int:null:6
 ├── group-by
 │    ├── columns: column5:int:null:5 column6:int:null:6
 │    ├── scan
 │    │    └── columns: kv.k:int:1 kv.v:int:null:2 kv.w:int:null:3 kv.s:string:null:4
 │    ├── groupings
 │    │    └── const: 3 [type=int]
 │    └── aggregations
 │         └── function: count_rows [type=NULL]
 └── projections
      └── variable: column6 [type=int]

