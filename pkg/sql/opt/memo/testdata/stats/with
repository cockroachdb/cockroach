# Regression test for #40296.
opt disable=InlineWith
WITH
  t0 AS ((VALUES (0, 0:::OID, NULL, '')) UNION (VALUES (NULL, 0:::OID,'1970-09-08'::DATE, NULL)))
SELECT
  NULL
FROM
  a, t0
WHERE
  EXISTS(
    WITH
      t1 AS (SELECT NULL)
    SELECT
      t0.column2, a.y
  );
----
