exec-ddl
CREATE TABLE ltable(
  k int primary key,
  geom geometry
)
----

exec-ddl
CREATE TABLE rtable(
  k int primary key,
  geom geometry,
  INVERTED INDEX geom_index(geom)
)
----

opt
SELECT ltable.k, rtable.k FROM ltable JOIN rtable ON ST_Intersects(ltable.geom, rtable.geom)
----
