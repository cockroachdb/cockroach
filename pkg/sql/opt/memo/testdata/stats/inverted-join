exec-ddl
CREATE TABLE json_multi_col (
  k INT PRIMARY KEY,
  j JSONB,
  s STRING,
  i INT,
  INVERTED INDEX sj_idx (s, j)
)
----

exec-ddl
ALTER TABLE json_multi_col INJECT STATISTICS '[
  {
    "columns": ["j"],
    "created_at": "2018-01-01 1:00:00.00000+00:00",
    "row_count": 1000,
    "distinct_count": 1000,
    "null_count": 0,
    "avg_size": 34
  },
  {
    "columns": ["s"],
    "created_at": "2018-01-01 1:00:00.00000+00:00",
    "row_count": 1000,
    "distinct_count": 100,
    "null_count": 0,
    "avg_size": 12
  },
  {
    "columns": ["i"],
    "created_at": "2018-01-01 1:00:00.00000+00:00",
    "row_count": 1000,
    "distinct_count": 10,
    "null_count": 0,
    "avg_size": 2
  }
]'
