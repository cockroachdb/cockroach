exec-ddl
DROP INDEX sij_idx
----

exec-ddl
CREATE INVERTED INDEX ij_idx ON json_multi_col (i, j)
----

opt
SELECT t1.k
FROM json_multi_col AS t1
JOIN json_arr2 AS t2
ON t1.i = t2.k AND t1.j @> t2.j
