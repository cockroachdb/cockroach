# Regression test for #36442.
norm
WITH q (a, b) AS (SELECT * FROM (VALUES (true, NULL), (false, NULL), (true, 5)))
  SELECT 1
    FROM q
   WHERE q.a
GROUP BY q.b
  HAVING bool_or(q.a)
