# ----------------------------------
# Tests for partial inverted indexes
# ----------------------------------

exec-ddl
CREATE TABLE inv (
  k INT PRIMARY KEY,
  i INT,
  j JSON,
  s STRING,
  INVERTED INDEX partial (j) WHERE s IN ('foo', 'bar')
)
----

exec-ddl
ALTER TABLE inv INJECT STATISTICS '[
  {
    "columns": ["k"],
    "created_at": "2018-01-01 1:00:00.00000+00:00",
    "row_count": 5000,
    "distinct_count": 5000
  },
  {
    "columns": ["i"],
    "created_at": "2018-01-01 1:30:00.00000+00:00",
    "row_count": 5000,
    "distinct_count": 500
  },
  {
    "columns": ["j"],
    "created_at": "2018-01-01 1:30:00.00000+00:00",
    "row_count": 5000,
    "distinct_count": 500
  },
  {
    "columns": ["s"],
    "created_at": "2018-01-01 1:30:00.00000+00:00",
    "row_count": 5000,
    "distinct_count": 500,
    "null_count": 50
  }
]'
----

opt
