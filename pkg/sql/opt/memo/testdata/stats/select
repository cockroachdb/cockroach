# Regression test for #100582. A bucket with an infinite upper bound should not
# create NaNs in filtered histograms.
exec-ddl
CREATE TABLE t100582 (
  id INT PRIMARY KEY,
  s STRING NULL,
  d DECIMAL NULL
);
----

exec-ddl
ALTER TABLE t100582 INJECT STATISTICS '[
  {
    "columns": ["d"],
    "row_count": 3,
    "distinct_count": 3,
    "null_count": 0,
    "histo_buckets": [
      {
        "distinct_range": 0,
        "num_eq": 0,
        "num_range": 0,
        "upper_bound": "0"
      },
      {
        "distinct_range": 0,
        "num_eq": 1,
        "num_range": 0,
        "upper_bound": "1"
      },
      {
        "distinct_range": 1,
        "num_eq": 0,
        "num_range": 1,
        "upper_bound": "Infinity"
      }
    ],
    "histo_col_type": "DECIMAL",
    "created_at": "2023-04-04 07:47:43.132805",
    "histo_version": 2
  }
]'
