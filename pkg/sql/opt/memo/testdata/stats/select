    "distinct_count": 1,
    "null_count": 3
  },
  {
    "columns": ["rowid"],
    "created_at": "2020-01-28 03:03:03.012072+00:00",
    "row_count": 2,
    "distinct_count": 2,
    "null_count": 0,
    "histo_buckets": [
      {
        "distinct_range": 0,
        "num_eq": 1,
        "num_range": 0,
        "upper_bound": "3"
      },
      {
        "distinct_range": 0,
        "num_eq": 1,
        "num_range": 0,
        "upper_bound": "4"
      }
