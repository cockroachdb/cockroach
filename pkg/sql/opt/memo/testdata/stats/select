 ├── columns: a:1(int!null) b:2(jsonb!null) c:3(jsonb) d:4(int[])
 ├── key columns: [1] = [1]
 ├── lookup columns are key
 ├── immutable
 ├── stats: [rows=61.7284]
 ├── key: (1)
 ├── fd: (1)-->(2-4)
 ├── inner-join (zigzag t_json_arr@b_idx,inverted t_json_arr@b_idx,inverted)
 │    ├── columns: a:1(int!null)
 │    ├── eq columns: [1] = [1]
