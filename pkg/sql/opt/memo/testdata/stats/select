# Regression test for support issue #1821
exec-ddl
CREATE TABLE t1821 (
    n INT8 NOT NULL,
    a INT8 NULL,
    b INT8 NULL,
    c STRING NULL,
    CONSTRAINT t1_pkey PRIMARY KEY (n ASC),
    INDEX a_idx (a ASC),
    INDEX b_idx (b ASC)
)
