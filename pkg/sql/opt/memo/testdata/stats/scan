opt
SELECT
  *
FROM
  abcde
WHERE
  b = 10
  AND d = 1
  AND c IN (6, 4)
  AND a IS NULL
  AND e IN (1, 2, 3, 4, 5, 6, 6, 7, 8)
  AND start_time BETWEEN '2022-02-04 19:25:25.356+00:00' AND '2022-02-11 19:25:25.356+00:00'
ORDER BY
  start_time DESC
LIMIT
  1000
----
