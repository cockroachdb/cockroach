# Multi-column stats tests.
exec-ddl
CREATE TABLE multi_col (
  a UUID,
  b BOOL,
  c INT,
  d STRING,
  e INT,
  f FLOAT,
  INDEX abcde_idx (a, b, c DESC, d, e),
  INDEX ce_idx (c, e),
  INDEX bad_idx (b, a DESC, d),
  INDEX def_idx (d, e, f),
  INDEX bef_idx (b, e, f)
)
----

