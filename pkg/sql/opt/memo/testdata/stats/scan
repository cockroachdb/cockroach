# ---------------------
# Tests with Histograms
# ---------------------

exec-ddl
CREATE TABLE hist (
  a INT,
  b DATE,
  c DECIMAL,
  d FLOAT,
  e TIMESTAMP,
  f TIMESTAMPTZ,
  g STRING,
  INDEX idx_a (a),
  INDEX idx_b (b),
  INDEX idx_c (c),
  INDEX idx_d (d),
  INDEX idx_e (e),
  INDEX idx_f (f),
  INDEX idx_g (g)
)
----

