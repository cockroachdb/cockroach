exec-ddl
CREATE TABLE a_check2 (
  x INT8 PRIMARY KEY,
  y INT8,
  s STRING NOT NULL CHECK (s = 'a' OR s = 'b' OR s = 'c' OR s = 'd'),
  d DECIMAL NOT NULL,
  b BOOL,
  UNIQUE (s DESC, d)
)
----

exec-ddl
ALTER TABLE a_check2 INJECT STATISTICS '[
  {
    "columns": ["x"],
    "created_at": "2018-01-01 1:00:00.00000+00:00",
    "row_count": 2000,
    "distinct_count": 2000,
    "avg_size": 2
  },
  {
    "columns": ["y"],
    "created_at": "2018-01-01 1:00:00.00000+00:00",
    "row_count": 2000,
    "distinct_count": 2000,
    "avg_size": 3
  },
  {
    "columns": ["d"],
    "created_at": "2018-01-01 1:00:00.00000+00:00",
    "row_count": 2000,
    "distinct_count": 2000,
    "avg_size": 5
  },
  {
    "columns": ["b"],
