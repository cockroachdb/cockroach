        "distinct_count": 10,
        "histo_buckets": [
            {
                "distinct_range": 0,
                "num_eq": 0,
                "num_range": 0,
                "upper_bound": "10"
            },
            {
                "distinct_range": 9,
                "num_eq": 10000,
                "num_range": 9,
                "upper_bound": "20"
            }
        ],
        "histo_col_type": "INT8",
        "histo_version": 1,
        "name": "__auto__",
        "null_count": 0,
        "row_count": 10009
    }
]'
