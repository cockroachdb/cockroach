CREATE TABLE item
(
    i_id     integer      not null,
    i_im_id  integer,
    i_name   varchar(24),
    i_price  decimal(5,2),
    i_data   varchar(50),
    primary key (i_id)
)
----
TABLE item
 ├── i_id int not null
 ├── i_im_id int
 ├── i_name varchar
 ├── i_price decimal
 ├── i_data varchar
 └── INDEX primary
      └── i_id int not null

