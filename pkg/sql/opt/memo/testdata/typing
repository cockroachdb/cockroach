      │    ├── const: '[1, 2]' [type=jsonb]
      │    └── const: 1 [type=int]
      ├── fetch-val-path [type=jsonb]
      │    ├── const: '{"a": 5}' [type=jsonb]
      │    └── array: string[] [type=string[]]
      │         └── const: 'a' [type=string]
      └── fetch-text-path [type=string]
           ├── const: '{"a": 5}' [type=jsonb]
           └── array: string[] [type=string[]]
                └── const: 'a' [type=string]

