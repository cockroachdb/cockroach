      │    │         ├── assignment-cast: INT8 [as=column14:14, type=int, outer=(10), immutable]
      │    │         │    └── variable: y:10 [type=int]
      │    │         └── assignment-cast: INT8 [as=column15:15, type=int, outer=(10), immutable]
      │    │              └── variable: y:10 [type=int]
      │    └── projections
      │         ├── const: 10 [as=c_default:16, type=int]
      │         ├── function: unique_rowid [as=rowid_default:17, type=int, volatile]
      │         └── cast: INT8 [as=e_default:18, type=int, immutable]
      │              └── null [type=unknown]
      └── projections
           └── plus [as=d_comp:19, type=int, outer=(15,16), immutable]
                ├── plus [type=int]
                │    ├── variable: column15:15 [type=int]
                │    └── variable: c_default:16 [type=int]
                └── const: 1 [type=int]

