      ├── fd: ()-->(3), (1)==(2), (2)==(1), (1)-->(4)
      └── project
           ├── columns: column15:15(int!null) y:9(int!null) column12:12(int!null) column13:13(int) column14:14(int)
           ├── volatile
           ├── fd: ()-->(12,14), (9)-->(15)
           ├── prune: (9,12-15)
           ├── project
           │    ├── columns: column12:12(int!null) column13:13(int) column14:14(int) y:9(int!null)
           │    ├── volatile
           │    ├── fd: ()-->(12,14)
           │    ├── prune: (9,12-14)
           │    ├── project
