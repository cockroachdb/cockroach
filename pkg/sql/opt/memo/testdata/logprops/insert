# Input is cardinality 1 VALUES expression.
build
INSERT INTO abcde (a, b) (VALUES (1, 2)) RETURNING *, rowid;
----
insert abcde
 ├── columns: a:1(int!null) b:2(int!null) c:3(int!null) d:4(int!null) rowid:5(int!null)
 ├── insert-mapping:
 │    ├── column11:11 => a:1
 │    ├── column12:12 => b:2
 │    ├── c_default:13 => c:3
 │    ├── d_comp:16 => d:4
 │    ├── rowid_default:14 => rowid:5
 │    └── e_default:15 => e:6
 ├── cardinality: [1 - 1]
 ├── volatile, mutations
 ├── key: ()
 ├── fd: ()-->(1-5)
 └── project
      ├── columns: d_comp:16(int!null) column11:11(int!null) column12:12(int!null) c_default:13(int!null) rowid_default:14(int) e_default:15(int)
      ├── cardinality: [1 - 1]
      ├── volatile
      ├── key: ()
      ├── fd: ()-->(11-16)
      ├── prune: (11-16)
      ├── project
      │    ├── columns: c_default:13(int!null) rowid_default:14(int) e_default:15(int) column11:11(int!null) column12:12(int!null)
      │    ├── cardinality: [1 - 1]
      │    ├── volatile
      │    ├── key: ()
      │    ├── fd: ()-->(11-15)
      │    ├── prune: (11-15)
      │    ├── project
      │    │    ├── columns: column11:11(int!null) column12:12(int!null)
      │    │    ├── cardinality: [1 - 1]
      │    │    ├── immutable
      │    │    ├── key: ()
      │    │    ├── fd: ()-->(11,12)
      │    │    ├── prune: (11,12)
      │    │    ├── values
      │    │    │    ├── columns: column1:9(int!null) column2:10(int!null)
      │    │    │    ├── cardinality: [1 - 1]
      │    │    │    ├── key: ()
      │    │    │    ├── fd: ()-->(9,10)
      │    │    │    ├── prune: (9,10)
      │    │    │    └── tuple [type=tuple{int, int}]
      │    │    │         ├── const: 1 [type=int]
      │    │    │         └── const: 2 [type=int]
      │    │    └── projections
      │    │         ├── assignment-cast: INT8 [as=column11:11, type=int, outer=(9), immutable]
      │    │         │    └── variable: column1:9 [type=int]
      │    │         └── assignment-cast: INT8 [as=column12:12, type=int, outer=(10), immutable]
      │    │              └── variable: column2:10 [type=int]
      │    └── projections
      │         ├── const: 10 [as=c_default:13, type=int]
      │         ├── function: unique_rowid [as=rowid_default:14, type=int, volatile]
      │         └── cast: INT8 [as=e_default:15, type=int, immutable]
      │              └── null [type=unknown]
      └── projections
           └── plus [as=d_comp:16, type=int, outer=(12,13), immutable]
                ├── plus [type=int]
                │    ├── variable: column12:12 [type=int]
                │    └── variable: c_default:13 [type=int]
                └── const: 1 [type=int]

