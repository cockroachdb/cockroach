 ├── prune: (1,2)
 ├── tuple [type=tuple{int, int}]
 │    ├── const: 1 [type=int8]
 │    └── const: 2 [type=int8]
 ├── tuple [type=tuple{int, int}]
 │    ├── const: 3 [type=int8]
 │    └── const: 4 [type=int8]
 └── tuple [type=tuple{int, int}]
      ├── const: 5 [type=int8]
      └── const: 6 [type=int8]

build
