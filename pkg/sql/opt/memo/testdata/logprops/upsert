# ensure-upsert-distinct-on should create strict key in case where all grouping
# columns are not NULL.
build
INSERT INTO abc (a)
SELECT y FROM xyz WHERE y IS NOT NULL
ON CONFLICT (a) DO
UPDATE SET b=2
