build
SELECT catalog_name, sql_path
FROM (SELECT * FROM information_schema.schemata WHERE SCHEMA_NAME='public')
LEFT JOIN information_schema.tables
ON CATALOG_NAME=TABLE_CATALOG AND SCHEMA_NAME=TABLE_SCHEMA
