exec-ddl
CREATE TABLE a (x INT PRIMARY KEY, y INT, z FLOAT NOT NULL)
----
TABLE a
 ├── x int not null
 ├── y int
 ├── z float not null
 └── INDEX primary
      └── x int not null

build
SELECT a.y, SUM(a.z), a.x, False FROM a GROUP BY a.x, a.y
----
project
 ├── columns: y:2(int) column4:4(float) x:1(int!null) column5:5(bool)
 ├── stats: [rows=250, distinct(1)=250, distinct(2)=250]
 ├── group-by
 │    ├── columns: a.x:1(int!null) a.y:2(int) column4:4(float)
 │    ├── grouping columns: a.x:1(int!null) a.y:2(int)
 │    ├── stats: [rows=250, distinct(1)=250, distinct(2)=250]
 │    ├── scan a
 │    │    ├── columns: a.x:1(int!null) a.y:2(int) a.z:3(float!null)
 │    │    └── stats: [rows=1000, distinct(1)=250, distinct(2)=250, distinct(3)=250, distinct(1-3)=900]
 │    └── aggregations [outer=(3)]
 │         └── function: sum [type=float, outer=(3)]
 │              └── variable: a.z [type=float, outer=(3)]
 └── projections [outer=(1,2,4)]
      ├── variable: a.y [type=int, outer=(2)]
      ├── variable: column4 [type=float, outer=(4)]
      ├── variable: a.x [type=int, outer=(1)]
      └── false [type=bool]

# Scalar groupby.
build
SELECT SUM(a.x), MAX(a.y) FROM a
----
group-by
 ├── columns: column4:4(decimal) column5:5(int)
 ├── stats: [rows=1, distinct(4)=1, distinct(5)=1]
 ├── project
 │    ├── columns: a.x:1(int!null) a.y:2(int)
 │    ├── stats: [rows=1000, distinct(1)=250, distinct(2)=250]
 │    ├── scan a
 │    │    ├── columns: a.x:1(int!null) a.y:2(int) a.z:3(float!null)
 │    │    └── stats: [rows=1000, distinct(1)=250, distinct(2)=250, distinct(3)=250, distinct(1-3)=900]
 │    └── projections [outer=(1,2)]
 │         ├── variable: a.x [type=int, outer=(1)]
 │         └── variable: a.y [type=int, outer=(2)]
 └── aggregations [outer=(1,2)]
      ├── function: sum [type=decimal, outer=(1)]
      │    └── variable: a.x [type=int, outer=(1)]
      └── function: max [type=int, outer=(2)]
           └── variable: a.y [type=int, outer=(2)]

# No aggregate.
build
SELECT x FROM a GROUP BY x, y
----
project
 ├── columns: x:1(int!null)
 ├── stats: [rows=250, distinct(1)=250]
 ├── group-by
 │    ├── columns: a.x:1(int!null) a.y:2(int)
 │    ├── grouping columns: a.x:1(int!null) a.y:2(int)
 │    ├── stats: [rows=250, distinct(1)=250, distinct(2)=250]
 │    ├── project
 │    │    ├── columns: a.x:1(int!null) a.y:2(int)
 │    │    ├── stats: [rows=1000, distinct(1)=250, distinct(2)=250]
 │    │    ├── scan a
 │    │    │    ├── columns: a.x:1(int!null) a.y:2(int) a.z:3(float!null)
 │    │    │    └── stats: [rows=1000, distinct(1)=250, distinct(2)=250, distinct(3)=250, distinct(1-3)=900]
 │    │    └── projections [outer=(1,2)]
 │    │         ├── variable: a.x [type=int, outer=(1)]
 │    │         └── variable: a.y [type=int, outer=(2)]
 │    └── aggregations
 └── projections [outer=(1)]
      └── variable: a.x [type=int, outer=(1)]

# Group by single column key.
build
SELECT max(y) FROM a GROUP BY x
----
project
 ├── columns: column4:4(int)
 ├── stats: [rows=250]
 ├── group-by
 │    ├── columns: a.x:1(int!null) column4:4(int)
 │    ├── grouping columns: a.x:1(int!null)
 │    ├── stats: [rows=250, distinct(1)=250]
 │    ├── project
 │    │    ├── columns: a.x:1(int!null) a.y:2(int)
 │    │    ├── stats: [rows=1000, distinct(1)=250, distinct(2)=250]
 │    │    ├── scan a
 │    │    │    ├── columns: a.x:1(int!null) a.y:2(int) a.z:3(float!null)
 │    │    │    └── stats: [rows=1000, distinct(1)=250, distinct(2)=250, distinct(3)=250, distinct(1-3)=900]
 │    │    └── projections [outer=(1,2)]
 │    │         ├── variable: a.x [type=int, outer=(1)]
 │    │         └── variable: a.y [type=int, outer=(2)]
 │    └── aggregations [outer=(2)]
 │         └── function: max [type=int, outer=(2)]
 │              └── variable: a.y [type=int, outer=(2)]
 └── projections [outer=(4)]
      └── variable: column4 [type=int, outer=(4)]

# Group by non-key.
build
SELECT y, sum(z) FROM a GROUP BY y
----
group-by
 ├── columns: y:2(int) column4:4(float)
 ├── grouping columns: a.y:2(int)
 ├── stats: [rows=250, distinct(2)=250]
 ├── project
 │    ├── columns: a.y:2(int) a.z:3(float!null)
 │    ├── stats: [rows=1000, distinct(2)=250, distinct(3)=250]
 │    ├── scan a
 │    │    ├── columns: a.x:1(int!null) a.y:2(int) a.z:3(float!null)
 │    │    └── stats: [rows=1000, distinct(1)=250, distinct(2)=250, distinct(3)=250, distinct(1-3)=900]
 │    └── projections [outer=(2,3)]
 │         ├── variable: a.y [type=int, outer=(2)]
 │         └── variable: a.z [type=float, outer=(3)]
 └── aggregations [outer=(3)]
      └── function: sum [type=float, outer=(3)]
           └── variable: a.z [type=float, outer=(3)]

# Group by all columns.
build
SELECT y, sum(z) FROM a GROUP BY x, y, z
----
project
 ├── columns: y:2(int) column4:4(float)
 ├── stats: [rows=900, distinct(2)=250]
 ├── group-by
 │    ├── columns: a.x:1(int!null) a.y:2(int) a.z:3(float!null) column4:4(float)
 │    ├── grouping columns: a.x:1(int!null) a.y:2(int) a.z:3(float!null)
 │    ├── stats: [rows=900, distinct(1)=250, distinct(2)=250, distinct(3)=250, distinct(1-3)=900]
 │    ├── project
 │    │    ├── columns: a.x:1(int!null) a.y:2(int) a.z:3(float!null)
 │    │    ├── stats: [rows=1000, distinct(1)=250, distinct(2)=250, distinct(3)=250, distinct(1-3)=900]
 │    │    ├── scan a
 │    │    │    ├── columns: a.x:1(int!null) a.y:2(int) a.z:3(float!null)
 │    │    │    └── stats: [rows=1000, distinct(1)=250, distinct(2)=250, distinct(3)=250, distinct(1-3)=900]
 │    │    └── projections [outer=(1-3)]
 │    │         ├── variable: a.x [type=int, outer=(1)]
 │    │         ├── variable: a.y [type=int, outer=(2)]
 │    │         └── variable: a.z [type=float, outer=(3)]
 │    └── aggregations [outer=(3)]
 │         └── function: sum [type=float, outer=(3)]
 │              └── variable: a.z [type=float, outer=(3)]
 └── projections [outer=(2,4)]
      ├── variable: a.y [type=int, outer=(2)]
      └── variable: column4 [type=float, outer=(4)]
