# WithScan should not have outer columns.
build
SELECT
    *
FROM
    (VALUES (1), (2)) AS v (x),
    LATERAL (SELECT * FROM (WITH foo AS (SELECT 1 + x) SELECT * FROM foo))
----
error (0A000): CTEs may not be correlated

# Regression test for #40930.

exec-ddl
CREATE TABLE t40930 (
  s string
)
----

opt
WITH
    with_4189
        AS (
            SELECT
                tab_10102.s
            FROM
                t40930 AS tab_10102, (SELECT NULL) AS tab_10103 (col_24444)
        )
SELECT
    NULL
FROM
    t40930, with_4189
----
project
 ├── columns: "?column?":7(unknown)
 ├── fd: ()-->(7)
 ├── prune: (7)
 ├── inner-join (cross)
 │    ├── scan t40930
 │    │    └── unfiltered-cols: (4,5)
 │    ├── scan tab_10102
 │    │    └── unfiltered-cols: (1,2)
 │    └── filters (true)
 └── projections
      └── null [as="?column?":7, type=unknown]

build
SELECT
    *
FROM
    (VALUES (1), (2)) AS v (x),
    LATERAL (SELECT * FROM (WITH foo AS (SELECT 1 + x) SELECT * FROM foo))
----
error (0A000): CTEs may not be correlated
