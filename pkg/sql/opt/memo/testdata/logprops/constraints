 ├── scan abc
 │    ├── columns: a:1(int) b:2(bool) c:3(string)
 │    └── prune: (1-3)
 └── filters
      └── ne [type=bool, outer=(1), constraints=(/1: (/NULL - /4] [/6 - ]; tight)]
           ├── variable: a:1 [type=int]
           └── const: 5 [type=int]

