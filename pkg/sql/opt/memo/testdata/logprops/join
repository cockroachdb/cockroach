 └── exists [type=bool, outer=(1-3)]
      └── inner-join
           ├── columns: x:1(int) y:2(int)
           ├── outer: (1-3)
           ├── cardinality: [0 - 1]
           ├── stats: [rows=0]
           ├── project
           │    ├── columns: x:1(int)
           │    ├── outer: (1)
           │    ├── cardinality: [1 - 1]
           │    ├── stats: [rows=1]
           │    └── values
           │         ├── cardinality: [1 - 1]
           │         ├── stats: [rows=1]
           │         └── tuple [type=tuple{}]
           ├── project
