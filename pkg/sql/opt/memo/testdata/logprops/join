 ├── key: (6)
 ├── fd: (1)-->(2-4), (6)-->(7), (7)~~>(6), (1)==(6), (6)==(1)
 ├── prune: (2-4,7)
 ├── interesting orderings: (+1) (+6) (+7,+6)
 ├── scan fk
 │    ├── columns: k:1(int!null) v:2(int) r1:3(int!null) r2:4(int)
 │    ├── key: (1)
 │    ├── fd: (1)-->(2-4)
 │    ├── prune: (1-4)
 │    ├── interesting orderings: (+1)
 │    └── unfiltered-cols: (1-5)
 ├── scan mn
 │    ├── columns: m:6(int!null) n:7(int)
