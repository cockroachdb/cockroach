 │    │         ├── fd: (3)-->(1,2,4)
 │    │         ├── limit hint: 2.00
 │    │         ├── prune: (1-4)
 │    │         └── interesting orderings: (+3)
 │    └── const: 2 [type=int]
 ├── values
 │    ├── columns: column1:5(int!null)
 │    ├── cardinality: [3 - 3]
 │    ├── prune: (5)
 │    ├── tuple [type=tuple{int}]
 │    │    └── const: 1 [type=int]
 │    ├── tuple [type=tuple{int}]
