                ├── cardinality: [0 - 1]
                ├── multiplicity: left-rows(zero-or-one), right-rows(zero-or-one)
                ├── immutable
                ├── key: ()
                ├── fd: ()-->(5,6)
                ├── prune: (6)
                ├── project
                │    ├── columns: x:5(int)
