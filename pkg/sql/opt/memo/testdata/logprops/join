 │    └── unfiltered-cols: (6-9)
 └── filters
      ├── eq [type=bool, outer=(1,6), constraints=(/1: (/NULL - ]; /6: (/NULL - ]), fd=(1)==(6), (6)==(1)]
      │    ├── variable: r1:1 [type=int]
      │    └── variable: a:6 [type=int]
      ├── eq [type=bool, outer=(2,7), constraints=(/2: (/NULL - ]; /7: (/NULL - ]), fd=(2)==(7), (7)==(2)]
