      ├── scan xysd
      │    ├── columns: x:1(int!null) y:2(int) s:3(string) d:4(decimal!null)
      │    ├── key: (1)
      │    ├── fd: (1)-->(2-4), (3,4)~~>(1,2)
      │    ├── prune: (1-4)
      │    └── interesting orderings: (+1) (-3,+4,+1)
      ├── max1-row
      │    ├── columns: v:6(int!null)
      │    ├── outer: (1)
      │    ├── cardinality: [0 - 1]
      │    ├── key: ()
      │    ├── fd: ()-->(6)
      │    └── project
      │         ├── columns: v:6(int!null)
      │         ├── outer: (1)
      │         ├── fd: ()-->(6)
      │         ├── prune: (6)
      │         └── inner-join
      │              ├── columns: v:6(int!null) n:9(int!null)
      │              ├── outer: (1)
      │              ├── fd: ()-->(6,9)
      │              ├── interesting orderings: (+9)
      │              ├── scan uv
