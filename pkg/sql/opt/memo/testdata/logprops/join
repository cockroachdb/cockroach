 ├── fd: (1)-->(2-4), (3,4)~~>(1,2), (1)==(6), (6)==(1)
 ├── prune: (2-5)
 ├── interesting orderings: (+1) (-3,+4,+1)
 ├── scan xysd
 │    ├── columns: x:1(int!null) y:2(int) s:3(string) d:4(decimal!null)
 │    ├── key: (1)
 │    ├── fd: (1)-->(2-4), (3,4)~~>(1,2)
 │    ├── prune: (1-4)
 │    ├── interesting orderings: (+1) (-3,+4,+1)
 │    └── unfiltered-cols: (1-4)
 ├── scan uv
 │    ├── columns: u:5(int) v:6(int!null)
