 │    │    ├── key: (9)
 │    │    ├── fd: (9)-->(10-12), (11,12)~~>(9,10)
 │    │    ├── prune: (9-12)
 │    │    ├── interesting orderings: (+9) (-11,+12,+9)
 │    │    └── unfiltered-cols: (9-12)
 │    └── filters
