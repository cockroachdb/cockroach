 │              ├── variable: xysd.x:5 [type=int]
 │              └── variable: a.x:9 [type=int]
 └── filters
      └── eq [type=bool, outer=(3,5), constraints=(/3: (/NULL - ]; /5: (/NULL - ]), fd=(3)==(5), (5)==(3)]
           ├── variable: r1:3 [type=int]
           └── variable: xysd.x:5 [type=int]

