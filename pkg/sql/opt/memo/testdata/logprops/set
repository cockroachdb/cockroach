 │         └── keys: (1)
 └── project
      ├── columns: u:3(int!null) v:4(int!null)
      ├── stats: [rows=1, distinct(3,4)=1]
      └── project
           ├── columns: u:3(int!null) v:4(int!null)
           ├── stats: [rows=1, distinct(3,4)=1]
           └── select
                ├── columns: u:3(int!null) v:4(int!null) rowid:5(int!null)
                ├── stats: [rows=1, distinct(3)=1, distinct(3,4)=1]
                ├── keys: (5)
                ├── scan uv
                │    ├── columns: u:3(int) v:4(int!null) rowid:5(int!null)
