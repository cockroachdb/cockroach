exec-ddl
CREATE TABLE a (x INT PRIMARY KEY, y INT)
----
TABLE a
 ├── x int not null
 ├── y int
 └── INDEX primary
      └── x int not null

exec-ddl
CREATE TABLE b (x STRING PRIMARY KEY, z DECIMAL NOT NULL)
----
TABLE b
 ├── x string not null
 ├── z decimal not null
 └── INDEX primary
      └── x string not null

build
SELECT y, b.x, y+1
FROM a, b
WHERE a.y=1 AND a.x::string=b.x
ORDER BY y
LIMIT 10
----
limit
 ├── columns: y:2(int) x:3(string!null) column5:5(int)
 ├── stats: [rows=10]
 ├── cost: 127000.00
 ├── ordering: +2
 ├── sort
 │    ├── columns: a.y:2(int) b.x:3(string!null) column5:5(int)
 │    ├── stats: [rows=100000]
 │    ├── cost: 127000.00
 │    ├── ordering: +2
 │    └── project
 │         ├── columns: a.y:2(int) b.x:3(string!null) column5:5(int)
 │         ├── stats: [rows=100000]
 │         ├── cost: 102000.00
 │         ├── select
 │         │    ├── columns: a.x:1(int!null) a.y:2(int) b.x:3(string!null) b.z:4(decimal!null)
 │         │    ├── stats: [rows=100000]
 │         │    ├── cost: 102000.00
 │         │    ├── inner-join
 │         │    │    ├── columns: a.x:1(int!null) a.y:2(int) b.x:3(string!null) b.z:4(decimal!null)
 │         │    │    ├── stats: [rows=1000000]
 │         │    │    ├── cost: 2000.00
 │         │    │    ├── scan a
 │         │    │    │    ├── columns: a.x:1(int!null) a.y:2(int)
 │         │    │    │    ├── stats: [rows=1000]
 │         │    │    │    ├── cost: 1000.00
 │         │    │    │    └── keys: (1)
 │         │    │    ├── scan b
 │         │    │    │    ├── columns: b.x:3(string!null) b.z:4(decimal!null)
