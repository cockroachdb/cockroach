index-constraints int
@1 > 2
----
[/3 - ]

index-constraints int
@1 >= 2
----
[/2 - ]

index-constraints int
@1 < 2
----
[ - /1]

index-constraints int
@1 = 2
----
[/2 - /2]

index-constraints int
@1 > 2 AND @1 < 4
----
[/3 - /3]

index-constraints int
@1 >= 2 AND @1 <= 4
----
[/2 - /4]

index-constraints int int
@1 > 2 AND @2 > 5
----
[/3/6 - ]

index-constraints int int
@1 >= 1 AND @1 <= 2 AND @2 >= 8 AND @2 <= 9
----
[/1/8 - /2/9]

index-constraints int int
@1 > 1 AND @1 < 4 AND @2 > 5 AND @2 < 8
----
[/2/6 - /3/7]

index-constraints int int
@1 > 1 AND @1 < 4 AND @2 = 5
----
[/2/5 - /3/5]

index-constraints int int
@1 = 1 AND @2 > 3 AND @2 < 5
----
[/1/4 - /1/4]

index-constraints int int
@1 = 1 AND @2 > 3 AND @2 < 8
----
[/1/4 - /1/7]

index-constraints int
@1 > 2 AND @1 < 1
----

# Tests with a type that doesn't support Prev.
index-constraints string
@1 > 'a' AND @1 < 'z'
----
[/e'a\x00' - /'z')

index-constraints string int
@1 > 'a' AND @1 < 'z' AND @2 = 5
----
[/e'a\x00'/5 - /'z')

# Tests with a type that doesn't support Next or Prev.
index-constraints decimal
@1 > 1.5
----
(/1.5 - ]

index-constraints decimal
@1 > 1.5 AND @1 < 2
----
(/1.5 - /2)

# Note the difference here between decimal and int: we
# can't extend the exclusive start key.
index-constraints decimal decimal
@1 > 1.5 AND @2 > 2
----
(/1.5 - ]
