exec-ddl
CREATE TABLE kv (
    k INT PRIMARY KEY,
    v INT
)
----

exec-ddl
SHOW CREATE kv
----
TABLE kv
 ├── k int not null
 ├── v int
 └── INDEX primary
      └── k int not null

exec-ddl
CREATE TABLE abcdef (
    a INT NOT NULL,
    b INT,
    c INT DEFAULT (10),
    d INT AS (abcde.b + c + 1) STORED,
    e INT AS (a) STORED,
    f INT CHECK (f > 2)
)
----

exec-ddl
SHOW CREATE abcdef
----
TABLE abcdef
 ├── a int not null
 ├── b int
 ├── c int default ((10))
 ├── d int as ((abcde.b + c) + 1) stored
 ├── e int as (a) stored
 ├── f int
 ├── rowid int not null default (unique_rowid()) [hidden]
 ├── CHECK (f > 2)
 └── INDEX primary
      └── rowid int not null default (unique_rowid()) [hidden]

exec-ddl
CREATE TABLE uvwxy (
    u INT,
    v INT,
    w INT,
    x INT,
    y INT,
    PRIMARY KEY (u,v),
    FAMILY (u,v,w),
    FAMILY (x),
    FAMILY (y)
)
----


exec-ddl
SHOW CREATE uvwxy
----
TABLE uvwxy
 ├── u int not null
 ├── v int not null
 ├── w int
 ├── x int
 ├── y int
 ├── FAMILY family1 (u, v, w)
 ├── FAMILY family2 (x)
 ├── FAMILY family3 (y)
 └── INDEX primary
      ├── u int not null
      └── v int not null
