exec-ddl
CREATE TABLE abc (a INT, b INT, c INT, INDEX abc_idx(a, b, c))
----

exec-ddl
CREATE TABLE def (d INT, e INT, f INT)
----

expr
(Intersect
  (Scan [ (Table "abc") (Index "abc@abc_idx") (Cols "a,b,c") ])
  (Sort (Scan [ (Table "def") (Cols "d,e,f") ]))
  [
    (LeftCols "a,b,c")
    (RightCols "d,e,f")
    (OutCols "a,b,c")
    (Ordering "+a,+b,+c")
  ]
)
