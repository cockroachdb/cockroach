expr
(IndexJoin
  (Scan
    [
      (Table "abc")
      (Index "abc@ab")
      (Cols "a")
      (HardLimit 10)
    ]
  )
  [
    (Table (FindTable "abc"))
    (Cols "c")
  ]
)
