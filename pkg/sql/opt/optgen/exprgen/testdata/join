(InnerJoinApply
  (Sort (Scan [ (Table "abc") (Cols "a,b,c") ]))
  (Select
    (Scan [ (Table "def") (Cols "d,e,f") ])
    [ (Eq (Var "a") (Plus (Var "d") (Var "e"))) ]
  )
  [ ]
  [ ]
)
