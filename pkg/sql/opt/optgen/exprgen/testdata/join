(MergeJoin
  (Sort (Scan [ (Table "abc") (Cols "a,b,c") ]))
  (Sort (Scan [ (Table "def") (Cols "d,e,f") ]))
  [ ]
  [
    (JoinType "inner-join")
    (LeftEq "+a")
    (RightEq "+d")
    (LeftOrdering "+a")
    (RightOrdering "+d")
  ]
)
