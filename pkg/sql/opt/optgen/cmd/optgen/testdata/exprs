type ColPrivate struct {
	// Col is a column ID.
	Col opt.ColumnID

	// scalar are props.
	scalar props.Scalar
}

type SortExpr struct {
	Input RelExpr
	best  bestProps
}

func (e *SortExpr) Op() opt.Operator {
	return opt.SortOp
}

func (e *SortExpr) ChildCount() int {
	return 1
}

