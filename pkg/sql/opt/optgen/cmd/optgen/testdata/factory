#
# Generate numeric match and replace.
#
optgen factory test.opt
define Limit {
    Input Expr
    Limit Expr
}

define Scan {
}

[LimitOne, Normalize]
(Limit $input:(Scan) 1)
=>
(LimitScan $input)

[LimitNotOne, Normalize]
(Limit $input:(Scan) ^1)
=>
(LimitScan $input)
----
