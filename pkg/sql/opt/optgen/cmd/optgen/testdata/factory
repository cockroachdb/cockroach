#
# Construct dynamic replacement that uses private shared by both operators.
#
optgen factory test.opt

define Select {
    Input  Expr
    Filter Expr
}

[GroupByOps]
define GroupBy {
    Input Expr
    Aggs  Expr
    Def   GroupByDef
}

[GroupByOps]
define ScalarGroupBy {
    Input Expr
    Aggs  Expr
    Def   GroupByDef
}

define LookupJoin {
    Input Expr
    On    Expr
    Def   LookupJoinDef
}

[Test, Normalize]
(Select
    $input:(LookupJoin | GroupByOps
        $expr1:*
        $expr2:*
        $private:*
    )
    (True)
)
=>
((OpName $input) $expr1 $expr2 $private)
----
