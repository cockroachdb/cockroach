	// JoinOp
	dynConstructLookup[opt.JoinOp] = func(f *Factory, operands memo.DynamicOperands) memo.GroupID {
		return f.ConstructJoin(memo.GroupID(operands[0]), memo.GroupID(operands[1]), memo.GroupID(operands[2]))
