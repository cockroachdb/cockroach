(Project $input:*) => $input

[EliminateProject1, Normalize, HighPriority]
(Project $input:*) => $input

[EliminateProject6, Normalize, LowPriority]
(Project $input:*) => $input

[EliminateProject2, Normalize, HighPriority]
(Project $input:*) => $input

[EliminateProject4, Normalize]
(Project $input:*) => $input
