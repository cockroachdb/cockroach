var dynConstructLookup [opt.NumOperators]dynConstructFunc

func init() {
	// UnknownOp
	dynConstructLookup[opt.UnknownOp] = func(f *Factory, operands memo.DynamicOperands) memo.GroupID {
		panic("op type not initialized")
