define And {
    Left  Expr
    Right Expr
}

define True {}

define Join {
    Left  Expr
    Right Expr
}

[EliminateEmptyAnd, Normalize]
(And $conditions:[]) => (True)

[EliminateSingletonAnd, Normalize]
(And $conditions:[ $item:* ]) => $item

[CommuteJoin, Explore]
(Join $r:* $s:*) => (Join $s $r)
