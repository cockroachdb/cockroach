define And {
    Left  Expr
    Right Expr
}

define True {}

[EliminateEmptyAnd, Normalize]
(And $conditions:[]) => (True)

[EliminateSingletonAnd, Normalize]
(And $conditions:[ $item:* ]) => $item

# This rule shouldn't be added to the enumeration.
[NotNormalizeRule]
(And $left:* $right:*) => (And $right $left)
