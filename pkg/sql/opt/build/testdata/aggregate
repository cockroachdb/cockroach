 │    ├── projections [type=tuple{string}]
 │    │    └── variable: kv.s [type=string]
 │    └── projections [type=tuple{NULL}]
 │         └── function: count_rows [type=NULL]
 └── projections [type=tuple{int, string}]
      ├── variable: column5 [type=int]
      └── variable: kv.s [type=string]

