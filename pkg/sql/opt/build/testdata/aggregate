 │    │    ├── scan
 │    │    │    └── columns: xy.x:string:null:3 xy.y:string:null:4 xy.rowid:int:5
 │    │    └── true [type=bool]
 │    ├── projections [type=tuple{string, int, int}]
 │    │    ├── variable: xy.x [type=string]
 │    │    ├── variable: ab.a [type=int]
