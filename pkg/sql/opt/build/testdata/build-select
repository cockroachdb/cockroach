build
SELECT * FROM t.a WHERE x > 10
----
select
 ├── columns: a.x:1 a.y:null:2
 ├── scan
 │    └── columns: a.x:1 a.y:null:2
 └── gt
      ├── variable: a.x
      └── const: 10

build
SELECT * FROM t.a WHERE (x > 10 AND (x < 20 AND x != 13))
----
select
 ├── columns: a.x:1 a.y:null:2
 ├── scan
 │    └── columns: a.x:1 a.y:null:2
 └── and
      ├── gt
      │    ├── variable: a.x
      │    └── const: 10
      └── and
           ├── lt
           │    ├── variable: a.x
           │    └── const: 20
           └── ne
                ├── variable: a.x
                └── const: 13

build
SELECT * FROM t.a WHERE x IN (1, 2, 3)
----
select
 ├── columns: a.x:1 a.y:null:2
 ├── scan
 │    └── columns: a.x:1 a.y:null:2
 └── in
      ├── variable: a.x
      └── tuple
           ├── const: 1
           ├── const: 2
           └── const: 3

build
SELECT * FROM t.a AS A(X, Y)
----
scan
 └── columns: a.x:1 a.y:null:2
