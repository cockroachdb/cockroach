----
plus
 ├── variable: @1
 └── const: 2

build-scalar vars=(int, int)
@1 >= 5 AND @1 <= 10 AND @2 < 4
----
and
 ├── and
 │    ├── ge
 │    │    ├── variable: @1
 │    │    └── const: 5
 │    └── le
 │         ├── variable: @1
 │         └── const: 10
 └── lt
      ├── variable: @2
      └── const: 4

build-scalar vars=(int, int)
