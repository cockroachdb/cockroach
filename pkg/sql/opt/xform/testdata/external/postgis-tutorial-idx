# 12.2.4
opt
SELECT
    sum(popn_total)
FROM
    nyc_census_blocks
WHERE
    st_dwithin(
        geom,
        st_geomfromtext(
            'LINESTRING(586782 4504202,586864 4504216)',
            26918
        ),
        50
    )
----
scalar-group-by
 ├── columns: sum:13
 ├── cardinality: [1 - 1]
 ├── immutable
 ├── key: ()
 ├── fd: ()-->(13)
 ├── select
 │    ├── columns: popn_total:3 geom:10
 │    ├── immutable
 │    ├── index-join nyc_census_blocks
 │    │    ├── columns: popn_total:3 geom:10
 │    │    └── inverted-filter
 │    │         ├── columns: gid:1!null
 │    │         ├── inverted expression: /12
 │    │         │    ├── tight: false
 │    │         │    └── union spans: ["B\xfd\xff\xff\xff\xff\xff\xff\xff\xff", "B\xfd\xff\xff\xff\xff\xff\xff\xff\xff"]
 │    │         ├── key: (1)
 │    │         └── scan nyc_census_blocks@nyc_census_blocks_geom_idx
 │    │              ├── columns: gid:1!null geom_inverted_key:12!null
