# 15.0
opt
SELECT
    blocks.blkid
FROM
    nyc_census_blocks AS blocks
    JOIN nyc_subway_stations AS subways ON
            st_contains(blocks.geom, subways.geom)
WHERE
    subways.name = 'Broad St'
----
