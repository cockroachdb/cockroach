create table Person (
   id int8 not null,
    address varchar(255),
    createdOn timestamp,
    name varchar(255),
    nickName varchar(255),
    version int4 not null,
    primary key (id)
)
----
TABLE person
 ├── id int not null
 ├── address string
 ├── createdon timestamp
 ├── name string
 ├── nickname string
 ├── version int not null
 └── INDEX primary
      └── id int not null

exec-ddl
create table Person_addresses (
   Person_id int8 not null,
    addresses varchar(255),
    addresses_KEY varchar(255) not null,
