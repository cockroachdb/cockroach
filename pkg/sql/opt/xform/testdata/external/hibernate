 ├── scan this_
 │    ├── columns: this_.id:1!null this_.c_name:2
 │    ├── key: (1)
 │    └── fd: (1)-->(2)
 └── projections
      └── length(this_.c_name:2) [as=formula0_0_:6, outer=(2), immutable]

