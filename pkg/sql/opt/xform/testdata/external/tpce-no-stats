UPDATE
  exchange
SET
  ex_desc = IF(
      (
        SELECT count(*)
        FROM exchange
        WHERE ex_desc LIKE '%LAST UPDATED%'
      ) = 0,
      concat(
        ex_desc::STRING,
        ' LAST UPDATED ',
        current_timestamp::STRING
      ),
      concat(
        substring(
          ex_desc,
          1,
          char_length(ex_desc)-char_length(current_timestamp::STRING)
        ),
        current_timestamp::STRING
      )
  )
