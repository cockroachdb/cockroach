# Q19
opt
INSERT
INTO trade (
              t_id,
              t_dts,
              t_st_id,
              t_tt_id,
              t_is_cash,
              t_s_symb,
              t_qty,
              t_bid_price,
              t_ca_id,
              t_exec_name,
              t_trade_price,
              t_chrg,
              t_comm,
              t_tax,
              t_lifo
           )
VALUES (
          0,
          '2020-06-17 22:27:42.148484+00:00'::TIMESTAMP,
          'SBMT',
          'TMB',
          True,
          'SYMB',
          10,
          '100.00':::FLOAT8::DECIMAL,
          0,
          'Name',
          NULL,
