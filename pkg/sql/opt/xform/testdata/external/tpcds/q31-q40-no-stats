# --------------------------------------------------
# Q40
# NOTE: Added conversion of 30 days to an interval.
opt
	SELECT
		w_state,
		i_item_id,
		sum(
			CASE
			WHEN (
				CAST(d_date AS DATE)
				< CAST('2001-05-02' AS DATE)
			)
			THEN cs_sales_price - COALESCE(cr_refunded_cash, 0)
			ELSE 0
			END
		)
			AS sales_before,
		sum(
			CASE
			WHEN (
				CAST(d_date AS DATE)
				>= CAST('2001-05-02' AS DATE)
			)
			THEN cs_sales_price - COALESCE(cr_refunded_cash, 0)
			ELSE 0
			END
		)
			AS sales_after
	FROM
		catalog_sales
		LEFT JOIN catalog_returns ON
				cs_order_number = cr_order_number
				AND cs_item_sk = cr_item_sk,
		warehouse,
		item,
		date_dim
	WHERE
		i_current_price BETWEEN 0.99 AND 1.49
		AND i_item_sk = cs_item_sk
		AND cs_warehouse_sk = w_warehouse_sk
		AND cs_sold_date_sk = d_date_sk
		AND d_date BETWEEN (CAST('2001-05-02' AS DATE) - '30 days'::INTERVAL) AND (CAST('2001-05-02' AS DATE) + '30 days'::INTERVAL)
	GROUP BY
		w_state, i_item_id
	ORDER BY
		w_state, i_item_id
	LIMIT
		100;
----
