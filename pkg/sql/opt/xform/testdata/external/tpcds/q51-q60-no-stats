 ├── ordering: +112,+111,+14
 └── top-k
      ├── columns: i_manufact_id:14 d_qoy:60 sum:111 avg:112
      ├── internal-ordering: +112,+111,+14
      ├── k: 100
      ├── cardinality: [0 - 100]
      ├── immutable
      ├── key: (14,60)
      ├── fd: (14,60)-->(111,112)
      ├── ordering: +112,+111,+14
      └── select
           ├── columns: i_manufact_id:14 d_qoy:60 sum:111 avg:112
