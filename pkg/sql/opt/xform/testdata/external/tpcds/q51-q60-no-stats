# --------------------------------------------------
# Q53
opt
	SELECT
		*
	FROM
		(
			SELECT
				i_manufact_id,
				sum(ss_sales_price) AS sum_sales,
				avg(sum(ss_sales_price)) OVER (
					PARTITION BY i_manufact_id
				)
					AS avg_quarterly_sales
			FROM
				item, store_sales, date_dim, store
			WHERE
				ss_item_sk = i_item_sk
				AND ss_sold_date_sk = d_date_sk
				AND ss_store_sk = s_store_sk
				AND d_month_seq
					IN (
							1186,
							1186 + 1,
							1186 + 2,
							1186 + 3,
							1186 + 4,
							1186 + 5,
							1186 + 6,
							1186 + 7,
							1186 + 8,
							1186 + 9,
							1186 + 10,
							1186 + 11
						)
				AND (
						(
							i_category
							IN (
									'Books',
									'Children',
									'Electronics'
								)
							AND i_class
								IN (
										'personal',
										'portable',
										'reference',
										'self-help'
									)
							AND i_brand
								IN (
										'scholaramalgamalg #14',
										'scholaramalgamalg #7',
										'exportiunivamalg #9',
										'scholaramalgamalg #9'
									)
						)
						OR (
								i_category
								IN ('Women', 'Music', 'Men')
								AND i_class
									IN (
											'accessories',
											'classical',
											'fragrances',
											'pants'
										)
								AND i_brand
									IN (
											'amalgimporto #1',
											'edu packscholar #1',
											'exportiimporto #1',
											'importoamalg #1'
										)
							)
					)
			GROUP BY
				i_manufact_id, d_qoy
		)
			AS tmp1
	WHERE
		CASE
		WHEN avg_quarterly_sales > 0
		THEN abs(sum_sales - avg_quarterly_sales)
		/ avg_quarterly_sales
		ELSE NULL
		END
		> 0.1
	ORDER BY
		avg_quarterly_sales, sum_sales, i_manufact_id
	LIMIT
		100;
----
