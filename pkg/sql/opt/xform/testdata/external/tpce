# Q17
opt
  SELECT s_symb
    FROM security
   WHERE s_symb > 'GOOG'
     AND s_symb
         NOT IN (
                SELECT wi_s_symb
                  FROM watch_item, watch_list
                 WHERE wl_c_id = 0 AND wi_wl_id = wl_id
            )
ORDER BY s_symb ASC
   LIMIT 1;
----
limit
 ├── columns: s_symb:1!null
 ├── internal-ordering: +1
 ├── cardinality: [0 - 1]
 ├── key: ()
