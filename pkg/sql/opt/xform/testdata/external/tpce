# Q1 (With Hints)
opt
          SELECT t_id,
                 t_bid_price::FLOAT8,
                 t_exec_name,
                 t_is_cash,
                 t_trade_price::FLOAT8,
                 tt_is_mrkt,
                 se_amt::FLOAT8,
                 se_cash_due_date,
                 se_cash_type,
                 ct_amt::FLOAT8,
                 ct_dts,
                 ct_name
            FROM trade
  LEFT HASH JOIN trade_type       ON tt_id   = t_tt_id
LEFT LOOKUP JOIN settlement       ON se_t_id = t_id
LEFT LOOKUP JOIN cash_transaction ON ct_t_id = t_id
           WHERE t_id = ANY ARRAY [1, 2, 3, 4, 5];
