# Q18
opt
SELECT wi_s_symb
FROM (
  SELECT ORDINALITY AS rownum, wi_s_symb
  FROM watch_item, watch_list
  WITH ORDINALITY
  WHERE wl_c_id = 0 and wi_wl_id = wl_id
  ORDER BY wi_s_symb asc
)
WHERE rownum = (
     SELECT count(*)
     FROM watch_item, watch_list
     WHERE wl_c_id = 0 AND wi_wl_id = wl_id
)
