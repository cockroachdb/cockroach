# Q4
opt
WITH empty_trade_requests AS (
    DELETE FROM trade_request WHERE tr_t_id > 0 ORDER BY tr_t_id LIMIT 20 RETURNING tr_t_id
)
SELECT max(tr_t_id) FROM empty_trade_requests;
