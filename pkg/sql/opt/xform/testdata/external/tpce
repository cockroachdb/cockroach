         t_ca_id,
         t_exec_name,
         t_is_cash,
         t_trade_price::FLOAT8,
         t_qty,
         s_name,
         t_dts,
         t_tt_id,
         tt_name
    FROM trade, trade_type, security
   WHERE t_s_symb = 'SYMB'
     AND t_dts >= '2020-06-15 22:27:42.148484+00:00'::TIMESTAMP
     AND t_dts <= '2020-06-17 22:27:42.148484+00:00'::TIMESTAMP
     AND tt_id = t_tt_id
     AND s_symb = t_s_symb
ORDER BY t_dts ASC
