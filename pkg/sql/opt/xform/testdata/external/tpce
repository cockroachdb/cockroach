# Q4
opt
SELECT s_ex_id, s_name, co_name, lt_price::FLOAT8, tt_is_mrkt, tt_is_sell
FROM security, company, last_trade, trade_type
WHERE tt_id = 'TLS' AND
      s_symb = 'ROACH' AND
      co_id     = s_co_id AND
      lt_s_symb = s_symb;
