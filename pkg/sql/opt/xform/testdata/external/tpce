# Q13 (With Hints)
opt
           SELECT s_name, cr_rate::FLOAT8
             FROM customer
       CROSS JOIN security
INNER LOOKUP JOIN commission_rate ON cr_c_tier = c_tier AND cr_ex_id = s_ex_id
            WHERE c_id = 0
              AND s_symb = 'ROACH'
              AND cr_tt_id = 'TLS'
              AND cr_from_qty <= 100
              AND cr_to_qty >= 200
            LIMIT 1;
