# Q3
opt
   SELECT t_id,
          t_bid_price::FLOAT8,
          t_exec_name,
          t_is_cash,
          t_trade_price::FLOAT8,
          se_amt::FLOAT8,
          se_cash_due_date,
          se_cash_type,
          ct_amt::FLOAT8,
          ct_dts,
          ct_name
     FROM trade
LEFT JOIN settlement       ON se_t_id = t_id
LEFT JOIN cash_transaction ON ct_t_id = t_id
    WHERE t_ca_id = 0 AND
          t_dts >= '2020-06-15 22:27:42.148484+00:00'::TIMESTAMP AND
          t_dts <= '2020-06-25 22:27:42.148484+00:00'::TIMESTAMP
