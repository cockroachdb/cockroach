# Q7
opt
  SELECT t_id,
         t_bid_price::FLOAT8,
         t_exec_name,
         t_is_cash,
         t_trade_price::FLOAT8
    FROM trade
   WHERE t_ca_id = 0 AND
         t_dts >= '2020-06-15 22:27:42.148484+00:00'::TIMESTAMP AND
         t_dts <= '2020-06-17 22:27:42.148484+00:00'::TIMESTAMP
ORDER BY t_dts ASC
   LIMIT 20
----
