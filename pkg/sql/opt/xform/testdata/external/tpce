# Q11
opt
UPDATE cash_transaction
   SET ct_name = concat_ws(
                ' ',
                'TMB',
                10:::INT4::STRING,
                IF(ct_name LIKE '% shares of %', 'Shares of', 'shares of'),
                'SYMB'
               )
 WHERE ct_t_id = 0
----
