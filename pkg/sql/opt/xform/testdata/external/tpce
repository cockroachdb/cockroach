# Q10
opt
SELECT (ca_bal
        + (SELECT COALESCE(sum(hs_qty * lt_price), 0) AS ca_assets
             FROM holding_summary
 LEFT LOOKUP JOIN last_trade ON lt_s_symb = hs_s_symb
            WHERE hs_ca_id = 0)
       )::FLOAT8
  FROM customer_account
 WHERE ca_id = 0;
