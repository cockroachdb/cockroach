  FROM customer, security, commission_rate
 WHERE c_id = 0
   AND s_symb = 'ROACH'
   AND cr_tt_id = 'TLS'
   AND cr_from_qty <= 100
   AND cr_to_qty >= 200
   AND cr_ex_id = s_ex_id
   AND cr_c_tier = c_tier
 LIMIT 1;
