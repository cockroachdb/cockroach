# Q3 (With Hints)
opt
          SELECT ca_id,
                 ca_bal::FLOAT8,
                 IFNULL((sum(hs_qty * lt_price)), 0)::FLOAT8
            FROM customer_account
LEFT LOOKUP JOIN holding_summary ON hs_ca_id  = ca_id
LEFT LOOKUP JOIN last_trade      ON lt_s_symb = hs_s_symb
           WHERE ca_c_id = 0
        GROUP BY ca_id, ca_bal
        ORDER BY 3 ASC
           LIMIT 10;
