# Q1 (With Hints)
opt
          SELECT t_id,
                 t_bid_price::FLOAT8,
                 t_exec_name,
                 t_is_cash,
                 t_trade_price::FLOAT8,
                 tt_is_mrkt,
                 se_amt::FLOAT8,
                 se_cash_due_date,
                 se_cash_type,
                 ct_amt::FLOAT8,
                 ct_dts,
                 ct_name
