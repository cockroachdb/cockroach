# Q8
opt
UPDATE settlement
   SET se_cash_type = IF(
                        True,
                        IF(
                            se_cash_type = 'Cash Account',
                            'Cash',
                            'Cash Account'
                        ),
                        IF(
                            se_cash_type = 'Margin Account',
                            'Margin',
                            'Margin Account'
                        )
                    )
 WHERE se_t_id = 0
