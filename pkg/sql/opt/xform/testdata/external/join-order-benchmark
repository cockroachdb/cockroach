opt
SELECT MIN(n.name) AS of_person,
       MIN(t.title) AS biography_movie
FROM aka_name AS an,
     cast_info AS ci,
     info_type AS it,
     link_type AS lt,
     movie_link AS ml,
     name AS n,
     person_info AS pi,
     title AS t
WHERE an.name LIKE '%a%'
  AND it.info ='mini biography'
  AND lt.link ='features'
  AND n.name_pcode_cf LIKE 'D%'
  AND n.gender='m'
  AND pi.note ='Volker Boehm'
  AND t.production_year BETWEEN 1980 AND 1984
  AND n.id = an.person_id
  AND n.id = pi.person_id
  AND ci.person_id = n.id
  AND t.id = ci.movie_id
  AND ml.linked_movie_id = t.id
  AND lt.id = ml.link_type_id
  AND it.id = pi.info_type_id
  AND pi.person_id = an.person_id
  AND pi.person_id = ci.person_id
  AND an.person_id = ci.person_id
  AND ci.movie_id = ml.linked_movie_id;
----
scalar-group-by
 ├── columns: of_person:50(string) biography_movie:51(string)
 ├── cardinality: [1 - 1]
 ├── stats: [rows=1]
 ├── key: ()
 ├── fd: ()-->(50,51)
 ├── inner-join (lookup title)
 │    ├── columns: aka_name.person_id:2(int!null) aka_name.name:3(string!null) cast_info.person_id:10(int!null) cast_info.movie_id:11(int!null) info_type.id:16(int!null) info_type.info:17(string!null) link_type.id:18(int!null) link:19(string!null) linked_movie_id:22(int!null) link_type_id:23(int!null) name.id:24(int!null) name.name:25(string!null) gender:28(string!null) name.name_pcode_cf:29(string) person_info.person_id:34(int!null) info_type_id:35(int!null) person_info.note:37(string!null) title.id:38(int!null) title:39(string!null) production_year:42(int!null)
