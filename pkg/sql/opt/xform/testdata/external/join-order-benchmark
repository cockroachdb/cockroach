 │    │    │    │    │    │    │    │         └── movie_info.info IN ('Horror', 'Thriller') [type=bool, outer=(30), constraints=(/30: [/'Horror' - /'Horror'] [/'Thriller' - /'Thriller']; tight)]
 │    │    │    │    │    │    │    └── filters [type=bool, outer=(3,15,23,28,29), constraints=(/3: (/NULL - ]; /15: (/NULL - ]; /23: (/NULL - ]; /28: (/NULL - ]; /29: (/NULL - ]), fd=(3)==(23,28), (28)==(3,23), (23)==(3,28), (15)==(29), (29)==(15)]
 │    │    │    │    │    │    │         ├── cast_info.movie_id = movie_info.movie_id [type=bool, outer=(3,28), constraints=(/3: (/NULL - ]; /28: (/NULL - ])]
