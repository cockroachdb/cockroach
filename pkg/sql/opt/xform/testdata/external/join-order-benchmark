 │    │         └── gender = 'm' [type=bool, outer=(32), constraints=(/32: [/'m' - /'m']; tight)]
 │    └── filters [type=bool, outer=(3,16,21,26,37,38,41), constraints=(/3: (/NULL - ]; /16: (/NULL - ]; /21: (/NULL - ]; /26: (/NULL - ]; /37: (/NULL - ]; /41: [/2011 - ]), fd=(16)==(3,21,26,37), (37)==(3,16,21,26), (21)==(3,16,26,37), (3)==(16,21,26,37), (26)==(3,16,21,37)]
 │         ├── title.id = movie_info.movie_id [type=bool, outer=(16,37), constraints=(/16: (/NULL - ]; /37: (/NULL - ])]
 │         ├── title.id = movie_info_idx.movie_id [type=bool, outer=(21,37), constraints=(/21: (/NULL - ]; /37: (/NULL - ])]
