opt
SELECT MIN(a1.name) AS writer_pseudo_name,
       MIN(t.title) AS movie_title
FROM aka_name AS a1,
     cast_info AS ci,
     company_name AS cn,
     movie_companies AS mc,
     name AS n1,
     role_type AS rt,
     title AS t
WHERE cn.country_code ='[us]'
  AND rt.role ='writer'
  AND a1.person_id = n1.id
  AND n1.id = ci.person_id
  AND ci.movie_id = t.id
  AND t.id = mc.movie_id
  AND mc.company_id = cn.id
  AND ci.role_id = rt.id
  AND a1.person_id = ci.person_id
  AND ci.movie_id = mc.movie_id;
----
scalar-group-by
 ├── columns: writer_pseudo_name:51(string) movie_title:52(string)
 ├── cardinality: [1 - 1]
 ├── stats: [rows=1]
 ├── key: ()
 ├── fd: ()-->(51,52)
 ├── inner-join (lookup title)
 │    ├── columns: aka_name.person_id:2(int!null) aka_name.name:3(string!null) cast_info.person_id:10(int!null) cast_info.movie_id:11(int!null) role_id:15(int!null) company_name.id:16(int!null) country_code:18(string!null) movie_companies.movie_id:24(int!null) company_id:25(int!null) name.id:28(int!null) role_type.id:37(int!null) role:38(string!null) title.id:39(int!null) title:40(string!null)
 │    ├── key columns: [11] = [39]
 │    ├── stats: [rows=2.26254282, distinct(11)=1.74007115, distinct(24)=1.74007115, distinct(39)=1.74007115]
