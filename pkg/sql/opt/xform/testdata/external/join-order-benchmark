 │    │    │    │    │         └── info_type.id = movie_info_idx.info_type_id [type=bool, outer=(20,39), constraints=(/20: (/NULL - ]; /39: (/NULL - ])]
 │    │    │    │    └── filters [type=bool, outer=(2,28,33,38,40), constraints=(/2: (/NULL - ]; /28: (/NULL - ]; /33: (/NULL - ]; /38: (/NULL - ]; /40: (/NULL - /'8.5')), fd=(33)==(2,28,38), (38)==(2,28,33), (28)==(2,33,38), (2)==(28,33,38)]
 │    │    │    │         ├── movie_info.movie_id = movie_info_idx.movie_id [type=bool, outer=(33,38), constraints=(/33: (/NULL - ]; /38: (/NULL - ])]
 │    │    │    │         ├── movie_companies.movie_id = movie_info_idx.movie_id [type=bool, outer=(28,38), constraints=(/28: (/NULL - ]; /38: (/NULL - ])]
 │    │    │    │         ├── movie_info_idx.movie_id = complete_cast.movie_id [type=bool, outer=(2,38), constraints=(/2: (/NULL - ]; /38: (/NULL - ])]
