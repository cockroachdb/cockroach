 │    │         └── role = 'actress' [type=bool, outer=(73), constraints=(/73: [/'actress' - /'actress']; tight)]
 │    └── filters [type=bool, outer=(10,26,46,51,56,74,75,78), constraints=(/10: (/NULL - ]; /26: (/NULL - ]; /46: (/NULL - ]; /51: (/NULL - ]; /56: (/NULL - ]; /74: (/NULL - ]; /75: [/'Shrek 2' - /'Shrek 2']; /78: [/2000 - /2005]), fd=()-->(75), (51)==(10,26,46,56,74), (74)==(10,26,46,51,56), (46)==(10,26,51,56,74), (26)==(10,46,51,56,74), (56)==(10,26,46,51,74), (10)==(26,46,51,56,74)]
