 │    │    │    │         ├── movie_companies.movie_id = movie_info_idx.movie_id [type=bool, outer=(20,30), constraints=(/20: (/NULL - ]; /30: (/NULL - ])]
 │    │    │    │         └── movie_info_idx.info < '7.0' [type=bool, outer=(32), constraints=(/32: (/NULL - /'7.0'); tight)]
 │    │    │    └── filters [type=bool, outer=(14,36), constraints=(/14: (/NULL - ]; /36: (/NULL - ]), fd=(14)==(36), (36)==(14)]
 │    │    │         └── keyword.id = keyword_id [type=bool, outer=(14,36), constraints=(/14: (/NULL - ]; /36: (/NULL - ])]
 │    │    └── filters [type=bool, outer=(20,25,30,35), constraints=(/20: (/NULL - ]; /25: (/NULL - ]; /30: (/NULL - ]; /35: (/NULL - ]), fd=(25)==(20,30,35), (35)==(20,25,30), (30)==(20,25,35), (20)==(25,30,35)]
 │    │         ├── movie_keyword.movie_id = movie_info.movie_id [type=bool, outer=(25,35), constraints=(/25: (/NULL - ]; /35: (/NULL - ])]
 │    │         ├── movie_keyword.movie_id = movie_info_idx.movie_id [type=bool, outer=(30,35), constraints=(/30: (/NULL - ]; /35: (/NULL - ])]
 │    │         └── movie_keyword.movie_id = movie_companies.movie_id [type=bool, outer=(20,35), constraints=(/20: (/NULL - ]; /35: (/NULL - ])]
 │    └── filters [type=bool, outer=(17,20,25,30,35,37,40,41), constraints=(/17: (/NULL - ]; /20: (/NULL - ]; /25: (/NULL - ]; /30: (/NULL - ]; /35: (/NULL - ]; /37: (/NULL - ]; /40: (/NULL - ]; /41: [/2010 - ]), fd=(17)==(40), (40)==(17), (25)==(20,30,35,37), (37)==(20,25,30,35), (35)==(20,25,30,37), (30)==(20,25,35,37), (20)==(25,30,35,37)]
 │         ├── kind_type.id = kind_id [type=bool, outer=(17,40), constraints=(/17: (/NULL - ]; /40: (/NULL - ])]
 │         ├── title.id = movie_info.movie_id [type=bool, outer=(25,37), constraints=(/25: (/NULL - ]; /37: (/NULL - ])]
 │         ├── title.id = movie_keyword.movie_id [type=bool, outer=(35,37), constraints=(/35: (/NULL - ]; /37: (/NULL - ])]
 │         ├── title.id = movie_info_idx.movie_id [type=bool, outer=(30,37), constraints=(/30: (/NULL - ]; /37: (/NULL - ])]
 │         ├── title.id = movie_companies.movie_id [type=bool, outer=(20,37), constraints=(/20: (/NULL - ]; /37: (/NULL - ])]
 │         └── production_year > 2009 [type=bool, outer=(41), constraints=(/41: [/2010 - ]; tight)]
 └── aggregations [outer=(2,32,38)]
      ├── min [type=string, outer=(2)]
      │    └── variable: name [type=string, outer=(2)]
      ├── min [type=string, outer=(32)]
      │    └── variable: movie_info_idx.info [type=string, outer=(32)]
      └── min [type=string, outer=(38)]
           └── variable: title [type=string, outer=(38)]

opt
SELECT MIN(cn.name) AS movie_company,
       MIN(mi_idx.info) AS rating,
       MIN(t.title) AS western_violent_movie
FROM company_name AS cn,
     company_type AS ct,
     info_type AS it1,
     info_type AS it2,
     keyword AS k,
     kind_type AS kt,
     movie_companies AS mc,
     movie_info AS mi,
     movie_info_idx AS mi_idx,
     movie_keyword AS mk,
     title AS t
WHERE cn.country_code != '[us]'
  AND it1.info = 'countries'
  AND it2.info = 'rating'
  AND k.keyword IN ('murder',
                    'murder-in-title',
                    'blood',
                    'violence')
  AND kt.kind IN ('movie',
                  'episode')
  AND mc.note NOT LIKE '%(USA)%'
  AND mc.note LIKE '%(200%)%'
  AND mi.info IN ('Sweden',
                  'Norway',
                  'Germany',
                  'Denmark',
                  'Swedish',
                  'Danish',
                  'Norwegian',
                  'German',
                  'USA',
                  'American')
  AND mi_idx.info < '8.5'
  AND t.production_year > 2005
  AND kt.id = t.kind_id
  AND t.id = mi.movie_id
  AND t.id = mk.movie_id
  AND t.id = mi_idx.movie_id
  AND t.id = mc.movie_id
  AND mk.movie_id = mi.movie_id
  AND mk.movie_id = mi_idx.movie_id
  AND mk.movie_id = mc.movie_id
  AND mi.movie_id = mi_idx.movie_id
  AND mi.movie_id = mc.movie_id
  AND mc.movie_id = mi_idx.movie_id
  AND k.id = mk.keyword_id
  AND it1.id = mi.info_type_id
  AND it2.id = mi_idx.info_type_id
  AND ct.id = mc.company_type_id
  AND cn.id = mc.company_id;
