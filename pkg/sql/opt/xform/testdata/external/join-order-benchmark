 │    │         └── name LIKE '%Tim%' [type=bool, outer=(23)]
 │    └── filters [type=bool, outer=(3,13,18,31), constraints=(/3: (/NULL - ]; /13: (/NULL - ]; /18: (/NULL - ]; /31: (/NULL - ]), fd=(13)==(3,18,31), (31)==(3,13,18), (18)==(3,13,31), (3)==(13,18,31)]
 │         ├── title.id = movie_info.movie_id [type=bool, outer=(13,31), constraints=(/13: (/NULL - ]; /31: (/NULL - ])]
