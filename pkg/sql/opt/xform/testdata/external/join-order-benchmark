 │         ├── title.id = movie_companies.movie_id [type=bool, outer=(25,31), constraints=(/25: (/NULL - ]; /31: (/NULL - ])]
 │         ├── title.id = cast_info.movie_id [type=bool, outer=(10,31), constraints=(/10: (/NULL - ]; /31: (/NULL - ])]
 │         └── production_year > 2010 [type=bool, outer=(35), constraints=(/35: [/2011 - ]; tight)]
 └── aggregations [outer=(2,32)]
      ├── min [type=string, outer=(2)]
      │    └── variable: char_name.name [type=string, outer=(2)]
      └── min [type=string, outer=(32)]
           └── variable: title [type=string, outer=(32)]

opt
SELECT MIN(chn.name) AS uncredited_voiced_character,
       MIN(t.title) AS russian_movie
FROM char_name AS chn,
     cast_info AS ci,
     company_name AS cn,
     company_type AS ct,
     movie_companies AS mc,
     role_type AS rt,
     title AS t
WHERE ci.note LIKE '%(voice)%'
  AND ci.note LIKE '%(uncredited)%'
  AND cn.country_code = '[ru]'
  AND rt.role = 'actor'
  AND t.production_year > 2005
  AND t.id = mc.movie_id
  AND t.id = ci.movie_id
  AND ci.movie_id = mc.movie_id
  AND chn.id = ci.person_role_id
  AND rt.id = ci.role_id
  AND cn.id = mc.company_id
  AND ct.id = mc.company_type_id;
----
