 │    │    │         └── cast_info.movie_id = movie_keyword.movie_id [type=bool, outer=(3,12), constraints=(/3: (/NULL - ]; /12: (/NULL - ])]
 │    │    └── filters [type=bool, outer=(2,14), constraints=(/2: (/NULL - ]; /14: (/NULL - ]), fd=(2)==(14), (14)==(2)]
 │    │         └── name.id = person_id [type=bool, outer=(2,14), constraints=(/2: (/NULL - ]; /14: (/NULL - ])]
 │    └── filters [type=bool, outer=(3,12,23,27), constraints=(/3: (/NULL - ]; /12: (/NULL - ]; /23: (/NULL - ]; /27: [/2001 - ]), fd=(12)==(3,23), (23)==(3,12), (3)==(12,23)]
 │         ├── title.id = movie_keyword.movie_id [type=bool, outer=(12,23), constraints=(/12: (/NULL - ]; /23: (/NULL - ])]
