 │    │    │    │    │         ├── cast_info.movie_id = movie_info_idx.movie_id [type=bool, outer=(11,29), constraints=(/11: (/NULL - ]; /29: (/NULL - ])]
 │    │    │    │    │         ├── movie_info.movie_id = movie_info_idx.movie_id [type=bool, outer=(24,29), constraints=(/24: (/NULL - ]; /29: (/NULL - ])]
 │    │    │    │    │         └── movie_info_idx.movie_id = complete_cast.movie_id [type=bool, outer=(2,29), constraints=(/2: (/NULL - ]; /29: (/NULL - ])]
 │    │    │    │    └── filters [type=bool, outer=(20,35), constraints=(/20: (/NULL - ]; /35: (/NULL - ]), fd=(20)==(35), (35)==(20)]
 │    │    │    │         └── keyword.id = keyword_id [type=bool, outer=(20,35), constraints=(/20: (/NULL - ]; /35: (/NULL - ])]
 │    │    │    └── filters [type=bool, outer=(2,11,24,29,34), constraints=(/2: (/NULL - ]; /11: (/NULL - ]; /24: (/NULL - ]; /29: (/NULL - ]; /34: (/NULL - ]), fd=(11)==(2,24,29,34), (34)==(2,11,24,29), (24)==(2,11,29,34), (29)==(2,11,24,34), (2)==(11,24,29,34)]
 │    │    │         ├── cast_info.movie_id = movie_keyword.movie_id [type=bool, outer=(11,34), constraints=(/11: (/NULL - ]; /34: (/NULL - ])]
 │    │    │         ├── movie_info.movie_id = movie_keyword.movie_id [type=bool, outer=(24,34), constraints=(/24: (/NULL - ]; /34: (/NULL - ])]
 │    │    │         ├── movie_info_idx.movie_id = movie_keyword.movie_id [type=bool, outer=(29,34), constraints=(/29: (/NULL - ]; /34: (/NULL - ])]
 │    │    │         └── movie_keyword.movie_id = complete_cast.movie_id [type=bool, outer=(2,34), constraints=(/2: (/NULL - ]; /34: (/NULL - ])]
 │    │    └── filters [type=bool, outer=(10,36,40), constraints=(/10: (/NULL - ]; /36: (/NULL - ]; /40: [/'m' - /'m']), fd=()-->(40), (10)==(36), (36)==(10)]
 │    │         ├── name.id = person_id [type=bool, outer=(10,36), constraints=(/10: (/NULL - ]; /36: (/NULL - ])]
 │    │         └── gender = 'm' [type=bool, outer=(40), constraints=(/40: [/'m' - /'m']; tight)]
 │    └── filters [type=bool, outer=(2,11,24,29,34,45,49), constraints=(/2: (/NULL - ]; /11: (/NULL - ]; /24: (/NULL - ]; /29: (/NULL - ]; /34: (/NULL - ]; /45: (/NULL - ]; /49: [/2001 - ]), fd=(24)==(2,11,29,34,45), (45)==(2,11,24,29,34), (29)==(2,11,24,34,45), (11)==(2,24,29,34,45), (34)==(2,11,24,29,45), (2)==(11,24,29,34,45)]
 │         ├── title.id = movie_info.movie_id [type=bool, outer=(24,45), constraints=(/24: (/NULL - ]; /45: (/NULL - ])]
 │         ├── title.id = movie_info_idx.movie_id [type=bool, outer=(29,45), constraints=(/29: (/NULL - ]; /45: (/NULL - ])]
 │         ├── title.id = cast_info.movie_id [type=bool, outer=(11,45), constraints=(/11: (/NULL - ]; /45: (/NULL - ])]
 │         ├── title.id = movie_keyword.movie_id [type=bool, outer=(34,45), constraints=(/34: (/NULL - ]; /45: (/NULL - ])]
 │         ├── title.id = complete_cast.movie_id [type=bool, outer=(2,45), constraints=(/2: (/NULL - ]; /45: (/NULL - ])]
 │         └── production_year > 2000 [type=bool, outer=(49), constraints=(/49: [/2001 - ]; tight)]
 └── aggregations [outer=(26,31,37,46)]
      ├── min [type=string, outer=(26)]
      │    └── variable: movie_info.info [type=string, outer=(26)]
      ├── min [type=string, outer=(31)]
      │    └── variable: movie_info_idx.info [type=string, outer=(31)]
      ├── min [type=string, outer=(37)]
      │    └── variable: name [type=string, outer=(37)]
      └── min [type=string, outer=(46)]
           └── variable: title [type=string, outer=(46)]

opt
SELECT MIN(mi.info) AS movie_budget,
       MIN(mi_idx.info) AS movie_votes,
       MIN(n.name) AS writer,
       MIN(t.title) AS complete_gore_movie
FROM complete_cast AS cc,
     comp_cast_type AS cct1,
     comp_cast_type AS cct2,
     cast_info AS ci,
     info_type AS it1,
     info_type AS it2,
     keyword AS k,
     movie_info AS mi,
     movie_info_idx AS mi_idx,
     movie_keyword AS mk,
     name AS n,
     title AS t
WHERE cct1.kind IN ('cast',
                    'crew')
  AND cct2.kind ='complete+verified'
  AND ci.note IN ('(writer)',
                  '(head writer)',
                  '(written by)',
                  '(story)',
                  '(story editor)')
  AND it1.info = 'genres'
  AND it2.info = 'votes'
  AND k.keyword IN ('murder',
                    'violence',
                    'blood',
                    'gore',
                    'death',
                    'female-nudity',
                    'hospital')
  AND mi.info IN ('Horror',
                  'Thriller')
  AND n.gender = 'm'
  AND t.production_year > 2000
  AND (t.title LIKE '%Freddy%'
       OR t.title LIKE '%Jason%'
       OR t.title LIKE 'Saw%')
  AND t.id = mi.movie_id
  AND t.id = mi_idx.movie_id
  AND t.id = ci.movie_id
  AND t.id = mk.movie_id
  AND t.id = cc.movie_id
  AND ci.movie_id = mi.movie_id
  AND ci.movie_id = mi_idx.movie_id
  AND ci.movie_id = mk.movie_id
  AND ci.movie_id = cc.movie_id
  AND mi.movie_id = mi_idx.movie_id
  AND mi.movie_id = mk.movie_id
  AND mi.movie_id = cc.movie_id
  AND mi_idx.movie_id = mk.movie_id
  AND mi_idx.movie_id = cc.movie_id
  AND mk.movie_id = cc.movie_id
  AND n.id = ci.person_id
  AND it1.id = mi.info_type_id
  AND it2.id = mi_idx.info_type_id
  AND k.id = mk.keyword_id
  AND cct1.id = cc.subject_id
  AND cct2.id = cc.status_id;
