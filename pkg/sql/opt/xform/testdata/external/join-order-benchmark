 │    │    │    │         └── (movie_info.info LIKE 'Japan:%2007%') OR (movie_info.info LIKE 'USA:%2008%') [type=bool, outer=(40)]
 │    │    │    └── filters [type=bool, outer=(2,17,42,43,46), constraints=(/2: (/NULL - ]; /17: (/NULL - ]; /42: (/NULL - ]; /46: [/'f' - /'f']), fd=()-->(46), (17)==(2,42), (42)==(2,17), (2)==(17,42)]
 │    │    │         ├── name.id = cast_info.person_id [type=bool, outer=(17,42), constraints=(/17: (/NULL - ]; /42: (/NULL - ])]
 │    │    │         ├── name.id = aka_name.person_id [type=bool, outer=(2,42), constraints=(/2: (/NULL - ]; /42: (/NULL - ])]
 │    │    │         ├── gender = 'f' [type=bool, outer=(46), constraints=(/46: [/'f' - /'f']; tight)]
 │    │    │         └── name.name LIKE '%Angel%' [type=bool, outer=(43)]
 │    │    └── filters [type=bool, outer=(22,51,52), constraints=(/22: (/NULL - ]; /51: (/NULL - ]; /52: [/'actress' - /'actress']), fd=()-->(52), (22)==(51), (51)==(22)]
 │    │         ├── role_type.id = role_id [type=bool, outer=(22,51), constraints=(/22: (/NULL - ]; /51: (/NULL - ])]
 │    │         └── role = 'actress' [type=bool, outer=(52), constraints=(/52: [/'actress' - /'actress']; tight)]
 │    └── filters [type=bool, outer=(18,33,38,53,54,57), constraints=(/18: (/NULL - ]; /33: (/NULL - ]; /38: (/NULL - ]; /53: (/NULL - ]; /57: [/2007 - /2008]), fd=(38)==(18,33,53), (53)==(18,33,38), (33)==(18,38,53), (18)==(33,38,53)]
 │         ├── title.id = movie_info.movie_id [type=bool, outer=(38,53), constraints=(/38: (/NULL - ]; /53: (/NULL - ])]
 │         ├── title.id = movie_companies.movie_id [type=bool, outer=(33,53), constraints=(/33: (/NULL - ]; /53: (/NULL - ])]
 │         ├── title.id = cast_info.movie_id [type=bool, outer=(18,53), constraints=(/18: (/NULL - ]; /53: (/NULL - ])]
 │         ├── production_year >= 2007 [type=bool, outer=(57), constraints=(/57: [/2007 - ]; tight)]
 │         ├── production_year <= 2008 [type=bool, outer=(57), constraints=(/57: (/NULL - /2008]; tight)]
 │         └── title LIKE '%Kung%Fu%Panda%' [type=bool, outer=(54)]
 └── aggregations [outer=(43,54)]
      ├── min [type=string, outer=(43)]
      │    └── variable: name.name [type=string, outer=(43)]
      └── min [type=string, outer=(54)]
           └── variable: title [type=string, outer=(54)]

opt
SELECT MIN(n.name) AS voicing_actress,
       MIN(t.title) AS jap_engl_voiced_movie
FROM aka_name AS an,
     char_name AS chn,
     cast_info AS ci,
     company_name AS cn,
     info_type AS it,
     movie_companies AS mc,
     movie_info AS mi,
     name AS n,
     role_type AS rt,
     title AS t
WHERE ci.note IN ('(voice)',
                  '(voice: Japanese version)',
                  '(voice) (uncredited)',
                  '(voice: English version)')
  AND cn.country_code ='[us]'
  AND it.info = 'release dates'
  AND mi.info IS NOT NULL
  AND (mi.info LIKE 'Japan:%200%'
       OR mi.info LIKE 'USA:%200%')
  AND n.gender ='f'
  AND n.name LIKE '%An%'
  AND rt.role ='actress'
  AND t.production_year > 2000
  AND t.id = mi.movie_id
  AND t.id = mc.movie_id
  AND t.id = ci.movie_id
  AND mc.movie_id = ci.movie_id
  AND mc.movie_id = mi.movie_id
  AND mi.movie_id = ci.movie_id
  AND cn.id = mc.company_id
  AND it.id = mi.info_type_id
  AND n.id = ci.person_id
  AND rt.id = ci.role_id
  AND n.id = an.person_id
  AND ci.person_id = an.person_id
  AND chn.id = ci.person_role_id;
