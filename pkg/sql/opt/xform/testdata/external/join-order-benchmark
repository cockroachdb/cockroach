 │    │    │    │    │    │    │    └── columns: movie_companies.movie_id:24(int!null) company_id:25(int!null) company_type_id:26(int!null)
 │    │    │    │    │    │    └── filters [type=bool, outer=(2,7,14,24-26), constraints=(/2: (/NULL - ]; /7: (/NULL - ]; /14: (/NULL - ]; /24: (/NULL - ]; /25: (/NULL - ]; /26: (/NULL - ]), fd=(2)==(24), (24)==(2), (7)==(25), (25)==(7), (14)==(26), (26)==(14)]
 │    │    │    │    │    │         ├── movie_companies.movie_id = complete_cast.movie_id [type=bool, outer=(2,24), constraints=(/2: (/NULL - ]; /24: (/NULL - ])]
 │    │    │    │    │    │         ├── company_name.id = company_id [type=bool, outer=(7,25), constraints=(/7: (/NULL - ]; /25: (/NULL - ])]
 │    │    │    │    │    │         └── company_type.id = company_type_id [type=bool, outer=(14,26), constraints=(/14: (/NULL - ]; /26: (/NULL - ])]
 │    │    │    │    │    └── filters [type=bool, outer=(16,30), constraints=(/16: (/NULL - ]; /30: (/NULL - ]), fd=(16)==(30), (30)==(16)]
 │    │    │    │    │         └── info_type.id = info_type_id [type=bool, outer=(16,30), constraints=(/16: (/NULL - ]; /30: (/NULL - ])]
 │    │    │    │    └── filters [type=bool, outer=(2,24,29,31,32), constraints=(/2: (/NULL - ]; /24: (/NULL - ]; /29: (/NULL - ]), fd=(24)==(2,29), (29)==(2,24), (2)==(24,29)]
 │    │    │    │         ├── movie_info.movie_id = movie_companies.movie_id [type=bool, outer=(24,29), constraints=(/24: (/NULL - ]; /29: (/NULL - ])]
 │    │    │    │         ├── movie_info.movie_id = complete_cast.movie_id [type=bool, outer=(2,29), constraints=(/2: (/NULL - ]; /29: (/NULL - ])]
 │    │    │    │         ├── movie_info.note LIKE '%internet%' [type=bool, outer=(32)]
 │    │    │    │         └── (movie_info.info LIKE 'USA:% 199%') OR (movie_info.info LIKE 'USA:% 200%') [type=bool, outer=(31)]
 │    │    │    └── filters [type=bool, outer=(18,35), constraints=(/18: (/NULL - ]; /35: (/NULL - ]), fd=(18)==(35), (35)==(18)]
 │    │    │         └── keyword.id = keyword_id [type=bool, outer=(18,35), constraints=(/18: (/NULL - ]; /35: (/NULL - ])]
 │    │    └── filters [type=bool, outer=(2,24,29,34), constraints=(/2: (/NULL - ]; /24: (/NULL - ]; /29: (/NULL - ]; /34: (/NULL - ]), fd=(29)==(2,24,34), (34)==(2,24,29), (24)==(2,29,34), (2)==(24,29,34)]
 │    │         ├── movie_keyword.movie_id = movie_info.movie_id [type=bool, outer=(29,34), constraints=(/29: (/NULL - ]; /34: (/NULL - ])]
 │    │         ├── movie_keyword.movie_id = movie_companies.movie_id [type=bool, outer=(24,34), constraints=(/24: (/NULL - ]; /34: (/NULL - ])]
 │    │         └── movie_keyword.movie_id = complete_cast.movie_id [type=bool, outer=(2,34), constraints=(/2: (/NULL - ]; /34: (/NULL - ])]
 │    └── filters [type=bool, outer=(2,21,24,29,34,36,39,40), constraints=(/2: (/NULL - ]; /21: (/NULL - ]; /24: (/NULL - ]; /29: (/NULL - ]; /34: (/NULL - ]; /36: (/NULL - ]; /39: (/NULL - ]; /40: [/2001 - ]), fd=(21)==(39), (39)==(21), (29)==(2,24,34,36), (36)==(2,24,29,34), (34)==(2,24,29,36), (24)==(2,29,34,36), (2)==(24,29,34,36)]
 │         ├── kind_type.id = kind_id [type=bool, outer=(21,39), constraints=(/21: (/NULL - ]; /39: (/NULL - ])]
 │         ├── title.id = movie_info.movie_id [type=bool, outer=(29,36), constraints=(/29: (/NULL - ]; /36: (/NULL - ])]
 │         ├── title.id = movie_keyword.movie_id [type=bool, outer=(34,36), constraints=(/34: (/NULL - ]; /36: (/NULL - ])]
 │         ├── title.id = movie_companies.movie_id [type=bool, outer=(24,36), constraints=(/24: (/NULL - ]; /36: (/NULL - ])]
 │         ├── title.id = complete_cast.movie_id [type=bool, outer=(2,36), constraints=(/2: (/NULL - ]; /36: (/NULL - ])]
 │         └── production_year > 2000 [type=bool, outer=(40), constraints=(/40: [/2001 - ]; tight)]
 └── aggregations [outer=(22,37)]
      ├── min [type=string, outer=(22)]
      │    └── variable: kind_type.kind [type=string, outer=(22)]
      └── min [type=string, outer=(37)]
           └── variable: title [type=string, outer=(37)]

opt
SELECT MIN(kt.kind) AS movie_kind,
       MIN(t.title) AS complete_nerdy_internet_movie
FROM complete_cast AS cc,
     comp_cast_type AS cct1,
     company_name AS cn,
     company_type AS ct,
     info_type AS it1,
     keyword AS k,
     kind_type AS kt,
     movie_companies AS mc,
     movie_info AS mi,
     movie_keyword AS mk,
     title AS t
WHERE cct1.kind = 'complete+verified'
  AND cn.country_code = '[us]'
  AND it1.info = 'release dates'
  AND k.keyword IN ('nerd',
                    'loner',
                    'alienation',
                    'dignity')
  AND kt.kind IN ('movie')
  AND mi.note LIKE '%internet%'
  AND mi.info LIKE 'USA:% 200%'
  AND t.production_year > 2000
  AND kt.id = t.kind_id
  AND t.id = mi.movie_id
  AND t.id = mk.movie_id
  AND t.id = mc.movie_id
  AND t.id = cc.movie_id
  AND mk.movie_id = mi.movie_id
  AND mk.movie_id = mc.movie_id
  AND mk.movie_id = cc.movie_id
  AND mi.movie_id = mc.movie_id
  AND mi.movie_id = cc.movie_id
  AND mc.movie_id = cc.movie_id
  AND k.id = mk.keyword_id
  AND it1.id = mi.info_type_id
  AND cn.id = mc.company_id
  AND ct.id = mc.company_type_id
  AND cct1.id = cc.status_id;
