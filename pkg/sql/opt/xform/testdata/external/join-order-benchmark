 │    │    │    │         ├── company_type_id = company_type.id [type=bool, outer=(8,18), constraints=(/8: (/NULL - ]; /18: (/NULL - ])]
 │    │    │    │         └── company_id = company_name.id [type=bool, outer=(1,17), constraints=(/1: (/NULL - ]; /17: (/NULL - ])]
 │    │    │    ├── scan movie_keyword
 │    │    │    │    └── columns: movie_keyword.movie_id:21(int!null) keyword_id:22(int!null)
 │    │    │    └── filters [type=bool, outer=(10,16,21,22), constraints=(/10: (/NULL - ]; /16: (/NULL - ]; /21: (/NULL - ]; /22: (/NULL - ]), fd=(10)==(22), (22)==(10), (16)==(21), (21)==(16)]
 │    │    │         ├── keyword_id = keyword.id [type=bool, outer=(10,22), constraints=(/10: (/NULL - ]; /22: (/NULL - ])]
 │    │    │         └── movie_keyword.movie_id = movie_companies.movie_id [type=bool, outer=(16,21), constraints=(/16: (/NULL - ]; /21: (/NULL - ])]
 │    │    ├── scan movie_link
 │    │    │    └── columns: movie_link.movie_id:24(int!null) link_type_id:26(int!null)
 │    │    └── filters [type=bool, outer=(13,16,21,24,26), constraints=(/13: (/NULL - ]; /16: (/NULL - ]; /21: (/NULL - ]; /24: (/NULL - ]; /26: (/NULL - ]), fd=(13)==(26), (26)==(13), (21)==(16,24), (24)==(16,21), (16)==(21,24)]
 │    │         ├── link_type.id = link_type_id [type=bool, outer=(13,26), constraints=(/13: (/NULL - ]; /26: (/NULL - ])]
 │    │         ├── movie_link.movie_id = movie_keyword.movie_id [type=bool, outer=(21,24), constraints=(/21: (/NULL - ]; /24: (/NULL - ])]
 │    │         └── movie_link.movie_id = movie_companies.movie_id [type=bool, outer=(16,24), constraints=(/16: (/NULL - ]; /24: (/NULL - ])]
 │    └── filters [type=bool, outer=(16,21,24,27,31), constraints=(/16: (/NULL - ]; /21: (/NULL - ]; /24: (/NULL - ]; /27: (/NULL - ]; /31: [/1951 - ]), fd=(24)==(16,21,27), (27)==(16,21,24), (21)==(16,24,27), (16)==(21,24,27)]
 │         ├── movie_link.movie_id = title.id [type=bool, outer=(24,27), constraints=(/24: (/NULL - ]; /27: (/NULL - ])]
 │         ├── title.id = movie_keyword.movie_id [type=bool, outer=(21,27), constraints=(/21: (/NULL - ]; /27: (/NULL - ])]
 │         ├── title.id = movie_companies.movie_id [type=bool, outer=(16,27), constraints=(/16: (/NULL - ]; /27: (/NULL - ])]
 │         └── production_year > 1950 [type=bool, outer=(31), constraints=(/31: [/1951 - ]; tight)]
 └── aggregations [outer=(2,19,28)]
      ├── min [type=string, outer=(2)]
      │    └── variable: name [type=string, outer=(2)]
      ├── min [type=string, outer=(19)]
      │    └── variable: note [type=string, outer=(19)]
      └── min [type=string, outer=(28)]
           └── variable: title [type=string, outer=(28)]

opt
SELECT MIN(cn.name) AS movie_company,
       MIN(mi_idx.info) AS rating,
       MIN(t.title) AS drama_horror_movie
FROM company_name AS cn,
     company_type AS ct,
     info_type AS it1,
     info_type AS it2,
     movie_companies AS mc,
     movie_info AS mi,
     movie_info_idx AS mi_idx,
     title AS t
WHERE cn.country_code = '[us]'
  AND ct.kind = 'production companies'
  AND it1.info = 'genres'
  AND it2.info = 'rating'
  AND mi.info IN ('Drama',
                  'Horror')
  AND mi_idx.info > '8.0'
  AND t.production_year BETWEEN 2005 AND 2008
  AND t.id = mi.movie_id
  AND t.id = mi_idx.movie_id
  AND mi.info_type_id = it1.id
  AND mi_idx.info_type_id = it2.id
  AND t.id = mc.movie_id
  AND ct.id = mc.company_type_id
  AND cn.id = mc.company_id
  AND mc.movie_id = mi.movie_id
  AND mc.movie_id = mi_idx.movie_id
  AND mi.movie_id = mi_idx.movie_id;
----
