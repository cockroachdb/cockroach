 │    │         ├── movie_keyword.movie_id = movie_companies.movie_id [type=bool, outer=(28,43), constraints=(/28: (/NULL - ]; /43: (/NULL - ])]
 │    │         └── movie_keyword.movie_id = complete_cast.movie_id [type=bool, outer=(2,43), constraints=(/2: (/NULL - ]; /43: (/NULL - ])]
