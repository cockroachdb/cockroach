 │    │    │    │         ├── cast_info.movie_id = movie_companies.movie_id [type=bool, outer=(18,31), constraints=(/18: (/NULL - ]; /31: (/NULL - ])]
 │    │    │    │         ├── movie_companies.note IS NOT NULL [type=bool, outer=(34), constraints=(/34: (/NULL - ]; tight)]
 │    │    │    │         └── (movie_companies.note LIKE '%(USA)%') OR (movie_companies.note LIKE '%(worldwide)%') [type=bool, outer=(34)]
