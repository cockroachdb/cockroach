exec-ddl
CREATE TABLE aka_name (
    id integer NOT NULL PRIMARY KEY,
    person_id integer NOT NULL,
    name text NOT NULL,
    imdb_index character varying(12),
    name_pcode_cf character varying(5),
    name_pcode_nf character varying(5),
    surname_pcode character varying(5),
    md5sum character varying(32),
    INDEX person_id_aka_name (person_id)
)
----
TABLE aka_name
 ├── id int not null
 ├── person_id int not null
 ├── name string not null
 ├── imdb_index string
 ├── name_pcode_cf string
 ├── name_pcode_nf string
 ├── surname_pcode string
 ├── md5sum string
 ├── INDEX primary
 │    └── id int not null
 └── INDEX person_id_aka_name
      ├── person_id int not null
      └── id int not null


exec-ddl
CREATE TABLE aka_title (
    id integer NOT NULL PRIMARY KEY,
    movie_id integer NOT NULL,
    title text NOT NULL,
    imdb_index character varying(12),
    kind_id integer NOT NULL,
    production_year integer,
    episode_of_id integer,
    season_nr integer,
    episode_nr integer,
    note text,
    md5sum character varying(32),
    INDEX kind_id_aka_title (kind_id),
    INDEX movie_id_aka_title (movie_id)
)
----
TABLE aka_title
 ├── id int not null
 ├── movie_id int not null
 ├── title string not null
 ├── imdb_index string
 ├── kind_id int not null
 ├── production_year int
 ├── episode_of_id int
 ├── season_nr int
 ├── episode_nr int
 ├── note string
 ├── md5sum string
 ├── INDEX primary
 │    └── id int not null
 ├── INDEX kind_id_aka_title
 │    ├── kind_id int not null
 │    └── id int not null
 └── INDEX movie_id_aka_title
      ├── movie_id int not null
      └── id int not null

exec-ddl
CREATE TABLE cast_info (
    id integer NOT NULL PRIMARY KEY,
    person_id integer NOT NULL,
    movie_id integer NOT NULL,
    person_role_id integer,
    note text,
    nr_order integer,
    role_id integer NOT NULL,
    INDEX movie_id_cast_info (movie_id),
    INDEX person_id_cast_info (person_id),
    INDEX person_role_id_cast_info (person_role_id),
    INDEX role_id_cast_info (role_id)
)
----
TABLE cast_info
 ├── id int not null
 ├── person_id int not null
 ├── movie_id int not null
 ├── person_role_id int
 ├── note string
 ├── nr_order int
 ├── role_id int not null
 ├── INDEX primary
 │    └── id int not null
 ├── INDEX movie_id_cast_info
 │    ├── movie_id int not null
 │    └── id int not null
 ├── INDEX person_id_cast_info
 │    ├── person_id int not null
 │    └── id int not null
 ├── INDEX person_role_id_cast_info
 │    ├── person_role_id int
 │    └── id int not null
 └── INDEX role_id_cast_info
      ├── role_id int not null
      └── id int not null

