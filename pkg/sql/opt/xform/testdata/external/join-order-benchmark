 │    │         ├── name.id = person_info.person_id [type=bool, outer=(24,34), constraints=(/24: (/NULL - ]; /34: (/NULL - ])]
 │    │         ├── person_info.person_id = aka_name.person_id [type=bool, outer=(2,34), constraints=(/2: (/NULL - ]; /34: (/NULL - ])]
 │    │         ├── person_info.person_id = cast_info.person_id [type=bool, outer=(10,34), constraints=(/10: (/NULL - ]; /34: (/NULL - ])]
 │    │         └── person_info.note = 'Volker Boehm' [type=bool, outer=(37), constraints=(/37: [/'Volker Boehm' - /'Volker Boehm']; tight)]
 │    └── filters [type=bool, outer=(11,22,38,42), constraints=(/11: (/NULL - ]; /22: (/NULL - ]; /38: (/NULL - ]; /42: [/1980 - /1995]), fd=(11)==(22,38), (38)==(11,22), (22)==(11,38)]
 │         ├── title.id = cast_info.movie_id [type=bool, outer=(11,38), constraints=(/11: (/NULL - ]; /38: (/NULL - ])]
 │         ├── linked_movie_id = title.id [type=bool, outer=(22,38), constraints=(/22: (/NULL - ]; /38: (/NULL - ])]
 │         ├── production_year >= 1980 [type=bool, outer=(42), constraints=(/42: [/1980 - ]; tight)]
 │         └── production_year <= 1995 [type=bool, outer=(42), constraints=(/42: (/NULL - /1995]; tight)]
 └── aggregations [outer=(25,39)]
      ├── min [type=string, outer=(25)]
      │    └── variable: name.name [type=string, outer=(25)]
      └── min [type=string, outer=(39)]
           └── variable: title [type=string, outer=(39)]

opt
SELECT MIN(n.name) AS of_person,
       MIN(t.title) AS biography_movie
FROM aka_name AS an,
     cast_info AS ci,
     info_type AS it,
     link_type AS lt,
     movie_link AS ml,
     name AS n,
     person_info AS pi,
     title AS t
WHERE an.name LIKE '%a%'
  AND it.info ='mini biography'
  AND lt.link ='features'
  AND n.name_pcode_cf LIKE 'D%'
  AND n.gender='m'
  AND pi.note ='Volker Boehm'
  AND t.production_year BETWEEN 1980 AND 1984
  AND n.id = an.person_id
  AND n.id = pi.person_id
  AND ci.person_id = n.id
  AND t.id = ci.movie_id
  AND ml.linked_movie_id = t.id
  AND lt.id = ml.link_type_id
  AND it.id = pi.info_type_id
  AND pi.person_id = an.person_id
  AND pi.person_id = ci.person_id
  AND an.person_id = ci.person_id
  AND ci.movie_id = ml.linked_movie_id;
----
