 │    │    │    │    │    │    │    │    ├── fd: ()-->(29)
 │    │    │    │    │    │    │    │    ├── scan kind_type
 │    │    │    │    │    │    │    │    │    ├── columns: kind_type.id:28(int!null) kind_type.kind:29(string!null)
 │    │    │    │    │    │    │    │    │    ├── key: (28)
 │    │    │    │    │    │    │    │    │    └── fd: (28)-->(29)
 │    │    │    │    │    │    │    │    └── filters [type=bool, outer=(29), constraints=(/29: [/'movie' - /'movie']; tight), fd=()-->(29)]
 │    │    │    │    │    │    │    │         └── kind_type.kind = 'movie' [type=bool, outer=(29), constraints=(/29: [/'movie' - /'movie']; tight)]
 │    │    │    │    │    │    │    └── true [type=bool]
 │    │    │    │    │    │    └── filters [type=bool, outer=(23,32), constraints=(/23: (/NULL - ]; /32: (/NULL - ]), fd=(23)==(32), (32)==(23)]
 │    │    │    │    │    │         └── info_type.id = info_type_id [type=bool, outer=(23,32), constraints=(/23: (/NULL - ]; /32: (/NULL - ])]
 │    │    │    │    │    └── filters [type=bool, outer=(2,18,31,33), constraints=(/2: (/NULL - ]; /18: (/NULL - ]; /31: (/NULL - ]; /33: [/e'8.0\x00' - ]), fd=(18)==(2,31), (31)==(2,18), (2)==(18,31)]
 │    │    │    │    │         ├── cast_info.movie_id = movie_info_idx.movie_id [type=bool, outer=(18,31), constraints=(/18: (/NULL - ]; /31: (/NULL - ])]
 │    │    │    │    │         ├── complete_cast.movie_id = movie_info_idx.movie_id [type=bool, outer=(2,31), constraints=(/2: (/NULL - ]; /31: (/NULL - ])]
 │    │    │    │    │         └── movie_info_idx.info > '8.0' [type=bool, outer=(33), constraints=(/33: [/e'8.0\x00' - ]; tight)]
 │    │    │    │    └── filters [type=bool, outer=(25,37), constraints=(/25: (/NULL - ]; /37: (/NULL - ]), fd=(25)==(37), (37)==(25)]
 │    │    │    │         └── keyword.id = keyword_id [type=bool, outer=(25,37), constraints=(/25: (/NULL - ]; /37: (/NULL - ])]
 │    │    │    └── filters [type=bool, outer=(2,18,31,36), constraints=(/2: (/NULL - ]; /18: (/NULL - ]; /31: (/NULL - ]; /36: (/NULL - ]), fd=(18)==(2,31,36), (36)==(2,18,31), (2)==(18,31,36), (31)==(2,18,36)]
 │    │    │         ├── movie_keyword.movie_id = cast_info.movie_id [type=bool, outer=(18,36), constraints=(/18: (/NULL - ]; /36: (/NULL - ])]
