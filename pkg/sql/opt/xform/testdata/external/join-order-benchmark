 │    │    └── filters [type=bool, outer=(2,40,44), constraints=(/2: (/NULL - ]; /40: (/NULL - ]; /44: [/'m' - /'m']), fd=()-->(44), (2)==(40), (40)==(2)]
 │    │         ├── name.id = person_id [type=bool, outer=(2,40), constraints=(/2: (/NULL - ]; /40: (/NULL - ])]
 │    │         └── gender = 'm' [type=bool, outer=(44), constraints=(/44: [/'m' - /'m']; tight)]
 │    └── filters [type=bool, outer=(3,23,28,33,38,49,50,53), constraints=(/3: (/NULL - ]; /23: (/NULL - ]; /28: (/NULL - ]; /33: (/NULL - ]; /38: (/NULL - ]; /49: (/NULL - ]; /53: [/2001 - ]), fd=(28)==(3,23,33,38,49), (49)==(3,23,28,33,38), (33)==(3,23,28,38,49), (3)==(23,28,33,38,49), (38)==(3,23,28,33,49), (23)==(3,28,33,38,49)]
 │         ├── title.id = movie_info.movie_id [type=bool, outer=(28,49), constraints=(/28: (/NULL - ]; /49: (/NULL - ])]
 │         ├── title.id = movie_info_idx.movie_id [type=bool, outer=(33,49), constraints=(/33: (/NULL - ]; /49: (/NULL - ])]
 │         ├── title.id = cast_info.movie_id [type=bool, outer=(3,49), constraints=(/3: (/NULL - ]; /49: (/NULL - ])]
 │         ├── title.id = movie_keyword.movie_id [type=bool, outer=(38,49), constraints=(/38: (/NULL - ]; /49: (/NULL - ])]
 │         ├── title.id = movie_companies.movie_id [type=bool, outer=(23,49), constraints=(/23: (/NULL - ]; /49: (/NULL - ])]
 │         ├── production_year > 2000 [type=bool, outer=(53), constraints=(/53: [/2001 - ]; tight)]
 │         └── ((title LIKE '%Freddy%') OR (title LIKE '%Jason%')) OR (title LIKE 'Saw%') [type=bool, outer=(50)]
 └── aggregations [outer=(30,35,41,50)]
      ├── min [type=string, outer=(30)]
      │    └── variable: movie_info.info [type=string, outer=(30)]
      ├── min [type=string, outer=(35)]
      │    └── variable: movie_info_idx.info [type=string, outer=(35)]
      ├── min [type=string, outer=(41)]
      │    └── variable: name.name [type=string, outer=(41)]
      └── min [type=string, outer=(50)]
           └── variable: title [type=string, outer=(50)]

opt
SELECT MIN(mi.info) AS movie_budget,
       MIN(mi_idx.info) AS movie_votes,
       MIN(n.name) AS writer,
       MIN(t.title) AS violent_liongate_movie
FROM cast_info AS ci,
     company_name AS cn,
     info_type AS it1,
     info_type AS it2,
     keyword AS k,
     movie_companies AS mc,
     movie_info AS mi,
     movie_info_idx AS mi_idx,
     movie_keyword AS mk,
     name AS n,
     title AS t
WHERE ci.note IN ('(writer)',
                  '(head writer)',
                  '(written by)',
                  '(story)',
                  '(story editor)')
  AND cn.name LIKE 'Lionsgate%'
  AND it1.info = 'genres'
  AND it2.info = 'votes'
  AND k.keyword IN ('murder',
                    'violence',
                    'blood',
                    'gore',
                    'death',
                    'female-nudity',
                    'hospital')
  AND mi.info IN ('Horror',
                  'Action',
                  'Sci-Fi',
                  'Thriller',
                  'Crime',
                  'War')
  AND t.id = mi.movie_id
  AND t.id = mi_idx.movie_id
  AND t.id = ci.movie_id
  AND t.id = mk.movie_id
  AND t.id = mc.movie_id
  AND ci.movie_id = mi.movie_id
  AND ci.movie_id = mi_idx.movie_id
  AND ci.movie_id = mk.movie_id
  AND ci.movie_id = mc.movie_id
  AND mi.movie_id = mi_idx.movie_id
  AND mi.movie_id = mk.movie_id
  AND mi.movie_id = mc.movie_id
  AND mi_idx.movie_id = mk.movie_id
  AND mi_idx.movie_id = mc.movie_id
  AND mk.movie_id = mc.movie_id
  AND n.id = ci.person_id
  AND it1.id = mi.info_type_id
  AND it2.id = mi_idx.info_type_id
  AND k.id = mk.keyword_id
  AND cn.id = mc.company_id;
