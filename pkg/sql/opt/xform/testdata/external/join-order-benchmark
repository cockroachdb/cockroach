 │    │         └── role = 'actress' [type=bool, outer=(52), constraints=(/52: [/'actress' - /'actress']; tight)]
 │    └── filters [type=bool, outer=(18,33,38,53,57), constraints=(/18: (/NULL - ]; /33: (/NULL - ]; /38: (/NULL - ]; /53: (/NULL - ]; /57: [/2001 - ]), fd=(38)==(18,33,53), (53)==(18,33,38), (33)==(18,38,53), (18)==(33,38,53)]
