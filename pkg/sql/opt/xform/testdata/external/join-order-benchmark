    id INTEGER NOT NULL PRIMARY KEY,
    name TEXT NOT NULL,
    country_code CHARACTER VARYING(255),
    imdb_id INTEGER,
    name_pcode_nf CHARACTER VARYING(5),
    name_pcode_sf CHARACTER VARYING(5),
    md5sum CHARACTER VARYING(32)
)
