opt
SELECT MIN(at.title) AS aka_title,
       MIN(t.title) AS internet_movie_title
FROM aka_title AS AT,
     company_name AS cn,
     company_type AS ct,
     info_type AS it1,
     keyword AS k,
     movie_companies AS mc,
     movie_info AS mi,
     movie_keyword AS mk,
     title AS t
WHERE cn.country_code = '[us]'
  AND it1.info = 'release dates'
  AND mi.note LIKE '%internet%'
  AND t.production_year > 1990
  AND t.id = at.movie_id
  AND t.id = mi.movie_id
  AND t.id = mk.movie_id
  AND t.id = mc.movie_id
  AND mk.movie_id = mi.movie_id
  AND mk.movie_id = mc.movie_id
  AND mk.movie_id = at.movie_id
  AND mi.movie_id = mc.movie_id
  AND mi.movie_id = at.movie_id
  AND mc.movie_id = at.movie_id
  AND k.id = mk.keyword_id
  AND it1.id = mi.info_type_id
  AND cn.id = mc.company_id
  AND ct.id = mc.company_type_id;
----
scalar-group-by
 ├── columns: aka_title:51(string) internet_movie_title:52(string)
 ├── cardinality: [1 - 1]
 ├── key: ()
 ├── fd: ()-->(51,52)
 ├── inner-join (lookup title)
 │    ├── columns: aka_title.movie_id:2(int!null) aka_title.title:3(string!null) company_name.id:12(int!null) country_code:14(string!null) company_type.id:19(int!null) info_type.id:21(int!null) info_type.info:22(string!null) keyword.id:23(int!null) movie_companies.movie_id:27(int!null) company_id:28(int!null) company_type_id:29(int!null) movie_info.movie_id:32(int!null) info_type_id:33(int!null) movie_info.note:35(string) movie_keyword.movie_id:37(int!null) keyword_id:38(int!null) title.id:39(int!null) title.title:40(string!null) title.production_year:43(int!null)
 │    ├── key columns: [2] = [39]
