 │    ├── key columns: [2] = [39]
 │    ├── stats: [rows=1, distinct(2)=0.0124455411, distinct(27)=0.0124455411, distinct(32)=0.0124455411, distinct(37)=0.0124455411, distinct(39)=0.0124455411]
