 │         ├── title.id = movie_keyword.movie_id [type=bool, outer=(36,47), constraints=(/36: (/NULL - ]; /47: (/NULL - ])]
 │         ├── title.id = cast_info.movie_id [type=bool, outer=(18,47), constraints=(/18: (/NULL - ]; /47: (/NULL - ])]
 │         ├── title.id = complete_cast.movie_id [type=bool, outer=(2,47), constraints=(/2: (/NULL - ]; /47: (/NULL - ])]
 │         ├── title.id = movie_info_idx.movie_id [type=bool, outer=(31,47), constraints=(/31: (/NULL - ]; /47: (/NULL - ])]
 │         └── production_year > 2000 [type=bool, outer=(51), constraints=(/51: [/2001 - ]; tight)]
 └── aggregations [outer=(10,33,39,48)]
      ├── min [type=string, outer=(10)]
      │    └── variable: char_name.name [type=string, outer=(10)]
      ├── min [type=string, outer=(33)]
      │    └── variable: movie_info_idx.info [type=string, outer=(33)]
      ├── min [type=string, outer=(39)]
      │    └── variable: name.name [type=string, outer=(39)]
      └── min [type=string, outer=(48)]
           └── variable: title [type=string, outer=(48)]

opt
SELECT MIN(chn.name) AS character_name,
       MIN(mi_idx.info) AS rating,
       MIN(t.title) AS complete_hero_movie
FROM complete_cast AS cc,
     comp_cast_type AS cct1,
     comp_cast_type AS cct2,
     char_name AS chn,
     cast_info AS ci,
     info_type AS it2,
     keyword AS k,
     kind_type AS kt,
     movie_info_idx AS mi_idx,
     movie_keyword AS mk,
     name AS n,
     title AS t
WHERE cct1.kind = 'cast'
  AND cct2.kind LIKE '%complete%'
  AND chn.name IS NOT NULL
  AND (chn.name LIKE '%man%'
       OR chn.name LIKE '%Man%')
  AND it2.info = 'rating'
  AND k.keyword IN ('superhero',
                    'marvel-comics',
                    'based-on-comic',
                    'fight')
  AND kt.kind = 'movie'
  AND mi_idx.info > '8.0'
  AND t.production_year > 2005
  AND kt.id = t.kind_id
  AND t.id = mk.movie_id
  AND t.id = ci.movie_id
  AND t.id = cc.movie_id
  AND t.id = mi_idx.movie_id
  AND mk.movie_id = ci.movie_id
  AND mk.movie_id = cc.movie_id
  AND mk.movie_id = mi_idx.movie_id
  AND ci.movie_id = cc.movie_id
  AND ci.movie_id = mi_idx.movie_id
  AND cc.movie_id = mi_idx.movie_id
  AND chn.id = ci.person_role_id
  AND n.id = ci.person_id
  AND k.id = mk.keyword_id
  AND cct1.id = cc.subject_id
  AND cct2.id = cc.status_id
  AND it2.id = mi_idx.info_type_id;
----
scalar-group-by
 ├── columns: character_name:59(string) rating:60(string) complete_hero_movie:61(string)
 ├── cardinality: [1 - 1]
 ├── key: ()
 ├── fd: ()-->(59-61)
 ├── inner-join (lookup title)
