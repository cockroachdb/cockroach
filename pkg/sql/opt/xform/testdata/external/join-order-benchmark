 │    │    │    │    │         ├── cast_info.movie_id = movie_info_idx.movie_id [type=bool, outer=(3,33), constraints=(/3: (/NULL - ]; /33: (/NULL - ])]
 │    │    │    │    │         ├── movie_info.movie_id = movie_info_idx.movie_id [type=bool, outer=(28,33), constraints=(/28: (/NULL - ]; /33: (/NULL - ])]
 │    │    │    │    │         └── movie_info_idx.movie_id = movie_companies.movie_id [type=bool, outer=(23,33), constraints=(/23: (/NULL - ]; /33: (/NULL - ])]
 │    │    │    │    └── filters [type=bool, outer=(19,39), constraints=(/19: (/NULL - ]; /39: (/NULL - ]), fd=(19)==(39), (39)==(19)]
 │    │    │    │         └── keyword.id = keyword_id [type=bool, outer=(19,39), constraints=(/19: (/NULL - ]; /39: (/NULL - ])]
 │    │    │    └── filters [type=bool, outer=(3,23,28,33,38), constraints=(/3: (/NULL - ]; /23: (/NULL - ]; /28: (/NULL - ]; /33: (/NULL - ]; /38: (/NULL - ]), fd=(3)==(23,28,33,38), (38)==(3,23,28,33), (28)==(3,23,33,38), (33)==(3,23,28,38), (23)==(3,28,33,38)]
 │    │    │         ├── cast_info.movie_id = movie_keyword.movie_id [type=bool, outer=(3,38), constraints=(/3: (/NULL - ]; /38: (/NULL - ])]
