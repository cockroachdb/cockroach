 │    │    │         └── name.name LIKE '%An%' [type=bool, outer=(49)]
 │    │    └── filters [type=bool, outer=(22,57,58), constraints=(/22: (/NULL - ]; /57: (/NULL - ]; /58: [/'actress' - /'actress']), fd=()-->(58), (22)==(57), (57)==(22)]
 │    │         ├── role_type.id = role_id [type=bool, outer=(22,57), constraints=(/22: (/NULL - ]; /57: (/NULL - ])]
 │    │         └── role = 'actress' [type=bool, outer=(58), constraints=(/58: [/'actress' - /'actress']; tight)]
 │    └── filters [type=bool, outer=(18,36,41,46,59,63), constraints=(/18: (/NULL - ]; /36: (/NULL - ]; /41: (/NULL - ]; /46: (/NULL - ]; /59: (/NULL - ]; /63: [/2011 - ]), fd=(41)==(18,36,46,59), (59)==(18,36,41,46), (36)==(18,41,46,59), (18)==(36,41,46,59), (46)==(18,36,41,59)]
 │         ├── title.id = movie_info.movie_id [type=bool, outer=(41,59), constraints=(/41: (/NULL - ]; /59: (/NULL - ])]
 │         ├── title.id = movie_companies.movie_id [type=bool, outer=(36,59), constraints=(/36: (/NULL - ]; /59: (/NULL - ])]
 │         ├── title.id = cast_info.movie_id [type=bool, outer=(18,59), constraints=(/18: (/NULL - ]; /59: (/NULL - ])]
 │         ├── title.id = movie_keyword.movie_id [type=bool, outer=(46,59), constraints=(/46: (/NULL - ]; /59: (/NULL - ])]
 │         └── production_year > 2010 [type=bool, outer=(63), constraints=(/63: [/2011 - ]; tight)]
 └── aggregations [outer=(10,49,60)]
      ├── min [type=string, outer=(10)]
      │    └── variable: char_name.name [type=string, outer=(10)]
      ├── min [type=string, outer=(49)]
      │    └── variable: name.name [type=string, outer=(49)]
      └── min [type=string, outer=(60)]
           └── variable: title [type=string, outer=(60)]

opt
SELECT MIN(chn.name) AS voiced_char_name,
       MIN(n.name) AS voicing_actress_name,
       MIN(t.title) AS kung_fu_panda
FROM aka_name AS an,
     char_name AS chn,
     cast_info AS ci,
     company_name AS cn,
     info_type AS it,
     keyword AS k,
     movie_companies AS mc,
     movie_info AS mi,
     movie_keyword AS mk,
     name AS n,
     role_type AS rt,
     title AS t
WHERE ci.note IN ('(voice)',
                  '(voice: Japanese version)',
                  '(voice) (uncredited)',
                  '(voice: English version)')
  AND cn.country_code ='[us]'
  AND cn.name = 'DreamWorks Animation'
  AND it.info = 'release dates'
  AND k.keyword IN ('hero',
                    'martial-arts',
                    'hand-to-hand-combat',
                    'computer-animated-movie')
  AND mi.info IS NOT NULL
  AND (mi.info LIKE 'Japan:%201%'
       OR mi.info LIKE 'USA:%201%')
  AND n.gender ='f'
  AND n.name LIKE '%An%'
  AND rt.role ='actress'
  AND t.production_year > 2010
  AND t.title LIKE 'Kung Fu Panda%'
  AND t.id = mi.movie_id
  AND t.id = mc.movie_id
  AND t.id = ci.movie_id
  AND t.id = mk.movie_id
  AND mc.movie_id = ci.movie_id
  AND mc.movie_id = mi.movie_id
  AND mc.movie_id = mk.movie_id
  AND mi.movie_id = ci.movie_id
  AND mi.movie_id = mk.movie_id
  AND ci.movie_id = mk.movie_id
  AND cn.id = mc.company_id
  AND it.id = mi.info_type_id
  AND n.id = ci.person_id
  AND rt.id = ci.role_id
  AND n.id = an.person_id
  AND ci.person_id = an.person_id
  AND chn.id = ci.person_role_id
  AND k.id = mk.keyword_id;
