 │    ├── key columns: [11] = [23]
 │    ├── stats: [rows=0.352838577, distinct(8)=0.00819663497, distinct(11)=0.00850247165, distinct(16)=0.00850247165, distinct(21)=0.00850247165, distinct(23)=0.00850247165, distinct(26)=0.00819663497]
