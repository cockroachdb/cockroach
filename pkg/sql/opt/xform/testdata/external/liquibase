CREATE TABLE pg_namespace (
    oid oid PRIMARY KEY,
    nspname text NOT NULL,
    nspowner oid NOT NULL,
    nspacl text[],
    UNIQUE INDEX pg_namespace_nspname_index (nspname)
);
----
TABLE pg_namespace
 ├── oid oid not null
 ├── nspname string not null
 ├── nspowner oid not null
 ├── nspacl string[]
 ├── INDEX primary
 │    └── oid oid not null
 └── INDEX pg_namespace_nspname_index
      ├── nspname string not null
      └── oid oid not null (storing)

