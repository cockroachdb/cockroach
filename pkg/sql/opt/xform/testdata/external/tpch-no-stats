 │    │    ├── key columns: [2] = [17]
 │    │    ├── lookup columns are key
 │    │    ├── fd: ()-->(14), (17)-->(20,22,23), (2)==(17), (17)==(2)
 │    │    ├── select
 │    │    │    ├── columns: l_partkey:2!null l_quantity:5!null l_extendedprice:6!null l_discount:7!null l_shipinstruct:14!null l_shipmode:15!null
