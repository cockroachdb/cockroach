 │         ├── sum [type=float, outer=(26)]
 │         │    └── variable: column26 [type=float, outer=(26)]
 │         └── sum [type=float, outer=(28)]
 │              └── variable: column28 [type=float, outer=(28)]
 └── projections [outer=(27,29)]
      └── (column27 * 100.0) / column29 [type=float, outer=(27,29)]

