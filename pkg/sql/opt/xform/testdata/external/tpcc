UPDATE customer
SET (c_balance, c_ytd_payment, c_payment_cnt, c_data)
  = (
    c_balance - (3860.61:::FLOAT8)::DECIMAL,
    c_ytd_payment + (3860.61:::FLOAT8)::DECIMAL,
    c_payment_cnt + 1,
    CASE c_credit
    WHEN 'BC'
    THEN "left"(
      c_id::STRING
      || c_d_id::STRING
      || c_w_id::STRING
      || (5:::INT8)::STRING
      || (10:::INT8)::STRING
      || (3860.61:::FLOAT8)::STRING
      || c_data,
      500
