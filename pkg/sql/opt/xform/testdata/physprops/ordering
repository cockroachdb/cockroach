# Verify that the internal ordering is required of the input.
opt
UPDATE a SET (x, y, z, s)=(SELECT a, b::float, c::decimal, 'foo' FROM abc) ORDER BY y
----
update a
 ├── columns: <none>
 ├── fetch columns: x:5(int) y:6(float) z:7(decimal) s:8(string)
 ├── update-mapping:
 │    ├──  a:9(int) => x:1(int)
 │    ├──  b:12(float) => y:2(float)
 │    ├──  c:13(decimal) => z:3(decimal)
 │    └──  "?column?":14(string) => s:4(string)
 ├── internal-ordering: +6
 └── sort
      ├── columns: x:5(int!null) y:6(float!null) z:7(decimal) s:8(string!null) a:9(int) b:12(float) c:13(decimal) "?column?":14(string)
      ├── ordering: +6
      └── left-join
           ├── columns: x:5(int!null) y:6(float!null) z:7(decimal) s:8(string!null) a:9(int) b:12(float) c:13(decimal) "?column?":14(string)
           ├── scan a
           │    └── columns: x:5(int!null) y:6(float!null) z:7(decimal) s:8(string!null)
           ├── max1-row
           │    ├── columns: a:9(int!null) b:12(float) c:13(decimal) "?column?":14(string!null)
           │    └── project
           │         ├── columns: b:12(float) c:13(decimal) "?column?":14(string!null) a:9(int!null)
           │         ├── scan abc
           │         │    └── columns: a:9(int!null) abc.b:10(int!null) abc.c:11(int!null)
           │         └── projections
           │              ├── abc.b::FLOAT8 [type=float]
           │              ├── abc.c::DECIMAL [type=decimal]
           │              └── const: 'foo' [type=string]
           └── filters (true)

# Verify that the external and internal orderings are intersected.
opt
