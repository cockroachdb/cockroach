 ├── fd: (5)==(6), (6)==(5)
 ├── ordering: +(5|6) [provided: +5]
 └── sort
      ├── columns: x:4(int!null) y:5(int!null) z:6(int!null)
      ├── key: (4,6)
      ├── fd: (5)==(6), (6)==(5)
      ├── ordering: +(5|6) [provided: +5]
      └── select
           ├── columns: x:4(int!null) y:5(int!null) z:6(int!null)
           ├── key: (4,6)
           ├── fd: (5)==(6), (6)==(5)
           ├── scan xyz
