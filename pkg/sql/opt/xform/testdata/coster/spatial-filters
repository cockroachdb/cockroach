exec-ddl
CREATE TABLE x (
  id INT PRIMARY KEY,
  a FLOAT,
  b BOOL,
  geog GEOGRAPHY
);
----

exec-ddl
ALTER TABLE x INJECT STATISTICS '[
  {
    "columns": [
      "id"
    ],
    "created_at": "2021-01-01 00:00:00",
    "distinct_count": 500000,
    "name": "__auto__",
    "row_count": 500000
  }
]';
----

opt
SELECT * FROM g JOIN x
ON g.id = x.id
WHERE st_area(g.geog) > st_area(x.geog);
