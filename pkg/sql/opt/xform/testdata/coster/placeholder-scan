 ├── columns: k:1!null
 ├── stats: [rows=1000]
 ├── cost: 1078.04001
 ├── cost-flags: unbounded-cardinality
 ├── key: (1)
 └── scan t@t_a_b_idx
      ├── columns: k:1!null a:2!null
      ├── constraint: /2/3/1: [/1 - /1]
      ├── stats: [rows=1000, distinct(2)=1, null(2)=0]
      ├── cost: 1068.02001
      ├── cost-flags: unbounded-cardinality
      ├── key: (1)
      └── fd: ()-->(2)

