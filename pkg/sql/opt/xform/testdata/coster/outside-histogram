opt set=(optimizer_prefer_bounded_cardinality=false,optimizer_min_row_count=0)
SELECT * FROM t WHERE k IN (100, 110, 120, 130) AND i = 400 AND s < 'apple'
----
select
 ├── columns: k:1!null i:2!null s:3!null
 ├── cardinality: [0 - 4]
 ├── stats: [rows=1e-10, distinct(1)=1e-10, null(1)=0, distinct(2)=1e-10, null(2)=0, distinct(3)=1e-10, null(3)=0, distinct(2,3)=1e-10, null(2,3)=0, distinct(1-3)=1e-10, null(1-3)=0]
 │   histogram(1)=  0 2.5e-11 0 2.5e-11 0 2.5e-11 0 2.5e-11
 │                <---- 100 ----- 110 ----- 120 ----- 130 -
 │   histogram(2)=  0 1e-10
 │                <--- 400
 │   histogram(3)=
 ├── cost: 18.07
 ├── key: (1)
 ├── fd: ()-->(2), (1)-->(3)
 ├── index-join t
 │    ├── columns: k:1!null i:2 s:3
 │    ├── cardinality: [0 - 4]
