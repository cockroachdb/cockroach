# --------------------------------------------------
# Q4
# --------------------------------------------------

opt set=(optimizer_prefer_bounded_cardinality=false,optimizer_min_row_count=0)
SELECT * FROM t WHERE k IN (100, 110, 120, 130) AND i = 400 AND s < 'apple'
----
select
 ├── columns: k:1!null i:2!null s:3!null
