SELECT * FROM t WHERE i = 400 AND s > 'z'
----
select
 ├── columns: k:1!null i:2!null s:3!null
 ├── stats: [rows=1e-10, distinct(2)=1e-10, null(2)=0, distinct(3)=1e-10, null(3)=0, distinct(2,3)=1e-10, null(2,3)=0]
 │   histogram(2)=  0 1e-10
 │                <--- 400
 │   histogram(3)=
 ├── cost: 18.07
 ├── key: (1)
 ├── fd: ()-->(2), (1)-->(3)
 ├── index-join t
 │    ├── columns: k:1!null i:2 s:3
 │    ├── stats: [rows=1e-10]
 │    ├── cost: 18.04
