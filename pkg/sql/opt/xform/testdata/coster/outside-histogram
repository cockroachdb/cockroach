 │    │    └── [/140 - /140]
 │    ├── cardinality: [0 - 4]
 │    ├── stats: [rows=4, distinct(1)=4, null(1)=0]
 │    │   histogram(1)=  0   1   0   1   0   1   0   1
 │    │                <--- 110 --- 120 --- 130 --- 140
 │    ├── cost: 24.25
 │    ├── key: (1)
 │    └── fd: (1)-->(2-6), (2)~~>(1,3-6)
 └── filters
      └── i:4 = 500 [outer=(4), constraints=(/4: [/500 - /500]; tight), fd=()-->(4)]

opt set=optimizer_clamp_low_histogram_selectivity=true
