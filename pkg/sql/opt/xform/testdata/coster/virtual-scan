exec-ddl
CREATE TABLE system.information_schema.schemata (
	CATALOG_NAME               STRING NOT NULL,
	SCHEMA_NAME                STRING NOT NULL,
	DEFAULT_CHARACTER_SET_NAME STRING,
	SQL_PATH                   STRING
)
----
TABLE schemata
 ├── catalog_name string not null
 ├── schema_name string not null
 ├── default_character_set_name string
 └── sql_path string

opt
