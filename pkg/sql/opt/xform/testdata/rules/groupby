 │    ├── limit: 1(rev)
 │    ├── key: ()
 │    └── fd: ()-->(1)
 └── aggregations
      └── const-agg [as=max:4, outer=(1)]
           └── x:1

opt
SELECT min(y) FROM xyz
----
scalar-group-by
 ├── columns: min:4
 ├── cardinality: [1 - 1]
 ├── key: ()
 ├── fd: ()-->(4)
 ├── scan xyz@yy
 │    ├── columns: y:2!null
 │    ├── constraint: /2/1: (/NULL - ]
 │    ├── limit: 1
 │    ├── key: ()
 │    └── fd: ()-->(2)
 └── aggregations
      └── const-agg [as=min:4, outer=(2)]
           └── y:2

opt
SELECT min(y), min(y) FROM xyz
----
