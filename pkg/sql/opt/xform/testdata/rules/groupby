 │         ├── best: (scalar-group-by G4 G5 cols=())
 │         └── cost: 5.10
 ├── G2: (scan abc,cols=(1))
 │    ├── [ordering: -1] [limit hint: 1.00]
 │    │    ├── best: (scan abc,rev,cols=(1))
 │    │    └── cost: 5.06
