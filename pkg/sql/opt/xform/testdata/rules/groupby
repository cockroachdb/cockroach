 │    └── fd: (1)-->(2)
 └── aggregations
      ├── max [as=max:4, outer=(2)]
      │    └── y:2
      └── max [as=max:5, outer=(1)]
           └── x:1

# ReplaceScalarMinWithLimit does not apply with
# multiple grouping columns
opt
SELECT x,min(y) FROM xyz GROUP BY x,y
----
