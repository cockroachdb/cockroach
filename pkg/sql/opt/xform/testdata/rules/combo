exec-ddl
CREATE TABLE abc
(
    a INT,
    b INT,
    c INT,
    INDEX ab (a,b) STORING (c)
)
----

exec-ddl
CREATE TABLE xyz
(
    x INT,
    y INT,
    z INT,
    INDEX xy (x,y) STORING (z)
)
----

# --------------------------------------------------
# Use exploretrace.
# --------------------------------------------------
exploretrace
SELECT * FROM abc, xyz WHERE a=x AND b=y
