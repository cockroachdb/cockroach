 │    ├── check constraint expressions
 │    │    └── i:2 IN (1, 2, 3) [outer=(2), constraints=(/2: [/1 - /1] [/2 - /2] [/3 - /3]; tight)]
 │    ├── key: (1)
 │    └── fd: (1)-->(2-5)
 └── filters
      └── s:4 % 'foo' [outer=(4), stable]

