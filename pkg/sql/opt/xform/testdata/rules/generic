      │    ├── fd: ()-->(8,9)
      │    └── values
      │         ├── columns: k:8
      │         ├── cardinality: [1 - 1]
      │         ├── has-placeholder
      │         ├── key: ()
      │         ├── fd: ()-->(8)
      │         └── ($1,)
      └── filters (true)

# Locking is preserved.
