exec-ddl
CREATE TABLE t (
  k INT PRIMARY KEY,
  i INT,
  s STRING,
  b BOOL,
  t TIMESTAMPTZ,
  INDEX (i, s, b),
  INDEX (i, t),
  INDEX (t)
)
----

exec-ddl
CREATE TABLE abc (
  a STRING,
  b STRING,
  c STRING,
  PRIMARY KEY (a, b)
)
----

# --------------------------------------------------
# GenerateParameterizedIndexJoin
# --------------------------------------------------

opt expect=GenerateParameterizedIndexJoin expect-not=GenerateParameterizedJoin
SELECT * FROM t WHERE k = $1
----
index-join t
 ├── columns: k:1!null i:2 s:3 b:4 t:5
 ├── cardinality: [0 - 1]
 ├── has-placeholder
 ├── key: ()
 ├── fd: ()-->(1-5)
 └── values
      ├── columns: k:1
      ├── cardinality: [1 - 1]
      ├── has-placeholder
      ├── key: ()
      ├── fd: ()-->(1)
      └── ($1,)

