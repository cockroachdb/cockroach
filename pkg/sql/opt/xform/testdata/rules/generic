      ├── key columns: [9] = [1]
      ├── lookup columns are key
      ├── cardinality: [0 - 1]
      ├── has-placeholder
      ├── key: ()
      ├── fd: ()-->(1-5,8,9), (1)==(9), (9)==(1)
      ├── index-join t
      │    ├── columns: k:8!null i:9
      │    ├── cardinality: [0 - 1]
      │    ├── has-placeholder
