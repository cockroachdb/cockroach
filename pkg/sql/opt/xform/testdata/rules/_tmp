exec-ddl
CREATE TYPE c AS ENUM ('c0', 'c1', 'c2', 'c3', 'c4')
----

exec-ddl
CREATE TABLE abc (a INT, b INT, c c, PRIMARY KEY (c, b), UNIQUE WITHOUT INDEX (b))
----

opt
