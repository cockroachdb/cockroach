 │    │    ├── columns: b1:8 b2:9 b3:10
 │    │    └── distinct-on
 │    │         ├── columns: b1:8 b2:9 b3:10 b.rowid:12!null
 │    │         ├── grouping columns: b.rowid:12!null
 │    │         ├── key: (12)
 │    │         ├── fd: (12)-->(8-10)
