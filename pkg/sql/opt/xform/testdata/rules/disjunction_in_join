# We should be able to split the disjunction even though the predicates are
# not equalities.
opt expect=SplitDisjunctionOfJoinTerms
SELECT e.*
FROM
  e
  INNER JOIN f ON f.f3 = e.e2
WHERE
  (e.e5 > 30 AND e.e5 < 40)
  OR (e.e4 IN ('a', 'b') AND f.f2 < 20 AND f.f2 > 10)
