 │    ├── fd: ()-->(1-4,10), (8)-->(7), (4)==(10), (10)==(4)
 │    ├── locality-optimized-search
 │    │    ├── columns: def_part.r:1!null d:2!null e:3 f:4
 │    │    ├── left columns: def_part.r:13 d:14 e:15 f:16
 │    │    ├── right columns: def_part.r:19 d:20 e:21 f:22
