 ├── columns: a:1!null b:2 c:3 x:7!null y:8 z:9
 ├── left ordering: +1
 ├── right ordering: +7
 ├── fd: (1)==(7), (7)==(1)
 ├── scan abc@ab
 │    ├── columns: a:1 b:2 c:3
 │    └── ordering: +1
 ├── scan xyz@xy
 │    ├── columns: x:7 y:8 z:9
 │    └── ordering: +7
 └── filters (true)

