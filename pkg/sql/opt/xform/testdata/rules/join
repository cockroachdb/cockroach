 │    ├── columns: m:1!null n:2 a:6!null b:7!null c:8!null abcd_comp.rowid:10!null
 │    ├── key columns: [1 13 14] = [6 7 8]
 │    ├── fd: ()-->(7,8), (10)-->(6), (1)==(6), (6)==(1)
