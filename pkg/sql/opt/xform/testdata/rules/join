 ├── columns: m:1!null n:2 a:5!null b:6 c:7
 ├── key columns: [8] = [8]
 ├── lookup columns are key
 ├── fd: (1)==(5), (5)==(1)
 ├── inner-join (lookup abcd@secondary)
 │    ├── columns: m:1!null n:2 a:5!null b:6 abcd.rowid:8!null
 │    ├── key columns: [1] = [5]
 │    ├── fd: (8)-->(5,6), (1)==(5), (5)==(1)
 │    ├── scan small
 │    │    └── columns: m:1 n:2
 │    └── filters (true)
 └── filters (true)

