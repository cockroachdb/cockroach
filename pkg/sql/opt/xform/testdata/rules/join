 │    │    ├── key: (9)
 │    │    └── fd: (9)-->(10-12)
 │    └── filters (true)
 └── filters
      ├── t1.a:4 @> t2.a:12 [outer=(4,12), immutable]
      └── t1.a:4 @> ARRAY['foo'] [outer=(4), immutable, constraints=(/4: (/NULL - ])]

