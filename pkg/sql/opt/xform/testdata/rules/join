 ├── columns: m:1 n:2 a:5 b:6 c:7
 ├── key columns: [8] = [8]
 ├── lookup columns are key
 ├── left-join (lookup abcd@secondary)
 │    ├── columns: m:1 n:2 a:5 b:6 abcd.rowid:8
