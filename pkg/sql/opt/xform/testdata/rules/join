      ├── lookup columns are key
      ├── immutable
      ├── fd: (1)-->(4)
      ├── inner-join (inverted json_arr1@a_idx [as=t1])
      │    ├── columns: t2.a:12 t1.k:15!null
      │    ├── inverted-expr
      │    │    └── t1.a:18 @> t2.a:12
      │    ├── scan json_arr2 [as=t2]
      │    │    └── columns: t2.a:12
      │    └── filters (true)
      └── filters
           └── t1.a:4 @> t2.a:12 [outer=(4,12), immutable]

