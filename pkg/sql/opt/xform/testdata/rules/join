 │    └── ordering: +11
 ├── inner-join (merge)
 │    ├── columns: a:1!null b:2!null c:3 x:6!null y:7!null z:8
 │    ├── left ordering: +1,+2
 │    ├── right ordering: +6,+7
 │    ├── fd: (1)==(6), (6)==(1), (2)==(7), (7)==(2)
 │    ├── ordering: +(1|6) [actual: +1]
 │    ├── scan abc@ab
 │    │    ├── columns: a:1 b:2 c:3
 │    │    └── ordering: +1,+2
 │    ├── scan xyz@xy
 │    │    ├── columns: x:6 y:7 z:8
 │    │    └── ordering: +6,+7
 │    └── filters (true)
 └── filters (true)

