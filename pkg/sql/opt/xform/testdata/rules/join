 ├── G21: (const 1)
 ├── G22: (variable b)
 └── G23: (const 2)

# No-op case because a single join can only be commuted (this is handled by
# CommuteJoin).
opt expect-not=ReorderJoins format=hide-all
SELECT * FROM abc, stu WHERE a = s
----
inner-join (merge)
 ├── scan abc@ab
 ├── scan stu
 └── filters (true)

# No-op case because the joins have join flags.
opt expect-not=ReorderJoins format=hide-all
SELECT *
FROM abc
INNER HASH JOIN stu
ON a = s
INNER MERGE JOIN xyz
ON s = x
----
inner-join (merge)
 ├── flags: force merge join
 ├── sort
 │    └── inner-join (hash)
 │         ├── flags: force hash join (store right side)
 │         ├── scan abc
 │         ├── scan stu
 │         └── filters
 │              └── a = s
 ├── scan xyz@xy
 └── filters (true)

# No-op case because InnerJoinApply is not matched.
opt expect-not=ReorderJoins format=hide-all
SELECT *
FROM abc
INNER JOIN LATERAL (SELECT * FROM (VALUES (a+1), (b*2)) f(v))
ON a = v
----
inner-join-apply
 ├── scan abc
 ├── values
 │    ├── (a + 1,)
 │    └── (b * 2,)
 └── filters
      └── a = column1

# No-op case because LeftJoin is (currently) not matched.
opt expect-not=ReorderJoins format=hide-all
SELECT *
FROM abc
LEFT JOIN stu
ON a = s
----
left-join (merge)
 ├── scan abc@ab
 ├── scan stu
 └── filters (true)

# Don't add a join between xyz and stu to the memo (because doing so would
# create a cross join).
memo
SELECT *
FROM stu
INNER JOIN abc
ON s = a
INNER JOIN xyz
ON b = y
----
