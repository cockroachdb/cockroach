 │    └── []
 │         ├── best: (lookup-join G10 G9 nyc_census_blocks,keyCols=[1],outCols=(3,9,10,13-15))
 │         └── cost: 7301.98
 ├── G5: (aggregations G11)
 ├── G6: (div G12 G13)
 ├── G7: (scan c,cols=(3,9,10))
 │    └── []
 │         ├── best: (scan c,cols=(3,9,10))
 │         └── cost: 43837.24
 ├── G8: (select G14 G15)
 │    └── []
 │         ├── best: (select G14 G15)
