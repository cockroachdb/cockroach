 ├── columns: m:1!null n:2 a:6!null b:7 c:8!null d:9 e:10
 ├── key columns: [11] = [11]
 ├── lookup columns are key
 ├── fd: ()-->(8), (1)==(6), (6)==(1)
 ├── inner-join (lookup abcde@secondary)
 │    ├── columns: m:1!null n:2 a:6!null b:7 c:8!null abcde.rowid:11!null
 │    ├── key columns: [1] = [6]
 │    ├── fd: ()-->(8), (11)-->(6,7), (1)==(6), (6)==(1)
 │    ├── scan small
 │    │    └── columns: m:1 n:2
 │    └── filters
