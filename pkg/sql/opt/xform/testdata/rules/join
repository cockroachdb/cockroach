 │    ├── fd: (7)-->(4,5)
 │    ├── scan small
 │    │    └── columns: m:1 n:2
 │    └── filters
 │         ├── b:5 > n:2 [outer=(2,5), constraints=(/2: (/NULL - ]; /5: (/NULL - ])]
 │         └── b:5 > 1 [outer=(5), constraints=(/5: [/2 - ]; tight)]
 └── filters (true)

# Non-covering case, extra filter not bound by index.
