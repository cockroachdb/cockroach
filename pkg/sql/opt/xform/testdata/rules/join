 ├── fd: ()-->(6,7), (1)==(5), (5)==(1)
 ├── inner-join (lookup abcde@secondary)
 │    ├── columns: m:1!null n:2 a:5!null b:6!null c:7!null abcde.rowid:10!null
 │    ├── key columns: [1 12 13] = [5 6 7]
 │    ├── fd: ()-->(6,7), (10)-->(5), (1)==(5), (5)==(1)
 │    ├── project
 │    │    ├── columns: "project_const_col_@6":12!null "project_const_col_@7":13!null m:1 n:2
