 ├── G25: (variable n.boroname)
 ├── G26: (scalar-list G29)
 ├── G27: (eq G31 G32)
 ├── G28: (eq G31 G33)
 ├── G29: (variable n.geom)
 ├── G30: (variable c.geom)
 ├── G31: (variable name)
 ├── G32: (const 'Upper West Side')
 └── G33: (const 'Upper East Side')

# --------------------------------------------------
# GenerateZigZagJoins
