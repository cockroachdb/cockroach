 ├── inner-join (lookup abcd@secondary)
 │    ├── columns: m:1!null n:2!null a:5!null b:6!null abcd.rowid:8!null
 │    ├── key columns: [1] = [5]
 │    ├── fd: (8)-->(5,6), (1)==(5), (5)==(1)
 │    ├── scan small
 │    │    └── columns: m:1 n:2
 │    └── filters
 │         ├── b:6 > n:2 [outer=(2,6), constraints=(/2: (/NULL - ]; /6: (/NULL - ])]
 │         └── b:6 > 1 [outer=(6), constraints=(/6: [/2 - ]; tight)]
 └── filters (true)

