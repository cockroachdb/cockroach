 │    └── []
 │         ├── best: (lookup-join G13 G9 nyc_census_blocks [as=c],keyCols=[33],outCols=(3,9,10,14-16))
 │         └── cost: 7316.67
 ├── G5: (aggregations G14)
 ├── G6: (div G15 G16)
 ├── G7: (scan nyc_census_blocks [as=c],cols=(3,9,10))
 │    └── []
