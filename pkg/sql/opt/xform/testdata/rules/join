 ├── columns: m:1!null i:7 v2:10!null
 ├── flags: force lookup join (into right side)
 ├── key columns: [1] = [10]
 ├── immutable
 ├── fd: (7)-->(10), (1)==(10), (10)==(1)
 ├── scan small
 │    └── columns: m:1
 └── filters (true)

