      ├── fd: (1)-->(4)
      ├── union-all
      │    ├── columns: k:1!null j:4
      │    ├── left columns: k:1!null j:4
      │    ├── right columns: k:5 j:8
      │    ├── immutable
      │    ├── scan b
      │    │    ├── columns: k:1!null j:4
      │    │    ├── constraint: /1: [/1 - /1]
      │    │    ├── cardinality: [0 - 1]
      │    │    ├── key: ()
      │    │    └── fd: ()-->(1,4)
      │    └── index-join b
      │         ├── columns: k:5!null j:8
      │         ├── immutable
      │         ├── key: (5)
      │         ├── fd: (5)-->(8)
      │         └── scan b@inv_idx
      │              ├── columns: k:5!null
      │              ├── constraint: /8/5: [/'{"foo": "bar"}' - /'{"foo": "bar"}']
