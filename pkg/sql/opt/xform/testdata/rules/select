 ├── G2: (scan b) (lookup-join G4 b,cols=(1-3)) (lookup-join G5 b,cols=(1-3))
 │    └── ""
 │         ├── best: (scan b)
 │         └── cost: 1000.00
 ├── G3: (select G5 G6) (scan b@v,cols=(1,3),constrained)
 │    └── ""
 │         ├── best: (scan b@v,cols=(1,3),constrained)
 │         └── cost: 1.00
 ├── G4: (scan b@u,cols=(1,2))
 │    └── ""
 │         ├── best: (scan b@u,cols=(1,2))
 │         └── cost: 1000.00
 ├── G5: (scan b@v,cols=(1,3))
 │    └── ""
 │         ├── best: (scan b@v,cols=(1,3))
 │         └── cost: 1000.00
 ├── G6: (filters G7)
