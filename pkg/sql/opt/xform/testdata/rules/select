select
 ├── columns: geom:1 geog:2 val:3
 ├── immutable
 ├── scan geom_geog
 │    └── columns: geom:1 geog:2 val:3
 └── filters
      └── st_distance(geom:1, '010100000000000000000000000000000000000000') > 5.0 [outer=(1), immutable]

