      │    │    ├── fd: ()-->(2), (1)-->(3)
      │    │    └── scan d@u
      │    │         ├── columns: k:1!null u:2!null
      │    │         ├── constraint: /2/1: [/1 - /1]
      │    │         ├── key: (1)
      │    │         └── fd: ()-->(2)
      │    └── index-join d
      │         ├── columns: k:5!null u:6 v:7!null
      │         ├── key: (5)
      │         ├── fd: ()-->(7), (5)-->(6)
      │         └── scan d@v
      │              ├── columns: k:5!null v:7!null
      │              ├── constraint: /7/5: [/1 - /1]
      │              ├── key: (5)
      │              └── fd: ()-->(7)
      └── aggregations
           ├── const-agg [as=u:2, outer=(2)]
           │    └── u:2
           └── const-agg [as=v:3, outer=(3)]
                └── v:3

