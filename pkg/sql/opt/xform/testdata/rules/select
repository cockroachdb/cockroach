 ├── G1: (project G2 G3)
 │    └── "[presentation: k:1]"
 │         ├── best: (project G2 G3)
 │         └── cost: 333.00
 ├── G2: (select G4 G5) (scan a@v,cols=(1,3),constrained)
 │    └── ""
 │         ├── best: (scan a@v,cols=(1,3),constrained)
 │         └── cost: 333.00
 ├── G3: (projections a.k)
 ├── G4: (scan a,cols=(1,3)) (scan a@u,cols=(1,3)) (scan a@v,cols=(1,3))
 │    └── ""
 │         ├── best: (scan a,cols=(1,3))
 │         └── cost: 1000.00
 ├── G5: (filters G6)
 ├── G6: (gt G7 G8)
 ├── G7: (variable a.v)
 └── G8: (const 1)

