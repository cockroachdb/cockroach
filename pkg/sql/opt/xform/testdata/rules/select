 ├── immutable
 ├── key: (1)
 └── inverted-filter
      ├── columns: k:1!null
      ├── inverted expression: /6
      │    ├── tight: true, unique: false
      │    └── union spans: ["", ""]
      ├── key: (1)
      └── scan c@a_inv_idx
           ├── columns: k:1!null a_inverted_key:6!null
           ├── inverted constraint: /6/1
           │    └── spans: ["", ""]
           ├── key: (1)
           └── fd: (1)-->(6)

