  project
   ├── columns: k:1(int!null)
   ├── key: (1)
   └── select
        ├── columns: k:1(int!null) u:2(int!null)
        ├── key: (1)
        ├── fd: ()-->(2)
        ├── scan a
        │    ├── columns: k:1(int!null) u:2(int)
        │    ├── key: (1)
        │    └── fd: (1)-->(2)
        └── filters [type=bool, outer=(2), constraints=(/2: [/1 - /1]; tight), fd=()-->(2)]
             └── u = 1 [type=bool, outer=(2), constraints=(/2: [/1 - /1]; tight)]

