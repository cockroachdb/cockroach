 ├── G1: (select G2 G3) (scan a,cols=(1),constrained)
 │    └── "[presentation: k:1]"
 │         ├── best: (scan a,cols=(1),constrained)
 │         └── cost: 1.04
 ├── G2: (scan a,cols=(1)) (scan a@u,cols=(1)) (scan a@v,cols=(1))
 │    └── ""
 │         ├── best: (scan a,cols=(1))
 │         └── cost: 1040.00
 ├── G3: (filters G4)
 ├── G4: (eq G5 G6)
 ├── G5: (variable k)
 └── G6: (const 1)

