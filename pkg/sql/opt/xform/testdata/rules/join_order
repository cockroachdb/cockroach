  d INT PRIMARY KEY,
  z INT
)
----
TABLE dz
 ├── d int not null
 ├── z int
 └── INDEX primary
      └── d int not null

exec-ddl
CREATE TABLE abc (
  a INT PRIMARY KEY,
  b INT,
  c INT,
  d INT
)
----
TABLE abc
 ├── a int not null
 ├── b int
 ├── c int
 ├── d int
 └── INDEX primary
      └── a int not null

