exec-raw
CREATE DATABASE t
----

exec-raw
CREATE TABLE t.a (x INT PRIMARY KEY, y INT)
----

exec-raw
INSERT INTO t.a VALUES (1, 10), (2, 20), (3, 30)
----

exec-explain
SELECT * FROM t.a WHERE x > 1
----
filter     0  filter  ·       ·          (x, y)  ·
 │         0  ·       filter  @1 > 1     ·       ·
 └── scan  1  scan    ·       ·          (x, y)  ·
·          1  ·       table   a@primary  ·       ·
·          1  ·       spans   ALL        ·       ·

exec
SELECT * FROM t.a WHERE x > 1
----
2  20
3  30

exec-explain
SELECT * FROM t.a WHERE y > 1
----
filter     0  filter  ·       ·          (x, y)  ·
 │         0  ·       filter  @2 > 1     ·       ·
 └── scan  1  scan    ·       ·          (x, y)  ·
·          1  ·       table   a@primary  ·       ·
·          1  ·       spans   ALL        ·       ·

exec
SELECT * FROM t.a WHERE y > 1
----
1  10
2  20
3  30

exec-explain
SELECT * FROM t.a WHERE x > 1 AND x < 3
----
filter     0  filter  ·       ·                      (x, y)  ·
 │         0  ·       filter  (@1 > 1) AND (@1 < 3)  ·       ·
 └── scan  1  scan    ·       ·                      (x, y)  ·
·          1  ·       table   a@primary              ·       ·
·          1  ·       spans   ALL                    ·       ·

exec
SELECT * FROM t.a WHERE x > 1 AND x < 3
----
2  20

exec-explain
SELECT x + 1 FROM t.a
----
render     0  render  ·         ·          (column3)  ·
 │         0  ·       render 0  x + 1      ·          ·
 └── scan  1  scan    ·         ·          (x, y)     ·
·          1  ·       table     a@primary  ·          ·
·          1  ·       spans     ALL        ·          ·

exec
SELECT x + 1 FROM t.a
----
2
3
4

exec-explain
SELECT x, x + 1, y, y + 1, x + y FROM t.a
----
render     0  render  ·         ·          ("a.x", column3, "a.y", column4, column5)  ·
 │         0  ·       render 0  x          ·                                          ·
 │         0  ·       render 1  x + 1      ·                                          ·
 │         0  ·       render 2  y          ·                                          ·
 │         0  ·       render 3  y + 1      ·                                          ·
 │         0  ·       render 4  x + y      ·                                          ·
 └── scan  1  scan    ·         ·          (x, y)                                     ·
·          1  ·       table     a@primary  ·                                          ·
·          1  ·       spans     ALL        ·                                          ·

exec
SELECT x, x + 1, y, y + 1, x + y FROM t.a
----
1  2  10  11  11
2  3  20  21  22
3  4  30  31  33

exec-explain
SELECT u + v FROM (SELECT x + 3, y + 10 FROM t.a) AS foo(u, v)
----
render          0  render  ·         ·                  (column5)           ·
 │              0  ·       render 0  column3 + column4  ·                   ·
 └── render     1  render  ·         ·                  (column3, column4)  ·
      │         1  ·       render 0  x + 3              ·                   ·
      │         1  ·       render 1  y + 10             ·                   ·
      └── scan  2  scan    ·         ·                  (x, y)              ·
·               2  ·       table     a@primary          ·                   ·
·               2  ·       spans     ALL                ·                   ·

exec
SELECT u + v FROM (SELECT x + 3, y + 10 FROM t.a) AS foo(u, v)
----
24
35
46

exec-explain
SELECT x, x, y, x FROM t.a
----
render     0  render  ·         ·          ("a.x", "a.x", "a.y", "a.x")  ·
 │         0  ·       render 0  x          ·                             ·
 │         0  ·       render 1  x          ·                             ·
 │         0  ·       render 2  y          ·                             ·
 │         0  ·       render 3  x          ·                             ·
 └── scan  1  scan    ·         ·          (x, y)                        ·
·          1  ·       table     a@primary  ·                             ·
·          1  ·       spans     ALL        ·                             ·

exec
SELECT x, x, y, x FROM t.a
----
1  1  10  1
2  2  20  2
3  3  30  3

exec-raw
CREATE TABLE t.b (x INT, y INT);
INSERT INTO t.b VALUES (1, 10), (2, 20), (3, 30)
----

# Test with a hidden column.
exec-explain
SELECT * FROM t.b
----
render     0  render  ·         ·          ("b.x", "b.y")         ·
 │         0  ·       render 0  x          ·                      ·
 │         0  ·       render 1  y          ·                      ·
 └── scan  1  scan    ·         ·          (x, y, rowid[hidden])  ·
·          1  ·       table     b@primary  ·                      ·
·          1  ·       spans     ALL        ·                      ·

exec
SELECT * FROM t.b
----
1  10
2  20
3  30
