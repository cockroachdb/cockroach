# Ensure that ordering propagates past local DISTINCT processors.
query T
EXPLAIN (VERBOSE) SELECT DISTINCT ON (x,y,z) x, y, z FROM xyz ORDER BY x
----
distribution: full
vectorized: true
·
• sort
│ columns: (x, y, z)
│ ordering: +x
│ estimated row count: 1,000 (missing stats)
│ order: +x
│
└── • distinct
    │ columns: (x, y, z)
    │ estimated row count: 1,000 (missing stats)
    │ distinct on: x, y, z
    │
    └── • scan
          columns: (x, y, z)
          estimated row count: 1,000 (missing stats)
          table: xyz@primary
          spans: FULL SCAN

query T
EXPLAIN (DISTSQL) SELECT DISTINCT ON (x,y,z) x, y, z FROM xyz ORDER BY x
----
distribution: full
vectorized: true
·
• sort
│ order: +x
│
└── • distinct
    │ distinct on: x, y, z
    │
    └── • scan
          missing stats
          table: xyz@primary
          spans: FULL SCAN
·
