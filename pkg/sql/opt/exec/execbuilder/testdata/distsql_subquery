#  LogicTest: 5node-dist-opt

statement ok
CREATE TABLE abc (a INT PRIMARY KEY, b INT, c INT)

statement ok
ALTER TABLE abc SPLIT AT VALUES ((SELECT 1))


query T
SELECT url FROM [EXPLAIN (DISTSQL) SELECT EXISTS (SELECT * FROM abc WHERE a = 1)]
