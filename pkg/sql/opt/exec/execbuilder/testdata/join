 │         pred         b = b
 ├── scan  ·            ·
 │         table        foo@primary
 │         spans        ALL
 └── scan  ·            ·
·          table        bar@primary
·          spans        ALL

query TTT
SELECT tree, field, description FROM [
EXPLAIN (VERBOSE) SELECT * FROM foo, bar WHERE foo.b = bar.b
]
----
·           distributed  false
·           vectorized   false
cross-join  ·            ·
 │          type         inner
 │          pred         b = b
 ├── scan   ·            ·
 │          table        foo@primary
 │          spans        ALL
 └── scan   ·            ·
·           table        bar@primary
·           spans        ALL

# Only a can be an equality column.
query TTT
SELECT tree, field, description FROM [
EXPLAIN (VERBOSE) SELECT * FROM foo, bar WHERE foo.a = bar.a AND foo.b = bar.b
]
----
·          distributed  false
·          vectorized   false
hash-join  ·            ·
