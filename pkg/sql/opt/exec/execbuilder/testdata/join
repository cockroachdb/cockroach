# Tests with joins with merged columns of collated string type.
statement ok
CREATE TABLE str1 (a INT PRIMARY KEY, s STRING COLLATE en_u_ks_level1)

statement ok
CREATE TABLE str2 (a INT PRIMARY KEY, s STRING COLLATE en_u_ks_level1)

query TTTTT
EXPLAIN (VERBOSE) SELECT s, str1.s, str2.s FROM str1 INNER JOIN str2 USING(s)
----
render          ·         ·             (s, s, s)  ·
 │              render 0  s             ·          ·
 │              render 1  s             ·          ·
 │              render 2  s             ·          ·
 └── join       ·         ·             (s, s)     ·
      │         type      inner         ·          ·
      │         equality  (s) = (s)     ·          ·
      ├── scan  ·         ·             (s)        ·
      │         table     str1@primary  ·          ·
      │         spans     ALL           ·          ·
      └── scan  ·         ·             (s)        ·
·               table     str2@primary  ·          ·
·               spans     ALL           ·          ·

query TTTTT
EXPLAIN (VERBOSE) SELECT s, str1.s, str2.s FROM str1 LEFT OUTER JOIN str2 USING(s)
----
render          ·         ·             (s, s, s)  ·
 │              render 0  s             ·          ·
 │              render 1  s             ·          ·
 │              render 2  s             ·          ·
 └── join       ·         ·             (s, s)     ·
      │         type      left outer    ·          ·
      │         equality  (s) = (s)     ·          ·
      ├── scan  ·         ·             (s)        ·
      │         table     str1@primary  ·          ·
      │         spans     ALL           ·          ·
      └── scan  ·         ·             (s)        ·
·               table     str2@primary  ·          ·
·               spans     ALL           ·          ·

query TTTTT
EXPLAIN (VERBOSE) SELECT s, str1.s, str2.s FROM str1 RIGHT OUTER JOIN str2 USING(s)
----
render          ·         ·               (s, s, s)  ·
 │              render 0  COALESCE(s, s)  ·          ·
 │              render 1  s               ·          ·
 │              render 2  s               ·          ·
 └── join       ·         ·               (s, s)     ·
      │         type      right outer     ·          ·
      │         equality  (s) = (s)       ·          ·
      ├── scan  ·         ·               (s)        ·
      │         table     str1@primary    ·          ·
      │         spans     ALL             ·          ·
      └── scan  ·         ·               (s)        ·
·               table     str2@primary    ·          ·
·               spans     ALL             ·          ·

query TTTTT
EXPLAIN (VERBOSE) SELECT s, str1.s, str2.s FROM str1 FULL OUTER JOIN str2 USING(s)
----
render          ·         ·               (s, s, s)  ·
 │              render 0  COALESCE(s, s)  ·          ·
 │              render 1  s               ·          ·
 │              render 2  s               ·          ·
 └── join       ·         ·               (s, s)     ·
      │         type      full outer      ·          ·
      │         equality  (s) = (s)       ·          ·
      ├── scan  ·         ·               (s)        ·
      │         table     str1@primary    ·          ·
      │         spans     ALL             ·          ·
      └── scan  ·         ·               (s)        ·
·               table     str2@primary    ·          ·
·               spans     ALL             ·          ·

