query T
SELECT info FROM [EXPLAIN (DISTSQL)
SELECT * FROM array_tab@array_tab_pkey AS a1, array_tab AS a2
WHERE a1.b @> a2.b AND a1.b @> '{1}' AND a2.a < 5
ORDER BY a1.a, a2.a
] WHERE info NOT LIKE 'distribution:%'
