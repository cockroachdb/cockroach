# This query performs a left inverted join with an additional filter.
query T
EXPLAIN (DISTSQL)
SELECT * FROM json_tab AS j2 LEFT INVERTED JOIN json_tab AS j1
ON j1.b @> j2.b AND j1.b @> '{"a": {}}' AND j2.a < 20
ORDER BY j1.a, j2.a
----
distribution: full
vectorized: true
