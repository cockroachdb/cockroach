# This query performs an anti inverted join with an additional filter.
query T
EXPLAIN (DISTSQL)
SELECT * FROM json_tab AS j2 WHERE NOT EXISTS (
  SELECT * FROM json_tab@foo_inv AS j1
  WHERE j1.b @> j2.b AND j2.a < 20
)
ORDER BY j2.a
----
distribution: full
vectorized: true
·
