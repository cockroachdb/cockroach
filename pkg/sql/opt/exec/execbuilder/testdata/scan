exec-raw
CREATE DATABASE t
----

exec-raw
CREATE TABLE t.a (x INT PRIMARY KEY, y FLOAT, s STRING);
INSERT INTO t.a VALUES (1, 1.0, 'apple'), (2, 2.0, 'banana'), (3, 3.0, 'cherry')
----

opt
SELECT * FROM t.a
----
scan a
 ├── columns: x:1(int!null) y:2(float) s:3(string)
 ├── stats: [rows=1000]
 └── cost: 1000.00

exec-explain
SELECT * FROM t.a
----
scan  0  scan  ·      ·          (x, y, s)  ·
·     0  ·     table  a@primary  ·          ·
·     0  ·     spans  ALL        ·          ·

exec
SELECT * FROM t.a
----
x:int  y:float  s:string
1      1.0      apple
2      2.0      banana
3      3.0      cherry

# Test projecting subset of table columns.
opt
SELECT s, x FROM t.a
----
scan a
 ├── columns: s:3(string) x:1(int!null)
 ├── stats: [rows=1000]
 └── cost: 1000.00

exec-explain
SELECT s, x FROM t.a
----
render     0  render  ·         ·          (s, x)  ·
 │         0  ·       render 0  s          ·       ·
 │         0  ·       render 1  x          ·       ·
 └── scan  1  scan    ·         ·          (x, s)  ·
·          1  ·       table     a@primary  ·       ·
·          1  ·       spans     ALL        ·       ·

exec
SELECT s, x FROM t.a
----
s:string  x:int
apple     1
banana    2
cherry    3

# Test with a hidden column.
exec-raw
CREATE TABLE t.b (x INT, y INT, s STRING);
INSERT INTO t.b VALUES (1, 10, 'apple'), (2, 20, 'banana'), (3, 30, 'cherry')
----

opt
SELECT s, x FROM t.b
----
scan b
 ├── columns: s:3(string) x:1(int)
 ├── stats: [rows=1000]
 └── cost: 1000.00

exec
SELECT s, x FROM t.b
----
s:string  x:int
apple     1
banana    2
cherry    3

exec-explain
SELECT s, x FROM t.b
----
render     0  render  ·         ·          (s, x)  ·
 │         0  ·       render 0  s          ·       ·
 │         0  ·       render 1  x          ·       ·
 └── scan  1  scan    ·         ·          (x, s)  ·
·          1  ·       table     b@primary  ·       ·
·          1  ·       spans     ALL        ·       ·
