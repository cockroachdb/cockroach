exec-raw
CREATE TABLE a (x INT PRIMARY KEY, y FLOAT, s STRING);
INSERT INTO a VALUES (1, 1.0, 'apple'), (2, 2.0, 'banana'), (3, 3.0, 'cherry')
----

opt
SELECT * FROM a
----
scan a
 ├── columns: x:1(int!null) y:2(float) s:3(string)
 ├── stats: [rows=1000]
 ├── cost: 1000.00
 └── keys: (1)

exec hide-colnames nodist
EXPLAIN (VERBOSE) SELECT * FROM a
----
scan  ·      ·          (x, y, s)  ·
·     table  a@primary  ·          ·
·     spans  ALL        ·          ·

exec
SELECT * FROM a
----
x:int  y:float  s:string
1      1.0      apple
2      2.0      banana
3      3.0      cherry

# Test projecting subset of table columns.
opt
SELECT s, x FROM a
----
scan a
 ├── columns: s:3(string) x:1(int!null)
 ├── stats: [rows=1000]
 ├── cost: 1000.00
 └── keys: (1)

exec hide-colnames nodist
EXPLAIN (VERBOSE) SELECT s, x FROM a
----
render     ·         ·          (s, x)  ·
 │         render 0  s          ·       ·
 │         render 1  x          ·       ·
 └── scan  ·         ·          (x, s)  ·
·          table     a@primary  ·       ·
·          spans     ALL        ·       ·

exec
SELECT s, x FROM a
----
s:string  x:int
apple     1
banana    2
cherry    3

# Test with a hidden column.
exec-raw
CREATE TABLE b (x INT, y INT, s STRING);
INSERT INTO b VALUES (1, 10, 'apple'), (2, 20, 'banana'), (3, 30, 'cherry')
----

opt
SELECT s, x FROM b
----
scan b
 ├── columns: s:3(string) x:1(int)
 ├── stats: [rows=1000]
 └── cost: 1000.00

exec
SELECT s, x FROM b
----
s:string  x:int
apple     1
banana    2
cherry    3

exec hide-colnames nodist
EXPLAIN (VERBOSE) SELECT s, x FROM b
----
render     ·         ·          (s, x)  ·
 │         render 0  s          ·       ·
 │         render 1  x          ·       ·
 └── scan  ·         ·          (x, s)  ·
·          table     b@primary  ·       ·
·          spans     ALL        ·       ·
