# LogicTest: local

query T
EXPLAIN (VERBOSE)
SELECT
    aggregated_ts,
    fingerprint_id,
    transaction_fingerprint_id,
    plan_hash,
    app_name,
    node_id,
    agg_interval,
    metadata,
    statistics,
    plan,
    index_recommendations,
    indexes_usage
FROM
    system.statement_statistics
----
  distribution: local
  vectorized: true
