exec-raw
CREATE DATABASE t
----

exec-raw
CREATE TABLE a (x INT PRIMARY KEY, y INT)
----

exec-raw
INSERT INTO a VALUES (1, 10), (2, 20), (3, 30)
----

exec hide-colnames
EXPLAIN (VERBOSE) SELECT * FROM a WHERE x > 1
----
scan  ·      ·          (x, y)  ·
·     table  a@primary  ·       ·
·     spans  /2-        ·       ·

exec
SELECT * FROM a WHERE x > 1
----
x:int  y:int
2      20
3      30

exec hide-colnames
EXPLAIN (VERBOSE) SELECT * FROM a WHERE y > 1
----
filter     ·       ·          (x, y)  ·
 │         filter  y > 1      ·       ·
 └── scan  ·       ·          (x, y)  ·
·          table   a@primary  ·       ·
·          spans   ALL        ·       ·

exec
SELECT * FROM a WHERE y > 1
----
x:int  y:int
1      10
2      20
3      30

exec hide-colnames
EXPLAIN (VERBOSE) SELECT * FROM a WHERE x > 1 AND x < 3
----
scan  ·      ·          (x, y)  ·
·     table  a@primary  ·       ·
·     spans  /2-/2/#    ·       ·

exec
SELECT * FROM a WHERE x > 1 AND x < 3
----
x:int  y:int
2      20

exec hide-colnames
EXPLAIN (VERBOSE) SELECT x + 1 FROM a
----
render     ·         ·          (column3)  ·
 │         render 0  x + 1      ·          ·
 └── scan  ·         ·          (x)        ·
·          table     a@primary  ·          ·
·          spans     ALL        ·          ·

exec
SELECT x + 1 FROM a
----
column3:int
2
3
4

exec hide-colnames
EXPLAIN (VERBOSE) SELECT x, x + 1, y, y + 1, x + y FROM a
----
render     ·         ·          (x, column3, y, column4, column5)  ·
 │         render 0  x          ·                                  ·
 │         render 1  x + 1      ·                                  ·
 │         render 2  y          ·                                  ·
 │         render 3  y + 1      ·                                  ·
 │         render 4  x + y      ·                                  ·
 └── scan  ·         ·          (x, y)                             ·
·          table     a@primary  ·                                  ·
·          spans     ALL        ·                                  ·

exec
SELECT x, x + 1, y, y + 1, x + y FROM a
----
x:int  column3:int  y:int  column4:int  column5:int
1      2            10     11           11
2      3            20     21           22
3      4            30     31           33

exec hide-colnames
EXPLAIN (VERBOSE) SELECT u * v + v FROM (SELECT x + 3, y + 10 FROM a) AS foo(u, v)
----
render          ·         ·                              (column5)           ·
 │              render 0  column4 + (column3 * column4)  ·                   ·
 └── render     ·         ·                              (column3, column4)  ·
      │         render 0  x + 3                          ·                   ·
      │         render 1  y + 10                         ·                   ·
      └── scan  ·         ·                              (x, y)              ·
·               table     a@primary                      ·                   ·
·               spans     ALL                            ·                   ·

exec
SELECT u + v FROM (SELECT x + 3, y + 10 FROM a) AS foo(u, v)
----
column5:int
24
35
46

exec hide-colnames
EXPLAIN (VERBOSE) SELECT x, x, y, x FROM a
----
render     ·         ·          (x, x, y, x)  ·
 │         render 0  x          ·             ·
 │         render 1  x          ·             ·
 │         render 2  y          ·             ·
 │         render 3  x          ·             ·
 └── scan  ·         ·          (x, y)        ·
·          table     a@primary  ·             ·
·          spans     ALL        ·             ·

exec
SELECT x, x, y, x FROM a
----
x:int  x:int  y:int  x:int
1      1      10     1
2      2      20     2
3      3      30     3

exec hide-colnames
EXPLAIN (VERBOSE) SELECT x + 1, x + y FROM a WHERE x + y > 20
----
render          ·         ·             (column3, column4)  ·
 │              render 0  x + 1         ·                   ·
 │              render 1  x + y         ·                   ·
 └── filter     ·         ·             (x, y)              ·
      │         filter    (x + y) > 20  ·                   ·
      └── scan  ·         ·             (x, y)              ·
·               table     a@primary     ·                   ·
·               spans     ALL           ·                   ·

exec
SELECT x + 1, x + y FROM a WHERE x + y > 20
----
column3:int  column4:int
3            22
4            33

exec-raw
CREATE TABLE b (x INT, y INT);
INSERT INTO b VALUES (1, 10), (2, 20), (3, 30)
----

# Test with a hidden column.
exec hide-colnames
EXPLAIN (VERBOSE) SELECT * FROM b
----
scan  ·      ·          (x, y)  ·
·     table  b@primary  ·       ·
·     spans  ALL        ·       ·

exec
SELECT * FROM b
----
x:int  y:int
1      10
2      20
3      30

exec hide-colnames
EXPLAIN (VERBOSE) SELECT x FROM b WHERE rowid > 0
----
render     ·         ·          (x)                 ·
 │         render 0  x          ·                   ·
 └── scan  ·         ·          (x, rowid[hidden])  ·
·          table     b@primary  ·                   ·
·          spans     /1-        ·                   ·

exec
SELECT x FROM b WHERE rowid > 0
----
x:int
1
2
3

# "*" must expand to zero columns if there are zero columns to select.
exec-raw
CREATE TABLE t.nocols(x INT); ALTER TABLE t.nocols DROP COLUMN x
----

catalog
t.nocols
----
TABLE nocols
 ├── rowid int not null (hidden)
 └── INDEX primary
      └── rowid int not null (hidden)

exec hide-colnames
EXPLAIN (VERBOSE) SELECT 1, * FROM t.nocols
----
render     ·         ·               (column2)  ·
 │         render 0  1               ·          ·
 └── scan  ·         ·               ()         ·
·          table     nocols@primary  ·          ·
·          spans     ALL             ·          ·

exec
SELECT 1, * FROM t.nocols
----
column2:int
