# LogicTest: local-opt

statement ok
CREATE TABLE d (
  a INT PRIMARY KEY,
  b JSONB
)

statement ok
CREATE INVERTED INDEX foo_inv ON d(b)

query TTTTT
EXPLAIN (VERBOSE) SELECT * from d where b @>'{"a": "b"}'
----
index-join  ·      ·                            (a, b)  ·
 │          table  d@primary                    ·       ·
 └── scan   ·      ·                            (a)     ·
·           table  d@foo_inv                    ·       ·
·           spans  /"a"/"b"-/"a"/"b"/PrefixEnd  ·       ·

query TTTTT
EXPLAIN (VERBOSE) SELECT * from d where b @>'{"a": {"b": [1]}}'
----
index-join  ·      ·                                        (a, b)  ·
 │          table  d@primary                                ·       ·
 └── scan   ·      ·                                        (a)     ·
·           table  d@foo_inv                                ·       ·
·           spans  /"a"/"b"/Arr/1-/"a"/"b"/Arr/1/PrefixEnd  ·       ·

query TTTTT
EXPLAIN (VERBOSE) SELECT * from d where b @> '{"a": {"b": [[2]]}}';
----
index-join  ·      ·                                                (a, b)  ·
 │          table  d@primary                                        ·       ·
 └── scan   ·      ·                                                (a)     ·
·           table  d@foo_inv                                        ·       ·
·           spans  /"a"/"b"/Arr/Arr/2-/"a"/"b"/Arr/Arr/2/PrefixEnd  ·       ·

query TTTTT
