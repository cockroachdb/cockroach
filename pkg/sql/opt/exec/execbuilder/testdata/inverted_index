# Make sure that querying for NULL equality doesn't use the inverted index.
query TTTTT
EXPLAIN (VERBOSE) SELECT * from d where b IS NULL
----
·          distribution  local      ·       ·
·          vectorized    true       ·       ·
filter     ·             ·          (a, b)  ·
 │         filter        b IS NULL  ·       ·
 └── scan  ·             ·          (a, b)  ·
·          table         d@primary  ·       ·
·          spans         FULL SCAN  ·       ·

query TTT
EXPLAIN SELECT * from d where b @> '{"a": []}' ORDER BY a;
----
·          distribution  local
·          vectorized    true
filter     ·             ·
 │         filter        b @> '{"a": []}'
 └── scan  ·             ·
·          table         d@primary
·          spans         FULL SCAN

query TTT
