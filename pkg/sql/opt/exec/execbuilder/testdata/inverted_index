·     filter       b @> '{"a": {}}'

# Multi-path contains queries. Should create zigzag joins.

query TTTTT
EXPLAIN (VERBOSE) SELECT * from d where b @> '{"a": {"b": "c"}, "f": "g"}'
----
·                 distributed            false                                ·       ·
·                 vectorized             false                                ·       ·
lookup-join       ·                      ·                                    (a, b)  ·
 │                table                  d@primary                            ·       ·
 │                type                   inner                                ·       ·
 │                equality               (a) = (a)                            ·       ·
 │                equality cols are key  ·                                    ·       ·
 │                parallel               ·                                    ·       ·
 │                pred                   @2 @> '{"a": {"b": "c"}, "f": "g"}'  ·       ·
 └── zigzag-join  ·                      ·                                    (a)     ·
      │           type                   inner                                ·       ·
      ├── scan    ·                      ·                                    (a)     ·
      │           table                  d@foo_inv                            ·       ·
      │           fixedvals              1 column                             ·       ·
      └── scan    ·                      ·                                    ()      ·
·                 table                  d@foo_inv                            ·       ·
·                 fixedvals              1 column                             ·       ·

query TTTTT
EXPLAIN (VERBOSE) SELECT * from d where b @> '{"a": {"b": "c", "d": "e"}, "f": "g"}'
----
·                 distributed            false                                          ·       ·
·                 vectorized             false                                          ·       ·
lookup-join       ·                      ·                                              (a, b)  ·
 │                table                  d@primary                                      ·       ·
 │                type                   inner                                          ·       ·
 │                equality               (a) = (a)                                      ·       ·
 │                equality cols are key  ·                                              ·       ·
 │                parallel               ·                                              ·       ·
 │                pred                   @2 @> '{"a": {"b": "c", "d": "e"}, "f": "g"}'  ·       ·
 └── zigzag-join  ·                      ·                                              (a)     ·
      │           type                   inner                                          ·       ·
      ├── scan    ·                      ·                                              (a)     ·
      │           table                  d@foo_inv                                      ·       ·
      │           fixedvals              1 column                                       ·       ·
      └── scan    ·                      ·                                              ()      ·
·                 table                  d@foo_inv                                      ·       ·
·                 fixedvals              1 column                                       ·       ·

