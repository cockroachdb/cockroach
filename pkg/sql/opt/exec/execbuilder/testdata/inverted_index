----
·          distribution  local
·          vectorized    true
filter     ·             ·
 │         filter        b @> '{"a": {}}'
 └── scan  ·             ·
·          table         d@primary
·          spans         FULL SCAN

# Multi-path contains queries. Should create zigzag joins.

query TTTTT
EXPLAIN (VERBOSE) SELECT * from d where b @> '{"a": {"b": "c"}, "f": "g"}'
----
·                 distribution           local                               ·       ·
·                 vectorized             true                                ·       ·
lookup-join       ·                      ·                                   (a, b)  ·
 │                table                  d@primary                           ·       ·
 │                type                   inner                               ·       ·
 │                equality               (a) = (a)                           ·       ·
 │                equality cols are key  ·                                   ·       ·
 │                parallel               ·                                   ·       ·
 │                pred                   b @> '{"a": {"b": "c"}, "f": "g"}'  ·       ·
 └── zigzag-join  ·                      ·                                   (a)     ·
      │           type                   inner                               ·       ·
      ├── scan    ·                      ·                                   (a)     ·
      │           table                  d@foo_inv                           ·       ·
      │           fixedvals              1 column                            ·       ·
      └── scan    ·                      ·                                   ()      ·
·                 table                  d@foo_inv                           ·       ·
·                 fixedvals              1 column                            ·       ·

query TTTTT
EXPLAIN (VERBOSE) SELECT * from d where b @> '{"a": {"b": "c", "d": "e"}, "f": "g"}'
----
·                 distribution           local                                         ·       ·
·                 vectorized             true                                          ·       ·
lookup-join       ·                      ·                                             (a, b)  ·
 │                table                  d@primary                                     ·       ·
 │                type                   inner                                         ·       ·
 │                equality               (a) = (a)                                     ·       ·
 │                equality cols are key  ·                                             ·       ·
 │                parallel               ·                                             ·       ·
 │                pred                   b @> '{"a": {"b": "c", "d": "e"}, "f": "g"}'  ·       ·
 └── zigzag-join  ·                      ·                                             (a)     ·
      │           type                   inner                                         ·       ·
      ├── scan    ·                      ·                                             (a)     ·
      │           table                  d@foo_inv                                     ·       ·
      │           fixedvals              1 column                                      ·       ·
      └── scan    ·                      ·                                             ()      ·
·                 table                  d@foo_inv                                     ·       ·
·                 fixedvals              1 column                                      ·       ·

