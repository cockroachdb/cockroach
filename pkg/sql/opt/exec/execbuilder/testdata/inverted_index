CREATE TABLE geo_table2(
  k int primary key,
  geom geometry
)

query T
EXPLAIN (OPT, VERBOSE) SELECT * FROM geo_table2 JOIN geo_table@geom_index
ON ST_Intersects(geo_table2.geom, geo_table.geom)
