# Verify that the built function can be executed.
statement ok
CREATE TABLE str (s STRING);
INSERT INTO str VALUES ('a'), ('ab'), ('abc')

query I rowsort
SELECT LENGTH(s) FROM str
----
1
2
3

query TTTTT
EXPLAIN (VERBOSE) SELECT j @> '{"a": 1}' FROM t
----
render     ·         ·                ("j @> '{""a"": 1}'")  ·
 │         render 0  j @> '{"a": 1}'  ·                      ·
 └── scan  ·         ·                (j)                    ·
·          table     t@primary        ·                      ·
·          spans     ALL              ·                      ·

