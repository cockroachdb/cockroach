·       size           2 columns, 1 row  ·                           ·
·       row 0, expr 0  random()          ·                           ·
·       row 0, expr 1  'test'            ·                           ·

# Don't fold non-constants.
query TTTTT
EXPLAIN (VERBOSE) SELECT 1::FLOAT + length(upper(concat('a', 'b', 'c')))::FLOAT AS r1,
