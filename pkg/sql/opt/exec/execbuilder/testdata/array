# LogicTest: local

# Add x to PRIMARY KEY once #50659 is fixed.
statement ok
CREATE TABLE t (x INT[])

# Test some scans of constrained spans on arrays.

query T
EXPLAIN SELECT x FROM t WHERE x = ARRAY[1,4,6]
----
distribution: local
vectorized: true
·
• filter
│ filter: x = ARRAY[1,4,6]
│
└── • scan
      missing stats
      table: t@primary
      spans: FULL SCAN

query T
EXPLAIN SELECT x FROM t WHERE x < ARRAY[1, 4, 3]
----
distribution: local
vectorized: true
·
• filter
│ filter: x < ARRAY[1,4,3]
│
└── • scan
      missing stats
      table: t@primary
      spans: FULL SCAN

query T
EXPLAIN SELECT x FROM t WHERE x > ARRAY [1, NULL]
----
distribution: local
vectorized: true
·
• filter
│ filter: x > ARRAY[1,NULL]
│
└── • scan
      missing stats
      table: t@primary
      spans: FULL SCAN

query T
EXPLAIN SELECT x FROM t WHERE x > ARRAY[1, 3] AND x < ARRAY[1, 4, 10] ORDER BY x
----
distribution: local
vectorized: true
·
• sort
│ order: +x
│
└── • filter
    │ filter: (x > ARRAY[1,3]) AND (x < ARRAY[1,4,10])
    │
    └── • scan
          missing stats
          table: t@primary
          spans: FULL SCAN

query T
EXPLAIN SELECT x FROM t WHERE x > ARRAY[1, 3] AND x < ARRAY[1, 4, 10] ORDER BY x DESC
----
distribution: local
vectorized: true
·
• sort
│ order: -x
│
└── • filter
    │ filter: (x > ARRAY[1,3]) AND (x < ARRAY[1,4,10])
    │
    └── • scan
          missing stats
          table: t@primary
          spans: FULL SCAN

statement ok
DROP TABLE t

# Add multicolumn INDEX i (x, y, z) once #50659 is fixed.
statement ok
CREATE TABLE t (x INT, y INT[], z INT)

query T
EXPLAIN SELECT x, y, z FROM t WHERE x = 2 AND y < ARRAY[10] ORDER BY y
----
distribution: local
vectorized: true
·
• sort
│ order: +y
│
└── • filter
    │ filter: (x = 2) AND (y < ARRAY[10])
    │
    └── • scan
          missing stats
          table: t@primary
          spans: FULL SCAN
