statement ok
CREATE TABLE b(
  a int primary key,
  geog geography(geometry, 4326),
  geom geometry(point, 4326),
  FAMILY (a, geog, geom)
)

statement ok
CREATE TABLE c(
  a int primary key,
  geog geography(geometry, 4326),
  geom geometry(point, 4326),
  FAMILY (a, geog, geom),
  INVERTED INDEX (geog),
  INVERTED INDEX (geom)
)

statement ok
CREATE INVERTED INDEX geog_idx ON c(geog)

statement ok
CREATE INVERTED INDEX geom_idx ON c(geom)

