statement ok
CREATE TABLE ltable(
  k int primary key,
  geom geometry
)

statement ok
CREATE TABLE rtable(
  k int primary key,
  geom geometry,
  INVERTED INDEX geom_index(geom)
)

statement ok
SELECT url FROM [EXPLAIN (DISTSQL)
SELECT ltable.k, rtable.k FROM ltable JOIN rtable@geom_index ON ST_Intersects(ltable.geom, rtable.geom)]
