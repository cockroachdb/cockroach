# This query performs a semi-join, which is converted to paired joins by the
# optimizer.
query T
EXPLAIN (DISTSQL)
SELECT lk FROM ltable WHERE EXISTS (SELECT * FROM rtable WHERE ST_Intersects(ltable.geom2, rtable.geom))
----
distribution: full
vectorized: true
·
