# If there are Limits, interleaved joins should not be used.
query T
SELECT url FROM [
  EXPLAIN (DISTSQL) SELECT * FROM (SELECT * FROM parent1 ORDER BY pid1 LIMIT 2) AS parent1 INNER MERGE JOIN child1 USING(pid1)
]
----
