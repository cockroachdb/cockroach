query T
EXPLAIN (OPT) SELECT * FROM tc WHERE a + 2 * b > 1 ORDER BY a*b
----
sort
 └── project
      ├── select
      │    ├── scan tc
      │    └── filters
      │         └── (a + (b * 2)) > 1
      └── projections
           └── a * b

query T
EXPLAIN (OPT, VERBOSE) SELECT * FROM tc WHERE a + 2 * b > 1 ORDER BY a*b
----
sort
 ├── columns: a:1 b:2  [hidden: column4:4]
 ├── stats: [rows=333.333333]
 ├── cost: 1179.25548
 ├── fd: (1,2)-->(4)
 ├── ordering: +4
 ├── prune: (1,2,4)
 └── project
      ├── columns: column4:4 a:1 b:2
      ├── stats: [rows=333.333333]
      ├── cost: 1116.70667
      ├── fd: (1,2)-->(4)
      ├── prune: (1,2,4)
      ├── select
      │    ├── columns: a:1 b:2
      │    ├── stats: [rows=333.333333]
      │    ├── cost: 1110.03
      │    ├── scan tc
      │    │    ├── columns: a:1 b:2
      │    │    ├── stats: [rows=1000]
      │    │    ├── cost: 1100.02
      │    │    └── prune: (1,2)
      │    └── filters
      │         └── (a + (b * 2)) > 1 [outer=(1,2)]
      └── projections
           └── a * b [outer=(1,2)]

