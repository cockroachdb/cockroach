CREATE TABLE tt (x INT, y INT, INDEX a(x), INDEX b(y))

query TTTTT
EXPLAIN (TYPES) SELECT * FROM tt WHERE x < 10 AND y > 10
----
scan  ·       ·                                                                          (x int, y int)  ·
·     table   tt@primary                                                                 ·               ·
·     spans   ALL                                                                        ·               ·
·     filter  ((((x)[int] < (10)[int])[bool]) AND (((y)[int] > (10)[int])[bool]))[bool]  ·               ·

