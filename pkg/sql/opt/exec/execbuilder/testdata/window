·                                   distribution  local                                                                                                               ·                                                              ·
·                                   vectorized    true                                                                                                                ·                                                              ·
render                              ·             ·                                                                                                                   (max int, "?column?" decimal)                                  ·
 │                                  render 0      (max)[int]                                                                                                          ·                                                              ·
