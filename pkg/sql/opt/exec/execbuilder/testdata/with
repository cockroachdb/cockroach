│     estimated row count: 1000 (missing stats)
│     table: table39010@primary
│     spans: FULL SCAN
│
└── • scan
      columns: (col)
      estimated row count: 1000 (missing stats)
      table: table39010@primary
      spans: FULL SCAN

query T
EXPLAIN (VERBOSE)
  WITH RECURSIVE t(n) AS (
      VALUES (1)
    UNION ALL
      SELECT n+1 FROM t WHERE n < 100
  )
  SELECT sum(n) FROM t
----
distribution: local
vectorized: false
