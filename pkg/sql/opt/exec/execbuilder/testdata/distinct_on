│ estimated row count: 100 (missing stats)
│ order: -x
│
└── • distinct
    │ columns: (x)
    │ estimated row count: 100 (missing stats)
    │ distinct on: x
    │
    └── • scan
          columns: (x)
          estimated row count: 1,000 (missing stats)
          table: xyz@primary
          spans: FULL SCAN

query T
