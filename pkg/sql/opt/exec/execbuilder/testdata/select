# ------------------------------------------------------------------------------
# String inequality filter.
# ------------------------------------------------------------------------------
statement ok
CREATE TABLE c (n INT PRIMARY KEY, str STRING, INDEX str(str DESC));

query T
EXPLAIN (VERBOSE) SELECT * FROM c WHERE str >= 'moo'
----
distribution: local
vectorized: true
·
• scan
  columns: (n, str)
  estimated row count: 333 (missing stats)
  table: c@str
  spans: -/"moo"/PrefixEnd

statement ok
DROP TABLE c

