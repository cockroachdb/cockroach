# ------------------------------------------------------------------------------
# Ensure that index is used when indexed column has collation.
# ------------------------------------------------------------------------------
statement ok
CREATE TABLE coll (
  a STRING COLLATE da,
  b INT,
  c BOOL,
  PRIMARY KEY (a, b),
  INDEX (b, a) STORING (c)
)

query T
EXPLAIN (TYPES) SELECT a, b FROM coll ORDER BY a, b
----
distribution: local
vectorized: true
·
• scan
  columns: (a collatedstring{da}, b int)
  ordering: +a,+b
  estimated row count: 1,000 (missing stats)
  table: coll@primary
  spans: FULL SCAN

query T
EXPLAIN (TYPES) SELECT b, a FROM coll ORDER BY b, a
----
distribution: local
vectorized: true
·
• scan
  columns: (b int, a collatedstring{da})
  ordering: +b,+a
  estimated row count: 1,000 (missing stats)
  table: coll@coll_b_a_idx
  spans: FULL SCAN

statement ok
DROP TABLE coll

