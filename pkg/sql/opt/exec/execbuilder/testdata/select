exec-raw
CREATE DATABASE t
----

exec-raw
CREATE TABLE t.a (x INT PRIMARY KEY, y INT);
INSERT INTO t.a VALUES (1, 10), (2, 20), (3, 30)
----

exec-explain
SELECT * FROM t.a WHERE x > 1
----
scan  0  scan  ·      ·          (x, y)  ·
·     0  ·     table  a@primary  ·       ·
·     0  ·     spans  /2-        ·       ·

exec
SELECT * FROM t.a WHERE x > 1
----
x:int  y:int
2      20
3      30

exec-explain
SELECT * FROM t.a WHERE y > 10
----
filter     0  filter  ·       ·          (x, y)  ·
 │         0  ·       filter  y > 10     ·       ·
 └── scan  1  scan    ·       ·          (x, y)  ·
·          1  ·       table   a@primary  ·       ·
·          1  ·       spans   ALL        ·       ·

exec
SELECT * FROM t.a WHERE y > 10
----
x:int  y:int
2      20
3      30

exec-explain
SELECT * FROM t.a WHERE x > 1 AND x < 3
----
scan  0  scan  ·      ·          (x, y)  ·
·     0  ·     table  a@primary  ·       ·
·     0  ·     spans  /2-/2/#    ·       ·

exec
SELECT * FROM t.a WHERE x > 1 AND x < 3
----
x:int  y:int
2      20

exec-explain
SELECT * FROM t.a WHERE x > 1 AND y < 30
----
filter     0  filter  ·       ·          (x, y)  ·
 │         0  ·       filter  y < 30     ·       ·
 └── scan  1  scan    ·       ·          (x, y)  ·
·          1  ·       table   a@primary  ·       ·
·          1  ·       spans   /2-        ·       ·

exec
SELECT * FROM t.a WHERE x > 1 AND y < 30
----
x:int  y:int
2      20

exec-raw
CREATE TABLE t.b (x INT, y INT);
INSERT INTO t.b VALUES (1, 10), (2, 20), (3, 30)
----

exec-explain
SELECT x, y, rowid FROM t.b WHERE rowid > 0
----
scan  0  scan  ·      ·          (x, y, rowid[hidden])  ·
·     0  ·     table  b@primary  ·                      ·
·     0  ·     spans  /1-        ·                      ·
