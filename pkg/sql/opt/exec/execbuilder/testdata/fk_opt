# LogicTest: local

statement ok
SET experimental_optimizer_foreign_keys = true

statement ok
CREATE TABLE parent (p INT PRIMARY KEY, other INT)

statement ok
CREATE TABLE child (c INT PRIMARY KEY, p INT NOT NULL REFERENCES parent(p))

query TTTTT
EXPLAIN (VERBOSE) INSERT INTO child VALUES (1,1), (2,2)
----
·                                          distributed            false              ·                   ·
·                                          vectorized             false              ·                   ·
root                                       ·                      ·                  ()                  ·
 ├── count                                 ·                      ·                  ()                  ·
 │    └── insert                           ·                      ·                  ()                  ·
 │         │                               into                   child(c, p)        ·                   ·
 │         │                               strategy               inserter           ·                   ·
 │         └── buffer node                 ·                      ·                  (column1, column2)  ·
 │              │                          label                  buffer 1           ·                   ·
 │              └── values                 ·                      ·                  (column1, column2)  ·
 │                                         size                   2 columns, 2 rows  ·                   ·
 │                                         row 0, expr 0          1                  ·                   ·
 │                                         row 0, expr 1          1                  ·                   ·
 │                                         row 1, expr 0          2                  ·                   ·
 │                                         row 1, expr 1          2                  ·                   ·
 └── postquery                             ·                      ·                  ()                  ·
      └── error if rows                    ·                      ·                  ()                  ·
           └── lookup-join                 ·                      ·                  (column2)           ·
                │                          table                  parent@primary     ·                   ·
                │                          type                   anti               ·                   ·
                │                          equality               (column2) = (p)    ·                   ·
                │                          equality cols are key  ·                  ·                   ·
                └── render                 ·                      ·                  (column2)           ·
                     │                     render 0               column2            ·                   ·
                     └── scan buffer node  ·                      ·                  (column1, column2)  ·
·                                          label                  buffer 1           ·                   ·

# Use data from a different table as input.
statement ok
CREATE TABLE xy (x INT, y INT)

query TTTTT
EXPLAIN (VERBOSE) INSERT INTO child SELECT x,y FROM xy
----
·                                          distributed         false           ·       ·
·                                          vectorized          false           ·       ·
root                                       ·                   ·               ()      ·
 ├── count                                 ·                   ·               ()      ·
 │    └── insert                           ·                   ·               ()      ·
 │         │                               into                child(c, p)     ·       ·
 │         │                               strategy            inserter        ·       ·
 │         └── buffer node                 ·                   ·               (x, y)  ·
 │              │                          label               buffer 1        ·       ·
 │              └── scan                   ·                   ·               (x, y)  ·
 │                                         table               xy@primary      ·       ·
 │                                         spans               ALL             ·       ·
 └── postquery                             ·                   ·               ()      ·
      └── error if rows                    ·                   ·               ()      ·
           └── hash-join                   ·                   ·               (y)     ·
                │                          type                anti            ·       ·
                │                          equality            (y) = (p)       ·       ·
                │                          right cols are key  ·               ·       ·
                ├── render                 ·                   ·               (y)     ·
                │    │                     render 0            y               ·       ·
                │    └── scan buffer node  ·                   ·               (x, y)  ·
                │                          label               buffer 1        ·       ·
                └── scan                   ·                   ·               (p)     ·
·                                          table               parent@primary  ·       ·
·                                          spans               ALL             ·       ·
