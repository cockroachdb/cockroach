#            │              type            inner                     ·                                                                                                                                                 ·
#            │              equality        (book) = (title)          ·                                                                                                                                                 ·
#            ├── scan       ·               ·                         (name, book, rowid[hidden,omitted])                                                                                                               rowid!=NULL; key(rowid)
#            │              table           authors@primary           ·                                                                                                                                                 ·
#            │              spans           ALL                       ·                                                                                                                                                 ·
