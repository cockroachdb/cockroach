  left table: abc@secondary
  left columns: (a, b)
  left fixed values: 1 column
  right table: abc@secondary
  right columns: (c)
  right fixed values: 1 column
• zigzag join
  left table: abc@secondary
  left columns: ()
  right table: abc@secondary
  right columns: ()

