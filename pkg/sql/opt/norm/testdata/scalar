 ├── columns: column1:1(int) column2:2(timestamptz)
 ├── values
 │    └── tuple [type=tuple{}]
 └── projections
      ├── null [type=int]
      └── null [type=timestamptz]

# --------------------------------------------------
# FoldNullUnary
# --------------------------------------------------
