exec-ddl
CREATE TABLE b (k INT PRIMARY KEY, i INT, f FLOAT, s STRING NOT NULL, j JSON)
----

exec-ddl
CREATE TABLE a (v INT PRIMARY KEY, w INT, x FLOAT, y STRING NOT NULL, z JSON)
----

# --------------------------------------------------
# EliminateUnionAllLeft
# --------------------------------------------------

norm expect=EliminateUnionAllLeft
SELECT k FROM
  (SELECT k FROM b)
  UNION ALL
  (SELECT k FROM b WHERE k IN ())
----
project
 ├── columns: k:11!null
 ├── key: (11)
 ├── scan b
 │    ├── columns: b.k:1!null
 │    └── key: (1)
 └── projections
      └── variable: b.k:1 [outer=(1)] [projected as k:11]

# --------------------------------------------------
# EliminateUnionAllRight
# --------------------------------------------------

norm expect=EliminateUnionAllRight
SELECT k FROM
  (SELECT k FROM b WHERE Null)
  UNION ALL
  (SELECT k FROM b)
----
project
 ├── columns: k:11!null
 ├── key: (11)
 ├── scan b
 │    ├── columns: b.k:6!null
 │    └── key: (6)
 └── projections
      └── variable: b.k:6 [outer=(6)] [projected as k:11]

norm
SELECT k FROM
  (SELECT k FROM b WHERE False)
  UNION ALL
  (SELECT k FROM b WHERE i IN ())
----
values
 ├── columns: k:11!null
 ├── cardinality: [0 - 0]
 ├── key: ()
 └── fd: ()-->(11)

# --------------------------------------------------
# PushFilterIntoSetOp
# --------------------------------------------------

norm expect=PushFilterIntoSetOp
SELECT k FROM
  ((SELECT k FROM b)
  UNION ALL
  (SELECT k FROM b))
WHERE k < 10
----
union-all
 ├── columns: k:11!null
 ├── left columns: b.k:1
 ├── right columns: b.k:6
 ├── select
 │    ├── columns: b.k:1!null
 │    ├── key: (1)
 │    ├── scan b
 │    │    ├── columns: b.k:1!null
 │    │    └── key: (1)
 │    └── filters
 │         └── b.k < 10 [outer=(1), constraints=(/1: (/NULL - /9]; tight)]
 └── select
      ├── columns: b.k:6!null
      ├── key: (6)
      ├── scan b
      │    ├── columns: b.k:6!null
      │    └── key: (6)
      └── filters
           └── b.k < 10 [outer=(6), constraints=(/6: (/NULL - /9]; tight)]

