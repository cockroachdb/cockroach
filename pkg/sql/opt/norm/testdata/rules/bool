# --------------------------------------------------
# ExtractRedundantClause + ExtractRedundantSubclause
# --------------------------------------------------
opt
SELECT b OR b FROM c
----
project
 ├── columns: "?column?":7(bool)
 ├── scan c
 │    └── columns: b:2(bool)
 └── projections
      └── variable: b [type=bool, outer=(2)]

