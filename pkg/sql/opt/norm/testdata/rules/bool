 ├── scan c
 │    └── columns: a:1(bool) b:2(bool) c:3(bool) d:4(bool) e:5(bool)
 └── filters
      ├── variable: a [type=bool, outer=(1), constraints=(/1: [/true - /true]; tight), fd=()-->(1)]
      └── variable: b [type=bool, outer=(2), constraints=(/2: [/true - /true]; tight), fd=()-->(2)]

opt
