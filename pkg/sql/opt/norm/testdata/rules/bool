 ├── scan a
 │    ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 │    ├── key: (1)
 │    └── fd: (1)-->(2-5)
 └── filters
      ├── s !~ 'foo' [type=bool, outer=(4), constraints=(/4: (/NULL - ])]
      ├── s ~ 'foo' [type=bool, outer=(4), constraints=(/4: (/NULL - ])]
      ├── s !~* 'foo' [type=bool, outer=(4), constraints=(/4: (/NULL - ])]
      └── s ~* 'foo' [type=bool, outer=(4), constraints=(/4: (/NULL - ])]

