exec-ddl
CREATE TABLE a (k INT PRIMARY KEY, i INT, f FLOAT, s STRING, j JSON)
----
TABLE a
 ├── k int not null
 ├── i int
 ├── f float
 ├── s string
 ├── j jsonb
 └── INDEX primary
      └── k int not null

exec-ddl
CREATE TABLE t.b (x INT PRIMARY KEY, z INT)
----
TABLE b
 ├── x int not null
 ├── z int
 └── INDEX primary
      └── x int not null

# --------------------------------------------------
# EliminateEmptyAnd
# --------------------------------------------------
opt
SELECT * FROM a WHERE True AND True
----
scan a
 ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 └── keys: (1)

# --------------------------------------------------
# EliminateEmptyOr
# --------------------------------------------------
opt
SELECT False OR False
----
project
 ├── columns: column1:1(bool!null)
 ├── cardinality: [1 - 1]
 ├── values
 │    ├── cardinality: [1 - 1]
 │    └── tuple [type=tuple{}]
 └── projections
      └── false [type=bool]

# --------------------------------------------------
# EliminateSingletonAndOr
# --------------------------------------------------
opt
SELECT (i=5 OR False) AND (s<'foo' AND True) FROM a
----
project
 ├── columns: column6:6(bool)
 ├── scan a
 │    └── columns: i:2(int) s:4(string)
 └── projections [outer=(2,4)]
      └── and [type=bool, outer=(2,4), constraints=(/2: [/5 - /5]; /4: (/NULL - /'foo'); tight)]
           ├── eq [type=bool, outer=(2), constraints=(/2: [/5 - /5]; tight)]
           │    ├── variable: a.i [type=int, outer=(2)]
           │    └── const: 5 [type=int]
           └── lt [type=bool, outer=(4), constraints=(/4: (/NULL - /'foo'); tight)]
                ├── variable: a.s [type=string, outer=(4)]
                └── const: 'foo' [type=string]

# --------------------------------------------------
