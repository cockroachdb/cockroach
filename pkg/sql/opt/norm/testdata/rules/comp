 ├── columns: "?column?":6
 ├── immutable
 ├── scan t
 │    └── columns: ts:1 tz:2
 └── projections
      └── ts:1 <= timezone('America/Denver', tz:2) [as="?column?":6, outer=(1,2), immutable]

# Don't normalize when the timezone() arguments are constants.
