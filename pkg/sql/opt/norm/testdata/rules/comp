exec-ddl
CREATE TABLE a (
  k INT PRIMARY KEY,
  i INT,
  f FLOAT,
  s STRING,
  j JSON,
  d DATE,
  ts TIMESTAMP,
  tz TIMESTAMPTZ,
  intv INTERVAL
)
----

exec-ddl
CREATE TABLE geom_geog (
  geom GEOMETRY,
  geog GEOGRAPHY,
  val FLOAT
)
----

# --------------------------------------------------
# CommuteVarInequality
# --------------------------------------------------

# Put variables on both sides of comparison operator to avoid matching constant
# patterns.
norm expect=CommuteVarInequality
SELECT * FROM a WHERE 1+i<k AND k-1<=i AND i*i>k AND k/2>=i
----
select
 â”œâ”€â”€ columns: k:1!null i:2!null f:3 s:4 j:5 d:6 ts:7 tz:8 intv:9
 â”œâ”€â”€ immutable
 â”œâ”€â”€ key: (1)
 â”œâ”€â”€ fd: (1)-->(2-9)
 â”œâ”€â”€ scan a
 â”‚    â”œâ”€â”€ columns: k:1!null i:2 f:3 s:4 j:5 d:6 ts:7 tz:8 intv:9
 â”‚    â”œâ”€â”€ key: (1)
 â”‚    â””â”€â”€ fd: (1)-->(2-9)
 â””â”€â”€ filters
      â”œâ”€â”€ k:1 > (i:2 + 1) [outer=(1,2), immutable, constraints=(/1: (/NULL - ])]
      â”œâ”€â”€ i:2 >= (k:1 - 1) [outer=(1,2), immutable, constraints=(/2: (/NULL - ])]
      â”œâ”€â”€ k:1 < (i:2 * i:2) [outer=(1,2), immutable, constraints=(/1: (/NULL - ])]
      â””â”€â”€ i:2 <= (k:1 / 2) [outer=(1,2), constraints=(/2: (/NULL - ])]

