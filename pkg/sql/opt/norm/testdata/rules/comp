    i+2+2 > 10 AND
    ts+'1 day'::INTERVAL > '2020-03-21 12:00:00'::TIMESTAMP AND
    tz+'1 day'::INTERVAL > '2020-03-21 12:00:00+00'::TIMESTAMPTZ
