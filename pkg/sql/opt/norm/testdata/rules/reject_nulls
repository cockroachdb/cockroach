# ConstNotNullAgg rejects nulls (regression test for #28810).
opt expect=RejectNullsGroupBy
SELECT *
FROM
(
    SELECT EXISTS(SELECT 1 FROM a WHERE a.s = ref_0.s) AS exists
    FROM a AS ref_0
    LEFT JOIN a AS ref_1
    ON True
)
WHERE exists
----
