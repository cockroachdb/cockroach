 │         └── k:1 = x:6 [outer=(1,6), constraints=(/1: (/NULL - ]; /6: (/NULL - ]), fd=(1)==(6), (6)==(1)]
 └── projections
      └── k:1 = x:6 [as="?column?":9, outer=(1,6)]

# LIKE case.
norm expect=RejectNullsProject
SELECT * FROM
(
  SELECT s LIKE 'blah' FROM xy
  LEFT JOIN a ON i = y
) f(p)
WHERE p
----
select
 ├── columns: p:9!null
 ├── fd: ()-->(9)
 ├── project
 │    ├── columns: "?column?":9!null
 │    ├── inner-join (hash)
