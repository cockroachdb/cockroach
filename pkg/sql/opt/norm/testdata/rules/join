 │    │    ├── columns: c.x:4!null c.y:5!null c.z:6!null
 │    │    ├── key: (4)
 │    │    └── fd: (4)-->(5,6)
 │    ├── scan d
 │    │    ├── columns: d.x:8!null d.y:9!null d.z:10!null
 │    │    ├── key: (8)
 │    │    └── fd: (8)-->(9,10)
 │    └── filters
 │         └── c.x:4 = d.x:8 [outer=(4,8), constraints=(/4: (/NULL - ]; /8: (/NULL - ]), fd=(4)==(8), (8)==(4)]
 └── filters
      └── b.x:1 = c.x:4 [outer=(1,4), constraints=(/1: (/NULL - ]; /4: (/NULL - ]), fd=(1)==(4), (4)==(1)]

norm expect=MapEqualityIntoJoinLeftAndRight
