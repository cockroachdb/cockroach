 ├── columns: x:17
 ├── immutable
 ├── ensure-distinct-on
 │    ├── columns: c.x:1!null b.x:5
 │    ├── grouping columns: c.x:1!null
 │    ├── error: "more than one row returned by a subquery used as an expression"
 │    ├── immutable
 │    ├── key: (1)
 │    ├── fd: (1)-->(5)
 │    ├── left-join-apply
 │    │    ├── columns: c.x:1!null b.x:5 k:11
 │    │    ├── immutable
 │    │    ├── fd: (5)==(11), (11)==(5)
 │    │    ├── scan c
 │    │    │    ├── columns: c.x:1!null
 │    │    │    └── key: (1)
 │    │    ├── inner-join (hash)
 │    │    │    ├── columns: b.x:5!null k:11!null
 │    │    │    ├── outer: (1)
 │    │    │    ├── multiplicity: left-rows(zero-or-one), right-rows(zero-or-more)
 │    │    │    ├── immutable
 │    │    │    ├── fd: (5)==(11), (11)==(5)
 │    │    │    ├── left-join (cross)
 │    │    │    │    ├── columns: b.x:5!null
 │    │    │    │    ├── outer: (1)
 │    │    │    │    ├── scan b
 │    │    │    │    │    ├── columns: b.x:5!null
 │    │    │    │    │    └── key: (5)
 │    │    │    │    ├── scan b [as=b2]
 │    │    │    │    └── filters
 │    │    │    │         └── c.x:1 = 5 [outer=(1), constraints=(/1: [/5 - /5]; tight), fd=()-->(1)]
 │    │    │    ├── select
 │    │    │    │    ├── columns: k:11!null
 │    │    │    │    ├── immutable
