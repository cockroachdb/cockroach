 │    └── fd: (1)-->(2-5)
 ├── scan b
 │    ├── columns: x:7!null y:8
 │    ├── key: (7)
 │    └── fd: (7)-->(8)
 └── filters
      ├── k:1 = x:7 [outer=(1,7), constraints=(/1: (/NULL - ]; /7: (/NULL - ]), fd=(1)==(7), (7)==(1)]
      └── or [outer=(1,7), correlated-subquery]
           ├── k:1 = x:7
           └── any: eq
                ├── project
                │    ├── columns: "?column?":13!null
                │    ├── fd: ()-->(13)
                │    ├── select
                │    │    ├── columns: x:10!null y:11!null
                │    │    ├── key: (10)
