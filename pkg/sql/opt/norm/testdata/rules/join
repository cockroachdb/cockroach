# Multiple extractable equalities.
norm expect=ExtractJoinEqualities
SELECT * FROM xy JOIN uv ON x+y=u AND x=u+v AND x*y+1=u*v+2
----
project
 ├── columns: x:1!null y:2 u:4!null v:5
 ├── immutable
 ├── key: (1)
 ├── fd: (1)-->(2,4,5), (4)-->(1,2,5)
 └── inner-join (hash)
      ├── columns: x:1!null y:2 u:4!null v:5 column7:7!null column8:8!null column9:9!null column10:10!null
      ├── multiplicity: left-rows(zero-or-one), right-rows(zero-or-one)
      ├── immutable
      ├── key: (1)
      ├── fd: (1)-->(2,7,9), (4)-->(5,8,10), (4)==(7), (7)==(4), (1)==(8), (8)==(1), (9)==(10), (10)==(9)
      ├── project
      │    ├── columns: column9:9 column7:7 x:1!null y:2
      │    ├── immutable
      │    ├── key: (1)
      │    ├── fd: (1)-->(2,7,9)
      │    ├── scan xy
      │    │    ├── columns: x:1!null y:2
      │    │    ├── key: (1)
      │    │    └── fd: (1)-->(2)
      │    └── projections
      │         ├── (x:1 * y:2) + 1 [as=column9:9, outer=(1,2), immutable]
      │         └── x:1 + y:2 [as=column7:7, outer=(1,2), immutable]
      ├── project
      │    ├── columns: column10:10 column8:8 u:4!null v:5
      │    ├── immutable
      │    ├── key: (4)
      │    ├── fd: (4)-->(5,8,10)
      │    ├── scan uv
      │    │    ├── columns: u:4!null v:5
      │    │    ├── key: (4)
      │    │    └── fd: (4)-->(5)
      │    └── projections
      │         ├── (u:4 * v:5) + 2 [as=column10:10, outer=(4,5), immutable]
      │         └── u:4 + v:5 [as=column8:8, outer=(4,5), immutable]
      └── filters
           ├── column7:7 = u:4 [outer=(4,7), constraints=(/4: (/NULL - ]; /7: (/NULL - ]), fd=(4)==(7), (7)==(4)]
           ├── x:1 = column8:8 [outer=(1,8), constraints=(/1: (/NULL - ]; /8: (/NULL - ]), fd=(1)==(8), (8)==(1)]
           └── column9:9 = column10:10 [outer=(9,10), constraints=(/9: (/NULL - ]; /10: (/NULL - ]), fd=(9)==(10), (10)==(9)]

# An extractable equality with another expression.
norm expect=ExtractJoinEqualities
SELECT * FROM xy JOIN uv ON x+y=u AND x+u=v
----
project
 ├── columns: x:1!null y:2 u:4!null v:5!null
 ├── immutable
 ├── key: (1)
 ├── fd: (1)-->(2,4,5), (4)-->(5)
 └── inner-join (hash)
      ├── columns: x:1!null y:2 u:4!null v:5!null column7:7!null
      ├── multiplicity: left-rows(zero-or-one), right-rows(zero-or-more)
      ├── immutable
      ├── key: (1)
      ├── fd: (1)-->(2,7), (4)-->(5), (4)==(7), (7)==(4)
      ├── project
      │    ├── columns: column7:7 x:1!null y:2
      │    ├── immutable
      │    ├── key: (1)
      │    ├── fd: (1)-->(2,7)
      │    ├── scan xy
      │    │    ├── columns: x:1!null y:2
      │    │    ├── key: (1)
      │    │    └── fd: (1)-->(2)
      │    └── projections
      │         └── x:1 + y:2 [as=column7:7, outer=(1,2), immutable]
      ├── scan uv
      │    ├── columns: u:4!null v:5
      │    ├── key: (4)
      │    └── fd: (4)-->(5)
      └── filters
           ├── v:5 = (x:1 + u:4) [outer=(1,4,5), immutable, constraints=(/5: (/NULL - ])]
           └── column7:7 = u:4 [outer=(4,7), constraints=(/4: (/NULL - ]; /7: (/NULL - ]), fd=(4)==(7), (7)==(4)]

# Cases with non-extractable equality.
