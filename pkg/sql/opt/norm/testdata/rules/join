 │    │    └── fd: (1)-->(2,3)
 │    └── filters
 │         └── c.x:1 = c.y:2 [outer=(1,2), constraints=(/1: (/NULL - ]; /2: (/NULL - ]), fd=(1)==(2), (2)==(1)]
 ├── select
 │    ├── columns: d.x:4!null d.y:5!null d.z:6!null
 │    ├── key: (4)
 │    ├── fd: (4)-->(6), (4)==(5), (5)==(4)
 │    ├── scan d
 │    │    ├── columns: d.x:4!null d.y:5!null d.z:6!null
