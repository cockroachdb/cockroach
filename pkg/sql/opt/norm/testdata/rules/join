      ├── a.i:2 = a.f:3 [outer=(2,3), constraints=(/2: (/NULL - ]; /3: (/NULL - ]), fd=(2)==(3), (3)==(2)]
      └── a2.i:8 = a2.f:9 [outer=(8,9), constraints=(/8: (/NULL - ]; /9: (/NULL - ]), fd=(8)==(9), (9)==(8)]

# Can't simplify: equality conditions have columns from different tables.
norm expect-not=(SimplifyRightJoin,SimplifyLeftJoin)
SELECT * FROM (SELECT * FROM a, b) AS a FULL JOIN a AS a2 ON a.k=a2.k AND a.x=a2.k
----
full-join (hash)
 ├── columns: k:1 i:2 f:3 s:4 j:5 x:7 y:8 k:10 i:11 f:12 s:13 j:14
 ├── multiplicity: left-rows(exactly-one), right-rows(exactly-one)
 ├── key: (1,7,10)
 ├── fd: (1)-->(2-5), (7)-->(8), (10)-->(11-14)
 ├── inner-join (cross)
 │    ├── columns: a.k:1!null a.i:2 a.f:3!null a.s:4 a.j:5 x:7!null y:8
 │    ├── key: (1,7)
 │    ├── fd: (1)-->(2-5), (7)-->(8)
 │    ├── scan a
