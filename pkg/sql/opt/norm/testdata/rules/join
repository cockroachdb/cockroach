 ├── fd: (7)-->(8-11), (13)-->(14-17), (7)==(13), (13)==(7), (1)-->(2-5), (3)==(9), (9)==(3)
 ├── inner-join (hash)
 │    ├── columns: a.k:7!null a.i:8 a.f:9!null a.s:10 a.j:11 a2.k:13!null a2.i:14 a2.f:15!null a2.s:16 a2.j:17
 │    ├── multiplicity: left-rows(exactly-one), right-rows(exactly-one)
 │    ├── key: (13)
 │    ├── fd: (7)-->(8-11), (13)-->(14-17), (7)==(13), (13)==(7)
 │    ├── scan a
 │    │    ├── columns: a.k:7!null a.i:8 a.f:9!null a.s:10 a.j:11
 │    │    ├── key: (7)
 │    │    └── fd: (7)-->(8-11)
 │    ├── scan a [as=a2]
 │    │    ├── columns: a2.k:13!null a2.i:14 a2.f:15!null a2.s:16 a2.j:17
 │    │    ├── key: (13)
 │    │    └── fd: (13)-->(14-17)
 │    └── filters
