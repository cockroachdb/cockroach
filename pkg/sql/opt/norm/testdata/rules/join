 │    │    │    │    └── filters
 │    │    │    │         └── (k:11 + k:11) < 5 [outer=(11), immutable]
 │    │    │    └── filters
 │    │    │         └── k:11 = b.x:5 [outer=(5,11), constraints=(/5: (/NULL - ]; /11: (/NULL - ]), fd=(5)==(11), (11)==(5)]
 │    │    └── filters (true)
 │    └── aggregations
 │         └── const-agg [as=b.x:5, outer=(5)]
 │              └── b.x:5
 └── projections
      └── b.x:5 [as=x:17, outer=(5)]

