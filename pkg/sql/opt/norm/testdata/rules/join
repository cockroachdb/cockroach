 ├── fd: (1)-->(2), (3)-->(4-7)
 ├── scan b
 │    ├── columns: x:1(int!null) y:2(int)
 │    ├── key: (1)
 │    ├── fd: (1)-->(2)
 │    └── ordering: +1
 ├── scan a
 │    ├── columns: k:3(int!null) i:4(int) f:5(float) s:6(string) j:7(jsonb)
 │    ├── key: (3)
 │    ├── fd: (3)-->(4-7)
 │    └── ordering: +3
 └── merge-on
      ├── left ordering: +1
      ├── right ordering: +3
      └── filters [type=bool, outer=(1,3,4), constraints=(/1: (/NULL - ]; /3: (/NULL - ]; /4: [/1 - /1]), fd=()-->(4), (1)==(3), (3)==(1)]
           ├── b.x = a.k [type=bool, outer=(1,3), constraints=(/1: (/NULL - ]; /3: (/NULL - ])]
           └── a.i = 1 [type=bool, outer=(4), constraints=(/4: [/1 - /1]; tight)]

