 ├── columns: x:1!null y:2 u:4!null v:5
 ├── key: (1,4)
 ├── fd: (1)-->(2), (1,4)-->(5)
 └── inner-join-apply
      ├── columns: x:1!null y:2 u:4!null v:5 k:7
      ├── key: (1,4)
      ├── fd: (1)-->(2), (1,4)-->(5,7), (4)==(7), (7)==(4)
      ├── scan xy
      │    ├── columns: x:1!null y:2
      │    ├── key: (1)
      │    └── fd: (1)-->(2)
      ├── ensure-distinct-on
      │    ├── columns: u:4!null v:5 k:7
      │    ├── grouping columns: u:4!null
      │    ├── error: "more than one row returned by a subquery used as an expression"
      │    ├── outer: (1)
      │    ├── key: (4)
      │    ├── fd: (4)-->(5,7)
      │    ├── left-join (cross)
      │    │    ├── columns: u:4!null v:5 k:7 i:8
      │    │    ├── outer: (1)
      │    │    ├── key: (4,7)
      │    │    ├── fd: (4)-->(5), (7)-->(8)
      │    │    ├── scan uv
      │    │    │    ├── columns: u:4!null v:5
      │    │    │    ├── key: (4)
      │    │    │    └── fd: (4)-->(5)
      │    │    ├── scan a
      │    │    │    ├── columns: k:7!null i:8
      │    │    │    ├── key: (7)
      │    │    │    └── fd: (7)-->(8)
      │    │    └── filters
      │    │         └── i:8 = x:1 [outer=(1,8), constraints=(/1: (/NULL - ]; /8: (/NULL - ]), fd=(1)==(8), (8)==(1)]
      │    └── aggregations
      │         ├── const-agg [as=v:5, outer=(5)]
      │         │    └── v:5
      │         └── const-agg [as=k:7, outer=(7)]
      │              └── k:7
      └── filters
           └── u:4 = k:7 [outer=(4,7), constraints=(/4: (/NULL - ]; /7: (/NULL - ]), fd=(4)==(7), (7)==(4)]

