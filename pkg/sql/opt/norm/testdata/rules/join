# SimplifyLeftJoinWithFilters + SimplifyRightJoinWithFilters
# --------------------------------------------------
norm expect=SimplifyLeftJoinWithFilters
SELECT * FROM a FULL JOIN a AS a2 ON a.k=a2.k
----
