# Left-join-apply.
norm expect=SimplifyLeftJoin
SELECT * FROM a WHERE (SELECT sum(column1) FROM (VALUES (k), (1))) = 1
----
project
 ├── columns: k:1!null i:2 f:3!null s:4 j:5
 ├── immutable
 ├── key: (1)
 ├── fd: (1)-->(2-5)
 └── select
      ├── columns: k:1!null i:2 f:3!null s:4 j:5 sum:8!null
      ├── immutable
      ├── key: (1)
      ├── fd: ()-->(8), (1)-->(2-5)
      ├── group-by
      │    ├── columns: k:1!null i:2 f:3!null s:4 j:5 sum:8
      │    ├── grouping columns: k:1!null
      │    ├── key: (1)
      │    ├── fd: (1)-->(2-5,8)
      │    ├── inner-join-apply
      │    │    ├── columns: k:1!null i:2 f:3!null s:4 j:5 column1:7
      │    │    ├── fd: (1)-->(2-5)
      │    │    ├── scan a
      │    │    │    ├── columns: k:1!null i:2 f:3!null s:4 j:5
      │    │    │    ├── key: (1)
      │    │    │    └── fd: (1)-->(2-5)
      │    │    ├── values
      │    │    │    ├── columns: column1:7
      │    │    │    ├── outer: (1)
      │    │    │    ├── cardinality: [2 - 2]
      │    │    │    ├── (k:1,)
      │    │    │    └── (1,)
      │    │    └── filters (true)
      │    └── aggregations
      │         ├── sum [as=sum:8, outer=(7)]
      │         │    └── column1:7
      │         ├── const-agg [as=i:2, outer=(2)]
      │         │    └── i:2
      │         ├── const-agg [as=f:3, outer=(3)]
      │         │    └── f:3
      │         ├── const-agg [as=s:4, outer=(4)]
      │         │    └── s:4
      │         └── const-agg [as=j:5, outer=(5)]
      │              └── j:5
      └── filters
           └── sum:8 = 1 [outer=(8), immutable, constraints=(/8: [/1 - /1]; tight), fd=()-->(8)]

