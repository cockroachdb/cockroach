# Simply multiple conditions, with other conditions present as well.
norm expect=SimplifyJoinNotNullEquality
SELECT *
FROM (SELECT * FROM a WHERE i>0) AS a
INNER JOIN (SELECT x, y, y+1 AS z FROM b WHERE y>10) AS b
ON a.f>=b.z::float AND (a.k=b.x) IS True AND a.f>=b.z::float AND (a.i=b.y) IS NOT False
----
inner-join (hash)
 ├── columns: k:1!null i:2!null f:3!null s:4 j:5 x:7!null y:8!null z:10!null
 ├── multiplicity: left-rows(zero-or-one), right-rows(zero-or-one)
 ├── immutable
 ├── key: (7)
 ├── fd: (1)-->(2-5), (7)-->(8), (8)-->(10), (1)==(7), (7)==(1), (2)==(8), (8)==(2)
 ├── select
 │    ├── columns: k:1!null i:2!null f:3!null s:4 j:5
 │    ├── key: (1)
 │    ├── fd: (1)-->(2-5)
 │    ├── scan a
 │    │    ├── columns: k:1!null i:2 f:3!null s:4 j:5
 │    │    ├── key: (1)
 │    │    └── fd: (1)-->(2-5)
 │    └── filters
 │         └── i:2 > 0 [outer=(2), constraints=(/2: [/1 - ]; tight)]
 ├── project
 │    ├── columns: z:10!null x:7!null y:8!null
 │    ├── immutable
 │    ├── key: (7)
 │    ├── fd: (7)-->(8), (8)-->(10)
 │    ├── select
 │    │    ├── columns: x:7!null y:8!null
 │    │    ├── key: (7)
 │    │    ├── fd: (7)-->(8)
 │    │    ├── scan b
 │    │    │    ├── columns: x:7!null y:8
 │    │    │    ├── key: (7)
 │    │    │    └── fd: (7)-->(8)
 │    │    └── filters
 │    │         └── y:8 > 10 [outer=(8), constraints=(/8: [/11 - ]; tight)]
 │    └── projections
 │         └── y:8 + 1 [as=z:10, outer=(8), immutable]
 └── filters
      ├── f:3 >= z:10::FLOAT8 [outer=(3,10), immutable, constraints=(/3: (/NULL - ])]
      ├── f:3 >= z:10::FLOAT8 [outer=(3,10), immutable, constraints=(/3: (/NULL - ])]
      ├── k:1 = x:7 [outer=(1,7), constraints=(/1: (/NULL - ]; /7: (/NULL - ]), fd=(1)==(7), (7)==(1)]
      └── i:2 = y:8 [outer=(2,8), constraints=(/2: (/NULL - ]; /8: (/NULL - ]), fd=(2)==(8), (8)==(2)]

