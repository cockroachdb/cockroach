# Can't simplify: joins on non-foreign keys.
norm expect-not=(SimplifyRightJoin,SimplifyLeftJoin)
SELECT *
FROM c
LEFT OUTER JOIN a
ON c.z = a.k
----
left-join (hash)
 ├── columns: x:1!null y:2!null z:3!null k:5 i:6 f:7 s:8 j:9
 ├── multiplicity: left-rows(exactly-one), right-rows(zero-or-more)
 ├── key: (1)
 ├── fd: (1)-->(2,3,5-9), (5)-->(6-9)
 ├── scan c
 │    ├── columns: x:1!null y:2!null z:3!null
 │    ├── key: (1)
 │    └── fd: (1)-->(2,3)
 ├── scan a
 │    ├── columns: k:5!null i:6 f:7!null s:8 j:9
 │    ├── key: (5)
 │    └── fd: (5)-->(6-9)
 └── filters
      └── z:3 = k:5 [outer=(3,5), constraints=(/3: (/NULL - ]; /5: (/NULL - ]), fd=(3)==(5), (5)==(3)]

# Can't simplify: joins on non-foreign keys still in foreign key index.
norm expect-not=(SimplifyRightJoin,SimplifyLeftJoin)
SELECT *
FROM c
LEFT OUTER JOIN a
ON c.x = a.k
----
left-join (hash)
 ├── columns: x:1!null y:2!null z:3!null k:5 i:6 f:7 s:8 j:9
 ├── multiplicity: left-rows(exactly-one), right-rows(zero-or-one)
 ├── key: (1)
 ├── fd: (1)-->(2,3,5-9), (5)-->(6-9)
 ├── scan c
 │    ├── columns: x:1!null y:2!null z:3!null
 │    ├── key: (1)
 │    └── fd: (1)-->(2,3)
 ├── scan a
 │    ├── columns: k:5!null i:6 f:7!null s:8 j:9
 │    ├── key: (5)
 │    └── fd: (5)-->(6-9)
 └── filters
      └── x:1 = k:5 [outer=(1,5), constraints=(/1: (/NULL - ]; /5: (/NULL - ]), fd=(1)==(5), (5)==(1)]

# Can't simplify: non-equality condition.
norm expect-not=(SimplifyRightJoin,SimplifyLeftJoin)
SELECT * FROM a FULL JOIN a AS a2 ON a.k<a2.k
----
full-join (cross)
 ├── columns: k:1 i:2 f:3 s:4 j:5 k:7 i:8 f:9 s:10 j:11
 ├── key: (1,7)
 ├── fd: (1)-->(2-5), (7)-->(8-11)
 ├── scan a
 │    ├── columns: a.k:1!null a.i:2 a.f:3!null a.s:4 a.j:5
 │    ├── key: (1)
 │    └── fd: (1)-->(2-5)
 ├── scan a [as=a2]
 │    ├── columns: a2.k:7!null a2.i:8 a2.f:9!null a2.s:10 a2.j:11
 │    ├── key: (7)
 │    └── fd: (7)-->(8-11)
 └── filters
      └── a.k:1 < a2.k:7 [outer=(1,7), constraints=(/1: (/NULL - ]; /7: (/NULL - ])]

# Can't simplify: non-join equality condition.
