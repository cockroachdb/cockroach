      ├── key: (1)
      ├── fd: (1)-->(2-7)
      ├── scan a
      │    ├── columns: k:1!null i:2 f:3!null s:4 j:5
      │    ├── key: (1)
      │    └── fd: (1)-->(2-5)
      ├── select
      │    ├── columns: x:6!null y:7!null
      │    ├── key: (6)
      │    ├── fd: ()-->(7)
      │    ├── scan b
      │    │    ├── columns: x:6!null y:7
