 ├── key: (5)
 ├── fd: ()-->(4), (5)-->(6-9)
 ├── scalar-group-by
 │    ├── columns: count_rows:4!null
 │    ├── cardinality: [1 - 1]
 │    ├── key: ()
 │    ├── fd: ()-->(4)
 │    ├── scan b
 │    └── aggregations
 │         └── count-rows [as=count_rows:4]
 ├── scan a
 │    ├── columns: k:5!null i:6 f:7!null s:8 j:9
 │    ├── key: (5)
 │    └── fd: (5)-->(6-9)
 └── filters (true)

