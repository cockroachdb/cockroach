# Regression test for #43039. Use transitive equalities for filter inference.
norm expect=PushFilterIntoJoinLeftAndRight
SELECT
    *
FROM
    a
    JOIN b ON a.k = b.x
    JOIN c ON b.x = c.x
    JOIN d ON c.x = d.x
    JOIN xy ON d.x = xy.x
WHERE
    a.k = 3;
