select
 ├── columns: s:4(string!null)
 ├── scan a
 │    └── columns: s:4(string)
 └── filters [type=bool, outer=(4), constraints=(/4: [/'foo' - /'foo']; tight)]
      └── eq [type=bool, outer=(4), constraints=(/4: [/'foo' - /'foo']; tight)]
           ├── variable: a.s [type=string, outer=(4)]
           └── const: 'foo' [type=string]
