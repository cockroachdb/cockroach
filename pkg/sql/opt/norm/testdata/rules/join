 ├── columns: k:1!null i:2 f:3!null s:4 j:5 x:7!null y:8
 ├── key: (1,7)
 ├── fd: (1)-->(2-5), (7)-->(8)
 ├── scan a
 │    ├── columns: k:1!null i:2 f:3!null s:4 j:5
 │    ├── key: (1)
 │    └── fd: (1)-->(2-5)
 ├── scan b
 │    ├── columns: x:7!null y:8
 │    ├── key: (7)
 │    └── fd: (7)-->(8)
 └── filters
      ├── (k:1 = y:8) IS true [outer=(1,8)]
      └── (i:2 = x:7) IS false [outer=(2,7)]

