# Left joins on a foreign key turn into inner joins.
norm expect=SimplifyLeftJoin
SELECT *
FROM c
LEFT OUTER JOIN a
ON c.y = a.k
----
inner-join (hash)
 ├── columns: x:1!null y:2!null z:3!null k:5!null i:6 f:7!null s:8 j:9
 ├── multiplicity: left-rows(exactly-one), right-rows(zero-or-more)
 ├── key: (1)
 ├── fd: (1)-->(2,3), (5)-->(6-9), (2)==(5), (5)==(2)
 ├── scan c
 │    ├── columns: x:1!null y:2!null z:3!null
 │    ├── key: (1)
 │    └── fd: (1)-->(2,3)
 ├── scan a
 │    ├── columns: k:5!null i:6 f:7!null s:8 j:9
 │    ├── key: (5)
 │    └── fd: (5)-->(6-9)
 └── filters
      └── y:2 = k:5 [outer=(2,5), constraints=(/2: (/NULL - ]; /5: (/NULL - ]), fd=(2)==(5), (5)==(2)]

# Left joins on a multiple-column foreign key turn into inner joins.
norm expect=SimplifyLeftJoin
SELECT *
FROM d
LEFT OUTER JOIN c
ON d.z = c.z
AND d.y = c.x
----
inner-join (hash)
 ├── columns: x:1!null y:2!null z:3!null x:5!null y:6!null z:7!null
 ├── multiplicity: left-rows(exactly-one), right-rows(zero-or-more)
 ├── key: (1)
 ├── fd: (1)-->(2,3), (5)-->(6,7), (3)==(7), (7)==(3), (2)==(5), (5)==(2)
 ├── scan d
 │    ├── columns: d.x:1!null d.y:2!null d.z:3!null
 │    ├── key: (1)
 │    └── fd: (1)-->(2,3)
 ├── scan c
 │    ├── columns: c.x:5!null c.y:6!null c.z:7!null
 │    ├── key: (5)
 │    └── fd: (5)-->(6,7)
 └── filters
      ├── d.z:3 = c.z:7 [outer=(3,7), constraints=(/3: (/NULL - ]; /7: (/NULL - ]), fd=(3)==(7), (7)==(3)]
      └── d.y:2 = c.x:5 [outer=(2,5), constraints=(/2: (/NULL - ]; /5: (/NULL - ]), fd=(2)==(5), (5)==(2)]

# Left join on a part of a foreign key turns into an inner join.
norm expect=SimplifyLeftJoin
SELECT *
FROM d
LEFT OUTER JOIN c
ON d.z = c.z
----
inner-join (hash)
 ├── columns: x:1!null y:2!null z:3!null x:5!null y:6!null z:7!null
 ├── multiplicity: left-rows(one-or-more), right-rows(zero-or-more)
 ├── key: (1,5)
 ├── fd: (1)-->(2,3), (5)-->(6,7), (3)==(7), (7)==(3)
 ├── scan d
 │    ├── columns: d.x:1!null d.y:2!null d.z:3!null
 │    ├── key: (1)
 │    └── fd: (1)-->(2,3)
 ├── scan c
 │    ├── columns: c.x:5!null c.y:6!null c.z:7!null
 │    ├── key: (5)
 │    └── fd: (5)-->(6,7)
 └── filters
      └── d.z:3 = c.z:7 [outer=(3,7), constraints=(/3: (/NULL - ]; /7: (/NULL - ]), fd=(3)==(7), (7)==(3)]

# Cross join case. The presence of a not-null foreign key implies that there
# will be at least one right row when there is at least one left row, so left
# rows will always be matched at least once.
norm expect=SimplifyLeftJoin
SELECT *
FROM d
LEFT OUTER JOIN c
ON True
----
inner-join (cross)
 ├── columns: x:1!null y:2!null z:3!null x:5!null y:6!null z:7!null
 ├── multiplicity: left-rows(one-or-more), right-rows(zero-or-more)
 ├── key: (1,5)
 ├── fd: (1)-->(2,3), (5)-->(6,7)
 ├── scan d
 │    ├── columns: d.x:1!null d.y:2!null d.z:3!null
 │    ├── key: (1)
 │    └── fd: (1)-->(2,3)
 ├── scan c
 │    ├── columns: c.x:5!null c.y:6!null c.z:7!null
 │    ├── key: (5)
 │    └── fd: (5)-->(6,7)
 └── filters (true)

norm expect=SimplifyRightJoin
SELECT * FROM (SELECT count(*) FROM b) FULL JOIN a ON True
----
left-join (cross)
 ├── columns: count:4!null k:5 i:6 f:7 s:8 j:9
 ├── cardinality: [1 - ]
 ├── multiplicity: left-rows(one-or-more), right-rows(exactly-one)
 ├── key: (5)
 ├── fd: ()-->(4), (5)-->(6-9)
 ├── scalar-group-by
 │    ├── columns: count_rows:4!null
 │    ├── cardinality: [1 - 1]
 │    ├── key: ()
 │    ├── fd: ()-->(4)
 │    ├── scan b
 │    └── aggregations
 │         └── count-rows [as=count_rows:4]
 ├── scan a
 │    ├── columns: k:5!null i:6 f:7!null s:8 j:9
 │    ├── key: (5)
 │    └── fd: (5)-->(6-9)
 └── filters (true)

