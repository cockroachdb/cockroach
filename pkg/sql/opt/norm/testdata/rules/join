 │    ├── (1.00,) [type=tuple{decimal}]
 │    └── (2.00,) [type=tuple{decimal}]
 └── filters
      └── column1 = column1 [type=bool, outer=(1,2), constraints=(/1: (/NULL - ]; /2: (/NULL - ]), fd=(1)==(2), (2)==(1)]

# Optimization does not apply if equality is only on one side.
