# Regression test for #46151. Do not push down a filter with a correlated
# subquery.
norm expect-not=PushFilterIntoJoinLeftAndRight
SELECT (SELECT i_name FROM item LIMIT 1)
  FROM history INNER JOIN order_line ON h_data = ol_dist_info
 WHERE (
        EXISTS(
            SELECT *
              FROM history
             WHERE h_data IS NOT NULL AND ol_dist_info IS NOT NULL
        )
       )
    OR (SELECT ol_i_id FROM order_line LIMIT 1) IS NOT NULL;
----
project
 ├── columns: i_name:52
 ├── fd: ()-->(52)
 ├── inner-join (hash)
 │    ├── columns: h_data:9!null ol_o_id:11!null ol_d_id:12!null ol_w_id:13!null ol_number:14!null ol_dist_info:20!null true_agg:44
 │    ├── fd: (11-14)-->(20,44), (9)==(20), (20)==(9)
 │    ├── scan history
 │    │    └── columns: h_data:9
 │    ├── select
 │    │    ├── columns: ol_o_id:11!null ol_d_id:12!null ol_w_id:13!null ol_number:14!null ol_dist_info:20 true_agg:44
 │    │    ├── key: (11-14)
 │    │    ├── fd: (11-14)-->(20,44)
 │    │    ├── group-by
 │    │    │    ├── columns: ol_o_id:11!null ol_d_id:12!null ol_w_id:13!null ol_number:14!null ol_dist_info:20 true_agg:44
 │    │    │    ├── grouping columns: ol_o_id:11!null ol_d_id:12!null ol_w_id:13!null ol_number:14!null
 │    │    │    ├── key: (11-14)
 │    │    │    ├── fd: (11-14)-->(20,44)
 │    │    │    ├── left-join (cross)
 │    │    │    │    ├── columns: ol_o_id:11!null ol_d_id:12!null ol_w_id:13!null ol_number:14!null ol_dist_info:20 true:43
 │    │    │    │    ├── fd: (11-14)-->(20)
 │    │    │    │    ├── scan order_line
 │    │    │    │    │    ├── columns: ol_o_id:11!null ol_d_id:12!null ol_w_id:13!null ol_number:14!null ol_dist_info:20
 │    │    │    │    │    ├── key: (11-14)
 │    │    │    │    │    └── fd: (11-14)-->(20)
 │    │    │    │    ├── project
 │    │    │    │    │    ├── columns: true:43!null
