      │    │    ├── key: (4)
      │    │    └── fd: (4)-->(5)
      │    └── projections
      │         └── u:4 + v:5 [as=column7:7, outer=(4,5), immutable]
      └── filters
           └── x:1 = column7:7 [outer=(1,7), constraints=(/1: (/NULL - ]; /7: (/NULL - ]), fd=(1)==(7), (7)==(1)]

