 │    ├── immutable
 │    ├── key: (3)
 │    ├── fd: (1)-->(2), (3)-->(4,7), (1)==(7), (7)==(1)
 │    ├── scan xy
 │    │    ├── columns: x:1!null y:2
 │    │    ├── key: (1)
 │    │    └── fd: (1)-->(2)
 │    ├── project
 │    │    ├── columns: column7:7!null k:3!null i:4
 │    │    ├── immutable
 │    │    ├── key: (3)
 │    │    ├── fd: (3)-->(4,7)
