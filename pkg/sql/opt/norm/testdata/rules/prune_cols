 ├── key: (1)
 ├── fd: (1)-->(2,3), (2,3)~~>(1)
 ├── scan abcde
 │    ├── columns: abcde.a:1!null abcde.b:2 abcde.c:3
 │    ├── key: (1)
 │    └── fd: (1)-->(2,3), (2,3)~~>(1)
 ├── semi-join (cross)
 │    ├── columns: "family".a:6!null
 │    ├── outer: (1)
 │    ├── key: (6)
 │    ├── scan "family"
 │    │    ├── columns: "family".a:6!null
 │    │    └── key: (6)
 │    ├── scan a
 │    │    ├── columns: k:11!null
 │    │    └── key: (11)
 │    └── filters
 │         └── abcde.a:1 = k:11 [outer=(1,11), constraints=(/1: (/NULL - ]; /11: (/NULL - ]), fd=(1)==(11), (11)==(1)]
 └── filters
      └── abcde.a:1 = "family".a:6 [outer=(1,6), constraints=(/1: (/NULL - ]; /6: (/NULL - ]), fd=(1)==(6), (6)==(1)]

