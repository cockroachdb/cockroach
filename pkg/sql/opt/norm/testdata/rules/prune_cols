 │    ├── key: (1)
 │    └── fd: (1)-->(2,3), (2,3)~~>(1)
 ├── semi-join (cross)
 │    ├── columns: "family".a:7!null
 │    ├── outer: (1)
 │    ├── key: (7)
 │    ├── scan family
 │    │    ├── columns: "family".a:7!null
 │    │    └── key: (7)
 │    ├── scan a
 │    │    ├── columns: k:13!null
 │    │    └── key: (13)
 │    └── filters
 │         └── abcde.a:1 = k:13 [outer=(1,13), constraints=(/1: (/NULL - ]; /13: (/NULL - ]), fd=(1)==(13), (13)==(1)]
 └── filters
      └── abcde.a:1 = "family".a:7 [outer=(1,7), constraints=(/1: (/NULL - ]; /7: (/NULL - ]), fd=(1)==(7), (7)==(1)]

