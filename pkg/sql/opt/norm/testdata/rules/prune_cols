      ├── fd: (1)-->(2), (7)-->(8), (3)==(7), (7)==(3)
      ├── scan xy
      │    ├── columns: x:1(int!null) y:2(int)
      │    ├── key: (1)
      │    └── fd: (1)-->(2)
      ├── inner-join (hash)
      │    ├── columns: k:3(int!null) x:7(int!null) y:8(int)
      │    ├── key: (7)
      │    ├── fd: (7)-->(8), (3)==(7), (7)==(3)
      │    ├── scan a
      │    │    ├── columns: k:3(int!null)
      │    │    └── key: (3)
      │    ├── scan xy
      │    │    ├── columns: x:7(int!null) y:8(int)
      │    │    ├── key: (7)
      │    │    └── fd: (7)-->(8)
      │    └── filters
      │         └── k = x [type=bool, outer=(3,7), constraints=(/3: (/NULL - ]; /7: (/NULL - ]), fd=(3)==(7), (7)==(3)]
