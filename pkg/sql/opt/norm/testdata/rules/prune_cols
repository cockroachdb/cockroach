 ├── update-mapping:
 │    └── column2:7 => b:2
 ├── cardinality: [0 - 0]
 ├── volatile, mutations
 └── left-join (cross)
      ├── columns: column1:6!null column2:7!null column8:8 a:9 b:10
      ├── cardinality: [1 - 1]
      ├── multiplicity: left-rows(exactly-one), right-rows(exactly-one)
      ├── key: ()
      ├── fd: ()-->(6-10)
      ├── values
      │    ├── columns: column1:6!null column2:7!null column8:8
      │    ├── cardinality: [1 - 1]
      │    ├── key: ()
      │    ├── fd: ()-->(6-8)
      │    └── (1, 2, NULL)
      ├── select
      │    ├── columns: a:9!null b:10
      │    ├── cardinality: [0 - 1]
      │    ├── key: ()
      │    ├── fd: ()-->(9,10)
      │    ├── scan "family"
      │    │    ├── columns: a:9!null b:10
      │    │    ├── key: (9)
      │    │    └── fd: (9)-->(10)
