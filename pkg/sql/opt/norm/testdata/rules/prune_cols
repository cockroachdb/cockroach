 │    │    ├── columns: k:1!null
 │    │    └── key: (1)
 │    ├── scan xy
 │    │    ├── columns: x:6!null y:7
 │    │    ├── key: (6)
 │    │    └── fd: (6)-->(7)
 │    └── filters (true)
 └── projections
      └── k:1 + 1 [as=r:9, outer=(1), immutable]

# No columns needed from left side of join.
norm expect=PruneJoinLeftCols
SELECT xy.* FROM a, xy
