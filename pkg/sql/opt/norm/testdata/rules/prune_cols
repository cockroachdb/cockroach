      ├── right ordering: +5
      ├── key: (1)
      ├── fd: (1)-->(2)
      ├── anti-join (merge)
      │    ├── columns: k:1(int!null) i:2(int)
      │    ├── left ordering: +1
      │    ├── right ordering: +7
      │    ├── key: (1)
      │    ├── fd: (1)-->(2)
      │    ├── ordering: +1
      │    ├── scan a
      │    │    ├── columns: k:1(int!null) i:2(int)
      │    │    ├── key: (1)
      │    │    ├── fd: (1)-->(2)
      │    │    └── ordering: +1
      │    ├── scan xy
      │    │    ├── columns: x:7(int!null) y:8(int)
      │    │    ├── key: (7)
      │    │    ├── fd: (7)-->(8)
      │    │    └── ordering: +7
      │    └── filters (true)
      ├── scan xy
      │    ├── columns: x:5(int!null)
      │    ├── key: (5)
      │    └── ordering: +5
      └── filters (true)

