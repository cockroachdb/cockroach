           ├── CASE WHEN k IS NULL THEN column8 ELSE f END [type=float, outer=(8,9,11)]
           └── CASE WHEN k IS NULL THEN column2 ELSE s END [type=string, outer=(6,9,12)]

# Prune column in column family that is not updated.
