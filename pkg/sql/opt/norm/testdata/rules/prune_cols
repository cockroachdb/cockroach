 │    │    │    ├── columns: k:3!null i:4 a.s:6
 │    │    │    ├── immutable
 │    │    │    ├── key: (3)
 │    │    │    ├── fd: (3)-->(4,6)
 │    │    │    ├── scan a
 │    │    │    │    ├── columns: k:3!null i:4 a.s:6
 │    │    │    │    ├── key: (3)
 │    │    │    │    └── fd: (3)-->(4,6)
 │    │    │    └── filters
 │    │    │         └── (a.s:6 || 'o') = 'foo' [outer=(6), immutable]
 │    │    └── projections
