      │    ├── columns: r:7(int)
      │    ├── outer: (1)
      │    ├── cardinality: [0 - 1]
      │    ├── key: ()
      │    ├── fd: ()-->(7)
      │    └── project
      │         ├── columns: r:7(int)
      │         ├── outer: (1)
      │         ├── fd: ()-->(7)
      │         ├── select
