# We apply the PruneMutationReturnCols rule multiple times, to get
# the minimal set of columns which is (a, rowid). Notice how c and b
# are pruned away.
norm
SELECT a FROM [SELECT a, b FROM [UPDATE returning_test SET a = a + 1 RETURNING a, b, c]]
----
with &1
 ├── columns: a:21(int)
 ├── side-effects, mutations
 ├── project
 │    ├── columns: returning_test.a:1(int) returning_test.b:2(int) returning_test.c:3(string)
 │    ├── side-effects, mutations
 │    └── update returning_test
 │         ├── columns: returning_test.a:1(int) returning_test.b:2(int) returning_test.c:3(string) rowid:8(int!null)
 │         ├── fetch columns: returning_test.a:9(int) returning_test.b:10(int) returning_test.c:11(string) rowid:16(int)
 │         ├── update-mapping:
 │         │    └──  column17:17 => returning_test.a:1
 │         ├── side-effects, mutations
 │         ├── key: (8)
 │         ├── fd: (8)-->(1-3)
 │         └── project
 │              ├── columns: column17:17(int) returning_test.a:9(int) returning_test.b:10(int) returning_test.c:11(string) rowid:16(int!null)
 │              ├── key: (16)
 │              ├── fd: (16)-->(9-11), (9)~~>(10,11,16), (9)-->(17)
 │              ├── scan returning_test
 │              │    ├── columns: returning_test.a:9(int) returning_test.b:10(int) returning_test.c:11(string) rowid:16(int!null)
 │              │    ├── key: (16)
 │              │    └── fd: (16)-->(9-11), (9)~~>(10,11,16)
 │              └── projections
 │                   └── returning_test.a + 1 [type=int, outer=(9)]
 └── project
      ├── columns: a:21(int)
      ├── with-scan &1
      │    ├── columns: a:18(int)
      │    └── mapping:
      │         └──  returning_test.a:1(int) => a:18(int)
      └── projections
           └── variable: a [type=int, outer=(18)]

# We derive the prune cols for the mutation appropriately so we
# can prune away columns even when the mutation is not under a
# projection. Another rule will fire to add the appropriate
# projection when this happens.
norm
SELECT a FROM [SELECT a, b FROM [UPDATE returning_test SET a = a + 1 RETURNING a, b, c] WHERE a > 1]
----
with &1
 ├── columns: a:21(int!null)
 ├── side-effects, mutations
 ├── project
 │    ├── columns: returning_test.a:1(int) returning_test.b:2(int) returning_test.c:3(string)
 │    ├── side-effects, mutations
 │    └── update returning_test
 │         ├── columns: returning_test.a:1(int) returning_test.b:2(int) returning_test.c:3(string) rowid:8(int!null)
 │         ├── fetch columns: returning_test.a:9(int) returning_test.b:10(int) returning_test.c:11(string) rowid:16(int)
 │         ├── update-mapping:
 │         │    └──  column17:17 => returning_test.a:1
 │         ├── side-effects, mutations
 │         ├── key: (8)
 │         ├── fd: (8)-->(1-3)
 │         └── project
 │              ├── columns: column17:17(int) returning_test.a:9(int) returning_test.b:10(int) returning_test.c:11(string) rowid:16(int!null)
 │              ├── key: (16)
 │              ├── fd: (16)-->(9-11), (9)~~>(10,11,16), (9)-->(17)
 │              ├── scan returning_test
 │              │    ├── columns: returning_test.a:9(int) returning_test.b:10(int) returning_test.c:11(string) rowid:16(int!null)
 │              │    ├── key: (16)
 │              │    └── fd: (16)-->(9-11), (9)~~>(10,11,16)
 │              └── projections
 │                   └── returning_test.a + 1 [type=int, outer=(9)]
 └── project
      ├── columns: a:21(int!null)
      ├── select
