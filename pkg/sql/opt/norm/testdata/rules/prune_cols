 ├── input binding: &1
 ├── cardinality: [0 - 0]
 ├── volatile, mutations
 ├── project
 │    ├── columns: w_new:15!null x_new:16!null uniq.k:8!null w:10 x:11 uniq.y:12
 │    ├── cardinality: [0 - 1]
 │    ├── key: ()
 │    ├── fd: ()-->(8,10-12,15,16)
 │    ├── select
 │    │    ├── columns: uniq.k:8!null w:10 x:11 uniq.y:12
 │    │    ├── cardinality: [0 - 1]
 │    │    ├── key: ()
 │    │    ├── fd: ()-->(8,10-12)
 │    │    ├── scan uniq
 │    │    │    ├── columns: uniq.k:8!null w:10 x:11 uniq.y:12
 │    │    │    ├── key: (8)
 │    │    │    └── fd: (8)-->(10-12)
 │    │    └── filters
 │    │         └── uniq.k:8 = 3 [outer=(8), constraints=(/8: [/3 - /3]; tight), fd=()-->(8)]
 │    └── projections
 │         ├── 1 [as=w_new:15]
 │         └── 2 [as=x_new:16]
 └── unique-checks
      ├── unique-checks-item: uniq(w)
      │    └── semi-join (hash)
      │         ├── columns: w_new:17!null k:18!null
      │         ├── cardinality: [0 - 1]
      │         ├── key: ()
      │         ├── fd: ()-->(17,18)
      │         ├── with-scan &1
      │         │    ├── columns: w_new:17!null k:18!null
      │         │    ├── mapping:
      │         │    │    ├──  w_new:15 => w_new:17
      │         │    │    └──  uniq.k:8 => k:18
      │         │    ├── cardinality: [0 - 1]
      │         │    ├── key: ()
      │         │    └── fd: ()-->(17,18)
      │         ├── scan uniq
