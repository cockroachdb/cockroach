 ├── columns: k:1!null i:2 f:3 s:4 j:5 one:8!null y:7!null
 ├── multiplicity: left-rows(zero-or-more), right-rows(zero-or-one)
 ├── fd: ()-->(8), (1)-->(2-5), (1)==(7), (7)==(1)
 ├── scan a
 │    ├── columns: k:1!null i:2 f:3 s:4 j:5
 │    ├── key: (1)
 │    └── fd: (1)-->(2-5)
 ├── project
 │    ├── columns: one:8!null y:7
 │    ├── fd: ()-->(8)
