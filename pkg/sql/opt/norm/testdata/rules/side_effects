# See Logical.CanHaveSideEffects comment for the optimizer's side-effect policy.

exec-ddl
CREATE TABLE a (k INT PRIMARY KEY, i INT, f FLOAT, s STRING, j JSON)
----

exec-ddl
CREATE TABLE xy (x INT PRIMARY KEY, y INT)
----

exec-ddl
CREATE TABLE uv (u INT PRIMARY KEY, v INT)
----

# Don't allow ORDER BY column to be eliminated if it has a side effect.
norm
SELECT * FROM a ORDER BY length('foo'), random()+1.0
----
sort
 ├── columns: k:1!null i:2 f:3 s:4 j:5  [hidden: column7:7]
 ├── side-effects
 ├── key: (1)
 ├── fd: (1)-->(2-5,7)
 ├── ordering: +7
 └── project
      ├── columns: column7:7 k:1!null i:2 f:3 s:4 j:5
      ├── side-effects
      ├── key: (1)
      ├── fd: (1)-->(2-5,7)
      ├── scan a
      │    ├── columns: k:1!null i:2 f:3 s:4 j:5
      │    ├── key: (1)
      │    └── fd: (1)-->(2-5)
      └── projections
           └── random() + 1.0 [side-effects]

# Don't allow GROUP BY column to be eliminated if it has a side effect.
norm
SELECT avg(f) FROM a WHERE i=5 GROUP BY i+(random()*10)::int, i+1
----
project
 ├── columns: avg:6
 ├── side-effects
 └── group-by
      ├── columns: avg:6 column7:7
      ├── grouping columns: column7:7
      ├── side-effects
      ├── key: (7)
      ├── fd: (7)-->(6)
      ├── project
      │    ├── columns: column7:7 f:3
      │    ├── side-effects
      │    ├── select
      │    │    ├── columns: i:2!null f:3
      │    │    ├── fd: ()-->(2)
      │    │    ├── scan a
      │    │    │    └── columns: i:2 f:3
      │    │    └── filters
      │    │         └── i = 5 [outer=(2), constraints=(/2: [/5 - /5]; tight), fd=()-->(2)]
      │    └── projections
      │         └── i + (random() * 10.0)::INT8 [outer=(2), side-effects]
      └── aggregations
           └── avg [outer=(3)] [aggregated as avg:6]
                └── variable: f:3

# Allow elimination of side effecting expressions during column pruning.
norm
SELECT i FROM (SELECT i, nextval('foo') FROM a)
----
scan a
 └── columns: i:2

# Allow duplication of side effecting expressions during predicate pushdown.
norm
SELECT * FROM a INNER JOIN xy ON k=x WHERE k=random()
----
inner-join (hash)
 ├── columns: k:1!null i:2 f:3 s:4 j:5 x:6!null y:7
 ├── side-effects
 ├── key: (6)
 ├── fd: (1)-->(2-5), (6)-->(7), (1)==(6), (6)==(1)
 ├── select
 │    ├── columns: k:1!null i:2 f:3 s:4 j:5
 │    ├── side-effects
 │    ├── key: (1)
 │    ├── fd: (1)-->(2-5)
 │    ├── scan a
 │    │    ├── columns: k:1!null i:2 f:3 s:4 j:5
 │    │    ├── key: (1)
 │    │    └── fd: (1)-->(2-5)
 │    └── filters
 │         └── k = random() [outer=(1), side-effects, constraints=(/1: (/NULL - ])]
 ├── select
 │    ├── columns: x:6!null y:7
 │    ├── side-effects
 │    ├── key: (6)
 │    ├── fd: (6)-->(7)
 │    ├── scan xy
 │    │    ├── columns: x:6!null y:7
 │    │    ├── key: (6)
 │    │    └── fd: (6)-->(7)
 │    └── filters
 │         └── x = random() [outer=(6), side-effects, constraints=(/6: (/NULL - ])]
 └── filters
      └── k = x [outer=(1,6), constraints=(/1: (/NULL - ]; /6: (/NULL - ]), fd=(1)==(6), (6)==(1)]

# Decorrelate CASE WHEN branch if there are no side effects.
norm
SELECT CASE WHEN i<0 THEN (SELECT y FROM xy WHERE x=i LIMIT 1) ELSE 5 END FROM a
----
project
 ├── columns: case:8
 ├── left-join (hash)
 │    ├── columns: i:2 x:6 y:7
 │    ├── fd: (6)-->(7)
 │    ├── scan a
 │    │    └── columns: i:2
 │    ├── scan xy
 │    │    ├── columns: x:6!null y:7
 │    │    ├── key: (6)
 │    │    └── fd: (6)-->(7)
 │    └── filters
 │         └── x = i [outer=(2,6), constraints=(/2: (/NULL - ]; /6: (/NULL - ]), fd=(2)==(6), (6)==(2)]
 └── projections
      └── CASE WHEN i < 0 THEN y ELSE 5 END [outer=(2,7)]

# Decorrelate CASE ELSE branch if there are no side effects.
norm
SELECT * FROM a WHERE (CASE WHEN i<0 THEN 5 ELSE (SELECT y FROM xy WHERE x=i LIMIT 1) END)=k
----
project
 ├── columns: k:1!null i:2 f:3 s:4 j:5
 ├── fd: (1)-->(2-5)
 └── select
      ├── columns: k:1!null i:2 f:3 s:4 j:5 x:6 y:7
      ├── key: (1,6)
      ├── fd: (1)-->(2-5), (6)-->(7)
      ├── left-join (hash)
      │    ├── columns: k:1!null i:2 f:3 s:4 j:5 x:6 y:7
      │    ├── key: (1,6)
      │    ├── fd: (1)-->(2-5), (6)-->(7)
      │    ├── scan a
      │    │    ├── columns: k:1!null i:2 f:3 s:4 j:5
      │    │    ├── key: (1)
      │    │    └── fd: (1)-->(2-5)
      │    ├── scan xy
      │    │    ├── columns: x:6!null y:7
      │    │    ├── key: (6)
      │    │    └── fd: (6)-->(7)
      │    └── filters
      │         └── x = i [outer=(2,6), constraints=(/2: (/NULL - ]; /6: (/NULL - ]), fd=(2)==(6), (6)==(2)]
      └── filters
           └── k = CASE WHEN i < 0 THEN 5 ELSE y END [outer=(1,2,7), constraints=(/1: (/NULL - ])]

# Don't decorrelate CASE WHEN branch if there are side effects.
norm
SELECT CASE WHEN i<0 THEN (SELECT y FROM xy WHERE x=i LIMIT (random()*10)::int) ELSE 5 END FROM a
----
project
 ├── columns: case:8
 ├── side-effects
 ├── scan a
 │    └── columns: i:2
 └── projections
      └── case [outer=(2), side-effects, correlated-subquery] [projected as case:8]
           ├── true
           ├── when
           │    ├── i < 0
           │    └── subquery
           │         └── project
           │              ├── columns: y:7
           │              ├── outer: (2)
           │              ├── cardinality: [0 - 1]
           │              ├── side-effects
           │              ├── key: ()
           │              ├── fd: ()-->(7)
           │              └── limit
           │                   ├── columns: x:6!null y:7
           │                   ├── outer: (2)
           │                   ├── cardinality: [0 - 1]
           │                   ├── side-effects
           │                   ├── key: ()
           │                   ├── fd: ()-->(6,7)
           │                   ├── select
           │                   │    ├── columns: x:6!null y:7
           │                   │    ├── outer: (2)
           │                   │    ├── cardinality: [0 - 1]
           │                   │    ├── key: ()
           │                   │    ├── fd: ()-->(6,7)
           │                   │    ├── scan xy
           │                   │    │    ├── columns: x:6!null y:7
           │                   │    │    ├── key: (6)
           │                   │    │    └── fd: (6)-->(7)
           │                   │    └── filters
           │                   │         └── x = i [outer=(2,6), constraints=(/2: (/NULL - ]; /6: (/NULL - ]), fd=(2)==(6), (6)==(2)]
           │                   └── (random() * 10.0)::INT8
           └── const: 5

# Don't decorrelate CASE ELSE branch if there are side effects.
norm
SELECT * FROM a WHERE (CASE WHEN i<0 THEN 5 ELSE (SELECT y FROM xy WHERE x=i AND 5/y>1) END)=k
----
select
 ├── columns: k:1!null i:2 f:3 s:4 j:5
 ├── side-effects
 ├── key: (1)
 ├── fd: (1)-->(2-5)
 ├── scan a
 │    ├── columns: k:1!null i:2 f:3 s:4 j:5
 │    ├── key: (1)
 │    └── fd: (1)-->(2-5)
 └── filters
      └── eq [outer=(1,2), side-effects, correlated-subquery, constraints=(/1: (/NULL - ])]
           ├── variable: k:1
           └── case
                ├── true
                ├── when
                │    ├── i < 0
                │    └── const: 5
                └── subquery
                     └── project
                          ├── columns: y:7
                          ├── outer: (2)
                          ├── cardinality: [0 - 1]
                          ├── side-effects
                          ├── key: ()
                          ├── fd: ()-->(7)
                          └── select
                               ├── columns: x:6!null y:7
                               ├── outer: (2)
                               ├── cardinality: [0 - 1]
                               ├── side-effects
                               ├── key: ()
                               ├── fd: ()-->(6,7)
                               ├── scan xy
                               │    ├── columns: x:6!null y:7
                               │    ├── key: (6)
                               │    └── fd: (6)-->(7)
                               └── filters
                                    ├── x = i [outer=(2,6), constraints=(/2: (/NULL - ]; /6: (/NULL - ]), fd=(2)==(6), (6)==(2)]
                                    └── (5 / y) > 1 [outer=(7), side-effects]


# Don't decorrelate IFERROR branch if there are side effects
norm
SELECT * FROM a WHERE IFERROR(1/0, (SELECT y::DECIMAL FROM xy WHERE x = i AND 5/y>1))=k
----
select
 ├── columns: k:1!null i:2 f:3 s:4 j:5
 ├── side-effects
 ├── key: (1)
 ├── fd: (1)-->(2-5)
 ├── scan a
 │    ├── columns: k:1!null i:2 f:3 s:4 j:5
 │    ├── key: (1)
 │    └── fd: (1)-->(2-5)
 └── filters
      └── eq [outer=(1,2), side-effects, correlated-subquery, constraints=(/1: (/NULL - ])]
           ├── variable: k:1
           └── if-err
                ├── 1 / 0
                └── else
                     └── subquery
                          └── project
                               ├── columns: y:8
                               ├── outer: (2)
                               ├── cardinality: [0 - 1]
                               ├── side-effects
                               ├── key: ()
                               ├── fd: ()-->(8)
                               ├── select
                               │    ├── columns: x:6!null xy.y:7
                               │    ├── outer: (2)
                               │    ├── cardinality: [0 - 1]
                               │    ├── side-effects
                               │    ├── key: ()
                               │    ├── fd: ()-->(6,7)
                               │    ├── scan xy
                               │    │    ├── columns: x:6!null xy.y:7
                               │    │    ├── key: (6)
                               │    │    └── fd: (6)-->(7)
                               │    └── filters
                               │         ├── x = i [outer=(2,6), constraints=(/2: (/NULL - ]; /6: (/NULL - ]), fd=(2)==(6), (6)==(2)]
                               │         └── (5 / xy.y) > 1 [outer=(7), side-effects]
                               └── projections
                                    └── xy.y::DECIMAL [outer=(7)]

# Decorrelate IFERROR branch if there are no side effects
norm
SELECT * FROM a WHERE IFERROR(1/0, (SELECT y::DECIMAL FROM xy WHERE x = i))=k
----
project
 ├── columns: k:1!null i:2 f:3 s:4 j:5
 ├── side-effects
 ├── fd: (1)-->(2-5)
 └── select
      ├── columns: k:1!null i:2 f:3 s:4 j:5 x:6 y:8
      ├── side-effects
      ├── key: (1,6)
      ├── fd: (1)-->(2-5), (6)-->(8)
      ├── left-join (hash)
      │    ├── columns: k:1!null i:2 f:3 s:4 j:5 x:6 y:8
      │    ├── key: (1,6)
      │    ├── fd: (1)-->(2-5), (6)-->(8)
      │    ├── scan a
      │    │    ├── columns: k:1!null i:2 f:3 s:4 j:5
      │    │    ├── key: (1)
      │    │    └── fd: (1)-->(2-5)
      │    ├── project
      │    │    ├── columns: y:8 x:6!null
      │    │    ├── key: (6)
      │    │    ├── fd: (6)-->(8)
      │    │    ├── scan xy
      │    │    │    ├── columns: x:6!null xy.y:7
      │    │    │    ├── key: (6)
      │    │    │    └── fd: (6)-->(7)
      │    │    └── projections
      │    │         └── xy.y::DECIMAL [outer=(7)]
      │    └── filters
      │         └── x = i [outer=(2,6), constraints=(/2: (/NULL - ]; /6: (/NULL - ]), fd=(2)==(6), (6)==(2)]
      └── filters
           └── k = IFERROR(1 / 0, y) [outer=(1,8), side-effects, constraints=(/1: (/NULL - ])]
