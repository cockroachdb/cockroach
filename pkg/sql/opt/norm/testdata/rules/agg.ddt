# The rule should not apply to these aggregations.
norm expect-not=EliminateAggDistinct
SELECT
    count(DISTINCT i),
    sum(DISTINCT i) FILTER (WHERE i > 5),
    sum_int(DISTINCT i),
    avg(DISTINCT i),
    stddev(DISTINCT f),
    variance(DISTINCT f),
    xor_agg(DISTINCT s::BYTES),
    array_agg(DISTINCT i),
    json_agg(DISTINCT j),
    regr_count(DISTINCT i, f)
FROM a
