 ├── columns: one:2
 ├── cardinality: [1 - 1]
 ├── volatile
 ├── key: ()
 ├── fd: ()-->(2)
 └── tuple
      └── udf: one
           └── values
                ├── columns: "?column?":1!null
                ├── cardinality: [1 - 1]
                ├── key: ()
                ├── fd: ()-->(1)
