exec-ddl
CREATE TABLE a (k INT PRIMARY KEY, i INT, f FLOAT, s STRING, j JSON)
----
TABLE a
 ├── k int not null
 ├── i int
 ├── f float
 ├── s string
 ├── j jsonb
 └── INDEX primary
      └── k int not null

exec-ddl
CREATE TABLE t.b (x INT PRIMARY KEY, y INT)
----
TABLE b
 ├── x int not null
 ├── y int
 └── INDEX primary
      └── x int not null

# --------------------------------------------------
# EliminateLimit
# --------------------------------------------------
opt
SELECT * FROM (SELECT * FROM a LIMIT 99) LIMIT 100
----
scan a
 ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 ├── limit: 99
 └── keys: (1)

opt
SELECT * FROM (SELECT * FROM a LIMIT 100) LIMIT 100
----
scan a
 ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 ├── limit: 100
 └── keys: (1)

# Don't eliminate the outer limit if it's less than the inner.
opt
SELECT * FROM (SELECT * FROM a LIMIT 100) LIMIT 99
----
limit
 ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 ├── cardinality: [0 - 99]
 ├── keys: (1)
 ├── scan a
 │    ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 │    ├── limit: 100
 │    └── keys: (1)
 └── const: 99 [type=int]

# High limits (> max uint32), can't eliminate in this case.
opt
SELECT * FROM (SELECT * FROM a LIMIT 5000000000) LIMIT 5100000000
----
limit
 ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 ├── keys: (1)
 ├── limit
 │    ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 │    ├── keys: (1)
 │    ├── scan a
 │    │    ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 │    │    └── keys: (1)
 │    └── const: 5000000000 [type=int]
 └── const: 5100000000 [type=int]

# Don't eliminate in case of zero or negative limit.
opt
SELECT * FROM (SELECT * FROM a LIMIT 0) LIMIT -1
----
limit
 ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 ├── cardinality: [0 - 0]
 ├── keys: (1)
 ├── limit
 │    ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 │    ├── cardinality: [0 - 0]
 │    ├── keys: (1)
 │    ├── scan a
 │    │    ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 │    │    └── keys: (1)
 │    └── const: 0 [type=int]
 └── const: -1 [type=int]

# --------------------------------------------------
# PushLimitIntoProject
# --------------------------------------------------
opt
SELECT k, f*2.0 FROM a LIMIT 5
----
project
 ├── columns: k:1(int!null) "f * 2.0":6(float)
 ├── cardinality: [0 - 5]
 ├── keys: (1)
 ├── scan a
 │    ├── columns: k:1(int!null) f:3(float)
 │    ├── limit: 5
 │    └── keys: (1)
 └── projections [outer=(1,3)]
      └── a.f * 2.0 [type=float, outer=(3)]

# Detect PushLimitIntoProject and FilterUnusedLimitCols dependency cycle.
opt
SELECT f, f+1.1 FROM (SELECT f, i FROM a GROUP BY f, i) a ORDER BY f LIMIT 5
----
project
 ├── columns: f:3(float) "f + 1.1":6(float)
 ├── cardinality: [0 - 5]
 ├── ordering: +3
 ├── limit
 │    ├── columns: i:2(int) f:3(float)
 │    ├── cardinality: [0 - 5]
 │    ├── keys: weak(2,3)
 │    ├── ordering: +3
 │    ├── sort
 │    │    ├── columns: i:2(int) f:3(float)
 │    │    ├── keys: weak(2,3)
 │    │    ├── ordering: +3
 │    │    └── group-by
 │    │         ├── columns: i:2(int) f:3(float)
 │    │         ├── grouping columns: i:2(int) f:3(float)
 │    │         ├── keys: weak(2,3)
 │    │         └── scan a
 │    │              └── columns: i:2(int) f:3(float)
 │    └── const: 5 [type=int]
 └── projections [outer=(3)]
      └── a.f + 1.1 [type=float, outer=(3)]

# Don't push zero limit into Scan.
opt
SELECT * FROM a LIMIT 0
----
limit
 ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 ├── cardinality: [0 - 0]
 ├── keys: (1)
 ├── scan a
 │    ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 │    └── keys: (1)
 └── const: 0 [type=int]

# Don't push negative limit into Scan.
opt
SELECT * FROM a LIMIT -1
----
limit
 ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 ├── cardinality: [0 - 0]
 ├── keys: (1)
 ├── scan a
 │    ├── columns: k:1(int!null) i:2(int) f:3(float) s:4(string) j:5(jsonb)
 │    └── keys: (1)
 └── const: -1 [type=int]

# --------------------------------------------------
# PushOffsetIntoProject
# --------------------------------------------------
opt
SELECT k, f*2.0 FROM a OFFSET 5
----
project
 ├── columns: k:1(int!null) "f * 2.0":6(float)
 ├── keys: (1)
 ├── offset
 │    ├── columns: k:1(int!null) f:3(float)
 │    ├── keys: (1)
 │    ├── scan a
 │    │    ├── columns: k:1(int!null) f:3(float)
 │    │    └── keys: (1)
 │    └── const: 5 [type=int]
 └── projections [outer=(1,3)]
      └── a.f * 2.0 [type=float, outer=(3)]

# Detect PushOffsetIntoProject and FilterUnusedOffsetCols dependency cycle.
opt
SELECT f, f+1.1 FROM (SELECT f, i FROM a GROUP BY f, i) a ORDER BY f OFFSET 5
----
project
 ├── columns: f:3(float) "f + 1.1":6(float)
 ├── ordering: +3
 ├── offset
 │    ├── columns: i:2(int) f:3(float)
 │    ├── keys: weak(2,3)
 │    ├── ordering: +3
 │    ├── sort
 │    │    ├── columns: i:2(int) f:3(float)
 │    │    ├── keys: weak(2,3)
 │    │    ├── ordering: +3
 │    │    └── group-by
 │    │         ├── columns: i:2(int) f:3(float)
 │    │         ├── grouping columns: i:2(int) f:3(float)
 │    │         ├── keys: weak(2,3)
 │    │         └── scan a
 │    │              └── columns: i:2(int) f:3(float)
 │    └── const: 5 [type=int]
 └── projections [outer=(3)]
      └── a.f + 1.1 [type=float, outer=(3)]

# --------------------------------------------------
# PushLimitIntoProject + PushOffsetIntoProject
# --------------------------------------------------
opt
SELECT k, f*2.0 FROM a OFFSET 5 LIMIT 10
----
project
 ├── columns: k:1(int!null) "f * 2.0":6(float)
 ├── cardinality: [0 - 10]
 ├── keys: (1)
 ├── limit
 │    ├── columns: k:1(int!null) f:3(float)
 │    ├── cardinality: [0 - 10]
 │    ├── keys: (1)
 │    ├── offset
 │    │    ├── columns: k:1(int!null) f:3(float)
 │    │    ├── keys: (1)
 │    │    ├── scan a
 │    │    │    ├── columns: k:1(int!null) f:3(float)
 │    │    │    └── keys: (1)
 │    │    └── const: 5 [type=int]
 │    └── const: 10 [type=int]
 └── projections [outer=(1,3)]
      └── a.f * 2.0 [type=float, outer=(3)]

opt
SELECT f, f+1.1 FROM (SELECT f, i FROM a GROUP BY f, i) a ORDER BY f OFFSET 5 LIMIT 10
----
project
 ├── columns: f:3(float) "f + 1.1":6(float)
 ├── cardinality: [0 - 10]
 ├── ordering: +3
 ├── limit
 │    ├── columns: i:2(int) f:3(float)
 │    ├── cardinality: [0 - 10]
 │    ├── keys: weak(2,3)
 │    ├── ordering: +3
 │    ├── offset
 │    │    ├── columns: i:2(int) f:3(float)
 │    │    ├── keys: weak(2,3)
 │    │    ├── ordering: +3
 │    │    ├── sort
 │    │    │    ├── columns: i:2(int) f:3(float)
 │    │    │    ├── keys: weak(2,3)
 │    │    │    ├── ordering: +3
 │    │    │    └── group-by
 │    │    │         ├── columns: i:2(int) f:3(float)
 │    │    │         ├── grouping columns: i:2(int) f:3(float)
 │    │    │         ├── keys: weak(2,3)
 │    │    │         └── scan a
 │    │    │              └── columns: i:2(int) f:3(float)
 │    │    └── const: 5 [type=int]
 │    └── const: 10 [type=int]
 └── projections [outer=(3)]
      └── a.f + 1.1 [type=float, outer=(3)]
