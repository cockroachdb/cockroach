limit
 ├── columns: k:1!null i:2 f:3 s:4 j:5 ordinality:6!null
 ├── internal-ordering: +2
 ├── cardinality: [0 - 10]
 ├── key: (1)
 ├── fd: (1)-->(2-6), (6)-->(1-5)
