 │    ├── fd: (1)-->(2-6), (6)-->(1-5)
 │    ├── ordering: +2
 │    ├── limit hint: 10.00
 │    └── ordinality
 │         ├── columns: k:1!null i:2 f:3 s:4 j:5 ordinality:6!null
 │         ├── key: (1)
 │         ├── fd: (1)-->(2-6), (6)-->(1-5)
 │         └── scan a
 │              ├── columns: k:1!null i:2 f:3 s:4 j:5
 │              ├── key: (1)
 │              ├── fd: (1)-->(2-5)
 │              └── ordering: +1
 └── 10

