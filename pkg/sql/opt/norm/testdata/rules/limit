 │    │    └── fd: (3)-->(4)
 │    └── filters
 │         └── a:1 = u:3 [outer=(1,3), constraints=(/1: (/NULL - ]; /3: (/NULL - ]), fd=(1)==(3), (3)==(1)]
 └── 10

# Ordering can be pushed down.
norm expect=PushLimitIntoLeftJoin
SELECT * FROM ab LEFT JOIN uv ON a = u ORDER BY a LIMIT 10
