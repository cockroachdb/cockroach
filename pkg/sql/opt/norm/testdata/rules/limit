 │    ├── columns: k:1!null i:2 y:9 row_number:12
 │    ├── fd: (1)-->(2)
 │    ├── left-join (hash)
 │    │    ├── columns: k:1!null i:2 y:9
 │    │    ├── fd: (1)-->(2)
 │    │    ├── offset
 │    │    │    ├── columns: k:1!null i:2
 │    │    │    ├── cardinality: [0 - 7]
 │    │    │    ├── key: (1)
 │    │    │    ├── fd: (1)-->(2)
 │    │    │    ├── limit
 │    │    │    │    ├── columns: k:1!null i:2
 │    │    │    │    ├── cardinality: [0 - 10]
 │    │    │    │    ├── key: (1)
 │    │    │    │    ├── fd: (1)-->(2)
 │    │    │    │    ├── scan a
 │    │    │    │    │    ├── columns: k:1!null i:2
 │    │    │    │    │    ├── key: (1)
 │    │    │    │    │    ├── fd: (1)-->(2)
 │    │    │    │    │    └── limit hint: 10.00
 │    │    │    │    └── 10
 │    │    │    └── 3
 │    │    ├── scan b
 │    │    │    └── columns: y:9
 │    │    └── filters
 │    │         └── i:2 = y:9 [outer=(2,9), constraints=(/2: (/NULL - ]; /9: (/NULL - ]), fd=(2)==(9), (9)==(2)]
