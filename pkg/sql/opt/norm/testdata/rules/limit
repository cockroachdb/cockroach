 ├── columns: a:1!null b:2 u:5 v:6
 ├── cardinality: [0 - 10]
 ├── key: (1)
 ├── fd: (1)-->(2,5,6), (5)-->(6)
 ├── ordering: +1
 └── left-join (hash)
      ├── columns: a:1!null b:2 u:5 v:6
      ├── cardinality: [0 - 10]
      ├── multiplicity: left-rows(exactly-one), right-rows(zero-or-one)
      ├── key: (1)
      ├── fd: (1)-->(2,5,6), (5)-->(6)
      ├── limit
      │    ├── columns: a:1!null b:2
      │    ├── internal-ordering: +1
      │    ├── cardinality: [0 - 10]
      │    ├── key: (1)
      │    ├── fd: (1)-->(2)
      │    ├── scan ab
      │    │    ├── columns: a:1!null b:2
      │    │    ├── key: (1)
      │    │    ├── fd: (1)-->(2)
      │    │    ├── ordering: +1
      │    │    └── limit hint: 10.00
      │    └── 10
      ├── scan uv
      │    ├── columns: u:5!null v:6
      │    ├── key: (5)
      │    └── fd: (5)-->(6)
      └── filters
           └── a:1 = u:5 [outer=(1,5), constraints=(/1: (/NULL - ]; /5: (/NULL - ]), fd=(1)==(5), (5)==(1)]

norm expect=PushLimitIntoJoinLeft
