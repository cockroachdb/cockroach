 │    │    ├── fd: (1)-->(2)
 │    │    ├── scan a
 │    │    │    ├── columns: k:1!null i:2
 │    │    │    ├── key: (1)
 │    │    │    └── fd: (1)-->(2)
 │    │    ├── inner-join-apply
 │    │    │    ├── columns: r:6 s:7 column1:8
 │    │    │    ├── outer: (1,2)
 │    │    │    ├── cardinality: [3 - 3]
 │    │    │    ├── fd: ()-->(6,7)
 │    │    │    ├── inner-join (cross)
 │    │    │    │    ├── columns: r:6 s:7
 │    │    │    │    ├── outer: (1,2)
 │    │    │    │    ├── cardinality: [1 - 1]
 │    │    │    │    ├── key: ()
 │    │    │    │    ├── fd: ()-->(6,7)
 │    │    │    │    ├── values
 │    │    │    │    │    ├── columns: r:6
 │    │    │    │    │    ├── outer: (2)
 │    │    │    │    │    ├── cardinality: [1 - 1]
 │    │    │    │    │    ├── key: ()
 │    │    │    │    │    ├── fd: ()-->(6)
 │    │    │    │    │    └── (i:2 + 1,)
 │    │    │    │    ├── values
 │    │    │    │    │    ├── columns: s:7
 │    │    │    │    │    ├── outer: (1)
 │    │    │    │    │    ├── cardinality: [1 - 1]
 │    │    │    │    │    ├── key: ()
 │    │    │    │    │    ├── fd: ()-->(7)
