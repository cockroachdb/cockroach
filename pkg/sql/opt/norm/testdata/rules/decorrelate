 │         │    ├── key: (4)
 │         │    └── fd: (4)-->(5)
 │         └── filters
 │              └── k:4 = u:1 [outer=(1,4), constraints=(/1: (/NULL - ]; /4: (/NULL - ]), fd=(1)==(4), (4)==(1)]
 └── windows
      └── row-number [as=row_number:10]

