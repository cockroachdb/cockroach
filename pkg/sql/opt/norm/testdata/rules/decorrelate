 │    │    ├── cardinality: [0 - 1]
 │    │    ├── key: ()
 │    │    ├── fd: ()-->(6,7)
 │    │    ├── select
 │    │    │    ├── columns: x:6(int!null) y:7(int!null)
 │    │    │    ├── outer: (1)
 │    │    │    ├── key: (6)
 │    │    │    ├── fd: ()-->(7)
 │    │    │    ├── scan xy
 │    │    │    │    ├── columns: x:6(int!null) y:7(int)
 │    │    │    │    ├── key: (6)
 │    │    │    │    └── fd: (6)-->(7)
 │    │    │    └── filters
