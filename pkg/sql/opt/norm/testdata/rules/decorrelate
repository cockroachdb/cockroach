 │              │    └── description:3
 │              ├── const-agg [as=title:4, outer=(4)]
 │              │    └── title:4
 │              ├── const-agg [as=slug:5, outer=(5)]
 │              │    └── slug:5
 │              ├── const-agg [as=tag_list:6, outer=(6)]
 │              │    └── tag_list:6
