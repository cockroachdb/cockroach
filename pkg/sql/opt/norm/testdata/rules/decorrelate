 │              ├── 0
 │              └── subquery
 │                   └── max1-row
 │                        ├── columns: generate_series:9
 │                        ├── error: "more than one row returned by a subquery used as an expression"
 │                        ├── cardinality: [0 - 1]
 │                        ├── immutable
 │                        ├── key: ()
