# ALL with non-trivial expression on left.
opt
SELECT i*i/100 < ALL(SELECT y FROM xy), s FROM a
----
project
 ├── columns: column8:8(bool) s:4(string)
 ├── inner-join-apply
 │    ├── columns: a.s:4(string) scalar:9(decimal) case:12(bool)
 │    ├── project
 │    │    ├── columns: a.s:4(string) scalar:9(decimal)
 │    │    ├── scan a
 │    │    │    └── columns: a.i:2(int) a.s:4(string)
 │    │    └── projections [outer=(2,4)]
 │    │         ├── variable: a.s [type=string, outer=(4)]
 │    │         └── div [type=decimal, outer=(2)]
 │    │              ├── mult [type=int, outer=(2)]
