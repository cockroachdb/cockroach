      ├── select
      │    ├── columns: x:7!null y:8!null
      │    ├── cardinality: [0 - 1]
      │    ├── key: ()
      │    ├── fd: ()-->(7,8)
      │    ├── limit
      │    │    ├── columns: x:7!null y:8
      │    │    ├── cardinality: [0 - 1]
      │    │    ├── key: ()
      │    │    ├── fd: ()-->(7,8)
      │    │    ├── scan xy
      │    │    │    ├── columns: x:7!null y:8
      │    │    │    ├── key: (7)
      │    │    │    ├── fd: (7)-->(8)
      │    │    │    └── limit hint: 1.00
      │    │    └── 1
      │    └── filters
      │         └── y:8 > 9 [outer=(8), constraints=(/8: [/10 - ]; tight)]
      └── filters
           └── x:7 = k:1 [outer=(1,7), constraints=(/1: (/NULL - ]; /7: (/NULL - ]), fd=(1)==(7), (7)==(1)]

