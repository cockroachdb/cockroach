 ├── columns: u:1!null v:2 row_number:10 i:5
 ├── key: (1)
 ├── fd: (1)-->(2,5)
 ├── project
 │    ├── columns: u:1!null v:2 i:5
 │    ├── key: (1)
 │    ├── fd: (1)-->(2,5)
 │    └── inner-join (hash)
