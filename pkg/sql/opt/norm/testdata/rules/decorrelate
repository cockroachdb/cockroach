 │    ├── fd: (1)-->(14)
 │    ├── left-join (hash)
 │    │    ├── columns: k:1!null x:8 true:13
 │    │    ├── multiplicity: left-rows(exactly-one), right-rows(zero-or-one)
 │    │    ├── key: (1)
 │    │    ├── fd: (1)-->(8,13)
 │    │    ├── scan a
 │    │    │    ├── columns: k:1!null
