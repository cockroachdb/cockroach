      ├── key: (1,6)
      ├── fd: (1)-->(2), (6)-->(7)
      ├── scan a
      │    ├── columns: k:1(int!null) i:2(int)
      │    ├── key: (1)
      │    └── fd: (1)-->(2)
      ├── scan xy
      │    ├── columns: x:6(int!null) y:7(int)
      │    ├── key: (6)
      │    └── fd: (6)-->(7)
      └── filters
           └── x = (k + 1) [type=bool, outer=(1,6), constraints=(/6: (/NULL - ])]

