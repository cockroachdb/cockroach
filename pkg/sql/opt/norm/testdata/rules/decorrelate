      ├── immutable
      ├── fd: (8)-->(9), (8)==(13), (13)==(8)
      ├── project
      │    ├── columns: column13:13!null i:2
      │    ├── immutable
      │    ├── scan a
      │    │    ├── columns: k:1!null i:2
      │    │    ├── key: (1)
      │    │    └── fd: (1)-->(2)
      │    └── projections
