 │         ├── a.s = 'foo' [type=bool, outer=(4), constraints=(/4: [/'foo' - /'foo']; tight)]
 │         └── a.i > 1 [type=bool, outer=(2), constraints=(/2: [/2 - ]; tight)]
 ├── scan xy
