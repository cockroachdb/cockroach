 │    │    ├── fd: (1)-->(7)
 │    │    ├── left-join (hash)
 │    │    │    ├── columns: k:1!null i:2 y:7
 │    │    │    ├── fd: (1)-->(2)
 │    │    │    ├── scan a
 │    │    │    │    ├── columns: k:1!null i:2
 │    │    │    │    ├── key: (1)
 │    │    │    │    └── fd: (1)-->(2)
 │    │    │    ├── scan xy
 │    │    │    │    └── columns: y:7
 │    │    │    └── filters
 │    │    │         └── y:7 = i:2 [outer=(2,7), constraints=(/2: (/NULL - ]; /7: (/NULL - ]), fd=(2)==(7), (7)==(2)]
 │    │    └── aggregations
 │    │         └── const-agg [as=y:7, outer=(7)]
 │    │              └── y:7
 │    └── projections
 │         └── y:7 [as=column8:8, outer=(7)]
 └── aggregations
      └── max [as=max:9, outer=(8)]
           └── column8:8

