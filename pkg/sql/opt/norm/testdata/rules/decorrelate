      │    └── key: (1)
      ├── project
      │    ├── columns: "?column?":9(int) "?column?":8(int) y:7(int)
      │    ├── outer: (1)
      │    ├── fd: ()-->(8)
      │    ├── scan xy
      │    │    ├── columns: x:6(int!null) y:7(int)
      │    │    ├── key: (6)
      │    │    └── fd: (6)-->(7)
      │    └── projections
      │         ├── x + 1 [type=int, outer=(6)]
