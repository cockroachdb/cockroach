 │    ├── group-by
 │    │    ├── columns: x:1!null y:2!null max:13
 │    │    ├── grouping columns: x:1!null
 │    │    ├── key: (1)
 │    │    ├── fd: ()-->(2), (1)-->(2,13)
 │    │    ├── inner-join (hash)
 │    │    │    ├── columns: x:1!null y:2!null k:6!null s:9
 │    │    │    ├── multiplicity: left-rows(zero-or-one), right-rows(zero-or-more)
 │    │    │    ├── key: (1)
 │    │    │    ├── fd: ()-->(2,6,9), (2)==(6), (6)==(2)
 │    │    │    ├── scan xy
 │    │    │    │    ├── columns: x:1!null y:2
 │    │    │    │    ├── key: (1)
 │    │    │    │    └── fd: (1)-->(2)
 │    │    │    ├── limit
 │    │    │    │    ├── columns: k:6!null s:9
 │    │    │    │    ├── cardinality: [0 - 1]
 │    │    │    │    ├── key: ()
 │    │    │    │    ├── fd: ()-->(6,9)
