 │    │    │    ├── columns: column13:13!null a.k:7!null a.s:10
 │    │    │    ├── key: (7)
 │    │    │    ├── fd: ()-->(13), (7)-->(10)
 │    │    │    ├── scan a
 │    │    │    │    ├── columns: a.k:7!null a.s:10
