      ├── key: (1,6)
      ├── fd: (6)-->(7)
      ├── right-join
      │    ├── columns: k:1(int) x:6(int!null) y:7(int)
      │    ├── key: (1,6)
      │    ├── fd: (6)-->(7)
      │    ├── scan a
      │    │    ├── columns: k:1(int!null)
      │    │    └── key: (1)
      │    ├── scan xy
      │    │    ├── columns: x:6(int!null) y:7(int)
      │    │    ├── key: (6)
      │    │    └── fd: (6)-->(7)
      │    └── true [type=bool]
      └── filters [type=bool, outer=(1,6)]
           └── (a.k + 1) = (xy.x + 1) [type=bool, outer=(1,6)]

# Hoist Exists in join filter disjunction.
opt
