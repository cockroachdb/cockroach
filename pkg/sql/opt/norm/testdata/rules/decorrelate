 │    │    ├── fd: (5)-->(6), (9)-->(10), (5)==(9), (9)==(5)
 │    │    ├── scan xy
 │    │    │    ├── columns: x:1!null
 │    │    │    └── key: (1)
 │    │    ├── inner-join (hash)
 │    │    │    ├── columns: u:5!null uv.v:6 k:9!null i:10
 │    │    │    ├── multiplicity: left-rows(zero-or-one), right-rows(zero-or-one)
 │    │    │    ├── key: (9)
 │    │    │    ├── fd: (5)-->(6), (9)-->(10), (5)==(9), (9)==(5)
 │    │    │    ├── scan uv
 │    │    │    │    ├── columns: u:5!null uv.v:6
 │    │    │    │    ├── key: (5)
 │    │    │    │    └── fd: (5)-->(6)
 │    │    │    ├── scan a
