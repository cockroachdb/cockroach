 │    ├── internal-ordering: +9
 │    ├── key: (1)
 │    ├── fd: (1)-->(2,14,15)
 │    ├── sort
 │    │    ├── columns: k:1!null i:2 y:9 canary:14
 │    │    ├── fd: (1)-->(2)
