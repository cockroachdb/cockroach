SELECT *
FROM xy, uv
WHERE x=v AND (SELECT max(i) FROM a WHERE k=x) IS DISTINCT FROM u
----
project
 ├── columns: x:1!null y:2 u:5!null v:6!null
 ├── key: (5)
 ├── fd: (1)-->(2), (5)-->(1,2,6), (1)==(6), (6)==(1)
 └── select
      ├── columns: x:1!null y:2 u:5!null v:6!null max:16
      ├── key: (5)
      ├── fd: (1)-->(2), (5)-->(1,2,6,16), (1)==(6), (6)==(1)
