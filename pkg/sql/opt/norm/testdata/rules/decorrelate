 │    │    ├── multiplicity: left-rows(exactly-one), right-rows(zero-or-more)
 │    │    ├── key: (1)
 │    │    ├── fd: (1)-->(2,7,10,13), (7)-->(10)
 │    │    ├── scan a [as=a2]
 │    │    │    ├── columns: a2.k:1!null a2.i:2
