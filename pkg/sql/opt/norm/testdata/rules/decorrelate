                │         └── true [type=bool]
                └── filters
                     └── y = i [type=bool, outer=(2,7), constraints=(/2: (/NULL - ]; /7: (/NULL - ]), fd=(2)==(7), (7)==(2)]

# Don't hoist uncorrelated subquery.
opt
SELECT i < ANY(SELECT y FROM xy) AS r FROM a
----
project
 ├── columns: r:8(bool)
 ├── scan a
 │    └── columns: i:2(int)
 └── projections
      └── any: lt [type=bool, outer=(2)]
           ├── scan xy
           │    └── columns: y:7(int)
           └── variable: i [type=int]

# --------------------------------------------------
# HoistJoinSubquery
# --------------------------------------------------
