 ├── scan xy
 │    ├── columns: xy.x:8(int!null) xy.y:9(int)
 │    └── keys: (8)
 └── filters [type=bool, outer=(2,8)]
      └── eq [type=bool, outer=(2,8)]
           ├── variable: xy.x [type=int, outer=(8)]
           └── variable: a.i [type=int, outer=(2)]

