 ├── columns: k:1!null i:2 f:3 s:4 j:5
 ├── key: (1)
 ├── fd: (1)-->(2-5)
 └── select
      ├── columns: k:1!null i:2 f:3 s:4 j:5 x:8!null
      ├── key: (1)
      ├── fd: (1)-->(2-5), (1)==(8), (8)==(1)
