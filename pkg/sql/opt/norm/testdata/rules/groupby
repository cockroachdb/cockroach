           │    │    │    ├── cardinality: [2 - 2]
           │    │    │    ├── (1, NULL, NULL)
           │    │    │    └── (1, NULL, NULL)
           │    │    └── projections
           │    │         ├── CAST(NULL AS FLOAT8) [as=column9:9]
           │    │         └── CAST(NULL AS JSONB) [as=column10:10]
           │    ├── scan a
           │    │    ├── columns: i:12!null s:14!null
           │    │    └── key: (12,14)
           │    └── filters
           │         ├── column2:7 = s:14 [outer=(7,14), constraints=(/7: (/NULL - ]; /14: (/NULL - ]), fd=(7)==(14), (14)==(7)]
