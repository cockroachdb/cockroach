 ├── key: ()
 ├── fd: ()-->(7,9)
 ├── project
 │    ├── columns: column6:6!null column8:8!null k:1!null i:2!null
 │    ├── key: (1)
 │    ├── fd: (1)-->(2,6), (2)-->(8)
 │    ├── scan a
 │    │    ├── columns: k:1!null i:2!null
 │    │    ├── key: (1)
 │    │    └── fd: (1)-->(2)
 │    └── projections
 │         ├── k:1 > 0 [as=column6:6, outer=(1)]
 │         └── i:2 > 0 [as=column8:8, outer=(2)]
 └── aggregations
      ├── agg-filter [as=sum:7, outer=(1,6)]
      │    ├── sum
      │    │    └── k:1
      │    └── column6:6
      └── agg-filter [as=sum:9, outer=(2,8)]
           ├── agg-distinct
           │    └── sum
           │         └── i:2
           └── column8:8

