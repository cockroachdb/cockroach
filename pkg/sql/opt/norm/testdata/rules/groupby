      ├── key: (2,4)
      ├── fd: (2,4)-->(6)
      ├── scan a
      │    ├── columns: i:2!null f:3 s:4!null
      │    ├── key: (2,4)
      │    └── fd: (2,4)-->(3), (2,3)~~>(4)
      └── aggregations
           └── min [as=min:6, outer=(3)]
                └── f:3

# All grouping columns eliminated.
