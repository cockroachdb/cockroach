 │    │    ├── columns: column1:1
 │    │    └── cardinality: [1 - ]
 │    ├── values
 │    │    ├── columns: column1:1!null
 │    │    ├── cardinality: [1 - 1]
 │    │    ├── key: ()
 │    │    ├── fd: ()-->(1)
 │    │    └── (1,)
 │    └── project
 │         ├── columns: "?column?":4!null
 │         ├── immutable
 │         ├── select
