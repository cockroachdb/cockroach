 │         └── limit
 │              ├── columns: i:6!null n:7!null rownum:11!null
 │              ├── internal-ordering: +7 opt(6,11)
 │              ├── cardinality: [0 - 1]
 │              ├── key: ()
 │              ├── fd: ()-->(6,7,11)
 │              ├── sort
 │              │    ├── columns: i:6!null n:7!null rownum:11!null
 │              │    ├── fd: ()-->(6,11)
