 ├── cost: 0.04
 ├── key: ()
 ├── fd: ()-->(2)
 ├── prune: (2)
 ├── values
 │    ├── columns: "?column?":1(int!null)
 │    ├── cardinality: [1 - 1]
 │    ├── stats: [rows=1]
 │    ├── cost: 0.02
 │    ├── key: ()
 │    ├── fd: ()-->(1)
 │    ├── prune: (1)
 │    └── tuple [type=tuple{int}]
 │         └── const: 1 [type=int]
 └── with-scan &1 (foo)
      ├── columns: "?column?":2(int!null)
