 │    ├── working table binding: &2
 │    ├── initial columns: column1:1
 │    ├── recursive columns: "?column?":4
 │    ├── cardinality: [3 - ]
 │    ├── immutable
 │    ├── fake-rel
 │    │    ├── columns: i:2
 │    │    └── cardinality: [1 - 3]
 │    ├── values
 │    │    ├── columns: column1:1!null
 │    │    ├── cardinality: [3 - 3]
 │    │    ├── (0,)
 │    │    ├── (1,)
 │    │    └── (2,)
 │    └── project
 │         ├── columns: "?column?":4
 │         ├── cardinality: [1 - 3]
 │         ├── immutable
 │         ├── with-scan &2 (cte)
