 │    ├── insert-mapping:
 │    │    ├── column1:7 => a.k:1
 │    │    ├── column8:8 => a.i:2
 │    │    ├── column9:9 => a.f:3
 │    │    ├── column10:10 => a.s:4
 │    │    └── column11:11 => a.j:5
 │    ├── cardinality: [1 - 1]
 │    ├── volatile, mutations
 │    ├── key: ()
 │    ├── fd: ()-->(1-5)
 │    └── values
 │         ├── columns: column1:7!null column8:8 column9:9 column10:10 column11:11
 │         ├── cardinality: [1 - 1]
 │         ├── key: ()
 │         ├── fd: ()-->(7-11)
 │         └── (1, NULL, NULL, NULL, NULL)
 └── with-scan &1 (foo)
      ├── columns: k:12!null i:13 f:14 s:15 j:16
