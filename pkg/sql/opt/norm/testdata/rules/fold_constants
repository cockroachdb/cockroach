 ├── columns: "?column?":1(varbit!null)
 ├── cardinality: [1 - 1]
 ├── key: ()
 ├── fd: ()-->(1)
 ├── values
 │    ├── cardinality: [1 - 1]
 │    ├── key: ()
 │    └── tuple [type=tuple]
 └── projections
      └── const: B'10' [type=varbit]

# Don't fold: cannot mix bit array sizes error.
opt expect-not=FoldBinary
SELECT B'01' # B'11001001010101'
----
