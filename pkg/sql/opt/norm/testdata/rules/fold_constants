# Don't fold: divide by zero error.
opt expect-not=FoldBinary
SELECT 1::INT / 0::INT
----
project
 ├── columns: "?column?":1(decimal)
 ├── cardinality: [1 - 1]
 ├── side-effects
 ├── key: ()
 ├── fd: ()-->(1)
 ├── values
 │    ├── cardinality: [1 - 1]
 │    ├── key: ()
 │    └── tuple [type=tuple]
 └── projections [side-effects]
      └── 1 / 0 [type=decimal, side-effects]

# Fold constant.
opt expect=FoldBinary
SELECT B'01' # B'11'
----
