SELECT * FROM a INNER JOIN xy ON NULL
----
values
 ├── columns: k:1(int!null) i:2(int!null) f:3(float!null) s:4(string!null) j:5(jsonb!null) x:6(int!null) y:7(int!null)
 ├── cardinality: [0 - 0]
 ├── key: ()
 └── fd: ()-->(1-7)

