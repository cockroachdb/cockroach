 │              ├── variable: a.i [type=int, outer=(2)]
 │              └── const: 2 [type=int]
 └── filters [type=bool, outer=(6), constraints=(/6: [/2 - /2]; tight)]
      └── eq [type=bool, outer=(6), constraints=(/6: [/2 - /2]; tight)]
           ├── variable: div [type=decimal, outer=(6)]
           └── const: 2 [type=decimal]

