 │    ├── key: (6)
 │    └── fd: (6)-->(7)
 └── filters
      ├── k = x [type=bool, outer=(1,6), constraints=(/1: (/NULL - ]; /6: (/NULL - ]), fd=(1)==(6), (6)==(1)]
