system.descriptor  CREATE TABLE descriptor (
		   id INT NOT NULL,
		   descriptor BYTES NULL,
		   CONSTRAINT "primary" PRIMARY KEY (id ASC),
		   FAMILY "primary" (id),
		   FAMILY fam_2_descriptor (descriptor)
)


