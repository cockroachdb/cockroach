# SELECT queries - used to exercise the KV tracing for other tests

statement ok
CREATE TABLE abc (
  a INT,
  b TEXT,
  c FLOAT,
  PRIMARY KEY (a, b),
  UNIQUE INDEX foo (b),
  INDEX bar (a),
  FAMILY (a, b),
  FAMILY (c)
)

query T
SELECT message FROM [SHOW KV TRACE FOR SELECT * FROM abc]
 WHERE message LIKE 'fetched:%' OR message LIKE 'output row%'
----

statement ok
