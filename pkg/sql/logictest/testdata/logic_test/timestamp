subtest regression_44774

statement ok
CREATE TABLE regression_44774 (
  a timestamp(3) DEFAULT '1970-02-03 12:13:14.123456',
  b timestamptz(3) DEFAULT '1970-02-03 12:13:14.123456'
)

statement ok
