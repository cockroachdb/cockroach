SET TIME ZONE -3

query R
SELECT extract(hour FROM '2001-01-01 13:00:00+01'::timestamptz)
----
9

query R
SELECT extract(hour FROM '2001-01-01 13:00:00'::timestamp)
----
13

query R
SELECT extract(timezone FROM '2001-01-01 13:00:00+01:15'::timestamptz)
----
-10800

statement ok
SET TIME ZONE +3

