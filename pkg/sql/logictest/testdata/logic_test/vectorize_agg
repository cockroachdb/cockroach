CREATE TABLE bools (a INT, b BOOL)

query BB
SELECT bool_and(b), bool_or(b) FROM bools
----
NULL NULL

query BB
SELECT bool_and(b), bool_or(b) FROM bools GROUP BY a
----

statement ok
