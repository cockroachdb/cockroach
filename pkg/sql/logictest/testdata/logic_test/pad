abcxyx

query T
SELECT RPAD('abc', 7, 'xy')
----
abcxyxy

query T
SELECT RPAD('abc', 1)
----
a

query T
SELECT RPAD('abc', 2)
----
ab

query T
SELECT RPAD('abc', 3)
