  c.relname,
  a.attname,
  c.oid,
  a.attnum,
  pg_relation_is_updatable(c.oid, false),
  pg_column_is_updatable(c.oid, a.attnum, false)
FROM pg_class c
JOIN pg_attribute a ON a.attrelid = c.oid
WHERE c.relname IN ('is_updatable', 'is_updatable_view', 'pg_class')
ORDER BY c.oid, a.attnum
