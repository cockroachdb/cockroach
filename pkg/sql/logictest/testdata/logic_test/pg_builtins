JOIN pg_type t
ON a.atttypid = t.oid
WHERE a.attname IN ('ufoo', 'ubar')
ORDER BY a.attname
----
attname  typname   information_schema._pg_interval_type
ubar     interval  NULL
ufoo     interval  NULL

subtest end
