statement ok
CREATE TABLE stock (item, quantity) AS VALUES ('cups', 10), ('plates', 15), ('forks', 30)

statement count 3
SELECT * FROM stock

statement ok
CREATE TABLE runningOut AS SELECT * FROM stock WHERE quantity < 12

statement count 1
SELECT * FROM runningOut

query TI
SELECT * FROM runningOut
----
cups 10

statement ok
