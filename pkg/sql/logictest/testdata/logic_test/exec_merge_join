# LogicTest: local-vec

# Basic tables, no nulls

statement ok
CREATE TABLE t1 (k INT PRIMARY KEY, v INT)

statement ok
INSERT INTO t1 VALUES (-1, -1), (0, 4), (2, 1), (3, 4), (5, 4)

statement ok
CREATE TABLE t2 (x INT, y INT, INDEX x (x))

statement ok
INSERT INTO t2 VALUES (0, 5), (1, 3), (1, 4), (3, 2), (3, 3), (4, 6)

query IIII
SELECT k, v, x, y FROM t1 INNER MERGE JOIN t2 ON k = x
----
0 4 0 5
3 4 3 2
3 4 3 3

statement ok
DROP TABLE t1

statement ok
DROP TABLE t2

# Basic tables with nulls

statement ok
CREATE TABLE t1 (k INT, INDEX k(k))

statement ok
INSERT INTO t1 VALUES (0), (null)

statement ok
CREATE TABLE t2 (x INT, INDEX x (x))

statement ok
INSERT INTO t2 VALUES (0), (null)

query II
SELECT k, x FROM t1 INNER MERGE JOIN t2 ON k = x
----
0 0

# Test to make sure that vectorized engine is used when merge joiner is
# streaming when vectorize=auto (when we're doing a local computation).
statement ok
SET vectorize=auto; SET vectorize_row_count_threshold=0

statement ok
CREATE TABLE t (k INT PRIMARY KEY)

statement ok
INSERT INTO t VALUES (1), (2), (3)

query T
SELECT url FROM [EXPLAIN ANALYZE SELECT t1.k FROM t AS t1, t AS t2 WHERE t1.k = t2.k]
----
https://cockroachdb.github.io/distsqlplan/decode.html#eJzMkk9v00AQxe98itGcWrGkXue2UqUEMCKQJiWJxJ_Kh613SFdde83uGCWK8t2R7UhtqhZUuHDbnXlvPO_n3WH84VDhMptmb1bQBAfvFvMLuMq-XE7HkxmMZ-Pp128ZnLydLFfLT9NTOEhZDm57LcN4CSzF4ZDC5_fZIusF58Dp4DZHgZU3NNMlRVRXKDEXWAdfUIw-tKVdJ5iYDapEoK3qhttyLrDwgVDtkC07QoUrfe1oQdpQOEtQoCHW1nVj62BLHbYjRoHLWldRwSsUeK25uKEIvuG6YQUSBXJTu3ulIQqM5Khg-9PyVkEySNrhkbVzwLYkBUnEfC-wtxyWi6zXhEruxd8FkP9JgPTJAHd7N5UPhgKZo53z1vknySMULiis6YO3FYWz9JiCo-98MpIvT8-DXd_0RxQ4b3OO5L_xoA0VDVtf3TERWOoNlFT6sAXtnC80k1EwTOCjff0ksuFz_vmCYu2rSA_RPTo5aXmRWVPPP_omFHQZfNF9pr_OO19XMBS576b9ZVJ1re5R3jfLZ5jTh-b0t-bhkTnZ5_sXvwIAAP__q8Ro7w==

statement ok
RESET vectorize; RESET vectorize_row_count_threshold

# Regression test for the inputs that have comparable but different types (see
# issue #44798).
statement ok
CREATE TABLE t44798_0(c0 INT4 PRIMARY KEY); CREATE TABLE t44798_1(c0 INT8 PRIMARY KEY)

statement ok
INSERT INTO t44798_0(c0) VALUES(0), (1), (2); INSERT INTO t44798_1(c0) VALUES(0), (2), (4)

# Note that integers of different width are still considered equal.
query I
SELECT * FROM t44798_0 NATURAL JOIN t44798_1
----
0
2
