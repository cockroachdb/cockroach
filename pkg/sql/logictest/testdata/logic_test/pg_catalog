SELECT * FROM pg_seclabel

statement ok
SELECT count(*) FROM pg_depend

statement ok
SET stub_catalog_tables=true

statement ok
SELECT * FROM pg_seclabel

statement ok
CREATE SEQUENCE serial START 101 INCREMENT 5

