query TT colnames
SHOW CREATE TABLE pg_catalog.pg_namespace
----
Table                    CreateTable
pg_catalog.pg_namespace  CREATE TABLE pg_namespace (
                             oid OID NULL,
                             nspname NAME NOT NULL,
                             nspowner OID NULL,
                             nspacl STRING[] NULL
)

