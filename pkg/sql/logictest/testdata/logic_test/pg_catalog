SELECT
  pg_database.datname,
  pg_authid.rolname,
  pg_shdepend.deptype
FROM pg_authid
JOIN pg_shdepend ON pg_authid.oid = pg_shdepend.refobjid
JOIN pg_database ON pg_shdepend.objid = pg_database.oid
