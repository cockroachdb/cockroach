JOIN pg_operator c ON c.oprname = '>' AND b.proargtypes[0] = c.oprleft AND b.proargtypes[0] = c.oprright
WHERE (b.proname = 'max' OR b.proname = 'bool_or') AND c.oid = a.aggsortop;
