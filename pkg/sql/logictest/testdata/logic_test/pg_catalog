# Test that pg_shadow doesn't include roles that can't login
query B colnames
SELECT (count(1) = 0) AS PASSED
FROM pg_shadow
WHERE EXISTS (
  SELECT 1 FROM pg_authid WHERE oid = usesysid AND NOT rolcanlogin
)
----
passed
true

