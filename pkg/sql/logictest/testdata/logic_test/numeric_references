# LogicTest: local-opt local

statement ok
CREATE TABLE x (a INT PRIMARY KEY, xx INT, b INT, c INT, INDEX bc (b,c))

statement ok
INSERT INTO x VALUES (1), (2), (3)

statement ok
CREATE VIEW view_ref AS SELECT a, 1 FROM x

statement error cannot specify an explicit column list when accessing a view by reference
SELECT * FROM [54(1) AS _]

query II
SELECT * FROM [54 AS _]
----
1  1
2  1
3  1

query I
SELECT foo.a FROM [54 AS foo]
----
1
2
3

statement ok
CREATE SEQUENCE seq

query IIB
SELECT * FROM [55 AS _]
----
0 0 true

# Col refs in sequences are ignored.
query IIB
SELECT * FROM [55(1) AS _]
----
0 0 true

query IIB
SELECT * FROM [55(1, 2) AS _]
----
0 0 true
