SELECT y, str, REPEAT('test', y) FROM NumToStr ORDER BY str

# Verify we use the "top K" strategy and thus don't hit OOM as above.
statement ok
SELECT y, str, REPEAT('test', y) FROM NumToStr ORDER BY str LIMIT 10
