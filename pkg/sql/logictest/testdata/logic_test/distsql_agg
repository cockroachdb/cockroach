# Test that orderings on GROUP BY columns are propagated through aggregations.
statement ok
CREATE TABLE sorted_data (a INT PRIMARY KEY, b INT, c FLOAT, INDEX foo(b))

statement ok
INSERT INTO sorted_data VALUES
(1, 4, 5.0),
(2, 3, 3.4),
(3, 9, 2.2),
(4, 13, 1.99),
(5, 2, 5.7),
(6, 7, 6.2),
(7, 9, 8.9),
(8, 1, 1.22),
(9, -2, 23.0),
(10, 100, -3.1)

# Split into ten parts.
statement ok
ALTER TABLE sorted_data SPLIT AT SELECT i FROM GENERATE_SERIES(1, 9) AS g(i)

# Relocate the ten parts to the five nodes.
statement ok
ALTER TABLE sorted_data TESTING_RELOCATE
  SELECT ARRAY[i%5+1], i FROM GENERATE_SERIES(0, 9) AS g(i)

# The ordering is on all the GROUP BY columns, and isn't preserved after the
# aggregation.
query II rowsort
SELECT a, max(b) FROM sorted_data GROUP BY a
----
6   7
7   9
10  100
1   4
2   3
3   9
4   13
5   2
8   1
9   -2

query T
SELECT "URL" FROM [EXPLAIN (DISTSQL) SELECT a, max(b) FROM sorted_data GROUP BY a]
----
https://cockroachdb.github.io/distsqlplan/decode.html?eJzUlk1r20AQhu_9FWFOCd1CVpLdRCf5GEjlElJoKcJsvINicLRidwUNwf-9yDookd0Zm4XaOurjmXc2j3jjN6iMxly9oIP0N0gQEIGAGAQkIGAChYDamiU6Z2z7Sgfc6T-QXgtYVXXj29uFgKWxCOkb-JVfI6TwqJ7W-IBKowUBGr1arbchtV29KPuaOWM96oVWXoGAeePTi0yKLIJiI8A0vh_tvCoRUrkRh8fPytJiqbwZpGftGedWo0XdBoKAWf5rkc8fF_mP-_vLTF6BgG-zn5dZdDVYpZ_-9HrxrNzzzuhi068b_XPdfo7pFhnO-dwN-h9n6veND9i3qfZtvHfZ3Hwx9eC1_cHJh2B52s-KiT-3z0qO7LOKTmuXiT83u9HI7MantcvEn5vdeGR2k9PaZeLPzW4yMrvM76kHdLWpHB70H_26PQzqErvDO9PYJX63ZrmN6S7nW257Q6Pz3VPZXdxV3aN2wfewHMLyPRx9gOVx8DQEvg2BZdDeckLTEfn3jmk4pmVNaVsJSU9oeBKimoYZ1TTMqKZhTjVDM6qnIaq_kvANLesmRBYNM7JomJFFw5wshmZk3YbIkkyLcjUa1qNhRRrWpIFVGtalMqhMJdOmCSNtp06PkkbTnDSa5qTRNCuNwTlpO6VKSis2n_4GAAD__yWoNTs=

# The ordering is on all the GROUP BY columns, and is preserved after the
# aggregation.
query II
SELECT a, max(b) FROM sorted_data GROUP BY a ORDER BY a
----
1   4
2   3
3   9
4   13
5   2
6   7
7   9
8   1
9   -2
10  100

query T
SELECT "URL" FROM [EXPLAIN (DISTSQL) SELECT a, max(b) FROM sorted_data GROUP BY a ORDER BY a]
----
