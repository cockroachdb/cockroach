# LogicTest: default parallel-stmts

statement error pgcode 42P01 relation "kv" does not exist
INSERT INTO kv VALUES ('a', 'b')

statement ok
CREATE TABLE kv (
  k CHAR PRIMARY KEY,
  v CHAR,
  UNIQUE INDEX a (v),
  FAMILY (k),
  FAMILY (v)
)

query TT
SELECT * FROM kv
----

