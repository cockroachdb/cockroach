CREATE DATABASE d

statement ok
USE d

query T colnames
SHOW CREATE ALL TYPES
----
create_statement

statement ok
CREATE TYPE status AS ENUM ('open', 'closed', 'inactive');

query T colnames
SHOW CREATE ALL TYPES
