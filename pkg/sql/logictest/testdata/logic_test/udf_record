  SELECT 1, 2.0, 'welcome roacher', '2021-07-12 09:02:10-08:00'::TIMESTAMPTZ;
$$ LANGUAGE SQL;

query T
SELECT f_const();
----
(1,2.0,"welcome roacher","2021-07-12 17:02:10+00")

