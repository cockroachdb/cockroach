# Regression test for #23001.

statement ok
CREATE TABLE tab0(pk INTEGER PRIMARY KEY, a INTEGER, b INTEGER);

statement ok
INSERT INTO tab0 VALUES(0,1,2);

statement ok
CREATE INDEX on tab0 (a);

query III
SELECT pk, a, b FROM tab0 WHERE a < 10 AND b = 2 ORDER BY a DESC, pk;
----
0 1 2
