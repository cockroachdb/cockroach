# Regression test for #46217. Histogram type doesn't match column type.
statement ok
CREATE TABLE ts (d DATE PRIMARY KEY, x INT);

statement ok
ALTER TABLE ts INJECT STATISTICS '[
  {
    "columns": ["d"],
    "created_at": "2020-03-24 15:34:22.863634+00:00",
    "distinct_count": 1000,
    "histo_buckets": [
      {
        "distinct_range": 0,
        "num_eq": 1,
        "num_range": 0,
