# DE-9IM relations
query B
SELECT
  ST_RelateMatch(m, pattern)
FROM ( VALUES
  ('101202FFF', 'TTTTTTFFF'),
  ('101202FTF', 'TTTTTTFFF')
) tbl(m, pattern)
----
true
false

query TTTTB
SELECT
  a.dsc,
  b.dsc,
  ST_Relate(a.geom, b.geom),
  ST_Relate(a.geom, b.geom, 3),
  ST_Relate(a.geom, b.geom, 'T**FF*FF*')
FROM geom_operators_test a
JOIN geom_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
