subtest st_summary

query T
SELECT ST_Summary('POINT(0 0)'::geometry)
----
Point[B]

query T
SELECT ST_Summary('SRID=4326;POINT(0 0)'::geometry)
----
Point[BS]

query T
SELECT ST_Summary('POINT(0 0 0)'::geometry)
----
Point[B]

query T
SELECT ST_Summary('POINT(0 0 0 0)'::geometry)
----
Point[B]

