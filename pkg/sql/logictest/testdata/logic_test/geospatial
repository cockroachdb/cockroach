# DE-9IM relations
query TTTB
SELECT
  a.dsc,
  b.dsc,
  ST_Relate(a.geom, b.geom),
  ST_Relate(a.geom, b.geom, 'T**FF*FF*')
FROM geom_operators_test a
JOIN geom_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
----
