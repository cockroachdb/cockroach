# Binary operators
query TTRRR
SELECT
  a.dsc,
  b.dsc,
  ST_Distance(a.geog, b.geog), ST_Distance(a.geog, b.geog, false), ST_Distance(a.geog, b.geog, true)
FROM geog_operators_test a
JOIN geog_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
