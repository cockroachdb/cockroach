query TTTT
SELECT
  dsc,
  ST_AsEWKT(ST_CollectionExtract(geom, 1)),
  ST_AsEWKT(ST_CollectionExtract(geom, 2)),
  ST_AsEWKT(ST_CollectionExtract(geom, 3))
FROM geom_operators_test
ORDER BY dsc ASC
