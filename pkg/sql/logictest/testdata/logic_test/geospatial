query TTRR
SELECT
  a.dsc,
  b.dsc,
  ST_HausdorffDistance(a.geom, b.geom),
  ST_HausdorffDistance(a.geom, b.geom, 0.4)
FROM geom_operators_test a
JOIN geom_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
