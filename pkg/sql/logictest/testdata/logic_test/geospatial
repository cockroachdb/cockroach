query TTBBB
SELECT
  a.dsc,
  b.dsc,
  _ST_DWithin(a.geog, b.geog, 70558),
  _ST_DWithin(a.geog, b.geog, 70558, false),
  _ST_DWithin(a.geog, b.geog, 70558, true)
FROM geog_operators_test a
JOIN geog_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
