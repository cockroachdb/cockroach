# ST_LongestLine, ST_ShortestLine
query TTTT
SELECT
  a.dsc,
  b.dsc,
  ST_AsText(ST_LongestLine(a.geom, b.geom)),
  ST_AsText(ST_ShortestLine(a.geom, b.geom))
FROM geom_operators_test a
JOIN geom_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
