# Binary operators
query TTRR
SELECT
  a.dsc,
  b.dsc,
  ST_Distance(a.geom, b.geom),
  ST_MaxDistance(a.geom, b.geom)
FROM geom_operators_test a
JOIN geom_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
