CREATE TABLE public.geom_table_negative_values (
   a GEOMETRY(GEOMETRY) NULL,
   rowid INT8 NOT VISIBLE NOT NULL DEFAULT unique_rowid(),
   CONSTRAINT "primary" PRIMARY KEY (rowid ASC),
   FAMILY "primary" (a, rowid)
)

