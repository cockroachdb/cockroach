# ST_Buffer
query TTTT
SELECT
  a.dsc,
  ST_AsEWKT(ST_Buffer(a.geog, 10), 5),
  ST_AsEWKT(ST_Buffer(a.geog, 10, 2), 5),
  ST_AsEWKT(ST_Buffer(a.geog, 10, 'quad_segs=4 endcap=flat'), 5)
FROM geog_operators_test a
ORDER BY a.dsc
