query TTT
SELECT
  a.dsc,
  ST_AsEWKT(ST_Simplify(a.geom, 0.2)),
  ST_AsEWKT(ST_SimplifyPreserveTopology(a.geom, 0.2))
FROM geom_operators_test a
ORDER BY a.dsc
----
