# Binary predicates
query TTBBB
SELECT
  a.dsc,
  b.dsc,
  ST_Covers(a.geog, b.geog),
  ST_CoveredBy(a.geog, b.geog),
  ST_Intersects(a.geog, b.geog)
FROM geog_operators_test a
JOIN geog_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
