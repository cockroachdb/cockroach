query TTIIITTT
SELECT
  a.dsc,
  ST_GeometryType(a.geom),
  ST_NDims(a.geom),
  ST_NPoints(a.geom),
  ST_NumGeometries(a.geom),
  ST_GeometryN(a.geom, 0),
  ST_GeometryN(a.geom, 1),
  ST_GeometryN(a.geom, 2)
FROM geom_operators_test a
ORDER BY a.dsc
