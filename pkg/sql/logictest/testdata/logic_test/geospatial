query TT
SELECT
 ST_AsEWKT(ST_SymDifference(a.geom, b.geom)),
 ST_AsEWKT(ST_SymmetricDifference(a.geom, b.geom))
FROM geom_operators_test a
JOIN geom_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
