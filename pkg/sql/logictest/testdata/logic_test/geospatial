# DWithin
query TTBBB
SELECT
  a.dsc,
  b.dsc,
  ST_DWithin(a.geog, b.geog, 70558),
  ST_DWithin(a.geog, b.geog, 70558, false),
  ST_DWithin(a.geog, b.geog, 70558, true)
FROM geog_operators_test a
JOIN geog_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
