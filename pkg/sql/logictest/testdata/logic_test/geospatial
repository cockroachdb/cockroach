# DWithin
query TTBB
SELECT
  a.dsc,
  b.dsc,
  ST_DWithin(a.geom, b.geom, 1),
  ST_DFullyWithin(a.geom, b.geom, 1)
FROM geom_operators_test a
JOIN geom_operators_test b ON (1=1)
ORDER BY a.dsc, b.dsc
