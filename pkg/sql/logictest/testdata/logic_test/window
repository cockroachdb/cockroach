statement ok
CREATE TABLE kv (
  -- don't add column "a"
  k INT PRIMARY KEY,
  v INT,
  w INT,
  f FLOAT,
  d DECIMAL,
  s STRING,
  b BOOL,
  i INTERVAL,
  FAMILY (k, v, w, f, b),
  FAMILY (d),
  FAMILY (s)
)

