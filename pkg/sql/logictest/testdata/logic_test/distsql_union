# EXCEPT ALL with compatible ORDER BY columns that are not in the final result.
query T
SELECT "URL" FROM [EXPLAIN (DISTSQL) (SELECT y FROM xyz ORDER BY z) EXCEPT ALL (SELECT y FROM xyz ORDER BY z)]
