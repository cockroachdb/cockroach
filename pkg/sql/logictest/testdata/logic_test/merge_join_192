# LogicTest: local

# Test to make sure that vectorized engine is used when merge joiner is
# streaming when vectorize=201auto.
statement ok
SET vectorize='201auto'; SET vectorize_row_count_threshold=0

statement ok
CREATE TABLE t1 (k INT PRIMARY KEY);
INSERT INTO t1 VALUES (1), (2), (3);
CREATE TABLE t2 (k INT PRIMARY KEY);
INSERT INTO t2 VALUES (1), (2), (3)

query T
SELECT url FROM [EXPLAIN ANALYZE SELECT t1.k FROM t1, t2 WHERE t1.k = t2.k]
