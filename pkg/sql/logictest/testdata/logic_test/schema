user testuser

statement error pq: user testuser does not have CREATE privilege on schema privs
CREATE TABLE test.privs.fail_tbl();

statement error pq: user testuser does not have CREATE privilege on schema privs
CREATE TABLE otherdb.privs.fail_tbl();

