(DATE '2000-01-01',
INTERVAL '1 day')
OVERLAPS
(DATE '2000-01-02',
INTERVAL '2 days');
----
false

query T
SELECT
(NULL,
INTERVAL '1 day')
OVERLAPS
(DATE '2000-01-02',
INTERVAL '2 days');
----
NULL

query T
SELECT
(DATE '2000-01-01',
INTERVAL '1 day')
OVERLAPS
(NULL,
INTERVAL '2 days');
----
NULL

query T
SELECT
(NULL,
NULL)
OVERLAPS
(NULL,
INTERVAL '2 days');
----
NULL

query error pq: overlaps\(\): function overlaps\(unknown, unknown, unknown, unknown\) is not unique
SELECT
(NULL,
NULL)
OVERLAPS
(NULL,
NULL);

query error pq: overlaps\(\): wrong number of parameters on left side of OVERLAPS expression
SELECT
(DATE '2000-01-03',
DATE '2000-02-03',
DATE '2000-03-03')
OVERLAPS
(DATE '2000-01-03',
DATE '2000-01-04');

query error pq: overlaps\(\): wrong number of parameters on right side of OVERLAPS expression
SELECT
(DATE '2000-02-03',
DATE '2000-03-03')
OVERLAPS
(DATE '2000-01-03',
DATE '2000-01-03',
DATE '2000-01-04');

query error pq: overlaps\(\): overlaps does not support the current overload
SELECT
(TIMESTAMP '2000-01-01 00:00:00',
TIME '2000-01-01 01:00:00')
OVERLAPS
(TIME '2000-01-01 00:30:00',
TIMESTAMP '2000-01-01 01:30:00');

query B
SELECT
(TIMESTAMP '2000-01-01 00:00:00',
INTERVAL '2 days')
OVERLAPS
(DATE '2000-01-01',
TIMESTAMPTZ '2000-01-01 01:30:00');
----
true

query error pq: overlaps\(\): overlaps does not support the current overload
SELECT
(TIMESTAMP '2000-01-01 00:00:00',
INTERVAL '2 hours')
OVERLAPS
(TIMETZ '01:00:00',
TIMESTAMPTZ '2000-01-01 01:30:00');

query error pq: overlaps\(\): overlaps does not support the current overload
SELECT (
TIMESTAMP '2000-01-01 00:00:00',
INTERVAL '2 hours')
OVERLAPS
(TIMETZ '01:00:00',
TIMETZ '02:00:00');

query error pq: overlaps\(\): overlaps does not support the current overload
SELECT (
DATE '2000-01-01',
INTERVAL '2 hours')
OVERLAPS
(TIMETZ '01:00:00',
TIMETZ '02:00:00');
