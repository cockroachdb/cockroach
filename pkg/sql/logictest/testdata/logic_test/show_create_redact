b  CREATE TABLE public.b (
     b BOOL NULL DEFAULT ‹×›,
     rowid INT8 NOT VISIBLE NOT NULL DEFAULT unique_rowid(),
     CONSTRAINT b_pkey PRIMARY KEY (rowid ASC)
   );

