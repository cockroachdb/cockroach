statement ok
CREATE TABLE t1 (x INT PRIMARY KEY);
CREATE TABLE t2 (x INT PRIMARY KEY);
INSERT INTO t1 VALUES (1), (2);
INSERT INTO t2 VALUES (1), (2);

query OI rowsort
SELECT tableoid, x FROM t1
----
53 1
53 2

query OI rowsort
SELECT tableoid, x FROM t2
----
54 1
54 2

query OIOI rowsort
SELECT t1.tableoid, t1.x, t2.tableoid, t2.x FROM t1 JOIN t2 ON t1.x = t2.x
----
53 1 54 1
53 2 54 2

query OIOI rowsort
SELECT t1.tableoid, t1.x, t2.tableoid, t2.x FROM t1 INNER LOOKUP JOIN t2 ON t1.x = t2.x
----
53 1 54 1
53 2 54 2

# We should be able to produce both system columns.
query OB
SELECT tableoid, crdb_internal_mvcc_timestamp IS NOT NULL FROM t1
----
53 true
53 true
