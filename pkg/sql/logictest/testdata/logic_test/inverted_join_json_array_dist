# This query performs an inverted join with an additional filter.
query T
SELECT url FROM [EXPLAIN (DISTSQL)
SELECT * FROM json_tab@foo_inv AS j1, json_tab AS j2
WHERE j1.b @> j2.b AND j1.b @> '{"a": {}}' AND j2.a < 20
ORDER BY j1.a, j2.a]
----
