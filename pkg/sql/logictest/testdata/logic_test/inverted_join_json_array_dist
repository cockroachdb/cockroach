# This query performs a semi inverted join.
query T
SELECT url FROM [EXPLAIN (DISTSQL)
SELECT a2.* FROM array_tab@primary AS a2 WHERE EXISTS (
  SELECT * FROM array_tab@foo_inv AS a1
  WHERE a1.b @> a2.b
)
ORDER BY a2.a]
----
