SELECT a, b, a < b AND NULL, a > b AND NULL, NULL OR a < b, NULL OR a > b, NULL AND NULL, NULL OR NULL FROM a WHERE a = 0
----
0 0 false false NULL NULL NULL NULL
0 1 NULL  false true NULL NULL NULL

