# LogicTest: local

statement ok
CREATE DATABASE a

statement ok
CREATE USER readwrite

statement ok
GRANT ALL ON DATABASE a TO readwrite

query TTTB colnames,rowsort
SHOW GRANTS ON DATABASE a
----
database_name  grantee    privilege_type  is_grantable
a              admin      ALL             true
a              public     CONNECT         false
a              readwrite  ALL             false
a              root       ALL             true

# Show that by default GRANT is restricted to the current database
query TTTTTTB colnames,rowsort
SHOW GRANTS
