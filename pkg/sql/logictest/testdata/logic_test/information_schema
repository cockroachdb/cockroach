admin     test          pg_catalog   int        ALL             NO
public    test          pg_catalog   int        USAGE           NO
admin     test          public       typ2       ALL             YES
public    test          public       typ2       USAGE           NO
root      test          public       typ2       ALL             YES
testuser  test          public       typ2       ALL             NO

