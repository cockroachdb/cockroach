WHERE table_schema = 'public' AND table_name = 'generated_as_identity'
ORDER BY column_name
----
column_name  is_identity  identity_generation  is_nullable
a            NO           ·                    YES
b            YES          ALWAYS               NO
c            YES          BY DEFAULT           NO
rowid        NO           ·                    NO

