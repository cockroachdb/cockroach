CREATE TABLE xyz (
  x INT PRIMARY KEY,
  y INT,
  z FLOAT,
  w INT,
  INDEX xy (x, y),
  INDEX zyx (z, y, x),
  INDEX w (w),
  FAMILY (x),
  FAMILY (y),
  FAMILY (z)
)

statement ok
