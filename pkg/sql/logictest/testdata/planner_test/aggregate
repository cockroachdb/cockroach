statement ok
CREATE TABLE xyz (
  x INT PRIMARY KEY,
  y INT,
  z FLOAT,
  INDEX xy (x, y),
  INDEX zyx (z, y, x),
  FAMILY (x),
  FAMILY (y),
  FAMILY (z)
)

query TTT
SELECT tree, field, description FROM [
EXPLAIN (VERBOSE) SELECT min(x) FROM xyz
]
----
group           ·            ·
 │              aggregate 0  min(x)
 │              scalar       ·
 └── render     ·            ·
      │         render 0     test.public.xyz.x
      └── scan  ·            ·
·               table        xyz@xy
·               spans        ALL
·               limit        1

query TTT
SELECT tree, field, description FROM [
EXPLAIN (VERBOSE) SELECT min(x) FROM xyz WHERE x in (0, 4, 7)
]
----
