parse
DECLARE
BEGIN
IF x THEN
  COMMIT;
END IF;
END
----
DECLARE
BEGIN
IF x THEN
	COMMIT;
END IF;
END;
 -- normalized!
DECLARE
BEGIN
IF (x) THEN
	COMMIT;
END IF;
END;
 -- fully parenthesized
DECLARE
BEGIN
IF x THEN
	COMMIT;
END IF;
END;
 -- literals removed
DECLARE
BEGIN
IF _ THEN
	COMMIT;
END IF;
END;
 -- identifiers removed

parse
DECLARE
BEGIN
IF x THEN
  ROLLBACK;
END IF;
END
----
DECLARE
BEGIN
IF x THEN
	ROLLBACK;
END IF;
END;
 -- normalized!
DECLARE
BEGIN
IF (x) THEN
	ROLLBACK;
END IF;
END;
 -- fully parenthesized
DECLARE
BEGIN
IF x THEN
	ROLLBACK;
END IF;
END;
 -- literals removed
DECLARE
BEGIN
IF _ THEN
	ROLLBACK;
END IF;
END;
 -- identifiers removed

parse
DECLARE
BEGIN
IF x THEN
  COMMIT;
ELSIF y THEN
  ROLLBACK;
END IF;
END
----
DECLARE
BEGIN
IF x THEN
	COMMIT;
ELSIF y THEN
	ROLLBACK;
END IF;
END;
 -- normalized!
DECLARE
BEGIN
IF (x) THEN
	COMMIT;
ELSIF (y) THEN
	ROLLBACK;
END IF;
END;
 -- fully parenthesized
DECLARE
BEGIN
IF x THEN
	COMMIT;
ELSIF y THEN
	ROLLBACK;
END IF;
END;
 -- literals removed
DECLARE
BEGIN
IF _ THEN
	COMMIT;
ELSIF _ THEN
	ROLLBACK;
END IF;
END;
 -- identifiers removed


parse
DECLARE
BEGIN
  INSERT INTO t1 VALUES (1,2) RETURNING x INTO y;
  COMMIT;
END
----
DECLARE
BEGIN
INSERT INTO t1 VALUES (1, 2) RETURNING x INTO y;
COMMIT;
END;
 -- normalized!
DECLARE
BEGIN
INSERT INTO t1 VALUES ((1), (2)) RETURNING (x) INTO y;
COMMIT;
END;
 -- fully parenthesized
DECLARE
BEGIN
INSERT INTO t1 VALUES (_, _) RETURNING x INTO y;
COMMIT;
END;
 -- literals removed
DECLARE
BEGIN
INSERT INTO _ VALUES (1, 2) RETURNING _ INTO _;
COMMIT;
END;
 -- identifiers removed
