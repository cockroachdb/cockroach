parse
DECLARE
BEGIN
  RAISE EXCEPTION USING MESSAGE = "why is this so involved?";
END
----
DECLARE
BEGIN
RAISE EXCEPTION
USING MESSAGE = "why is this so involved?";
END;
 -- normalized!
DECLARE
BEGIN
RAISE EXCEPTION
USING MESSAGE = ("why is this so involved?");
END;
 -- fully parenthesized
DECLARE
BEGIN
RAISE EXCEPTION
USING MESSAGE = "why is this so involved?";
END;
 -- literals removed
DECLARE
BEGIN
RAISE EXCEPTION
USING MESSAGE = _;
END;
 -- identifiers removed

