BEGIN
RAISE LOG 'Nonexistent ID --> %', user_id;
END;
 -- normalized!
DECLARE
BEGIN
RAISE LOG 'Nonexistent ID --> %', (user_id);
END;
 -- fully parenthesized
DECLARE
BEGIN
RAISE LOG '_', user_id;
END;
 -- literals removed
DECLARE
BEGIN
RAISE LOG 'Nonexistent ID --> %', _;
END;
 -- identifiers removed

parse
DECLARE
BEGIN
  RAISE LOG 'Nonexistent ID --> %', user_id
  USING HINT = "check...userid?" ;
END
----
