----
30.0

eval
extract(second from '12:00:30+01'::timetz)
----
30.0

eval
extract(millisecond from '12:00:00.123456+01'::timetz)
----
123.456

eval
extract(microsecond from '12:00:00.123456+01'::timetz)
----
123456.0
