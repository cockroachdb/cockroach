# String concatenation.

eval
'a' || 'b'
----
'ab'

eval
'a' || (1 + 2)::char
----
'a3'

eval
b'hello' || b'world'
----
'\x68656c6c6f776f726c64'

eval
b'hello' || 'world'
----
'\x68656c6c6f776f726c64'

eval
array['foo'] || '{a,b}'
----
ARRAY['foo','a','b']

# String || any; any || String

eval
'b' || (5)::char || (8)::char || 'c'
----
'b58c'

eval
3 || 'a' || 3
----
3a3

eval
3::oid || 'a' || 3::oid
----
3a3

eval
3.33 || 'a' || 3.33
----
3.33a3.33
