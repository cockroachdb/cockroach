# Need two interval ops to verify the return type matches the return struct type.
eval
'2010-09-28 12:00:00.1-04:00'::timestamptz - '0s'::interval - '0s'::interval
----
'2010-09-28 12:00:00.1-04:00'

eval
'12h2m1s23ms'::interval + '1h'::interval
----
'13h2m1s23ms'

eval
'12 hours 2 minutes 1 second'::interval + '1h'::interval
----
'13h2m1s'

eval
'PT12H2M1S'::interval + '1h'::interval
----
'13h2m1s'

eval
'12:02:01'::interval + '1h'::interval
----
'13h2m1s'

eval
'12h2m1s23ms'::interval - '1h'::interval
----
'11h2m1s23ms'

eval
'12 hours 2 minutes 1 second'::interval - '1h'::interval
----
'11h2m1s'

eval
'PT12H2M1S'::interval - '1h'::interval
----
'11h2m1s'

eval
'1h'::interval - '12h2m1s23ms'::interval
----
'-11h-2m-1s-23ms'

eval
'PT1H'::interval - '12h2m1s23ms'::interval
----
'-11h-2m-1s-23ms'

eval
'1 hour'::interval - '12h2m1s23ms'::interval
----
'-11h-2m-1s-23ms'

eval
3 * '1h2m'::interval * 3
----
'9h18m'

eval
3 * '1 hour 2 minutes'::interval * 3
----
'9h18m'

eval
3 * 'PT1H2M'::interval * 3
----
'9h18m'

eval
'3h'::interval / 2
----
'1h30m'

eval
'PT3H'::interval / 2
----
'1h30m'

eval
'3:00'::interval / 2
----
'1h30m'

eval
'3 hours'::interval / 2
----
'1h30m'

eval
'3 hours'::interval * 2.5
----
'7h30m'

eval
'3 hours'::interval / 2.5
----
'1h12m'

eval
'1h2m'::interval * 4.5
----
'4h39m'

eval
4.5 * '1h2m'::interval
----
'4h39m'

eval
5.0 * '1h2m'::interval
----
'5h10m'

eval
'1h2m'::interval * 5.0
----
'5h10m'

eval
'1h2m'::interval * 0.0
----
'0s'

eval
00.0 * '1h2m'::interval
----
'0s'
