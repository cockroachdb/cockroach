query R
SELECT * FROM f;
----
10
1.5
1

statement ok
CREATE TABLE i (x INT);

statement error value type decimal doesn't match type INT of column "x"
INSERT INTO i(x) VALUES (4.5);

statement ok
INSERT INTO i(x) VALUES (((9 / 3) * (1 / 3))), (2.0), (2.4 + 4.6);

query I
SELECT * FROM i;
----
1
2
7

statement ok
