statement ok
CREATE TABLE t (
  k INT PRIMARY KEY,
  v INT
)

statement ok
INSERT INTO t VALUES (1, 2)

query ITT colnames
EXPLAIN SELECT * FROM t
----
Level  Type  Description
0      scan  t@primary

query ITT colnames
EXPLAIN (VERBOSE) SELECT * FROM t
----
Level  Type   Description
0      select (k, v)@t 
1      scan   t@primary


query ITT colnames
EXPLAIN SELECT * FROM t WHERE k = 1 OR k = 3
----
Level  Type  Description
0      scan  t@primary /1-/2 /3-/4

query ITT colnames
EXPLAIN VALUES (1, 2, 3), (4, 5, 6)
----
Level  Type    Description
0      values  3 columns, 2 rows

query ITT colnames
EXPLAIN VALUES (1)
----
Level  Type    Description
0      values  1 column, 1 row

query ITT colnames
EXPLAIN SELECT * FROM t LIMIT 1 OFFSET 1
----
Level  Type    Description
0      limit   count: 1, offset:  1
1      scan    t@primary

query ITT colnames
EXPLAIN SELECT DISTINCT * FROM t
----
Level  Type     Description
0      distinct k
1      scan     t@primary

query ITT colnames
EXPLAIN SELECT DISTINCT * FROM t LIMIT 1 OFFSET 1
----
Level  Type     Description
0      limit    count: 1, offset:  1
1      distinct k
2      scan     t@primary
