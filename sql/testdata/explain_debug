CREATE TABLE abc (
  a INT,
  b TEXT,
  c FLOAT,
  PRIMARY KEY (a, b),
  UNIQUE INDEX foo (b),
  INDEX bar (a)
)

query ITTT colnames
EXPLAIN (DEBUG) SELECT * FROM abc
----
RowIdx  Key  Value  Disposition

