SELECT (1, 2) > (1, 2, 3)

statement ok
CREATE TABLE t (a int, b int, c int)

statement ok
INSERT INTO t VALUES (1, 2, 3), (2, 3, 1), (3, 1, 2)

query III
SELECT * FROM t ORDER BY a, b, c
----
1 2 3
2 3 1
3 1 2

query III
SELECT * FROM t WHERE (a, b, c) > (1, 2, 3) ORDER BY a, b, c
----
