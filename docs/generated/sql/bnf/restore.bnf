restore_stmt ::=
	'RESTORE' 'FROM' partitioned_backup_list opt_as_of_clause opt_with_restore_options
	| 'RESTORE' ( ( 'TABLE' | ) table_pattern ( ( ',' table_pattern ) )* | 'DATABASE' database_name ( ( ',' database_name ) )* ) 'FROM' partitioned_backup_list opt_as_of_clause opt_with_restore_options
