show_backup_stmt ::=
	'SHOW' 'BACKUPS' 'IN' location
	| 'SHOW' 'BACKUP' show_backup_kind string_or_placeholder 'IN' string_or_placeholder 'WITH' kv_option_list
	| 'SHOW' 'BACKUP' show_backup_kind string_or_placeholder 'IN' string_or_placeholder 'WITH' 'OPTIONS' '(' kv_option_list ')'
	| 'SHOW' 'BACKUP' show_backup_kind string_or_placeholder 'IN' string_or_placeholder 
	| 'SHOW' 'BACKUP' show_backup_kind string_or_placeholder 'WITH' kv_option_list
	| 'SHOW' 'BACKUP' show_backup_kind string_or_placeholder 'WITH' 'OPTIONS' '(' kv_option_list ')'
	| 'SHOW' 'BACKUP' show_backup_kind string_or_placeholder 
