revoke_stmt ::=
	'REVOKE' 'ALL' 'ON' table_name ( ',' table_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'ALL' 'ON' 'TABLE' table_name ( ',' table_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'ALL' 'ON' 'DATABASE' database_name ( ',' database_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' name ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'ON' table_name ( ',' table_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' name ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'ON' 'TABLE' table_name ( ',' table_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' name ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'ON' 'DATABASE' database_name ( ',' database_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'CREATE' ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'ON' table_name ( ',' table_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'CREATE' ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'ON' 'TABLE' table_name ( ',' table_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'CREATE' ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'ON' 'DATABASE' database_name ( ',' database_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'SELECT' ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'ON' table_name ( ',' table_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'SELECT' ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'ON' 'TABLE' table_name ( ',' table_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'SELECT' ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'ON' 'DATABASE' database_name ( ',' database_name )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' name ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'CREATE' ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'SELECT' ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'ADMIN' 'OPTION' 'FOR' name ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'ADMIN' 'OPTION' 'FOR' 'CREATE' ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'FROM' database_name ( ',' database_name )*
	| 'REVOKE' 'ADMIN' 'OPTION' 'FOR' 'SELECT' ( ( ',' ( name | 'CREATE' | 'SELECT' ) ) )* 'FROM' database_name ( ',' database_name )*
