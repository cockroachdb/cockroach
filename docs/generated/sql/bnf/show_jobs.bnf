	'SHOW' 'AUTOMATIC' 'JOBS'
	| 'SHOW' 'JOBS'
	| 'SHOW' 'CHANGEFEED' 'JOBS'
	| 'SHOW' 'JOBS' select_stmt
	| 'SHOW' 'JOBS' 'WHEN' 'COMPLETE' select_stmt
