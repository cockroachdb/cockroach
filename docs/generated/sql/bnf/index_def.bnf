index_def ::=
	'INDEX' opt_index_name '(' index_elem ( ( ',' index_elem ) )* ')' 'COVERING' '(' storing_items ')' opt_interleave opt_partition_by
	| 'INDEX' opt_index_name '(' index_elem ( ( ',' index_elem ) )* ')' 'STORING' '(' storing_items ')' opt_interleave opt_partition_by
	| 'INDEX' opt_index_name '(' index_elem ( ( ',' index_elem ) )* ')'  opt_interleave opt_partition_by
	| 'UNIQUE' 'INDEX' opt_index_name '(' index_elem ( ( ',' index_elem ) )* ')' 'COVERING' '(' storing_items ')' opt_interleave opt_partition_by
	| 'UNIQUE' 'INDEX' opt_index_name '(' index_elem ( ( ',' index_elem ) )* ')' 'STORING' '(' storing_items ')' opt_interleave opt_partition_by
	| 'UNIQUE' 'INDEX' opt_index_name '(' index_elem ( ( ',' index_elem ) )* ')'  opt_interleave opt_partition_by
	| 'INVERTED' 'INDEX' name '(' index_elem ( ( ',' index_elem ) )* ')'
	| 'INVERTED' 'INDEX'  '(' index_elem ( ( ',' index_elem ) )* ')'
