import_stmt ::=
	'IMPORT' import_format uri 'WITH' kv_option_list
	| 'IMPORT' import_format uri 
	| 'IMPORT' 'TABLE' table_name 'FROM' import_format uri 'WITH' kv_option_list
	| 'IMPORT' 'TABLE' table_name 'FROM' import_format uri 
	| 'IMPORT' 'INTO' table_name '(' insert_column_list ')' import_format 'DATA' '(' uris ')' 'WITH' kv_option_list
	| 'IMPORT' 'INTO' table_name '(' insert_column_list ')' import_format 'DATA' '(' uris ')' 
	| 'IMPORT' 'INTO' table_name import_format 'DATA' '(' uris ')' 'WITH' kv_option_list
	| 'IMPORT' 'INTO' table_name import_format 'DATA' '(' uris ')' 
