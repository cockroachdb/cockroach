create_table_stmt ::=
	'CREATE' 'TABLE' table_name '(' table_definition ')' 'INTERLEAVE' 'IN' 'PARENT' table_name '(' parent_table_list ')' opt_partition_by
	| 'CREATE' 'TABLE' table_name '(' table_definition ')'  opt_partition_by
	| 'CREATE' 'TABLE' 'IF' 'NOT' 'EXISTS' table_name '(' table_definition ')' 'INTERLEAVE' 'IN' 'PARENT' table_name '(' parent_table_list ')' opt_partition_by
	| 'CREATE' 'TABLE' 'IF' 'NOT' 'EXISTS' table_name '(' table_definition ')'  opt_partition_by
