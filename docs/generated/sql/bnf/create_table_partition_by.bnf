	| 'CREATE' opt_persistence_temp_table 'TABLE' 'IF' 'NOT' 'EXISTS' table_name '(' table_definition ')'  'PARTITION' 'BY' 'RANGE' '(' name_list ')' '(' range_partitions ')' opt_table_with opt_create_table_on_commit
