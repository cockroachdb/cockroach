alter_rename_index_stmt ::=
	'ALTER' 'INDEX' table_name '@' unrestricted_name 'RENAME' 'TO' unrestricted_name
	| 'ALTER' 'INDEX' table_name 'RENAME' 'TO' unrestricted_name
	| 'ALTER' 'INDEX' 'IF' 'EXISTS' table_name '@' unrestricted_name 'RENAME' 'TO' unrestricted_name
	| 'ALTER' 'INDEX' 'IF' 'EXISTS' table_name 'RENAME' 'TO' unrestricted_name
