	'CREATE' 'TABLE' table_name '(' table_definition ')' 'INTERLEAVE' 'IN' 'PARENT' qualified_name '(' parent_table_list ')'
	| 'CREATE' 'TABLE' table_name '(' table_definition ')' 
	| 'CREATE' 'TABLE' 'IF' 'NOT' 'EXISTS' table_name '(' table_definition ')' 'INTERLEAVE' 'IN' 'PARENT' qualified_name '(' parent_table_list ')'
	| 'CREATE' 'TABLE' 'IF' 'NOT' 'EXISTS' table_name '(' table_definition ')' 
