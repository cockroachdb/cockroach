      <title>Snapshots</title>
      <chart>
        <search>
          <query>| mstats 
rate_sum(range_snapshots_generated) as "Generated"   
rate_sum(range_snapshots_applied_voter) as "Applied (Voters)"   
rate_sum(range_snapshots_applied_initial) as "Applied (Initial Upreplication)"   
