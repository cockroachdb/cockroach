        {
          "datasource": {
            "uid": "${DS_PROMETHEUS}"
          },
          "exemplar": true,
          "expr": "sum(sum(capacity_used{job=\"cockroachdb\",cluster=~\"$cluster\",instance=~\"$node\"}) by (instance))",
          "interval": "",
          "intervalFactor": 2,
          "legendFormat": "Used",
          "refId": "A"
        }
      ],
      "title": "Capacity",
      "type": "timeseries"
    }
